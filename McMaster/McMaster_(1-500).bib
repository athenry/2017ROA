Title = {{Surface modification and electrophoretic deposition of materials using
   carboxyalkylphosphonic acids}},
Journal = {{MATERIALS LETTERS}},
Year = {{2016}},
Volume = {{184}},
Pages = {{320-323}},
Month = {{DEC 1}},
Abstract = {{Bipolar carboxyalkylphosphonic acid molecules were investigated for
