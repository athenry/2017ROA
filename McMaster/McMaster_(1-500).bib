@article{ ISI:000379955000030,
Author = {Peng, Fei and Ye, Jin and Emadi, Ali},
Title = {{A Digital PWM Current Controller for Switched Reluctance Motor Drives}},
Journal = {{IEEE TRANSACTIONS ON POWER ELECTRONICS}},
Year = {{2016}},
Volume = {{31}},
Number = {{10}},
Pages = {{7087-7098}},
Month = {{OCT}},
Abstract = {{In this paper, a PWM current controller for the switched reluctance
   motor drives is proposed and digitally implemented. Parameter adaption
   is employed to guarantee both fast dynamics and robustness of the
   proposed current controller. The relationship between the proposed
   controller and the conventional controllers including PI and dead-beat
   controller is also presented. An improved sampling method is designed to
   avoid PWM delay in the control loop. Simulation and experimental results
   show that the proposed controller keeps similar dynamic response and
   accuracy as hysteresis controller under various testing conditions.
   However, compared with the hysteresis controller, the proposed current
   controller needs much lower sampling rate and has a constant switching
   frequency.}},
