@article{ ISI:000379704000026,
Author = {Fu, Qiang and Gray, Zachary Russell and van der Est, Art and Pelton,
   Robert H.},
Title = {{Phase Behavior of Aqueous Poly(acrylic acid-g-TEMPO)}},
Journal = {{MACROMOLECULES}},
Year = {{2016}},
Volume = {{49}},
Number = {{13}},
Pages = {{4935-4939}},
Month = {{JUL 12}},
Abstract = {{Poly(acrylic acid) with grafted TEMPO moieties, PAA-T, phase separates
   over the pH range 2-4, whereas at lower and higher pH, the polymer is
   water-soluble. The pH range of the two-phase region increases linearly
   with the content of grafted TEMPO moieties. As evidenced by surface
   tension measurements, the PAA-T molecules have little hydrophobic
   character. We propose that at low pH PAA-T is an amphoteric polymer
   that, in addition the anionic carboxyl groups, contains cationic species
   resulting from the disproportion of TEMPO at low pH. Furthermore, we
   propose that two-phase regions occur near the isoelectric points of the
   polymer, suggesting that phase separation is due to electrostatically
   driven association of polymer molecules.}},
