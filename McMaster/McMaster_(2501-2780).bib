@inproceedings{ ISI:000310776205161,
Author = {Aumi, Siam and Mhaskar, Prashant},
Book-Group-Author = {{IEEE}},
Title = {{Adaptive Data-based Model Predictive Control of Batch Systems}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2012}},
