@article{ ISI:000305363700157,
Author = {Mukherjee, C. and Sengupta, S. and Maiti, C. K. and Maiti, T. K.},
Title = {{Effects of substrate strain and electrical stress on lattice dynamics,
   defects, and traps in strained-Si/Si0.81Ge0.19 n-type
   metal-oxide-semiconductor field effect transistors}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2012}},
Volume = {{111}},
Number = {{10}},
Month = {{MAY 15}},
Abstract = {{Defects and traps in strained-Si n-type metal-oxide-semiconductor field
   effect transistors (MOSFETs) are studied in detail. The inelastic
   electron tunneling spectroscopy (JETS) technique is shown to be capable
   of probing traps in ultrathin gate dielectrics and obtain the energies
   and spatial locations of the traps. Detailed analyses of electrical
   stress-induced build-up of traps and electrically active bonding defects
   and identification of the trap-features including trap-assisted
   conduction and charge-trapping have been performed. The location and
   energies of the traps are estimated from the JETS spectra measured at
   both bias polarities. Identification of the acoustic and optical phonon
   modes (inelastic) as well as trap-features (elastic) helps in better
   understandings of the complex transport-mechanisms in gate dielectrics
   on strained layers. (C) 2012 American Institute of Physics.
   {[}http://dx.doi.org/10.1063/1.4718015]}},
