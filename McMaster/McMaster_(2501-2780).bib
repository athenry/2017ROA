Editor = {{Baozong, Y and Qiuqi, R and Xiaofang, T}},
Title = {{Distributed Concatenated Quasi-orthogonal Space Time Block Codes for
   Two-way Relay Networks}},
Booktitle = {{PROCEEDINGS OF 2012 IEEE 11TH INTERNATIONAL CONFERENCE ON SIGNAL
   PROCESSING (ICSP) VOLS 1-3}},
Series = {{International Conference on Signal Processing}},
Year = {{2012}},
Pages = {{1486+}},
