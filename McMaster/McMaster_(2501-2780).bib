@inproceedings{ ISI:000320276200011,
Author = {Medra, Ahmed and Davidson, Timothy N.},
Book-Group-Author = {{IEEE}},
Title = {{FLEXIBLE CODEBOOK DESIGN FOR LIMITED FEEDBACK DOWNLINK SYSTEMS VIA
   SMOOTH OPTIMIZATION ON THE GRASSMANNIAN MANIFOLD}},
Booktitle = {{2012 IEEE 13TH INTERNATIONAL WORKSHOP ON SIGNAL PROCESSING ADVANCES IN
   WIRELESS COMMUNICATIONS (SPAWC)}},
