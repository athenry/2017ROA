@article{ ISI:000303182600006,
Author = {Johari, G. P. and Aji, D. P. B.},
Title = {{Fictive temperatures of pharmaceutical glasses-A comparison of two
   methods for determining the enthalpy and entropy integrals}},
Journal = {{THERMOCHIMICA ACTA}},
Year = {{2012}},
Volume = {{536}},
Pages = {{41-46}},
Month = {{MAY 20}},
Abstract = {{Fictive temperature of a glass, T-f(H) is determined by matching the C-p
   dT integrals in a single DSC heating scan (method I), a method also
   useful for determining the fictive temperature T-f(sigma) by matching
   the C-p dln T integrals. But for quench-formed glasses, the heating scan
   contains a broad minimum that prevents one from determining T-f(H) and
   T-f(sigma) by method I. Therefore, an additional heating scan is used
   for the purpose (method II). For some quench-formed glasses, method II
   has yielded T-f(sigma) considerably different from T-f(H,) causing
   concerns on the use of the method and on the conclusions on the entropy
   of glass. To resolve it, we use method II to determine T-f(H) and
   T-f(sigma) of normally formed and annealed glasses of acetaminophen,
   carbamazepine and griseofulvin, and find that both the T-f(H) and
   T-f(sigma) values are identical to the values obtained by using method
   I. This confirms that the entropy production on the liquid-glass path is
   too small to affect the residual entropy of glass. Contribution from
   dynamically heterogeneous, beta or JG relaxation is not observed for
   these low-energy glasses. The difference between T-f(H) and T-f(sigma)
   previously observed for quench-formed glasses is due to data errors
   and/or extrapolation. (C) 2012 Elsevier B.V. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
