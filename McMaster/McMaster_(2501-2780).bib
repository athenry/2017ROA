@inproceedings{ ISI:000312264800041,
Author = {Di Bari, Sergio and Cotton, James S. and Robinson, Anthony J.},
Book-Group-Author = {{IOP}},
Title = {{Numerical Simulation of Exhaust Gas Cooling in Channels with Periodic
