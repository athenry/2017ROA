@article{ ISI:000303033600033,
Author = {Emigh, Brent and An, Ran and Hsu, Eugene M. and Crawford, Travis H. R.
   and Haugen, Harold K. and Wohl, Gregory R. and Hayward, Joseph E. and
   Fang, Qiyin},
Title = {{Porcine cortical bone ablation by ultrashort pulsed laser irradiation}},
Journal = {{JOURNAL OF BIOMEDICAL OPTICS}},
Year = {{2012}},
Volume = {{17}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{Ultrashort pulsed lasers in bone ablation show promise for many
   orthopedic applications. To minimize collateral tissue damage and
   control the ablation process, the ablation threshold fluence must be
   well characterized. Using an amplified femtoseconcl laser (170 fs, 800
   nm, 1 kHz), the ablation threshold on unaltered porcine cortical bone
   was measured using the D-2 method at multiple incident pulse numbers
   ranging from 25 to 1000 pulses per spot. The lowered threshold at
   greater pulse numbers indicated an incubation effect. Using a power law
   model, the incubation coefficient of unaltered porcine cortical bone was
   found to be 0.89 +/- 0.03. Through extrapolation, the single-pulse
   ablation threshold was found to be 3.29 +/- 0.14 J/cm(2). (c) 2012
