@inproceedings{ ISI:000312381403032,
Author = {Medra, Ahmed and Davidson, Timothy N.},
Book-Group-Author = {{IEEE}},
Title = {{AN INCREMENTAL GRASSMANNIAN FEEDBACK SCHEME FOR LINEARLY PRECODED
   SPATIAL MULTIPLEXING MIMO SYSTEMS}},
Booktitle = {{2012 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
