@article{ ISI:000298966200056,
Author = {Sundar, A. and Hughes, R. A. and Farzinpour, P. and Gilroy, K. D. and
   Devenyi, G. A. and Preston, J. S. and Neretina, S.},
Title = {{Manipulating the size distribution of supported gold nanostructures}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2012}},
Volume = {{100}},
Number = {{1}},
Month = {{JAN 2}},
Abstract = {{Gold nanostructures, with a wide size distribution, are confined between
   a metal foil and the oxide substrate upon which they were formed. When
   heated the surface energy gradient between the oxide and foil results in
   a net migration of gold atoms from the nanostructure to the foil. With
   time, the nanostructures show a size reduction and a narrowed size
   distribution. The narrowing results from the formation of foil contact
   points with only the largest nanostructures, a characteristic which
   leaves small nanostructures intact while consuming larger ones. Also
   demonstrated is the size reduction of arrayed gold structures to
   nanoscale dimensions. (C) 2012 American Institute of Physics.
   {[}doi:10.1063/1.3675569]}},
