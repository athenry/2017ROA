@inproceedings{ ISI:000310776205158,
Author = {Golshan, Masoud and MacGregor, John F.},
Book-Group-Author = {{IEEE}},
Title = {{Control Oriented Identification of Batch Processes Using Latent Variable
   Models}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
