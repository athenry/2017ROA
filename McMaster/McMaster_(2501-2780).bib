@inproceedings{ ISI:000321004000076,
Author = {Kinio, Steven and Patriciu, Alexandru},
Book-Group-Author = {{IEEE}},
Title = {{A Comparative Study of H-infinity and PID Control for Indirect
   Deformable Object Manipulation}},
Booktitle = {{2012 IEEE INTERNATIONAL CONFERENCE ON ROBOTICS AND BIOMIMETICS (ROBIO
   2012)}},
Year = {{2012}},
Note = {{IEEE International Conference on Robotics and Biomimetics (ROBIO),
   Guangzhou, PEOPLES R CHINA, DEC 11-14, 2012}},
Organization = {{IEEE; IEEE Robot \& Automat Soc (RAS); S China Univ Technol (SCUT); Natl
   Nat Sci Fdn China (NSFC); HIT, State Key Lab Robot \& Syst; IEEE Syst,
   Man, \& Cybernet Soc (SMC); Robot Soc Japan (RSJ); Japan Soc Mech Engn
   (JSME); Guangdong Mech Engn Soc (GDMES)}},
Abstract = {{Manipulation of deformable objects is a field of research that has a
   variety of applications, including medical procedures, food industry,
   and manufacturing. This paper compares the performance of a standard PID
   controller and a robust H-infinity controller, both of which are
   designed to perform the task of indirect deformable object manipulation.
   The H-infinity controller is generated using methods outlined by Doyle
   et al., and the PID controller is tuned in a conventional trial and
   error approach. Unmodelled dynamics of the deformable object system are
   considered disturbance inputs and compensated by both controllers. Loop
   shaping is used to model the robotic actuators and position feedback
   system limitations within the process of generating the H-infinity
   controller. The H-infinity proved to be superior to the PID in both
   simulations and experiments. Maximal steady state error of the
   H-infinity controller was 0.5mm; PID maximal error was 1 mm at steady
   state. The key benefit of the H-infinity approach is that the controller
   generated using the numerical model of the deformable object performed
   well in simulations and experiments; the parameters of the PID
   controller required retuning between simulation and the experimental
   setting.}},
