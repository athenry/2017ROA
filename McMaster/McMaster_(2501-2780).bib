@inproceedings{ ISI:000300400100078,
Author = {Sivakumaran, K. S. and Arasaratnam, P.},
Editor = {{Mazzolani, F and Herrera, R}},
Title = {{Impact of flange holes on the strength and ductility of steel beams}},
Booktitle = {{STESSA 2012: PROCEEDINGS OF THE 7TH INTERNATIONAL CONFERENCE ON
   BEHAVIOUR OF STEEL STRUCTURES IN SEISMIC AREAS}},
Year = {{2012}},
Pages = {{555-561}},
Note = {{7th International Conference on Behaviour of Steel Structures in Seismic
   Areas (STESSA), Santiago, CHILE, JAN 09-11, 2012}},
Organization = {{Univ Chile, Dept Civil Engn; GERDAU AZA; ILAFA; ECCS CECM EKS; Chilean
   Assoc Seismol \& Earthquake Engn (ACHISINA)}},
Abstract = {{The research program presented in this paper investigated the effects of
   flange holes and flange fastener holes on the strength and rotation
   capacity of ASTM A992 steel grade I-beams. It involved four point
   flexural testing of twenty five steel beams having circular holes of
   various diameters, ranging from 0\% to 50\% of the gross flange area.
   The tests were divided into four series as follows: solid flanges (no
   holes), holes in tension flanges only, holes in both tension and
   compression flanges, and holes in both flanges with bolt fasteners
   placed in these holes. These experiments reveal that the beam specimens
   having the A(fn)F(u)/A(fg)F(y) >= 1.0 were able to reach the
   gross-section plastic moment and exhibit substantial inelastic rotation
   capacity (R-y of more than 9). If this condition is violated
   (A(fn)F(u)/A(fg)F(y) < 1.0) then the beam specimens failed primarily due
   to rupture of tension flange at the location of the flange holes, which
   substantially reduced the inelastic rotational capacity. Based on the
   experimental results, this research study recommends a design approach
   analogous to the axial tension member design provisions.}},
