@article{ ISI:000349893100006,
Author = {Jiao, Liangbao and Shu, Xiao and Cheng, Yong and Wu, Xiaolin and Burini,
   Nino},
Title = {{Optimal Backlight Modulation With Crosstalk Control in Stereoscopic
   Display}},
Journal = {{JOURNAL OF DISPLAY TECHNOLOGY}},
Year = {{2015}},
Volume = {{11}},
Number = {{2, SI}},
Pages = {{157-164}},
Month = {{FEB}},
Abstract = {{Crosstalk between the left-eye and right-eye images is one of the main
   artifacts affecting the visual quality of stereoscopic liquid crystal
   display (LCD) systems. In this paper, a novel technique, called Optimal
   Backlight Modulation (OBM), is proposed to reduce crosstalk by taking
   the advantage of newly emerged light-emitting diode (LED) backlight
   panel. Unlike most existing techniques which solely aim at eliminating
   the effect of crosstalk, ignoring image brightness and contrast, OBM
   strikes a better balance by maximizing the output luminance while
   confining the crosstalk within a given level. A simple closed-form
   approximation of the optimization problem can be easily employed and
   solved in real time on LCD control hardware. Simulation results show
   that the proposed OBM algorithm provides the same or higher luminance
   while reducing the crosstalk by 60\% compared with the other tested
   methods.}},
