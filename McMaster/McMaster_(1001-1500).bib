@article{ ISI:000334156800005,
Author = {Liu, Wenjuan and Zhao, Dongmei and Zhu, Gang},
Title = {{End-to-end delay and packet drop rate performance for a wireless sensor
   network with a cluster-tree topology}},
Journal = {{WIRELESS COMMUNICATIONS \& MOBILE COMPUTING}},
Year = {{2014}},
Volume = {{14}},
Number = {{7}},
Pages = {{729-744}},
Month = {{MAY}},
Abstract = {{In this paper, we study the delay performance in a wireless sensor
   network (WSN) with a cluster-tree topology. The end-to-end delay in such
   a network can be strongly dependent on the relative location between the
   sensors and the sink and the resource allocations of the cluster heads
   (CHs). For real-time traffic, packets transmitted with excessive delay
   are dropped. Given the timeline allocations of each CH for local and
   inter-cluster traffic transmissions, an analytical model is developed to
   find the distribution of the end-to-end transmission delay for packets
   originated from different clusters. Based on this result, the packet
   drop rate is derived. A heuristic scheme is then proposed to jointly
   find the timeline allocations of all the CHs in a WSN in order to
   achieve the minimum and balanced packet drop rate for traffic originated
   from different levels of the cluster tree. Simulation results are shown
   to verify the analysis and to demonstrate the effectiveness of the
   proposed CH timeline allocation scheme. Copyright (c) 2012 John Wiley \&
   Sons, Ltd.}},
