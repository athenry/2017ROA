@inproceedings{ ISI:000380904701089,
Author = {Etezadi, Farrokh and Khisti, Ashish and Chen, Jun},
Book-Group-Author = {{IEEE}},
Title = {{Price of Perfection: Limited Prediction for Streaming over Erasure
   Channels}},
Booktitle = {{2015 IEEE INTERNATIONAL SYMPOSIUM ON INFORMATION THEORY (ISIT)}},
Series = {{IEEE International Symposium on Information Theory}},
Year = {{2015}},
Pages = {{1392-1396}},
Note = {{IEEE International Symposium on Information Theory (ISIT), Hong Kong,
   PEOPLES R CHINA, JUN 14-19, 2015}},
Organization = {{IEEE; IEEE Informat Theory Soc; HUAWEI; VTECH; Qualcomm; Google;
   Croucher Fdn; IBM; BROADCOM; Mediatek; iSN State Key Lab; INC; ITS;
   SENG; Hong Kong Polytechn Univ; K C Wong Educ Fdn; NSF; Hong Kong Univ
   Sci \& Technol, Sch Engn}},
Abstract = {{We study sequential transmission of Gauss-Markov sources over erasure
   channels under a zero decoding delay constraint. A two-stage coding
   scheme which can be described as a hybrid between predictive coding with
   limited past and quantization \& binning is proposed. This scheme can
   achieve significant performance gains over baseline schemes in
   simulations involving i.i.d. erasure channels, and in certain regimes
   can attain performance close to a fundamental lower bound. We consider
   an information theoretic model for streaming that explains the weakness
   of baseline schemes (e.g., predictive coding, memoryless binning, etc.)
   and illustrates the advantage of our proposed hybrid scheme over these.
   Techniques from multi-terminal source coding are used to derive a new
   lower bound on the compression rate and identify cases when the hybrid
   coding scheme is close to optimal. We discuss qualitatively the
   interplay between the parameters of our information theoretic model and
   the statistical models used in simulations.}},
