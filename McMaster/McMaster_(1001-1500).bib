@article{ ISI:000346023500011,
Author = {Kuryllo, Kacper and Jahanshahi, Shahrzad and Zhu, Weijia and Brown, Eric
   D. and Li, Yingfu},
Title = {{A Dual Reporter System for Detecting RNA Interactions in Bacterial Cells}},
Journal = {{CHEMBIOCHEM}},
Year = {{2014}},
Volume = {{15}},
Number = {{18}},
Pages = {{2703-2709}},
Month = {{DEC 15}},
Abstract = {{Detecting RNA-partner interactions in cells is often difficult due to a
   lack of suitable tools. Here we describe a dual reporter system capable
   of detecting intracellular interactions in which one of the partners is
   an RNA. The system utilizes two fluorescent proteins with similar
   maturation rates but distinct spectral properties, specifically cyan
   fluorescent protein (CFP) and yellow fluorescent protein (YFP). By
   placing the CFP gene upstream and the YFP gene downstream of an RNA gene
   of interest, the production of YFP becomes sensitive to RNA-partner
   interaction, whereas the synthesis of CFP is not disturbed. Therefore,
   the RNA-partner interaction can be simply measured by the change in the
   ratio of fluorescence of YFP over CFP. The utility of our approach is
   demonstrated through verification of three known RNA-partner
   interactions in the model bacterium Escherichia coli. Our two-reporter
   strategy should be broadly useful to the study of RNA-targeted
   interactions in bacteria.}},
