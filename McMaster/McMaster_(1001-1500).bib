@article{ ISI:000335905300088,
Author = {Liang, Xiaojun and Kumar, Shiva},
Title = {{Analytical modeling of XPM in dispersion-managed coherent fiber-optic
   systems}},
Journal = {{OPTICS EXPRESS}},
Year = {{2014}},
Volume = {{22}},
Number = {{9}},
Pages = {{10579-10592}},
Month = {{MAY 5}},
Abstract = {{An analytical model for the cross-phase modulation ( XPM) variance in
   dispersion-managed coherent fiber-optic systems is developed based on
   the first order perturbation theory. The XPM variance is analytically
   calculated for arbitrary pulse shapes. For a non-Gaussian pulse, the
   summation of time-shifted Gaussian pulses is used to fit the target
   pulse shape, which not only provides a good approximation of the
   non-Gaussian pulse but also allows explicit derivation of the XPM
   variance. The analytically estimated XPM variance is found to be in good
   agreement with numerical simulations. (C) 2014 Optical Society of
