@inproceedings{ ISI:000368929600034,
Author = {Thong, Jason and Nicolici, Nicola},
Book-Group-Author = {{IEEE}},
Title = {{SAT Solving using FPGA-based Heterogeneous Computing}},
Booktitle = {{2015 IEEE/ACM INTERNATIONAL CONFERENCE ON COMPUTER-AIDED DESIGN (ICCAD)}},
Series = {{ICCAD-IEEE ACM International Conference on Computer-Aided Design}},
Year = {{2015}},
Pages = {{232-239}},
Note = {{34th IEEE/ACM International Conference on Computer-Aided Design (ICCAD),
   IEEE Council Elect Design Automat, Austin, TX, NOV 02-06, 2015}},
Organization = {{IEEE; ACM; Assoc Comp Machinery; Assoc Comp Machinery SIGDA; IEEE
   Electron Devices Soc; Taiwan Minist Educ CAD Contest; IEEE Circuits \&
   Syst Soc}},
Abstract = {{We present a heterogeneous computing solution to the Boolean
   satisfiability (SAT) problem. Our field-programmable gate array
   (FPGA)-based implementation for accelerating the common case computation
   within a SAT solver utilizes most of the FPGA resources and it
   seamlessly integrates with our software host. Algorithms and data
   structures were redesigned to maximize the strengths of customized
