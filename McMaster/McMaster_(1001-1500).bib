@article{ ISI:000335375700013,
Author = {Jafari, Hamed Mazhab and Abdelhalim, Karim and Soleymani, Leyla and
   Sargent, Edward H. and Kelley, Shana O. and Genov, Roman},
Title = {{Nanostructured CMOS Wireless Ultra-Wideband Label-Free PCR-Free DNA
   Analysis SoC}},
