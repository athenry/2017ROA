@article{ ISI:000311177700056,
Author = {Hyung, Gun Woo and Lee, Dong Hyung and Lee, Ho Won and Kim, You Hyun and
   Lee, Seok Jae and Koo, Ja Ryong and Kim, Woo Young and Kim, Young Kwan},
Title = {{Bottom-emission organic light-emitting diodes using semitransparent
   anode electrode by O-2 plasma}},
Journal = {{ORGANIC ELECTRONICS}},
Year = {{2012}},
Volume = {{13}},
Number = {{11}},
Pages = {{2594-2599}},
Month = {{NOV}},
Abstract = {{To improve the performance of bottom-emission organic light-emitting
   diodes (BEOLEDs), the effect of oxygen plasma treatment duration on the
   electrical properties of multi-metal Ni/Ag/Ni thin film anode was
