@inproceedings{ ISI:000329611505025,
Author = {Dong, Zheng and Zhang, Jian-Kang},
Book-Group-Author = {{IEEE}},
Title = {{DISTRIBUTED CONCATENATED ALAMOUTI CODE DESIGNS FOR ONE-WAY RELAY
   NETWORKS USING UNIQUELY-FACTORABLE PSK CONSTELLATION}},
