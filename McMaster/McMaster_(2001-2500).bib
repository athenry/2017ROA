@article{ ISI:000305447100018,
Author = {Zhang, Bin and Zhang, Liang and Pan, Xuefeng and Tao, Weidong and Pan,
   Jianguo and Dong, Jianfeng and Xu, Changqing},
Title = {{Optical filter based on internal reflection and optical rotatory
   dispersion of NaBrO3 crystal}},
Journal = {{CHINESE OPTICS LETTERS}},
Year = {{2012}},
Volume = {{10}},
Number = {{6}},
Month = {{JUN 10}},
Abstract = {{Based on the optical rotatory dispersion and zero reflection of the
   p-polarization light at the Brewster angle, a novel optical filter that
