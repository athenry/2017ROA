@article{ ISI:000313644500098,
Author = {Holloway, Gregory W. and Song, Yipu and Haapamaki, Chris M. and
   LaPierre, Ray R. and Baugh, Jonathan},
Title = {{Trapped charge dynamics in InAs nanowires}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2013}},
Volume = {{113}},
Number = {{2}},
Month = {{JAN 14}},
Abstract = {{We study random telegraph noise in the conductance of InAs nanowire
   field-effect transistors due to single electron trapping in defects. The
   electron capture and emission times are measured as functions of
   temperature and gate voltage for individual traps, and are consistent
   with traps residing in the few-nanometer-thick native oxide, with a
   Coulomb barrier to trapping. These results suggest that oxide removal
   from the nanowire surface, with proper passivation to prevent regrowth,
   should lead to the reduction or elimination of random telegraph noise,
   an important obstacle for sensitive experiments at the single electron
   level. (C) 2013 American Institute of Physics.
   {[}http://dx.doi.org/10.1063/1.4773820]}},
