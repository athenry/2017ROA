@inproceedings{ ISI:000326736500010,
Author = {Al-Shabi, Mohammad and Habibi, Saeid},
Book-Group-Author = {{IEEE}},
Title = {{New Novel Time-Varying and Robust Smoothing Boundary Layer Width for the
   Smooth Variable Structure Filter}},
Booktitle = {{2013 9TH INTERNATIONAL SYMPOSIUM ON MECHATRONICS AND ITS APPLICATIONS
   (ISMA)}},
Year = {{2013}},
Note = {{9th International Symposium on Mechatronics and its Applications (ISMA),
   Univ Jordan, Fac Engn \& Technol, Amman, JORDAN, APR 09-11, 2013}},
Organization = {{Univ Jordan; German Jordanian Univ; Philadelphia Univ; Hashemite Univ;
   Amer Univ Sharjah; IEEE Jordan Grad Last Decade Affin Grp; IEEE Jordan
   Chapter Robot \& Automat Soc; Emirates Aluminium; IEEE Jordan Sect}},
Abstract = {{In this paper, a new novel form of the Smooth Variable Structure Filter
   (SVSF) is proposed. The SVSF is a recently proposed predictor-corrector
   filter for state and parameter estimation, and it is based on the
   sliding mode control concepts. It defines a hyperplane and then applies
   a discontinuous corrective action that forces the estimate to go back
   and forth across that plane. Thereby, the SVSF is robust and stable
   technique. The proposed SVSF uses a saturated function with an
   associated fixed-width smoothing boundary layer to eliminate chattering.
   However, it has been noticed that the filter performance could be
   improved by using a time-varying smoothing boundary layer that is
   directly linked to the output's a priori estimation error. In this
   paper, a solution for a time-varying, stable and robust smoothing
   boundary layer width is obtained based on using two filters; the first
   is to maintain stability, and the second is to obtain the modified
   estimate vector that minimizes the diagonal elements of the error
   covariance matrix. The proposed method is tested on an
   electro-hydrostatic actuator proposed in {[}1] and its performance is
   compared with the Kalman Filter performance.}},
