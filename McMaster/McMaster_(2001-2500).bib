@inproceedings{ ISI:000319876400024,
Author = {Qiao, H. and Wang, H. and Wu, P. D.},
Editor = {{Zhang, SH and Liu, XH and Cheng, M and Li, J}},
Title = {{Analysis of Anisotropy of Extruded Magnesium Alloy AZ31 Bar}},
Booktitle = {{11TH INTERNATIONAL CONFERENCE ON NUMERICAL METHODS IN INDUSTRIAL FORMING
   PROCESSES (NUMIFORM 2013)}},
Series = {{AIP Conference Proceedings}},
Year = {{2013}},
Volume = {{1532}},
Pages = {{214-221}},
Note = {{11th International Conference on Numerical Methods in Industrial Forming
   Processes (NUMIFORM), Chinese Acad Sci (CAS), Inst Met Res, Shenyang,
   PEOPLES R CHINA, JUL 06-10, 2013}},
Organization = {{Chinese Acad Sci (CAS); Natl Nat Sci Fdn China (NSFC); Chinese Soc
   Technol Plastic (CSTP); Shenyang Sci \& Technol Bur; TRANSVALOR S A}},
Abstract = {{The large strain Elastic Visco-Plastic Self-Consistent (EVPSC) model,
   with the recently developed Twinning and De-Twinning (TDT) description,
   is applied to study the mechanical behavior of magnesium alloy AZ31
   extrusion. Five different specimen orientations with tilt angles of
   alpha of 0 degrees, 22.5 degrees, 45 degrees, 67.5 degrees and 90
   degrees between extrusion direction and longitudinal specimen axis are
   used to study the mechanical anisotropy under both uniaxial tension and
   compression. It is found that the uniform strain under tension is
   sensitive to the tilt angle and the maximum uniform strain is found at
   alpha = 45 degrees. The effect of pre-strain in uniaxial compression
   along the extrusion direction on subsequent uniaxial tension and
   compression along different directions with respect to the extrusion
   direction is also investigated. It is demonstrated that the influence of
   the twinning during pre-strain in compression and the detwinning in the
   subsequent uniaxial tension on the uniaxial tension response depends
   significantly on the tilt angle. By comparing the differences in the
   flow stress vs. orientation curves between the as-received and
   pre-strained samples, the effect of pre-straining on material anisotropy
   is investigated. It is found that the pre-compression lowers the uniform
   strain regardless of the tilt angle alpha.}},
