@article{ ISI:000325145900016,
Author = {Koivula, Hanna and Pelton, Robert and Brennan, John D. and Grenon,
   Joelle and Manfred, Tony},
Title = {{Flexographic printability of sol-gel precursor dispersions for bioactive
   paper}},
Journal = {{NORDIC PULP \& PAPER RESEARCH JOURNAL}},
Year = {{2013}},
Volume = {{28}},
Number = {{3}},
Pages = {{450-457}},
Abstract = {{Flexography was evaluated for printing sol-gel precursor solutions onto
   filter paper for biosensor applications. Three sol-gel precursor
   chemistries were compared and all could be printed, however, in every
   case there was significant deposit formation on the anilox cylinders and
   the deposits were difficult to remove. Of the three formulations, the
   one based on trimethoxy-methylsilane gave the best printing performance
   whereas sodium silicate and tetramethyl orthosilicate were inferior.
   Although bioactive enzyme solutions were well behaved in flexo printing,
