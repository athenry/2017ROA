@article{ ISI:000327704900007,
Author = {Jia, Ming Xia and Lu, Ya Ping and Xu, Ling Feng and Song, Yue Peng},
Title = {{The Influence of Friction on the Texture Formation of a IF Steel during
   Hot Rolling in the Ferrite Region}},
Journal = {{STEEL RESEARCH INTERNATIONAL}},
Year = {{2013}},
Volume = {{84}},
Number = {{8}},
Pages = {{761-765}},
Month = {{AUG}},
Abstract = {{The effect of friction on evolution of the microstructure and texture
   during hot rolling in the ferrite region was studied with secondary
   electromicroscopy (SEM), X-ray diffractometer, and FEM simulation. The
   friction between a roll and a steel sheet surface leads to the formation
   of notable through-thickness texture gradients. The finite element
   method (FEM) simulation shows that the large friction coefficient
   between a roll and sample results in a pronounced variation of (epsilon)
   over dot(13) (shear strain rate) and large epsilon(13)/epsilon(11) value
   which accounts for strong \{110\}< 001 > texture components and weak
   gamma-fiber components at rolled sheet surface; whereas the friction
   condition do not exert notable effect on texture formation at the sheet
   center.}},
