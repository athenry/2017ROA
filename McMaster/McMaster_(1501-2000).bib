@article{ ISI:000324791700005,
Author = {Fallah, Vahid and Korinek, Andreas and Ofori-Opoku, Nana and Provatas,
   Nikolas and Esmaeili, Shahrzad},
Title = {{Atomistic investigation of clustering phenomenon in the Al-Cu system:
   Three-dimensional phase-field crystal simulation and HRTEM/HRSTEM
   characterization}},
Journal = {{ACTA MATERIALIA}},
Year = {{2013}},
Volume = {{61}},
Number = {{17}},
Pages = {{6372-6386}},
Month = {{OCT}},
Abstract = {{Early-stage clustering in quenched/aged supersaturated Al-Cu alloys is
   simulated and characterized at the atomic scale to study the structural
   and compositional evolution of clusters. A three-dimensional analysis of
   the nucleation and growth mechanisms of early clusters in binary alloys
   is built upon a phase-field crystal (PFC) methodology previously
   developed in two dimensions by Fallah et al. {[}Phys. Rev. B. 86, 134112
   (2012)]. The dislocation-mediated nucleation of clusters, reproduced in
   three dimensions, is compared against new results of atomistic
   characterization using high-resolution transmission electron microscopy
   (HRTEM) and high-resolution scanning transmission electron microscopy
   (HRSTEM). Consistent with the simulation results, analysis of HRTEM
   micrographs reveals the association of quenched-in dislocations with the
   early clusters. Moreover, the PFC model and HRSTEM observations show
   remarkable agreement on the non-equilibrium evolution of composition
   within the early clusters as well as their preferred growth orientation
   and spherical-to-ellipsoidal morphological transition. (C) 2013 Acta
   Materialia Inc. Published by Elsevier Ltd. All rights reserved.}},
