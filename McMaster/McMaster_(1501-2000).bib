@article{ ISI:000323143700057,
Author = {Liu, Jieyi and Pelton, Robert and Obermeyer, Jaclyn M. and Esser, Anton},
Title = {{Laccase Complex with Polyvinylamine Bearing Grafted TEMPO is a Cellulose
   Adhesion Primer}},
Journal = {{BIOMACROMOLECULES}},
Year = {{2013}},
Volume = {{14}},
Number = {{8}},
Pages = {{2953-2960}},
Month = {{AUG}},
Abstract = {{Polyelectrolyte complexes formed between laccase and polyvinylamine with
   grafted TEMPO moieties, PVAm-T, adsorb onto cellulose, causing
   oxidation. All evidence supports the view that aldehyde groups on
   oxidized cellulose condense with primary amine groups, giving a grafted
   layer of PVAm-T complexed with laccase. The grafted PVAm-T serves as a
   primer layer promoting wet cellulose-to-cellulose adhesion in the
   presence of PVAm adhesive. The cellulose modification occurs at ambient
   temperatures and pH 5. The adhesion improvements with mixtures of PVAm-T
   and laccase are remarkable because both components are macromolecular,
   which should inhibit the ability of the TEMPO to act as a shuttle
   between the enzyme and the primary hydroxyl groups on cellulose. It is
   proposed that PVAm-bound oxoammonium ions exchange with neighboring
   TEMPO moieties, providing a mechanism for the transfer of oxidation
   activity from immobilized enzyme to the cellulose surfaces.}},
