@article{ ISI:000323546500008,
Author = {Cervantes, Ilse and Mendoza-Torres, Angelica and Emadi, Ali and
   Diaz-Diaz, Irwin A.},
Title = {{Robust switched current control of converters}},
Journal = {{IET CONTROL THEORY AND APPLICATIONS}},
Year = {{2013}},
Volume = {{7}},
Number = {{10}},
Pages = {{1398-1407}},
Month = {{JUL}},
Abstract = {{In this study, a robust current switching control of converters is
   proposed. It is proved that the controller is able to guarantee global
   current regulation, as well as the maximum voltage and current ripple,
   simultaneously. Such features make it ideal for implementing
   power-management strategies and to be used along fuel cells. The control
   scheme is shown to be robust against model uncertainty and since it is
   based on the piece-continuous model instead of average models, it is
   suitable for operation under a wide range of conditions. Moreover, its
   complexity does not increase with the number of cells. Experimental
   evidence of the advantages and features of the proposed control is
   presented in a two-cell boost converter.}},
