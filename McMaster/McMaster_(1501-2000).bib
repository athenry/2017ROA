@article{ ISI:000324690400007,
Author = {Losquin, Arthur and Camelio, Sophie and Rossouw, David and Besbes,
   Mondher and Pailloux, Frederic and Babonneau, David and Botton,
   Gianluigi A. and Greffet, Jean-Jacques and Stephan, Odile and Kociak,
   Mathieu},
Title = {{Experimental evidence of nanometer-scale confinement of plasmonic
   eigenmodes responsible for hot spots in random metallic films}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2013}},
Volume = {{88}},
Number = {{11}},
Month = {{SEP 23}},
Abstract = {{We report on the identification and nanometer scale characterization
   over a large energy range of random, disorder-driven, surface plasmons
   in silver semicontinuous films embedded in silicon nitride. By
   performing spatially resolved electron energy loss spectroscopy
   experiments, we experimentally demonstrate that these plasmons
   eigenmodes arise when the films become fractal, leading to the emergence
   of strong electrical fields ({''}hot spots{''}) localized over few
   nanometers. We show that disorder-driven surface plasmons strongly
   depart from those usually found in nanoparticles, being strongly
   confined and randomly and densely distributed in space and energy.
   Beyond that, we show that they have no obvious relation with the local
   morphology of the films, in stark contrast with surface plasmon
   eigenmodes of nanoparticles.}},
