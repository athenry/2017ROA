@inproceedings{ ISI:000346492601065,
Author = {Chen, Song and Yan, Fengjun},
Book-Group-Author = {{IEEE}},
Title = {{Combustion Phasing and Work Output Modeling for Homogeneous Charge
   Compression Ignition (HCCI) Engines}},
Booktitle = {{2014 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
