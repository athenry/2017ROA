@inproceedings{ ISI:000344116400004,
Author = {Liang, Xiaojun and Kumar, Shiva},
Editor = {{Maclean, S and Plant, DV}},
Title = {{An analytical XPM model for dispersion-managed fiber-optic systems}},
Booktitle = {{PHOTONICS NORTH 2014}},
Series = {{Proceedings of SPIE}},
Year = {{2014}},
Volume = {{9288}},
Note = {{Conference on Photonics North, Montreal, CANADA, MAY 28-30, 2014}},
Organization = {{Univ Laval, Ctr doptique, Photonique Laser; Canadian Photon Ind
   Consortium; Inst Natl dOptique; Quebec Photon Network; Sytacom; Agilent
   Technologies}},
Abstract = {{An analytical cross-phase modulation (XPM) model is developed for
   coherent fiber-optic transmission systems. The XPM fields are calculated
   using a first-order perturbation theory. Statistical analysis is then
   applied to calculate the XPM variance. The analytical XPM model is
   applicable to dispersion-managed fiber-optic systems with arbitrary
   pulse shapes. For non-Gaussian pulses, a summation of time-shifted
