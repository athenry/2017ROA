@article{ ISI:000361194200009,
Author = {El Sherif, Mohamed H. and Bakr, Mohamed H. and Soliman, Ezzeldin A.},
Title = {{E-shaped wideband plasmonic nantennas with linear and dual-linear
   polarizations}},
Journal = {{PHOTONICS RESEARCH}},
Year = {{2015}},
Volume = {{3}},
Number = {{4}},
Pages = {{140-145}},
Month = {{AUG 1}},
Abstract = {{In this paper we present a novel nanoantenna (nantenna) design for
   energy harvesting. The nantenna has an ``E{''} shape and is placed on a
   SiO2 substrate. Its operation is based on the excitation of surface
   plasmon polaritons through the gold arms of the E shape. By varying the
   lengths and widths of the arms, two overlapping working bandwidths can
   be achieved. This results in a wideband behavior characterized by a full
   width at half-maximum of about 2.2 mu m centered around 3.6 mu m. Two
   orthogonal E nantennas are placed perpendicular to each other to realize
   a dual-polarized nantenna. This nantenna can receive the two incident
   polarizations at two separate gap locations with very high isolation.
   The proposed structure can be used in several energy harvesting
   applications, such as scavenging the infrared heat from the Earth and
   other hot objects, in addition to optical communications. (C) 2015
   Chinese Laser Press}},
