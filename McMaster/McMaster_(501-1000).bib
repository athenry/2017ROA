@article{ ISI:000360744900002,
Author = {Mahboubi, S. and Botton, G. A. and Kish, J. R.},
Title = {{Technical Note: Corrosion Resistance of Alloy 33 (Fe-33Cr-32Ni) in
   Supercritical Water}},
Journal = {{CORROSION}},
Year = {{2015}},
Volume = {{71}},
Number = {{9}},
Pages = {{1064-1070}},
Month = {{SEP}},
Abstract = {{This study examines the corrosion resistance of wrought Alloy 33
   (Fe-33Cr-32Ni) exposed in a static autoclave containing 25 MPa
   supercritical water at 625 degrees C for 500 h. Gravimetric measurements
   in combination with electron microscopy analyses showed that the
   formation of a thin, dense external single-layer Cr2O3-based scale
   provides Alloy 33 with a promising corrosion resistance. However, the
   microstructure was not stable, as Cr-rich alpha-ferrite precipitates
   formed in the subsurface during the short exposure. The suitability of
   this alloy as a candidate fuel cladding for the Generation IV
   supercritical water-cooled nuclear reactors concept is discussed in
   light of the findings presented.}},
