@article{ ISI:000350211300002,
Author = {Musicki, Darko and Song, Taek Lyul and Lee, Hae Ho and Chen, Xin and
   Kirubarajan, Thialingam},
Title = {{Track-to-track fusion with target existence}},
Journal = {{IET RADAR SONAR AND NAVIGATION}},
Year = {{2015}},
Volume = {{9}},
Number = {{3}},
Pages = {{241-248}},
Month = {{MAR}},
Abstract = {{Local tracking in clutter initialises and updates true and false tracks.
   Local false track discrimination uses a track quality measure to confirm
   most of the true tracks, and to terminate most of the false tracks.
   Confirmed tracks are transmitted for track-to-track fusion. The sets of
   tracks being considered for fusion may contain both true and false
   tracks. The authors assume that each track information also includes the
   track quality measure in the form of the probability of target existence
   information. This information is used for additional false track
   discrimination at the fusion centre. They also use this information to
   enhance the track-to-track association. They propose three different
   strategies for track fusion with the target existence information: the
   `single target', the `joint multitarget' and the `linear multitarget'.}},
