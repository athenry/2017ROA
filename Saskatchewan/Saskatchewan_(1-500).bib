@article{ ISI:000381951000024,
Author = {Islam, A. S. M. Atiqul and Adoo, N. A. and Bergstrom, D. J.},
Title = {{Prediction of mono-disperse gas-liquid turbulent flow in a vertical pipe}},
Journal = {{INTERNATIONAL JOURNAL OF MULTIPHASE FLOW}},
Year = {{2016}},
Volume = {{85}},
Pages = {{236-244}},
Month = {{OCT}},
Abstract = {{A two-fluid model in the Eulerian-Eulerian framework has been
   implemented for the prediction of gas volume fraction, mean phasic
   velocities, and the liquid phase turbulence properties for gas-liquid
   upward flow in a vertical pipe. The governing two-fluid transport
   equations are discretized using the finite volume method and a low
   Reynolds number k-epsilon model is used to predict the turbulence field
   for the continuous liquid phase. In the present analysis, a fully
   developed one-dimensional flow is considered where the gas volume
   fraction profile is predicted using the radial force balance for the
   bubble phase. The current study investigates: (1) the turbulence
   modulation terms which represent the effect of bubbles on the liquid
   phase turbulence in the k-epsilon transport equations; (2) the role of
   the bubble induced turbulent viscosity compared to turbulence generated
   by shear; and (3) the effect of bubble size on the radial forces which
   results in either a center-peak or a wall-peak in the gas volume
   fraction profiles. The results obtained from the current simulation are
   generally in good agreement with the experimental data, and somewhat
   improved over the predictions of some previous numerical studies. (C)
   2016 Elsevier Ltd. All rights reserved.}},
