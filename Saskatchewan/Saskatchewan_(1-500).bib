@article{ ISI:000392429300017,
Author = {Boyko, Teak Dagan and Moewes, Alexander},
Title = {{The hardness of group 14 spinel nitrides revisited}},
Journal = {{JOURNAL OF THE CERAMIC SOCIETY OF JAPAN}},
Year = {{2016}},
Volume = {{124}},
Number = {{10}},
Pages = {{1063-1066}},
Month = {{OCT}},
Abstract = {{The hypothetical spinel carbon nitride (gamma-C3N4) has received a large
   amount of attention due to its predicted hardness being comparable to
   that of diamond. The group 14 spinel binary nitrides that have been
   synthesized are limited so far to: gamma-Si3N4, gamma-Ge3N4 and
   gamma-Sn3N4. However, there still remains significant interest in
   gamma-C3N4 in the hope that it will eventually be synthesized, but there
   are no successful reports, thus making the study of gamma-C3N4 strictly
   theoretical. Through an empirical relationship that correlates hardness,
   crystal structure and the electronic band gap, we examine a series of
   group 14 spinel nitrides: gamma-C3N4 gamma-Si3N4, gamma-Ge3N4 and
   gamma-Sn3N4, as well as their ternary compounds. The hardness and
   electronic band gap of these materials are calculated using ab initio
   density functional theory. These results show that in the case of the
   solid solutions, gamma-(Si, Ge)(3)N-4 and gamma-(Ge, Sn)(3)N-4, the
   tetrahedral site is filled first by the larger cation, Ge and Sn,
   respectively. Furthermore, the deviation of carbon containing group 14
   spinel nitrides from the expected hardness and bandgap trend suggests
   that gamma-Si3N4 may remain the hardest known group 14 spinel nitride.
   Additionally, an improved method to calculate the hardness using the
   nitrogen bonding tetrahedron provides more unambiguous results and the
   trend of the hardness agrees with experimental measurements. (C) 2016
   The Ceramic Society of Japan. All rights reserved.}},
