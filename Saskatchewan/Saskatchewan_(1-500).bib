@article{ ISI:000375056700003,
Author = {McPhedran, Kerry N. and Seth, Rajesh},
Title = {{Wastewater Colloidal Organic Carbon: Characterization of Filtration
   Fractions Using H-1 NMR}},
Journal = {{WATER ENVIRONMENT RESEARCH}},
Year = {{2016}},
Volume = {{88}},
Number = {{4}},
Pages = {{308-317}},
Month = {{APR 1}},
Abstract = {{The current study separates colloidal organic carbon (COC) of municipal
   wastewater using membrane and ultrafiltration filters followed by
   characterization using H-1 nuclear magnetic resonance (NMR) and UV
   absorbance with the goal of determination of size-specific
   characteristics, which may be used to correlate contaminant partitioning
   to natural COC. Passing fractions included 49.7, 44.8, 39.3, and 33.1
   mg/L COC for filter sizes 1.5 mu m, 0.45 mu m, 100 kDa, and 1 kDa,
   respectively. The methodology used for processing COC prior to H-1 NMR
   characterization was novel and successful in concentrating COC without
   modification of structures, which is the general drawback of other
   separation techniques such as resin extractions. This concentration
   technique is quite simple (i.e., not dependent on specialized
   instrumentation) and allows much shorter NMR experimental durations
   saving time and cost of analysis. Further work using NMR techniques will
   allow for greater understanding of COC molecular characteristics and be
   valuable for use in predictive modeling improvements.}},
