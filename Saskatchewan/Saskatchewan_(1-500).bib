Title = {{Learning from Imbalanced Data: A Comprehensive Comparison of Classifier
   Performance for Bleeding Detection in Endoscopic Video}},
Booktitle = {{2016 5TH INTERNATIONAL CONFERENCE ON INFORMATICS, ELECTRONICS AND VISION
   (ICIEV)}},
Year = {{2016}},
Pages = {{1006-1009}},
