@article{ ISI:000384074000002,
Author = {Huang, Zhuo-Ran and Jin, Hong-Ying and Steele, T. G. and Zhang, Zhu-Feng},
Title = {{Revisiting the b(1)pi and rho pi decay modes of the 1(-+) light hybrid
   state with light-cone QCD sum}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2016}},
Volume = {{94}},
Number = {{5}},
Month = {{SEP 27}},
Abstract = {{We study the rho pi and b(1)pi decay modes of the 1(-+) light hybrid
   state within the framework of light-cone QCD sum rules. We use both the
   tensor current (psi) over bar sigma(mu upsilon)psi and the derivative
   current (psi) over bar(D) over left right arrow (mu)gamma(5)psi as
   interpolating currents to calculate the partial decay width of the
   b(1)pi decay mode. Comparing the sum rules obtained by using different
   currents, we obtain Gamma(pi(1) -> b(1)pi) =8-23, 32-86 and 52-151 MeV
   for m(1)-+ = 1.6, 1.8 and 2.0 GeV respectively, which favor the results
   from the flux tube models and lattice simulations. We also use the
   tensor current to study the.p decay mode, and although an extended
   stability criterion is needed, our results suggest a small partial decay
   width.}},
