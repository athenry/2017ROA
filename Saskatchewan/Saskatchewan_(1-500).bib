@article{ ISI:000378563200023,
Author = {Feng, Xiaolei and Zhang, Xinyu and Liu, Hanyu and Qu, Xin and Redfern,
   Simon A. T. and Tse, John S. and Li, Quan},
Title = {{Low-density superhard materials: computational study of Li-inserted
   B-substituted closo-carboranes LiBC11 and Li2B2C10}},
Journal = {{RSC ADVANCES}},
Year = {{2016}},
Volume = {{6}},
Number = {{58}},
Pages = {{52695-52699}},
Abstract = {{Insertion of Li atoms into a B-substituted carbon cage produces two
   superhard compounds with relatively low density: LiBC11 and Li2B2C10.
   For each structure, phonon frequencies across the whole Brillouin zone
   are positive, indicating dynamic stability. Electronic structure
   calculations indicate that they are semiconductors under ambient
   conditions. Estimates of the Vickers hardness, based on a semi-empirical
   model, highlight the incompressible nature of these two compounds. We
   then performed calculations on the ideal strengths of these two
   structures to confirm the hardness and investigate origins of the
   mechanical properties. Strikingly, both LiBC11 and Li2B2C10 can be
   classed as superhard materials, with hardness values of 49 GPa and 41
   GPa, respectively. The current results shed light on the properties of
   new superhard carbon cage structures more generally.}},
