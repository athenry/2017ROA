@inproceedings{ ISI:000399925504073,
Author = {Mohammed, Shahed K. and Deeba, Farah and Bui, Francis M. and Wahid, Khan
   A.},
Book-Group-Author = {{IEEE}},
Title = {{Application of Modified Ant Colony Optimization for Computer Aided
   Bleeding Detection System}},
Booktitle = {{2016 INTERNATIONAL JOINT CONFERENCE ON NEURAL NETWORKS (IJCNN)}},
Series = {{IEEE International Joint Conference on Neural Networks (IJCNN)}},
