@article{ ISI:000378584800039,
Author = {Zhou, Chunyu and Szpunar, Jerzy A. and Cui, Xiaoyu},
Title = {{Synthesis of Ni/Graphene Nanocomposite for Hydrogen Storage}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2016}},
Volume = {{8}},
Number = {{24, SI}},
Pages = {{15232-15241}},
Month = {{JUN 22}},
Abstract = {{We have designed a Ni graphene composite for hydrogen storage with Ni
   nanoparticles of 10 nm in size, uniformly dispersed over a graphene
   substrate. This system exhibits attractive features like high
   gravimetric density, ambient conditions, and low activation temperature
   for hydrogen release. When charged at room temperature and an
   atmospheric hydrogen pressure of 1 bar, it could yield a hydrogen
   capacity of 0.14 wt \%. When hydrogen pressure increased to 60 bar, the
   sorbent had a hydrogen gravimetric density of 1.18 wt \%. The hydrogen
   release could occur at an operating temperature below 150 degrees C and
