@article{ ISI:000375581400001,
Author = {Naito, Tomoyuki and Yoshida, Takafumi and Mochizuki, Hidehiko and
   Fujishiro, Hiroyuki and Basu, Ritwik and Szpunar, Jerzy A.},
Title = {{Vortex Pinning Properties of Dense Ti-Doped MgB2 Bulks Sintered at
   Different Temperatures}},
Journal = {{IEEE TRANSACTIONS ON APPLIED SUPERCONDUCTIVITY}},
Year = {{2016}},
Volume = {{26}},
Number = {{3}},
Month = {{APR}},
Abstract = {{We have studied the trapped field by field-cooled magnetization and
   microstructure of the Ti-doped MgB2 bulks fabricated at 700 degrees C by
   an in situ hot isostatic pressing method, which were compared with those
   at 900 degrees C. The maximum trapped field B-T of 3.0 T at 19 K was
   realized at the Ti-doping level of 10\%-20\%. In spite of the lower
   filling factor of 700 degrees C Ti-doped bulks, i.e., f similar to 80\%,
   the obtained B-T = 3.0 T was 1.3 times larger than that of the nondoped
   bulk (f similar to 90\%) and was comparable with those for the 900
   degrees C Ti-doped bulks (f similar to 90\%) with the same Ti contents.
   The quantitative analysis of the chemical composition revealed that fine
   particles of higher borides, i.e., MgBy (y > 2), existed in the MgB2
   matrix. The low sintering temperature of 700 degrees C and the Ti doping
   strongly suppressed the grain growth of the MgB2 matrix. The increase in
   both the grain boundaries of MgB2 and the nonsuperconducting fine
   particles enhanced the trapped field of the present 700 degrees C
   Ti-doped MgB2 bulks.}},
