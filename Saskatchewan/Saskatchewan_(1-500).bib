@article{ ISI:000390880700016,
Author = {Gupta, Jharna and Agarwal, Madhu and Dalai, A. K.},
Title = {{Optimization of biodiesel production from mixture of edible and
   nonedible vegetable oils}},
Journal = {{BIOCATALYSIS AND AGRICULTURAL BIOTECHNOLOGY}},
Year = {{2016}},
Volume = {{8}},
Pages = {{112-120}},
Month = {{OCT}},
Abstract = {{In the present study, optimization of biodiesel production from mixture
   of edible and nonedible vegetable oils with low to high free fatty acid
   (FFA) has been investigated. The selection of oils was based on richness
   of particular fatty acid in it. The combination of oils has been
   optimized to get suitable mixture for production of biodiesel. The
   mixture was analyzed in terms of physical properties and accordingly two
   step esterification process was applied. For optimization study, the
   response surface methodology (RSM) based central composite design (CCD)
   was used in Design of Experiments (DOE) software to optimize the various
   process variables such as reaction time, methanol to oil molar ratio,
   reaction temperature and catalyst concentration for biodiesel
   production. A quadratic model was created for the prediction of the
   Biodiesel yield. The R-2 value of the model was 0.96 which indicates the
   satisfactory accuracy of the model. The optimum conditions were obtained
   as follows: reaction temperature of 43.50 degrees C, methanol to oil
   molar ratio of 8.8:1, catalyst concentration of 1.9 g/100 cc feed,
   reaction time of 58.4 min. At these reaction conditions, the predicted
   and observed biodiesel yield was 97.02\% and 97.00\%, respectively.
   These values experimentally satisfied the accuracy of the model. GC and
   FTIR analysis of biodiesel was also done for biodiesel characterization.
   (C) 2016 Elsevier Ltd. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
