@article{ ISI:000364575500009,
Author = {Mostaan, H. and Shamanian, M. and Hasani, S. and Szpunar, J. A.},
Title = {{Response of structural and magnetic properties of ultra-thin FeCo-V
   foils to high-energy beam welding processes}},
Journal = {{INTERNATIONAL JOURNAL OF MINERALS METALLURGY AND MATERIALS}},
Year = {{2015}},
Volume = {{22}},
Number = {{11}},
Pages = {{1190-1198}},
Month = {{NOV}},
Abstract = {{Microstructural evolutions and grain-boundary-character distribution
   during high-energy-beam welding of ultra-thin FeCo-V foils were studied.
   Detailed data about the boundaries, coincidence site lattice (CSL)
   relationships, grain sizes, and microstructural features were acquired
   from electron-backscatter diffraction (EBSD) maps. Moreover, the
   evolution of the magnetic properties during high-energy-beam welding was
   studied using vibrating sample magnetometry (VSM). The fraction of
   low-angle boundaries was observed to increase in the fusion zones of
   both electron- and laser-beam-welded foils. The results showed that the
   fractions of low-AE (c) CSL boundaries (particularly twin boundaries, AE
   (c) 3) in the fusion zones of the welded foils are higher than those in
   the base metal. Because the strain rates produced during
   high-energy-beam welding are very high (because of the extremely high
   cooling rate), grain deformation by a slip mechanism is limited;
   therefore, deformation by grain twinning is dominant. VSM analysis
   showed that the magnetic properties of the welded foils, i.e., their
   remanence, coercive force, and energy product, changed significantly.
   The formation of large grains with preferred orientation parallel to the
   easy axis of magnetization was the main reason for the diminished
   magnetic properties.}},
