@article{ ISI:000377894300011,
Author = {Nguyen, Tung T. and Salt, Joseph Eric and Nguyen, Ha H. and Berscheid,
   Brian},
Title = {{Optimizing Pulse Shaping Filter for DOCSIS Systems}},
Journal = {{IEEE TRANSACTIONS ON BROADCASTING}},
Year = {{2016}},
Volume = {{62}},
Number = {{2}},
Pages = {{470-481}},
Month = {{JUN}},
Abstract = {{This paper proposes a cost efficient nearly linear phase approximation
   to an square root raised cosine (SRRC) pulse shaping filter that
   satisfies the out-of-band spectral constraints of DOCSIS down-stream
   channels. A nearly linear phase filter structure is converted to an SRRC
   filter using a weighted and sampled least-squares criterion to fit the
   magnitude and phase responses. To ensure stability, the search for
   optimum coefficients is constrained using the Steigliz-McBride (SM) and
   Gauss-Newton (GN) methods, which unfortunately also eliminates sets of
   stable coefficients, one of which could be and probably is the optimum.
   To expand the sets of coefficients that produce stable filters in the SM
   and GN methods, the transfer function is parameterized in a special way.
   The effectiveness of the proposed filter is verified and compared with
   other approaches.}},
