@article{ ISI:000378561600020,
Author = {Ghezelbash, A. M.},
Title = {{Gravitational perturbation and Kerr/CFT correspondence}},
Journal = {{GENERAL RELATIVITY AND GRAVITATION}},
Year = {{2016}},
Volume = {{48}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{We find the explicit form of two-point function for the conformal spin-2
   energy momentum operators on the near horizon of a near extremal Kerr
   black hole by variation of a proper boundary action. In this regard, we
   consider an appropriate boundary action for the gravitational
   perturbation of the Kerr black hole. We show that the variation of the
   boundary action with respect to the boundary fields yields the two-point
   function for the energy momentum tensor of a conformal field theory. We
   find agreement between the two-point function and the correlators of the
   dual conformal field theory to the Kerr black hole.}},
