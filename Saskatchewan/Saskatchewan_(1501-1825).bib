@article{ ISI:000302536800016,
Author = {Zhang, Hui and Shi, Yang and Mehr, Aryan Saadat},
Title = {{On H-infinity Filtering for Discrete-Time Takagi-Sugeno Fuzzy Systems}},
Journal = {{IEEE TRANSACTIONS ON FUZZY SYSTEMS}},
Year = {{2012}},
Volume = {{20}},
Number = {{2}},
Pages = {{396-401}},
Month = {{APR}},
Abstract = {{In this paper, we present a new design method for the H-infinity
   filtering of discrete-time Takagi-Sugeno (TS) fuzzy systems. The
   parameters of the filter are assumed to be linearly dependent on the
   normalized fuzzy weighting functions. By using an augmentation
   technique, the design parameters are incorporated into a filtering error
   system. In order to derive less-conservative results and reduce the
   filtering error, a new condition is established to ensure the H-infinity
   performance of the filtering error system. By introducing more slack
   matrices, the solution set of the filter parameters is extended. By
   using a partitioning technique, a design method for the H-infinity
   filter is proposed in terms of linear matrix inequalities (LMIs). An
   example demonstrates the improvement of the proposed design method over
   an existing approach.}},
