@article{ ISI:000304513700008,
Author = {Adaramola, Muyiwa S. and Bergstrom, Donald J. and Sumner, David},
Title = {{Characteristics of turbulent flow in the near wake of a stack}},
Journal = {{EXPERIMENTAL THERMAL AND FLUID SCIENCE}},
Year = {{2012}},
Volume = {{40}},
Pages = {{64-73}},
Month = {{JUL}},
Abstract = {{The characteristics of the Reynolds stress field in the wake of a stack
   were experimentally studied in a low-speed wind tunnel using thermal
   anemometry. The cross-flow Reynolds number was Re-D = 2.3 x 10(4), and
   the jet-to-crossflow velocity ratio (R) was varied from zero to three.
   The stack was partially immersed in a turbulent flat-plate boundary
   layer, with a boundary layer thickness-to-stack-height ratio of delta/H
   = 0.5 at the location of the stack. In general, the time-averaged
   velocities and Reynolds stresses were both found to be strongly
   influenced by the velocity ratio. For example, the vertical profile of
   the Reynolds shear stress in the wake was strongly influenced by the
   local velocity gradients, especially for lower values of R in the stack
   wake, and for higher values of R in the jet wake. The spanwise
   characteristics of the time-averaged streamwise and wall-normal
   velocities also varied along the stack height (z/H) and with the value
   of R. The behaviour of the shear correlation coefficient indicated that
   there is more turbulent mixing in the vicinity of the free end of the
   stack than at mid-height. (C) 2012 Elsevier Inc. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE INC}},
Address = {{360 PARK AVE SOUTH, NEW YORK, NY 10010-1710 USA}},
Type = {{Article}},
Language = {{English}},
