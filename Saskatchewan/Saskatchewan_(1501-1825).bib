@article{ ISI:000307269400034,
Author = {Dick, Rainer and Sage, Frederick S.},
Title = {{A minimal dark matter interpretation for the CRESST-II signal}},
Journal = {{EUROPEAN PHYSICAL JOURNAL C}},
Year = {{2012}},
Volume = {{72}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{We report on the possible interpretation of the two proposed dark matter
   mass values m(chi) = 11.6 GeV and m(chi) = 25.3 GeV from CRESST-II
   within the framework of the Higgs portal minimal dark matter model. We
   find that the higher mass value yields a suitable fit with a dark
   matter-Higgs coupling eta/2 = 0.157 and a recoil cross section which is
   compatible with contemporary estimates of the effective Higgs-nucleon
   coupling. On the other hand, the lower mass solution would require a
   large strangeness component in the nucleon to explain the corresponding
   nucleon recoil cross section reported by CRESST-II.}},
