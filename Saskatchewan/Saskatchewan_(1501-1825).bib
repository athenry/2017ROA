@article{ ISI:000303290200004,
Author = {Soltanzadeh, Hamidreza and Hawkes, Christopher D.},
Title = {{Evaluation of caprock integrity during pore pressure change using a
   probabilistic implementation of a closed-form poroelastic model}},
Journal = {{INTERNATIONAL JOURNAL OF GREENHOUSE GAS CONTROL}},
Year = {{2012}},
Volume = {{7}},
Pages = {{30-38}},
Month = {{MAR}},
Abstract = {{Fracturing and fault reactivation induced by pore pressure change during
   fluid production or injection may threaten the hydraulic integrity of
   hydrocarbon reservoirs and CO2 storage projects. This paper uses
   probabilistic analysis to investigate the possibility of these risks.
   Ehselby's theory of inhomogeneities is used to develop closed-form
   solutions for determination of poroelastic induced stress change as a
   result of pressure change within reservoirs with idealized geometry.
   This solution is used with a Coulomb failure criterion to determine
   critical pressure changes required for induced fracturing and fault
   reactivation within the reservoir and in the surrounding rock.
   Uncertainties in the input data are addressed by implementing the
   probabilistic analysis within the framework of a Monte Carlo simulation.
   The developed approach is applied to study the potential for fracturing
   and fault reactivation induced by pressure changes due to historical oil
   production and planned acid gas injection operations, respectively, in a
   pinnacle reef. (C) 2011 Elsevier Ltd. All rights reserved.}},
