@article{ ISI:000312697900003,
Author = {Vishniac, E. T. and Pillsworth, S. and Eyink, G. and Kowal, G. and
   Lazarian, A. and Murray, S.},
Title = {{Reconnection current sheet structure in a turbulent medium}},
