@article{ ISI:000311190800050,
Author = {McLeod, John A. and Skorikov, Nikolai A. and Finkelstein, Larisa D. and
   Kurmaev, Ernst Z. and Moewes, Alexander},
Title = {{Chemical Bonding and Hybridization in 5p Binary Oxide}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2012}},
Volume = {{116}},
Number = {{45}},
Pages = {{24248-24254}},
Month = {{NOV 15}},
Abstract = {{We analyze the electronic structure of several 5p binary oxides (In2O3,
   SnO, SnO2, Sb2O3, Sb2O5, and TeO2) using density functional theory and
   soft X-ray spectroscopy. We find that there is bonding in hybridized
   cation 5s-O 2p states at the bottom of the valence band in oxides with
   cations in both the formal 5s(2) and 5s(0) valencies. We similarly find
   that there is antibonding in hybridized cation 5d- or 5s-O 2p states at
   the top of the valence band for oxides with cations in 5s(2) or 5s(0)
   valencies, respectively, and the density of O 2p states in these
   antibonding states is much greater in the 5s(0) oxides than the 5s(0)
   oxides. The calculated and quasi-empirical band gaps for these oxides
   are tabulated, and we identify the hybridizations responsible for the
   spectral features in the measured XES.}},
