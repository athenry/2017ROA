@article{ ISI:000298596000004,
Author = {Zhang, Hui and Shi, Yang and Mehr, Aryan Saadat},
Title = {{Robust H-infinity PID control for multivariable networked control
   systems with disturbance/noise attenuation}},
Journal = {{INTERNATIONAL JOURNAL OF ROBUST AND NONLINEAR CONTROL}},
Year = {{2012}},
Volume = {{22}},
Number = {{2}},
Pages = {{183-204}},
Month = {{JAN 25}},
Abstract = {{In this paper, we study the design problem of PID controllers for
   networked control systems (NCSs) with polyhedral uncertainties. The load
   disturbance and measurement noise are both taken into account in the
   modeling to better reflect the practical scenario. By using a novel
   technique, the design problem of PID controllers is converted into a
   design problem of output feedback controllers. Our goal of this paper is
   two-fold: (1) To design the robust PID tracking controllers for
   practical models; (2) To develop the robust H-infinity PID control such
   that load and reference disturbances can be attenuated with a prescribed
   level. Sufficient conditions are derived by employing advanced
   techniques for achieving delay dependence. The proposed controller can
   be readily designed based on iterative suboptimal algorithms. Finally,
   four examples are presented to show the effectiveness of the proposed
   methods. Copyright (C) 2011 John Wiley \& Sons, Ltd.}},
