@article{ ISI:000319085400034,
Author = {Kleiv, R. T. and Harnett, D. and Steele, T. G. and Jin, Hong-ying},
Title = {{J(PC)=1(++) heavy hybrid masses from QCD sum-rules}},
Journal = {{NUCLEAR PHYSICS B-PROCEEDINGS SUPPLEMENTS}},
Year = {{2013}},
Volume = {{234}},
Pages = {{150-153}},
Month = {{JAN}},
Note = {{16th International Conference on Quantum Chromodynamics, Montpellier,
   FRANCE, JUL 02-06, 2012}},
Abstract = {{QCD Laplace sum-rules are used to calculate axial vector (J(PC) = 1(++))
   charmonium and bottomonium hybrid masses. Previous sum-rule studies of
   axial vector heavy quark hybrids did not include the dimension-six gluon
   condensate, which has been shown to be important in the 1(--) and 0(-+)
   channels. An updated analysis of axial vector heavy quark hybrids is
   performed, including the effects of the dimension-six gluon condensate,
   yielding mass predictions of 5.13 GeV for hybrid charmonium and 11.32
   GeV for hybrid bottomonium. The charmonium hybrid mass prediction
   dis-favours a hybrid interpretation of the X(3872), if it has J(PC) =
   1(++), in agreement with the findings of other theoretical approaches.
   It is noted that QCD sum-rule results for the 1(--), 0(-+) and 1(++)
   channels are in qualitative agreement with the charmonium hybrid
   multiplet structure observed in recent lattice calculations.}},
