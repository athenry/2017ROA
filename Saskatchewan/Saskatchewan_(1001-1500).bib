@article{ ISI:000324233800006,
Author = {Smolyakov, A. I. and Frias, W. and Kaganovich, I. D. and Raitses, Y.},
Title = {{Sheath-Induced Instabilities in Plasmas with E-0 x B-0 Drift}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2013}},
Volume = {{111}},
Number = {{11}},
Month = {{SEP 11}},
Abstract = {{It is shown that ion acoustic waves in plasmas with E-0 x B-0 electron
   drift become unstable due to the closure of plasma current in the
   chamber wall. Such unstable modes may enhance both near-wall
   conductivity and turbulent electron transport in plasma devices with E-0
   x B-0 electron drift and unmagnetized ions. It is shown that the
   instability is sensitive to the wall material: a high value of the
   dielectric permittivity (such as in metal walls) reduces the mode growth
   rate by an order of magnitude but does not eliminate the instability
   completely.}},
