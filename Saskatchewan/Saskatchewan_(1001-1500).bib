@inproceedings{ ISI:000346423600007,
Author = {Garbet, X. and Esteve, D. and Sarazin, Y. and Dif-Pradalier, G. and
   Ghendrih, P. and Grandgirard, V. and Latu, G. and Smolyakov, A.},
