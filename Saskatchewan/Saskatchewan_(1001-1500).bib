@article{ ISI:000317466300008,
Author = {Xu, Qing and Jin, Hong-ying and Steele, T. G.},
Title = {{Two-pseudoscalar exchanges between heavy hadrons}},
Journal = {{JOURNAL OF PHYSICS G-NUCLEAR AND PARTICLE PHYSICS}},
Year = {{2013}},
Volume = {{40}},
Number = {{5}},
Month = {{MAY}},
Abstract = {{We calculate the two-pion and two-kaon exchange potentials of D (D) over
   bar and D (D) over bar{*} by using the Gaussian form factor to
   regularize loop integrals. Although the magnitude of the
   two-pseudo-goldstone exchange is small as expected, it is not negligible
   in dynamics because it modifies the shape of the potential. We find the
   bound state of D (D) over bar{*} starts to appear at the cut-off Lambda
   = 1360 MeV after consideration of the two-pseudo-goldstone exchange,
   significantly reducing the discrepancy between chiral perturbation
   theory and other phenomenological approaches.}},
