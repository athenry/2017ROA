@article{ ISI:000322958400004,
Author = {Dinsmore, W. Paul and Serediak, Mark S. and Putz, Gordon and Prepas,
   Ellie E. and Smith, Daniel W.},
Title = {{Use of Stabilized Stream-Monitoring Sections to Monitor Annual
   Streamflow on the Alberta Boreal Plain}},
Journal = {{JOURNAL OF COLD REGIONS ENGINEERING}},
Year = {{2013}},
Volume = {{27}},
Number = {{3}},
Pages = {{168-182}},
Month = {{SEP 1}},
Abstract = {{Streamflow in small forested watersheds on the Boreal Plain of western
   Canada can be a challenge to monitor due to high variation in flow,
   shifting channel morphology, aufeis obstructions, shallow channel depth,
   and debris in the channel. Intensive monitoring in natural channels can
   overcome some of these problems, but frequent assessment and
   recalibration of streamflow-stage relationships are necessary.
   Experience over 8years indicates that in-channel structures designed to
   provide a stabilized stream-monitoring section (SSMS) proved beneficial
   to monitoring efforts during the ice-free season. The SSMS facilitated
   accurate gauging of the highest and lowest flows encountered during this
   period, provided a relatively stable foundation against streambank and
   channel erosion, and allowed passage of fish and the majority of debris
   and sediments. Functionality of the structures was markedly limited
   during ice-in conditions, but was improved with the addition of fitted
   canopies and propane heating systems.}},
