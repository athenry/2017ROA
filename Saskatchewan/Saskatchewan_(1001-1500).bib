@article{ ISI:000329989600003,
Author = {Chen, Wei and Steele, T. G. and Zhu, Shi-Lin},
Title = {{Masses of the bottom-charm hybrid (b)over-barGc states}},
Journal = {{JOURNAL OF PHYSICS G-NUCLEAR AND PARTICLE PHYSICS}},
Year = {{2014}},
Volume = {{41}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{Quantum Chromodynamics (QCD) sum rules are used to study the mass
   spectrum of bottom-charm hybrid (b) over bar Gc systems. The correlation
   functions and the spectral densities are calculated up to dimension six
   condensates at leading order of alpha(s) for several J(P) quantum
   numbers. After performing the QCD sum rule analysis, we predict the
   masses of the J(P) = 0(-), 0(+), 1(-), 1(+), 2(-), 2(+) bottom-charm
   hybrids. These mass predictions show a similar supermultiplet structure
   as the bottomonium and charmonium hybrids. Using the QCD sum rule mass
   predictions we analyze the possible hadronic decay patterns of the (c)
   over bar Gc, (b) over bar Gc and (b) over bar Gc hybrids including the
   open-flavor and hidden-flavor mechanisms.}},
