@article{ ISI:000330720400009,
Author = {Ferguson, Grant and Grasby, Stephen E.},
Title = {{The geothermal potential of the basal clastics of Saskatchewan, Canada}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2014}},
Volume = {{22}},
Number = {{1}},
Pages = {{143-150}},
Month = {{FEB}},
Abstract = {{The Winnipeg and Deadwood formations form deep clastic reservoirs in
   Saskatchewan, Canada, with temperatures exceeding 40 A degrees C over
   most of southern Saskatchewan and reaching 100 A degrees C in
   southwestern Saskatchewan. At these temperatures, the formations have
   geothermal potential for development of direct use and electricity
   generation systems. Numerous disposal wells operating at rates of 30 L/s
   or more are currently installed in these formations, suggesting that
   electricity could be generated at rates exceeding 2 megawatts of
   electrical output (MWe) from individual wells. These basal clastic
   units, thus, could provide significant energy supply over a broad region
   of Saskatchewan.}},
