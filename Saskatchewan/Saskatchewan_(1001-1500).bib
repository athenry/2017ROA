@article{ ISI:000328511000007,
Author = {Zhang, Zhichao and Dinh, Anh and Chen, Li and Khan, Muhammad},
Title = {{A low noise figure 2-GHz bandwidth LNA using resistive feedback with
   additional input inductors}},
Journal = {{IEICE ELECTRONICS EXPRESS}},
Year = {{2013}},
Volume = {{10}},
Number = {{21}},
Abstract = {{In this paper, an input inductive network for the wideband LNA is
   proposed. The two input inductors located in and out of the feedback
   loop are set respectively to combine with the conventional resistive
   feedback structure. Input matching and NF of the LNA can be optimized
   separately by varying the value of the two input inductors without
