@article{ ISI:000327864700019,
Author = {Behura, Sanjay K. and Mukhopadhyay, Indrajit and Hirose, Akira and Yang,
   Qiaoqin and Jani, Omkar},
Title = {{Vertically oriented few-layer graphene as an electron field-emitter}},
Journal = {{PHYSICA STATUS SOLIDI A-APPLICATIONS AND MATERIALS SCIENCE}},
Year = {{2013}},
Volume = {{210}},
Number = {{9}},
Pages = {{1817-1821}},
Month = {{SEP}},
Abstract = {{Field emission from flat graphene sheets is a challenge due to fewer
   emission sites. Here, we have synthesized free-standing vertically
   oriented few-layer graphene (FLG) films directly on dielectric
   substrates by hot-filament chemical vapor deposition (HFCVD) without any
   catalyst or special substrate treatment. The fabricated FLGs with a
   large smooth surface topography, standing roughly vertical to the
   substrate are found to grow according to the Stranski-Krastanov growth
   mechanism. The ease of large-area preparation and the low turn-on field
   of 22V mu m(-1) in addition to the large field enhancement factor of
   approximate to 6520 for electron field emission suggest that the
   vertically oriented FLGs could be used as a potential edge emitter.
