@article{ ISI:000350874100013,
Author = {Liu, Yi and Song, Bing and Chen, Xiongbiao and Cai, Qiang and Tian,
   Weiming},
Title = {{Alginate-Based Hydrogels Encapsulated TWS119 for Inducing Neuronal
   Differentiation of Neural Stem Cells}},
Journal = {{JOURNAL OF BIOMATERIALS AND TISSUE ENGINEERING}},
Year = {{2014}},
Volume = {{4}},
Number = {{12, SI}},
Pages = {{1087-1092}},
Month = {{DEC}},
Abstract = {{Neural stem cells (NSCs) hold great promise for central nervous system
   injury repair. However, controlling their differentiation to functional
   neuron is challenging. In present study, we developed a novel injectable
   hydrogel encapsulated with TWS119 to guide the differentiation of NSCs
   into neurons. Specifically, we used alginate hydrogel as a 3-dimensional
   substrate for the attachment, proliferation, and differentiation of
   neural stem cell in the presence of TWS119. The alginate hydrogel
   exhibited a porous 3-dimensional network, thus allowing for culture
   medium to penetrate. With its suitable biocompatibility, the alginate
   hydrogel substrate is able to promote the attachment and proliferation
   of NSCs. Our results illustrate that on alginate hydrogel with the
   presence of TWS119, NSCs exhibited higher expression of beta tubulin III
   (a neural marker) and lower glial fibrillary acidic protein (GFAP) than
   those without TWS119. Also, our results show that NSCs on an alginate
   hydrogel in the presence of TWS119 can inhibit glial cell
   differentiation while promoting neuronal differentiation.}},
