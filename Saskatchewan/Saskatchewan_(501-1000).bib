@article{ ISI:000342654000035,
Author = {Zhang, Lina and Szpunar, Jerzy A. and Basu, Ritwik and Dong, Jianxin and
   Zhang, Maicang},
Title = {{Influence of cold deformation on the corrosion behavior of Ni-Fe-Cr
   alloy 028}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2014}},
Volume = {{616}},
Pages = {{235-242}},
Month = {{DEC 15}},
Abstract = {{Ni-Fe-Cr alloy 028 (a nickel based super alloy also known as UNS N08028)
   was subjected to different degree of cold deformation by laboratory
   rolling to investigate the influence of microstructure on corrosion
   behavior under two simulated environments, (0.5 M NaCl and 1 M H2SO4).
   Experimental results showed that texture had influence on the corrosion
   rate of this alloy. The corrosion rate under both environments
   significantly decreased with increase in the ND//< 1 1 0 > orientation.
   Significant differences in the corrosion rate with increased degree of
   deformation were mainly related to different amount of the CSL
   (coincident site lattice) Sigma 3 boundaries in the ND//< 1 1 0 >
   oriented grains. The present study contributes to discussion on the CSL
   boundary formation for improved corrosion resistance. (C) 2014 Elsevier
   B.V. All rights reserved.}},
