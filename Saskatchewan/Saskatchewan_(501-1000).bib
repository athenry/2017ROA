@article{ ISI:000350563300012,
Author = {Martuza, Muhammad and Wahid, Khan A.},
Title = {{Implementation of a cost-shared transform architecture for multiple
   video codecs}},
Journal = {{JOURNAL OF REAL-TIME IMAGE PROCESSING}},
Year = {{2015}},
Volume = {{10}},
Number = {{1}},
Pages = {{151-162}},
Month = {{MAR}},
Abstract = {{The paper presents a cost-shared architecture to compute multiple
   integer discrete cosine transform (Int-DCT) of four video codecs-AVS,
   H.264/AVC, VC-1 and HEVC (under development). Based on the symmetric
   structure of the matrices and the similarity in matrix operation, we
   develop a generalized ``decompose and share{''} algorithm to compute
   both 4 x 4 and 8 x 8 Int-DCT. The algorithm is later applied to the
   video codecs. The hardware share approach ensures maximum circuit reuse
   during the computation. The architecture is designed with only adders
   and shifters to reduce the hardware cost significantly. The design is
   implemented on FPGA and later synthesized in CMOS 0.18 mu m technology.}},
