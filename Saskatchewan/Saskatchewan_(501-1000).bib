@article{ ISI:000338995300034,
Author = {Frias, Winston and Smolyakov, Andrei I. and Kaganovich, Igor D. and
   Raitses, Yevgeny},
Title = {{Wall current closure effects on plasma and sheath fluctuations in Hall
   thrusters}},
Journal = {{PHYSICS OF PLASMAS}},
Year = {{2014}},
Volume = {{21}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{The excitation of negative energy, ion sound type modes driven by the E
   x B drift and the reactive/dissipative response of the wall sheath
   interface is analyzed for conditions typical in a Hall thruster. Such
   sheath impedance modes are sensitive to the dielectric properties of the
   thruster wall material, which therefore may have direct influence (other
   than via the secondary electron emission) on fluctuations and transport.
   Our results predict mode frequencies consistent with the frequencies of
   fluctuations observed experimentally. (C) 2014 AIP Publishing LLC.}},
