@article{ ISI:000341787500010,
Author = {Ghezelbash, A. M. and Siahaan, H. M.},
Title = {{Deformed hidden conformal symmetry for rotating black holes}},
Journal = {{GENERAL RELATIVITY AND GRAVITATION}},
Year = {{2014}},
Volume = {{46}},
Number = {{9}},
Month = {{SEP}},
Abstract = {{The generic non-extremal Kerr-Newman black holes are holographically
   dual to hidden conformal field theories in two different pictures. The
   two pictures can be merged together to the dual conformal field theories
   (CFTs) in general picture that are generated by SL(2, Z) modular group.
   We find some extensions of the conformal symmetry generators that yield
   an extended local family of SL(2, R)(L) x SL(2, R)(R) hidden conformal
   symmetries for the Kerr-Newman black holes, parameterized by one
   deformation parameter. The family of deformed hidden conformal symmetry
   for Kerr-Newman black holes also provides a set of deformed hidden
   conformal symmetry for the charged Reissner-Nordstrom black holes. The
   set of deformed hidden conformal generators reduce to the hidden SL(2,
   R) conformal generators for the Reissner-Nordstrom black hole for
   specific value of deformation parameter. We also find agreements between
   the macroscopic and microscopic entropy and absorption cross-section of
   scalars for the Kerr-Newman black hole by considering the appropriate
   temperatures and central charges for the deformed CFTs.}},
