@article{ ISI:000350779000025,
Author = {Koughia, Cyril and Craig, Chris and Hewak, Daniel W. and Kasap, Safa},
Title = {{Tailoring the I-4(9/2) -> I-4(13/2) emission in Er3+ ions in different
   hosts media}},
Journal = {{OPTICAL MATERIALS}},
Year = {{2015}},
Volume = {{41}},
Number = {{SI}},
Pages = {{116-121}},
Month = {{MAR}},
Note = {{5th International Workshop on Photoluminescence in Rare Earths (PRE
   14):Photonic Materials and Devices, San Sebastian, SPAIN, MAY 13-16,
   2014}},
Abstract = {{The I-4(9/2)-I-4(13/2) emission band of Er3+ is potentially interesting
   for CH4 detection because it can overlap with the optical absorption
   band of CH4. GaLaS and GeGaS glasses doped with Er3+ ions show the
   desirable emission under 808 nm excitation. An attempt of spectrally
   shifting I-4(9/2)-I-4(13/2) emission band by substituting S by O or Se
   to obtain a perfect match to the absorption band of CH4 leads to the
   weakening and disappearance of the I-4(9/2)-I-4(13/2) band. This
   phenomenon is tentatively related to the acceleration of non-radiative
   relaxation in the case of oxygen addition and to the suppression of
   up-conversion in the selenium substitution case. The relative intensity
   of the I-4(9/2)-I-4(13/2) band (with respect to the I-4(13/2)-I-4(15/2)
   band) varies significantly in powdered and bulk materials. This effect
   is discussed in terms of the radiation diffusion (i.e. radiation
   trapping) model and is confirmed by Monte-Carlo simulations. (C) 2014
   Elsevier B.V. All rights reserved.}},
