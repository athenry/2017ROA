@article{ ISI:000342245700102,
Author = {Shamanian, Morteza and Mohammadnezhad, Mahyar and Szpunar, Jerzy},
Title = {{Texture analysis of a friction stir welded ultrafine grained Al-Al2O3
   composite produced by accumulative roll-bonding}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2014}},
Volume = {{615}},
Pages = {{651-656}},
Month = {{DEC 5}},
Abstract = {{In recent years, several studies have been focused on friction stir
   welding of aluminum alloys, and some researchers have also been reported
   on welding of aluminum-based composites. In the present research,
   ultrafine grained sheets of aluminum matrix composite (Al-Al2O3) were
   produced by accumulative roll-bonding (ARB) technique. The aluminum
   composite sheets were then joined by friction stir welding. The present
   work describes the effect of the FSW process on the microstructure and
   crystallographic textures in the base metal and weld nugget. Electron
   backscattered diffraction (EBSD) results demonstrated the existence of
   different grain orientations within the weld nugget as compared to the
   base metal. Al composite plates have a Rotated Cube texture component.
   Moreover, in the nugget, grain structure with Rotated Cube and shear
   texture developed. Friction stir welding coarsened the grain size in the
   weld zone from the original grain size of 3-17 mu m. (C) 2014 Elsevier
   B.V. All rights reserved.}},
