@article{ ISI:000356123400013,
Author = {Ibrahim, Abdulwahab and Bishop, Donald P. and Kipouros, Georges J.},
Title = {{Sinterability and characterization of commercial aluminum powder
   metallurgy alloy Alumix 321}},
Journal = {{POWDER TECHNOLOGY}},
Year = {{2015}},
Volume = {{279}},
Pages = {{106-112}},
Month = {{JUL}},
Abstract = {{The sinterability of Al-Mg-Si powder metallurgy PM alloy was
   investigated. This alloy is equivalent to AA6061 and known commercially
   as Alumix 321. Commercially available Alumix 321 has been sintered in a
   broad range of temperatures. Green uniaxially pressed compacts in the
   range of 100-500 MPa were sintered at temperatures in the range of
   610-640 degrees C. The investigated alloy shows a good sintering
   response and 98\% theoretical density (TD) was achieved. An optimal
   sintering profile has been selected and the mechanical properties were
   measured in T1, T6 tempers. Hardness, tensile strength and yield
   strength values obtained were close to the industrially published
   values. In addition to the sintering and heat treatment, the
   microstructure of sintered material has been examined and characterized
   using optical and scanning electron microscopy. (C) 2015 Elsevier B.V.
   All rights reserved.}},
