@article{ ISI:000365378700018,
Author = {Hariharasubramanian, J. and Baik, O. -D. and Singhal, R. S.},
Title = {{ULTRASOUND-ASSISTED EXTRACTION (UAE) OF POLYSACCHARIDE FROM DRIED
   BIOMASS OF SARGASSUM MUTICUM}},
Journal = {{TRANSACTIONS OF THE ASABE}},
Year = {{2015}},
Volume = {{58}},
Number = {{5}},
Pages = {{1363-1370}},
Month = {{SEP-OCT}},
Abstract = {{This study was aimed at identifying the parameters influencing the
   extraction efficiency and polysaccharide yields from dried biomass of
   Sargassum muticum using ultrasound-assisted extraction (UAE). Factors
   found to positively influence the extraction outcome were sample
   quantity, solvent composition, ultrasonic horn type, horn immersion
   depth, extraction time, and sonication amplitude (duty cycle). Pulsed
   ultrasound treatment was found to be highly effective as compared to
   continuous treatment. The extraction efficiency was significantly
   improved by using 0.03 M HCl as extractant. Optimization of UAE
   parameters led to the development of a rapid, cost-effective,
   energy-efficient approach yielding 24.8\% +/- 1.28\% polysaccharide on
   dry weight basis, as compared to 11.08\% +/- 0.78\% by conventional
   extraction employing hot reflux after three cycles. The yield obtained
   by UAE is at least 3.5 times higher than the reported yield of 7\% DW.}},
