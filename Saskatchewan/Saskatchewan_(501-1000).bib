@article{ ISI:000340554800004,
Author = {Tilay, Ashwini and Azargohar, Ramin and Dalai, Ajay K. and Annapure,
   Uday S. and Kozinski, Janusz A.},
Title = {{Extraction and fractionation of polyunsaturated fatty acids from
   Mortierella sp using supercritical fluid: experimental and kinetic
