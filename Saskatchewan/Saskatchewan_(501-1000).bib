@article{ ISI:000348957400022,
Author = {Eskandari, M. and Zarei-Hazanki, A. and Mohtadi-Bonab, M. A. and Odeshi,
   A. G. and Szpunar, J. A.},
Title = {{Microstructure and texture evolution in 21Mn-2.5Si-1.6Al-Ti steel
   subjected to dynamic impact loading}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2015}},
Volume = {{622}},
Pages = {{160-167}},
Month = {{JAN 12}},
Abstract = {{The microstructure and texture evolutions in a microalloyed high Mn
   austenitic steel during dynamic impact loading was investigated.
   Microstructural investigations confirmed heterogeneous deformation
   leading to formation of adiabatic shear band which align parallel to the
   impact direction. Electron backscatter diffraction analysis indicated
   the formation of athermal epsilon martensite and alpha-martensite within
   the shear bands. The thermally induced epsilon martensite follows
   Shoji-Nishiyama crystal-lographic orientation relationship with parent
   austenite phase, while alpha-martensite shows Burgers relationship with
   epsilon martensite. The plastic deformation within the austenite phase
   is characterized by slip and mechanical twinning alongside with
   martensite formation in regions away from shear bands. The austenite
   texture outside shear band regions showed characteristic near ID
   parallel to < 110 > orientation (ID: impact direction), whereas the
   regions inside shear band exhibit both near ID parallel to < 100 > and
   ID parallel to < 110 > orientations. (C) 2014 Elsevier B.V. All rights
   reserved.}},
