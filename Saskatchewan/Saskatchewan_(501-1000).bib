@inproceedings{ ISI:000369566100041,
Author = {Ogunremi, Ayodele and Sumner, David},
Book-Group-Author = {{ASME}},
Title = {{THE EFFECTS OF INCIDENCE ANGLE ON THE MEAN WAKE OF A SURFACE-MOUNTED
   FINITE-HEIGHT SQUARE PRISM}},
