@article{ ISI:000349088800011,
Author = {Misra, Prachee and Chitanda, Jackson M. and Dalai, Ajay K. and Adjaye,
   John},
Title = {{Immobilization of fluorenone derived pi-acceptors on poly (GMA-co-EGDMA)
   for the removal of refractory nitrogen species from bitumen derived gas
   oil}},
Journal = {{FUEL}},
Year = {{2015}},
Volume = {{145}},
Pages = {{100-108}},
Month = {{APR 1}},
Abstract = {{A pretreatment process employing porous polymer support coupled with
   electron withdrawing pi-acceptors has been developed for the selective
   removal of refractory nitrogen compounds from gas oil via charge
   transfer complex (CTC) mechanism. Copolymer of glycidyl methacrylate and
   ethylene glycol dimethacrylate, poly (GMA-co-EGDMA) was used as the
   porous polymer support for the immobilization of fluorenone based
   pi-acceptors. In order to study the effect of various pi-acceptors,
   2,7-dinitro-9-fluorenone (DNF); 2,4,7-trinitro-9-fluorenone (TriNF) and
   2,4,5,7-tetranitro-9-fluorenone (TENF) were immobilized on poly
   (GMA-co-EGDMA) via hydroxylamine (ON) linker. BET, CHNOS elemental
   analysis, SEM and TG/DTA were employed for determining the chemical and
   physical properties of the synthesized compounds. Characterization by
   FT-IR revealed the immobilization of pi-acceptor on the polymers. Total
   Nitrogen/Sulfur analyzer affirmed the decrease in total nitrogen and
   sulfur content of bitumen derived light gas oil (LGO) after
   pre-treatment with immobilized polymers. This decrease in total nitrogen
   content of gas oil is a direct consequence of the formation of charge
   transfer complexes between pi-acceptors immobilized polymer and
   heterocyclic nitrogen species present in gas oil. Furthermore,
   spectroscopic features of the charge transfer interactions between
   pi-acceptors and basic and non-basic nitrogen species were confirmed by
   UV-vis spectrophotometer. C-13 NMR was used to calculate the decrease in
   aromatic content after treatment. The optimized polymers were successful
   in removing up to 14.4\% of the total nitrogen content in a single
   contact using 25 wt\% of the immobilized polymers with respect to LGO
   while only 1.4\% of the total sulfur moieties were removed at 25 degrees
   C. (C) 2014 Elsevier Ltd. All rights reserved.}},
