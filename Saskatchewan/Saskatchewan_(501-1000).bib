@article{ ISI:000358002500027,
Author = {Ferguson, Grant and Jasechko, Scott},
Title = {{The isotopic composition of the Laurentide Ice Sheet and fossil
   groundwater}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2015}},
Volume = {{42}},
Number = {{12}},
Pages = {{4856-4861}},
Month = {{JUN 28}},
Abstract = {{Stable isotopes have been used to study large-scale changes in hydrology
   during the Pleistocene epoch. Many of these efforts have required an
   estimate of the O-18 value of runoff generated by melting ice sheets.
   There is no consensus on representative values. Here we examine O-18
   values from fossil groundwater samples and isotope-enabled general
   circulation models (GCMs) to better understand the isotopic composition
   of the Laurentide Ice Sheet (LIS). Groundwater O-18 values ranged from
   -12.5 to -25.3 and tended to increase southward. The O-18 precipitation
   values predicted by GCMs follow a similar trend but increase more
   steeply southward. The difference in groundwater and GCM output can be
   explained by invoking movement of glacial ice and meltwater, along with
   mixing within groundwater systems. Most groundwater O-18 values are
   higher than an average LIS O-18 value of -25.42.5 calculated based on
   estimated ice sheet volumes and sea level data.}},
