@article{ ISI:000354984100531,
Author = {Miura, Kodai and Kobayashi, Satoru and Onuki, Yusuke and Szpunar, Jerzy
   A. and Kamada, Yasuhiro},
Title = {{Angular dependence of hysteresis scaling and coercivity for
   anisotropically distributed ferromagnetic nanoparticles in paramagnetic
   matrix}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2015}},
Volume = {{117}},
Number = {{17}},
Month = {{MAY 7}},
Abstract = {{We have investigated anisotropy behavior of coercivity and scaling law
   for nano-scale ferromagnetic phases embedded in a paramagnetic matrix of
   an austenitic stainless steel. Small ferromagnetic martensites are
   induced by uniaxial tensile deformation. A scaling-law relationship
   between the hysteresis loss and remanence, with a power law exponent of
   1.47 +/- 0.09, has been found, irrespective of stress, martensite volume
   fraction, and angle between the magnetization and tensile directions. A
   coefficient of the scaling law decreases with volume fraction, whereas
   it increases with increasing the angle and maximizes when the
   magnetization direction is perpendicular to that of the tensile. This
   trend is opposite to that of coercivity. The behavior of the coefficient
   and the coercivity was discussed from the viewpoint of morphology of
   martensite particles. (C) 2015 AIP Publishing LLC.}},
Publisher = {{AMER INST PHYSICS}},
Address = {{1305 WALT WHITMAN RD, STE 300, MELVILLE, NY 11747-4501 USA}},
