@inproceedings{ ISI:000381813000040,
Author = {Martinson, M. and Lewis, R. A. and Fouras, A. and Siew, M. and Wallace,
   M. and Hooper, S. B. and Babyn, P.},
Editor = {{Jaffray, DA}},
Title = {{Indirect measurement of average alveolar dimension using dynamic
   phase-contrast imaging}},
Booktitle = {{WORLD CONGRESS ON MEDICAL PHYSICS AND BIOMEDICAL ENGINEERING, 2015, VOLS
   1 AND 2}},
