@article{ ISI:000339114100040,
Author = {Nyrow, Alexander and Tse, John S. and Hiraoka, Nozomu and Desgreniers,
   Serge and Buening, Thomas and Mende, Kolja and Tolan, Metin and Wilke,
   Max and Sternemann, Christian},
Title = {{Pressure induced spin transition revealed by iron M-2,M-3-edge
   spectroscopy}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2014}},
Volume = {{104}},
Number = {{26}},
Month = {{JUN 30}},
Abstract = {{We present a method to characterize pressure induced magnetic high to
   low spin transition in iron sulphide using x-ray Raman scattering
   spectroscopy at the iron M-2,M-3-edge. The advantage of this method is
   that the observed spectral changes between pressures of 1.7 GPa and 10.1
   GPa can be used with the help of atomic multiplet calculations to
   determine the crystal field splitting parameters associated with the
   spin transition. We discuss the potential of this M-2,M-3-edge
   spectroscopy to investigate the irons electronic spin state in-situ at
   the conditions of the inner Earth, i.e., at high temperature and high
   pressure, providing exciting opportunities for geophysical and materials
   science applications. (C) 2014 AIP Publishing LLC.}},
