@article{ ISI:000356198700004,
Author = {Nasr, Mohammad Rafati and Kassai, Miklos and Ge, Gaoming and Simonson,
   Carey J.},
Title = {{Evaluation of defrosting methods for air-to-air heat/energy exchangers
   on energy consumption of ventilation}},
Journal = {{APPLIED ENERGY}},
Year = {{2015}},
Volume = {{151}},
Pages = {{32-40}},
Month = {{AUG 1}},
Abstract = {{Energy consumption for ventilation is extensive in cold climates.
   Air-to-air heat/energy recovery is a well-known and effective method to
   reduce the energy consumption. However, frosting commonly occurs inside
   heat/energy exchangers in cold climates, which would significantly
   degrade the performance of the exchangers. Preheating the outdoor air
   and bypassing the outdoor airflow are two effective methods for frosting
   prevention or defrosting. In this study, the performance of two
   cross-flow heat/energy exchangers at frosting and defrosting periods are
   experimentally tested under different operating conditions and the
   values of frosting limit and defrosting time ratio are presented. As
   well, the effects of these two defrosting methods on energy consumption
   of ventilation in three cold cities (i.e. Saskatoon, Anchorage and
   Chicago) are evaluated. The results show the outdoor air preheating
   method performs better than the outdoor air bypassing method. In
   addition, the heat/energy recovery potential in Saskatoon undergoes the
   largest reduction under frosting, and the cold weather conditions have
   less impact on energy exchangers than heat exchangers for heat/energy
   recovery. (C) 2015 Elsevier Ltd. All rights reserved.}},
