@article{ ISI:000355371100019,
Author = {Yue, Caixia and Wang, Jiancheng and Han, Lina and Chang, Liping and Hu,
   Yongfeng and Wang, Hui},
Title = {{Effects of pretreatment of Pd/AC sorbents on the removal of Hg-0 from
   coal derived fuel gas}},
