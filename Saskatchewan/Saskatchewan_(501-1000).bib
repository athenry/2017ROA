@article{ ISI:000356294400012,
Author = {Bassey, Bassey and Moreno, Beatriz and Chapman, Dean},
Title = {{Synchrotron radiation shielding design and ICRP radiological protection
   quantities}},
Journal = {{JOURNAL OF RADIOLOGICAL PROTECTION}},
