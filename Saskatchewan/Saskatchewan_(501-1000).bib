@article{ ISI:000389139600006,
Author = {Nanda, Sonil and Dalai, Ajay K. and Kozinski, Janusz A.},
Title = {{CELLULAR MODIFICATIONS IN PINEWOOD DURING DELIGNIFICATION AND
   HYDROTHERMAL PRETREATMENT}},
Journal = {{J-FOR-Journal of Science \& Technology for Forest Products and Processes}},
Year = {{2015}},
Volume = {{5}},
Number = {{3}},
Pages = {{27-35}},
Abstract = {{Lignocellulosic biomasses are potential feedstocks for biofuel
   production. However, the structural interface between cellulose,
   hemicellulose, and lignin in biomass is least understood. The current
   study investigates the effects of delignification and hydrothermal
   pre-treatment on forestry biomass (pinewood) to remove lignin and
   increase cellulose accessibility. The feedstocks were physico-chemically
   characterized through CHNS analysis, TGA, XRD, FTIR, Raman spectroscopy,
   SEM, and AFM. As a new concept, in-situ distribution of lignin in
   biomass was studied through Raman mapping. Delignification caused about
   95\% removal of lignin in pinewood. The cellulose content increased to
   82\% in delignified pinewood from 50.7\% in untreated pinewood. The
   lignin level decreased from 24.2\% in untreated pinewood to 1.2\% in
   delignified pinewood. Microscopic studies revealed interwoven cellulose
   fibres on delignified pinewood surfaces, suggesting their enhanced
   accessibility to cellulolytic enzymes. Delignification caused remarkable
   removal of lignin with partial exclusion of hemicellulose, thus
   increasing cellulose availability.}},
