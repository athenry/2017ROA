@article{ ISI:000347360200015,
Author = {Yao, Yansun and Yong, Xue and Tse, John S. and Greschner, Michael J.},
Title = {{Dihydrogen Bonding in Compressed Ammonia Borane and Its Roles in
   Structural Stability}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2014}},
Volume = {{118}},
Number = {{51}},
Pages = {{29591-29598}},
Month = {{DEC 25}},
Abstract = {{Theoretical structure prediction calculations have revealed that the
   conformation of ammonia borane (NH3BH3) in the crystalline state can be
   modified by pressure, changing from the staggered configuration at low
   pressure to an eclipsed geometry at high pressure. At low pressure, the
   crystalline structure is stabilized by the charge transfer NH
   delta+delta-HB dihydrogen interactions. In the high pressure polymorphs,
   the NH3BH3 is predicted to form a layered structure. The NH
   delta+delta-HB bonding is still predominant within the layer. The
   stacking of the layers, however, is determined by the occurrence of
   additional homopolar BH delta-delta-HB interaction unprecedented in
   NH3BH3 and facilitated by the eclipsed conformation. This bonding is
   shown to be the result of secondary interactions between BH3 groups from
   molecules of adjacent layers. Topological analysis of the charge density
   and perturbation calculations on the molecule fragments show
   unambiguously that the BH delta-delta-HB interaction is covalent in
   nature with the bond strength comparable to a conventional hydrogen
   bond.}},
