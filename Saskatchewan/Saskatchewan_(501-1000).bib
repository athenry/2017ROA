@article{ ISI:000352115600045,
Author = {Majhi, Sachchit and Dalai, Ajay K. and Pant, Kamal K.},
Title = {{Methanol assisted methane conversion for higher hydrocarbon over
   bifunctional Zn-modified Mo/HZSM-5 catalyst}},
Journal = {{JOURNAL OF MOLECULAR CATALYSIS A-CHEMICAL}},
Year = {{2015}},
Volume = {{398}},
Pages = {{368-375}},
Month = {{MAR}},
Abstract = {{The co-conversion of methane with methanol was studied over Mo/H-ZSM-5
   and Zn-Mo/H-ZSM-5 catalysts prepared by the wet impregnation method. The
   catalysts were calcined at 550 degrees C followed by reduction at 450
   degrees C for 5 h. The activity tests were carried out at three
   different temperature viz. 550, 600, and 650 degrees C. BET, BJH pore
   volume distribution, H-2-TPR, NH3-TPD, FTIR, SEM-EDX, TEM, and XRD
   techniques were used for the characterization of the pre-reduced
   catalyst. Incorporation of 2\% Zn on to 5\% Mo/H-ZSM-5 catalyst
   significantly improved the activity of the catalyst for methane
   conversion. Thermodynamic studies revealed that temperature below 600
   degrees C, methane aromatization is not possible, however, in the
   presence of methanol over bifunctional catalyst it becomes thermodynamic
   feasible (Delta G = -8.8, -15.5, and -22.2 kJ/mol at 400, 500, and 600
   degrees C). The main products of the reaction were ethylene, ethane,
   propane, butane butylenes and aromatics such as benzene, toluene, ethyl
   benzene and xylene. Selectivity of alkylated aromatic product enhance in
   the presence of methanol. (C) 2014 Elsevier B.V. All rights reserved.}},
