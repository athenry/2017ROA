@article{ ISI:000360785100014,
Author = {Hasani, Saeed and Shafyei, All and Shamanian, Morteza and Behjati,
   Peiman and Mostaan, Hossein and Juuti, Timu and Szpunar, Jerzy A.},
Title = {{Correlation Between Magnetic Properties and Allotropic Phase Transition
   of Fe-Co-V Alloy}},
Journal = {{ACTA METALLURGICA SINICA-ENGLISH LETTERS}},
Year = {{2015}},
Volume = {{28}},
Number = {{8}},
Pages = {{1055-1058}},
Month = {{AUG}},
Abstract = {{In this study, the allotropic phase transition and its effect on the
   magnetic behavior of Fe Co-7 wt\%V alloy were investigated. It was found
   that gamma phase is observed in the microstructure in the as-cast
   condition, and it diminishes after severe cold rolling (90\% reduction).
   After annealing at temperatures higher than 500 up to 750 degrees C, the
   y phase is observed in the structure, again. But, this phase is
   disappeared by annealing at temperatures above 750 degrees C due to the
   formation of vanadium-rich precipitates. Thermocalc software was used in
   order to elucidate the influence of vanadium percent on the stability of
   gamma phase in Fe-Co alloys. Also, magnetic studies showed that the
   saturation induction is reduced by annealing at temperatures from 500 up
   to 750 degrees C, which is related to the formation of residual
   non-magnetic gamma phase.}},
