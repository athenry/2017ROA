Title = {{Optimal Power Allocation in Cognitive Networks Using Non-Orthogonal AF
   Relays}},
Booktitle = {{2014 IEEE 39TH CONFERENCE ON LOCAL COMPUTER NETWORKS (LCN)}},
Year = {{2014}},
