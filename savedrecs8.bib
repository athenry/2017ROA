@article{ ISI:000327437000022,
Author = {Zeberoff, A. and Derda, R. and Elias, A. L.},
Title = {{Remote activation of a microactuator using a frequency specific
   photoresponsive gold nanoparticle composite}},
Journal = {{JOURNAL OF MICROMECHANICS AND MICROENGINEERING}},
Year = {{2013}},
Volume = {{23}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{We have demonstrated a photo-responsive microactuator comprised of
   monodispersed microspheres that contain gold nanoparticles (Au NPs),
   which, upon irradiation, transduce optical energy to thermal energy,
   driving a phase change in the matrix in which they are embedded.
   Firstly, we characterized the thermal energy transduced by the Au
   NP-loaded polymer microspheres upon optical irradiation from a laser at
   532 nm. The photo-responsive microspheres were then combined with
   polyethylene glycol (PEG, Mn = 1100 g mol(-1)) wax, a phase-change
   material that undergoes a 7.5\% expansion in volume upon heating. These
   materials were installed in a 1 mm(3) reservoir within a poly(methyl
   methacrylate) substrate, and capped with a thin silicone membrane. When
   irradiated with the low intensity light (36 mW), the PEG wax would melt,
   expand and deform against the silicone membrane. Multiple reversible
   cycles were demonstrated, and peak deformations of 110 mu m were
   achieved. The time to reach 99\% actuation displacement ranged from 42
   to 91 s.}},
