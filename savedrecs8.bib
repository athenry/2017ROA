Author = {Homenda, Wladyslaw and Jastrzebska, Agnieszka and Pedrycz, Witold},
Editor = {{Saeed, K and Snasel, V}},
Title = {{Joining Concept's Based Fuzzy Cognitive Map Model with Moving Window
   Technique for Time Series Modeling}},
