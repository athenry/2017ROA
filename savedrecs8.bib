@article{ ISI:000331153400012,
Author = {da Costa, Leonardo Moreira and Stoyanov, Stanislav R. and Gusarov,
   Sergey and Seidl, Peter R. and de M. Carneiro, Jose Walkimar and
   Kovalenko, Andriy},
Title = {{Computational Study of the Effect of Dispersion Interactions on the
   Thermochemistry of Aggregation of Fused Polycyclic Aromatic Hydrocarbons
   as Model Asphaltene Compounds in Solution}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY A}},
