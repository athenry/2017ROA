@inproceedings{ ISI:000345985300005,
Author = {Martin, Derek},
Editor = {{Alejano, R and Perucho, A and Olalla, C and Jimenez, R}},
Title = {{The impact of brittle behaviour of rocks on tunnel excavation design}},
Booktitle = {{ROCK ENGINEERING AND ROCK MECHANICS: STRUCTURES IN AND ON ROCK MASSES}},
Year = {{2014}},
Pages = {{51-62}},
Note = {{ISRM European Symposium, Vigo, SPAIN, MAY 26-29, 2014}},
Organization = {{ISRM; Spanish Soc Rock Mech; Univ Vigo, Sch Min Engn}},
Abstract = {{The design of tunnels in hard rocks must consider two general forms of
   potential instability: (1) blocks/wedges formed by intersecting
   discontinuities and falling under gravitational loading, and (2)
   spalling/slabbing caused by over-stressing of the intact matrix between
   the discontinuities. While much effort has been placed on identifying
   the ground conditions that can lead to gravity driven block/wedge
   instability, the ground conditions required for spalling remain poorly
   defined and often get mixed with the conditions for wedge instability.
   From a tunnel design perspective two issues must be addressed: (1) what
   procedure should be used during the site characterization phase to
   assess if spalling should be anticipated during construction, and (2) if
   spalling is expected, what design procedure should be used to assess the
   lateral and radial extent of the spalled zone. Both of these questions
   are addressed using case history examples.}},
