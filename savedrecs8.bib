Author = {Magdaleno-Adame, S. and Olivares-Galvan, J. C. and Fofana, Issouf},
Book-Group-Author = {{IEEE}},
Title = {{Electric Field Distributions to Analyze Possible Manufacturing Defects
   of a Preventive Autotransformer}},
Booktitle = {{2014 INTERNATIONAL CONFERENCE ON HIGH VOLTAGE ENGINEERING AND
   APPLICATION (ICHVE)}},
Series = {{International Conference on High Voltage Engineering and Application}},
