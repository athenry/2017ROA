@article{ ISI:000379310000016,
Author = {Ma, Youqiao and Eldlio, Mohamed and Maeda, Hiroshi and Zhou, Jun and
   Cada, Michael},
Title = {{Plasmonic properties of superconductor-insulator-superconductor
   waveguide}},
Journal = {{APPLIED PHYSICS EXPRESS}},
Year = {{2016}},
Volume = {{9}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{The simultaneous realization of low propagation loss and subwavelength
   mode localization remains one of the critical challenges in plasmonics.
   Aiming to simultaneously realize low propagation loss and subwavelength
   mode localization in plasmonics, we introduce a class of low-loss and
   deeply confined guiding schemes utilizing an alternative plasmonic
   material, i.e., a superconductor (SC). The optical properties of a
   SC-insulator-SC (SCISC) waveguide are analyzed both at terahertz (THz)
   and telecommunication (TC) frequencies. The SCISC waveguide features a
   deep-subwavelength confinement with a mode length as small as
   lambda/6000 (lambda/18) for THz (TC) frequency, while the propagation
   length can be extended up to 400 mm (1mm). (C) 2016 The Japan Society of
   Applied Physics}},
