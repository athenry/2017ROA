@article{ ISI:000359945900019,
Author = {Nikoofard, Sara and Ugursal, V. Ismet and Beausoleil-Morrison, Ian},
Title = {{Economic Analysis of Energy Upgrades Based on Tolerable Capital Cost}},
Journal = {{JOURNAL OF ENERGY ENGINEERING}},
Year = {{2015}},
Volume = {{141}},
Number = {{3}},
Month = {{SEP}},
Abstract = {{To evaluate the economic feasibility of energy efficiency or renewable
   energy upgrades, a variety of tests are used such as payback period,
   cost-benefit ratio, and return on investment. All of these tests require
   an estimate of the capital cost of the upgrade. However, it is not
   always possible to reliably estimate the capital cost of a potential
   energy upgrade. To deal with such situations, an alternative approach is
   proposed that involves the calculation of the tolerable capital cost
   (TCC) of the upgrade. The use of the TCC approach to evaluate economic
   feasibility is demonstrated with a case study involving photovoltaic
   panel installations in Canadian houses. (C) 2014 American Society of
   Civil Engineers.}},
