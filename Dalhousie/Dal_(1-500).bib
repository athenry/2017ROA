@article{ ISI:000359945500004,
Author = {Lywood, Justine and Robertson, Mark and Leavitt, Sloan and Diallo,
   Claver and Jamieson, Rob},
Title = {{Development of a Linear Program to Optimize Sludge Management Planning
   in Nunavut, Canada}},
Journal = {{JOURNAL OF COLD REGIONS ENGINEERING}},
Year = {{2015}},
Volume = {{29}},
Number = {{3}},
Month = {{SEP}},
Abstract = {{The preparation and development of sludge management plans for waste
   stabilization ponds in Nunavut communities require the selection of
   processing options that are both appropriate for each community and
   minimize the total regional operating cost. Considering Nunavut's unique
   regional constraints, five feasible sludge processing treatment trains
   were identified. A linear programming model was developed to identify
   the optimal selection of treatment technologies for 13 Nunavut
   communities that would minimize the total cost of sludge processing over
   a period of 40 years. In this study, preliminary cost estimates and
   literature-based sludge accumulation rates were used as inputs for the
