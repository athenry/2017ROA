@article{ ISI:000363820700017,
Author = {Sweet, G. A. and Brochu, M. and Hexemer, Jr., R. L. and Donaldson, I. W.
   and Bishop, D. P.},
Title = {{Consolidation of aluminum-based metal matrix composites via spark plasma
   sintering}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2015}},
Volume = {{648}},
Pages = {{123-133}},
Month = {{NOV 11}},
Abstract = {{Aluminum-based metal matrix composite (MMC) materials containing SiC,
   AIN, Si3N4 or BN were processed by spark plasma sintering (SPS). Ceramic
   powder type, size and content were investigated to determine their
   effect on sinter quality. SiC, AIN and Si3N4 proved to be amenable to
   this style of processing whereas all systems containing BN were not.
   Full densification of MMCs that incorporated finer ceramic particles was
   problematic due to the presence of ceramic clusters and the extent to
   which aluminum could be forced into these regions. MMCs that
   incorporated coarser ceramic particles were more easily densified, but
   their hardness benefits were inferior. Three point bend tests proved to
   be an effective tool in determining the relative sinter quality of MMC
   systems. SiC outperformed AIN MMC samples in bend tests. Excellent
   ductility and bend strength was developed at low temperatures, while
   equivalent AlN-bearing materials required significantly higher
   temperatures to achieve comparable properties. (C) 2015 Elsevier B.V.
   All rights reserved.}},
