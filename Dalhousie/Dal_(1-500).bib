@article{ ISI:000358970900053,
Author = {Celli, Giovana Bonat and Ghanem, Amyl and Brooks, Marianne Su-Ling},
Title = {{Optimization of ultrasound-assisted extraction of anthocyanins from
   haskap berries (Lonicera caerulea L.) using Response Surface Methodology}},
Journal = {{ULTRASONICS SONOCHEMISTRY}},
Year = {{2015}},
Volume = {{27}},
Pages = {{449-455}},
Month = {{NOV}},
Abstract = {{Haskap berries (Lonicera caerulea L.) are a rich source of bioactive
   molecules. As such, the extraction of anthocyanins is important for the
   development of many value-added products and functional food
   ingredients. In this paper, the ultrasound-assisted extraction (UAE) of
   anthocyanins from haskap berries was investigated. Significant
   independent variables were screened and optimized using Plackett-Burman
   (PB) and Box-Behnken (BB) designs, respectively. The mathematical model
   showed a high coefficient of determination (R-2 = 0.9396) and the
   optimum conditions for the extraction were as follows: liquid/solid
   ratio 25:1 (mL/g), solvent composition of 80\% ethanol, addition of
   0.5\% formic acid, ultrasound bath temperature of 35 degrees C for 20
   min. Under these conditions, the total anthocyanin content of 22.73 mg
   cyaniding 3-glucoside equivalents (C3G)/g dry weight (DW) was consistent
   with the predicted response of 22.45 mg C3G/g DW from the model (mean
   error of 1.28\%). Five anthocyanins were identified in the optimized
   extract, namely cyanidin 3,5-diglucoside, cyanidin 3-glucoside, cyanidin
   3-rutinoside, pelargonidin 3-glucoside, and peonidin 3-glucoside. Thus,
   UAE is a suitable technique for the extraction of anthocyanins from
   haskap berries. (C) 2015 Elsevier B.V. All rights reserved.}},
