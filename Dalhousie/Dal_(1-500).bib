@article{ ISI:000348653800002,
Author = {Amon-Armah, Frederick and Yiridoe, Emmanuel K. and Jamieson, Rob and
   Hebb, Dale},
Title = {{Comparison of Crop Yield and Pollution Production Response to Nitrogen
   Fertilization Models, Accounting for Crop Rotation Effect}},
Journal = {{AGROECOLOGY AND SUSTAINABLE FOOD SYSTEMS}},
Year = {{2015}},
Volume = {{39}},
Number = {{3}},
Pages = {{245-275}},
Month = {{MAR 16}},
Abstract = {{Alternative mathematical functional forms commonly applied in modelling
   crop and pollution production response to nitrogen (N) fertilizer use
   were investigated. Data were generated using soil and water assessment
   tool modeling, and explicitly accounted for rotation effects on
   regression parameters. The Mitscherlich-Baule model best represented
   potato, carrot, and alfalfa yield response, while the quadratic model
   best described corn, winter wheat, and barley yield response to N
   fertilization. The quadratic functional form also best represented
   nitrate-N leaching response to N fertilization for most crops. Maximum
   economic rates of N fertilization for crops were sensitive to residual N
   effects of previous crops.}},
