@article{ ISI:000388517100002,
Author = {Amyotte, Paul R. and Berger, Scott and Edwards, David W. and Gupta, Jai
   P. and Hendershot, Dennis C. and Khan, Faisal I. and Mannan, M. Sam and
   Willey, Ronald J.},
Title = {{Why major accidents are still occurring}},
Journal = {{CURRENT OPINION IN CHEMICAL ENGINEERING}},
Year = {{2016}},
Volume = {{14}},
Pages = {{1-8}},
Month = {{NOV}},
Abstract = {{Major accidents happen in the process industries with relatively low
   frequencies but extremely severe consequences. Harm to workers and the
   public, loss of company property and other assets, business
   interruption, and environmental degradation are all possible outcomes of
   such events. This paper explores the occurrence of major process
   accidents and offers suggestions for their prevention according to seven
   core concepts drawn from an analysis of the 1984 Bhopal tragedy: (i) the
   creation of paradigm-enhancing organizations, (ii) inherently safer
   design, (iii) awareness of the total cost of major accidents, (iv)
   consideration of the broader societal and cultural aspects of major
   accidents, (v) process safety culture, (vi) process safety competency,
   and (vii) dynamic operational risk management.}},
