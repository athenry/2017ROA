Title = {{M-ATTEMPT: A New Energy-Efficient Routing Protocol for Wireless Body
   Area Sensor Networks}},
Booktitle = {{4TH INTERNATIONAL CONFERENCE ON AMBIENT SYSTEMS, NETWORKS AND
   TECHNOLOGIES (ANT 2013), THE 3RD INTERNATIONAL CONFERENCE ON SUSTAINABLE
   ENERGY INFORMATION TECHNOLOGY (SEIT-2013)}},
