@article{ ISI:000299517100012,
Author = {Sun, Jiabing and Zhang, Chengjin and Gu, Jason},
Title = {{Decentralized Optimal Fusion Filtering for Multi-Sensor Multi-Delay
   Singular Systems}},
Journal = {{CIRCUITS SYSTEMS AND SIGNAL PROCESSING}},
Year = {{2012}},
Volume = {{31}},
Number = {{1}},
Pages = {{163-176}},
Month = {{FEB}},
Abstract = {{The optimal fusion problem for the state estimation of discrete-time
   multi-sensor multi-delay stochastic singular systems is considered. The
   key idea is to convert a multi-sensor stochastic singular system into a
   group of nonsingular systems. Applying projection theory, the state
   estimation and estimating error variance for each local nonsingular
   subsystem are derived. After obtaining the estimating error
   cross-covariance between local subsystems, and using the optimal fusion
   algorithm in the linear minimum variance sense, a fusion filter for the
   original singular system is derived. A simulation example shows that the
   fusion filter is better than each local filter.}},
