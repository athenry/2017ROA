@inproceedings{ ISI:000397484600091,
Author = {Fenton, G. A. and Hicks, M. A.},
Editor = {{Delage, P and Desrues, J and Frank, R and Puech, A and Schlosser, F}},
Title = {{Site Sampling: Assessing Residual Uncertainty}},
Booktitle = {{CHALLENGES AND INNOVATIONS IN GEOTECHNICS: PROCEEDINGS OF THE 18TH
   INTERNATIONAL CONFERENCE ON SOIL MECHANICS AND GEOTECHNICAL ENGINEERING,
   VOL 1}},
Year = {{2013}},
Pages = {{523-526}},
Note = {{18th International Conference on Soil Melchanics and Geotechnical
   Engineering (18th ICSMGE), Paris, FRANCE, SEP 02-06, 2013}},
Abstract = {{Geotechnical design is plagued by the uncertainty associated with site
   characterization. Common questions are ``How many samples should be
   taken?{''} and ``How do these samples reduce my uncertainty?{''} Of
   considerable interest is the question ``What site sampling plan will
   give the best cost to effectiveness ratio?{''} This papers looks
   specifically at the effect of the number of samples on residual
   uncertainty. The results can be used to quantitatively select the
   required number of samples needed to achieve a target maximum residual
   uncertainty level. To study this problem, a square domain is selected
   (the site) and a stationary Gaussian random field is simulated within
   the domain (the random soil properties). The random field is sampled at
   a series of locations and a trend is estimated from the samples. The
   trend is then removed from the random field and the residual random
   field is statistically analyzed to determine various measures of the
   effectiveness of the sampling scheme. These measures include: I) the
   variance of the residual field average (i.e. does the estimate represent
   the average?), 2) the residual standard deviation (i.e. how much
   residual uncertainty remains?), and 3) the residual correlation length
   (i.e. how does trend removal affect the perceived correlation lengths?).}},
