@article{ ISI:000325164800003,
Author = {Das, Siddhartha and Mitra, Sushanta K.},
Title = {{Electric double-layer interactions in a wedge geometry: Change in
   contact angle for drops and bubbles}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2013}},
Volume = {{88}},
Number = {{3}},
Month = {{SEP 26}},
Abstract = {{In this paper, we provide a theory to pinpoint the role of electric
   double layer (EDL) interactions in governing the contact angle of an
   electrolyte drop on a charged solid in air or a bubble on a charged
   surface within an electrolyte solution. The EDL interactions are
   analytically solved by representing the three phase contact line as a
   wedge edge, with the wedge being formed by the solid-liquid and the
   air-liquid interfaces, and calculating the corresponding Maxwell
   stresses. We demonstrate that the EDL effects induce an
   ``electrowetting-like{''} behavior, resulting in a lowering of the
   contact angle. As a specific example, we use this model to analyze the
   effect of added salt on preformed surface nanobubbles, and find, in
   contrast to what has been reported earlier, that even for most moderate
   conditions, added salt may have remarkable effect in altering the
   contact angle in preformed surface nanobubbles.}},
