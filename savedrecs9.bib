@article{ ISI:000325684400012,
Author = {Tahayori, Hooman and Sadeghian, Alireza and Pedrycz, Witold},
Title = {{Induction of Shadowed Sets Based on the Gradual Grade of Fuzziness}},
Journal = {{IEEE TRANSACTIONS ON FUZZY SYSTEMS}},
Year = {{2013}},
Volume = {{21}},
Number = {{5}},
Pages = {{937-949}},
Month = {{OCT}},
Abstract = {{The existing methods of determining an alpha-cut of a fuzzy set to
   construct its underlying shadowed set do not fully comply with the
   concept of shadowed sets, namely, a retention of the total amount of
   fuzziness and its localized redistribution throughout a universe of
   discourse. Moreover, no closed formula to calculate the corresponding
   alpha-cut is available. This paper proposes analytical formulas to
   calculate threshold values required in the construction of shadowed
   sets. We introduce a new algorithm to design a shadowed set from a given
   fuzzy set. The proposed algorithm, which adheres to the main premise of
   shadowed sets of capturing the essence of fuzzy sets, helps localize
   fuzziness present in a given fuzzy set. We represent the fuzziness of a
   fuzzy set as a gradual number. Through defuzzification of the gradual
   number of fuzziness, we determine the required threshold (i.e., some
   alpha-cut) used in the formation of the shadowed set. We show that the
   shadowed set obtained in this way comes with a measure of fuzziness that
   is equal to the one characterizing the original fuzzy set.}},
