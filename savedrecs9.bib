@article{ ISI:000209332200002,
Author = {Pedrycz, Witold},
Title = {{From logic descriptors to granular logic descriptors: a study in
   allocation of information granularity}},
Journal = {{JOURNAL OF AMBIENT INTELLIGENCE AND HUMANIZED COMPUTING}},
Year = {{2013}},
Volume = {{4}},
Number = {{4, SI}},
Pages = {{411-419}},
Month = {{AUG}},
Abstract = {{Local sources of knowledge structured in the form of logic
   descriptors-constructs of fuzzy logic, are arranged together
   (structured) in the form of a global model coming as a high-level
   granular logic descriptor. The inherent granularity of the global
   descriptor of this nature arises as a manifestation of the diversity of
   the locally available descriptors. The granular descriptor can be
   expressed with the aid of any of the formal models of information
   granules including sets, fuzzy sets, rough sets, probabilistic granules
   and others. The architectural essence of the granular descriptor, which
   supports a quantification of the variability among the sources of
   knowledge, is realized through an optimal allocation of information
