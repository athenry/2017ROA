@article{ ISI:000320245900031,
Author = {Hajireza, Parsin and Shi, Wei and Zemp, Roger},
Title = {{Label-free in vivo GRIN-lens optical resolution photoacoustic
   micro-endoscopy}},
Journal = {{LASER PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{10}},
Number = {{5}},
Month = {{MAY}},
Abstract = {{In this letter, the feasibility of label-free in vivo GRIN-lens optical
   resolution photoacoustic micro-endoscopy is demonstrated. An image guide
   with 100 000 single-mode fibers in a 1.4 mm diameter bundle in
   conjunction with a 0.29 pitch GRIN lens is used in order to transfer a
   focused scanning spot through the image guide and refocus it into
   tissue. A high-repetition-rate (up to 600 kHz) ytterbium fiber laser is
   used in order to enable near real-time imaging capability. Phantom
   studies indicate 6 mu m resolution. The system, with similar to 2 mm
   working distance, overcomes the penetration depth limitation and hence
   improves the surface laser fluence of previously reported fiber based
   optical resolution photoacoustic microscopy (OR-PAM). The proposed
   system with a sub-mm probe footprint is very flexible and now has a
   significant penetration depth which is another step towards clinical
   applications.}},
