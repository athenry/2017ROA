@inproceedings{ ISI:000377396200115,
Author = {Madore, Wendy-Julie and De Montigny, Etienne and Deschenes, Andreanne
   and Benboujja, Fouzi and Leduc, Mikael and Mes-Masson, Anne-Marie and
   Provencher, Diane M. and Rahimi, Kurosh and Boudoux, Caroline and
   Godbout, Nicolas},
Editor = {{Choi, B and Kollias, N and Zeng, H and Kang, HW and Wong, BJF and Ilgner, JF and Tearney, GJ and Gregory, KW and Marcu, L and Skala, MC and Campagnola, PJ and Mandelis, A}},
Title = {{Morphologic 3D scanning of fallopian tubes to assist ovarian cancer
   diagnosis}},
Booktitle = {{PHOTONIC THERAPEUTICS AND DIAGNOSTICS XII}},
Series = {{Proceedings of SPIE}},
Year = {{2016}},
Volume = {{9689}},
Note = {{Conference on Photonic Therapeutics and Diagnostics XII, San Francisco,
   CA, FEB 13-14, 2016}},
Organization = {{SPIE}},
Abstract = {{Pathological evaluation of the fallopian tubes is an important
   diagnostic result but tumors can be missed using routine approaches. As
   the majority of high-grade serous ovarian cancers are now believed to
   originate in the fallopian tubes, pathological examination should
   include in a thorough examination of the excised ovaries and fallopian
   tubes. We present an dedicated imaging system for diagnostic exploration
   of human fallopian tubes. This system is based on optical coherence
   tomography (OCT), a laser imaging modality giving access to sub
   epithelial tissue architecture. This system produces cross-sectional
   images up to 3 mm in depth, with a lateral resolution of approximate to
   15 mu m and an axial resolution of approximate to 12 mu m. An endoscopic
   single fiber probe was developed to fit in a human fallopian tube. This
   1.2 mm probe produces 3D volume data of the entire inner tube within a
   few minutes. To demonstrate the clinical potential of OCT for lesion
   identification, we studied 5 different ovarian lesions and healthy
   fallopian tubes. We imaged 52 paraffin-embedded human surgical specimens
   with a benchtop system and compared these images with histology slides.
   We also imaged and compared healthy oviducts from 3 animal models to
   find one resembling the human anatomy and to develop a functional ex
   vivo imaging procedure with the endoscopic probe. We also present an
   update on an ongoing clinical pilot study on women undergoing
   prophylactic or diagnostic surgery in which we image ex vivo fallopian
   tubes with the endoscopic probe.}},
