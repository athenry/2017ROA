Author = {Chioua, M. and Srinivasan, B. and Guay, M. and Perrier, M.},
Title = {{Performance Improvement of Extremum Seeking Control using Recursive
   Least Square Estimation with Forgetting Factor}},
Journal = {{IFAC PAPERSONLINE}},
