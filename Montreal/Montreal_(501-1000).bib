@article{ ISI:000358098300005,
Author = {Yang, Jixiang and Mayer, J. R. R. and Altintas, Yusuf},
Title = {{A position independent geometric errors identification and correction
   method for five-axis serial machines based on screw theory}},
Journal = {{INTERNATIONAL JOURNAL OF MACHINE TOOLS \& MANUFACTURE}},
Year = {{2015}},
Volume = {{95}},
Pages = {{52-66}},
Month = {{AUG}},
Abstract = {{Position independent geometric errors (PIGEs), which are caused by
   imperfect assembly of five-axis machine tools, need to be identified and
   compensated to improve the accuracy of machined parts. This paper
   presents the identification and correction of serial five-axis machine
   tools based on screw theory. The proposed identification model is based
   on a generalized Jacobian function for a variety of five-axis machine
   configurations as opposed to the models based on homogeneous
   transformation matrices (HTMs). The screw theory allows a global
   description of rigid body motion without constructing the local frames
   on each drive module as required by the HTMs method. An explicit inverse
   kinematics model is proposed to compensate the geometric errors directly
   as opposed to the existing approximate linearized or iterative methods.
   The proposed identification and correction of PIGEs methods are verified
   through simulations and experiments with ball-bar tests on a five-axis
   machine tool. (C) 2015 Elsevier Ltd. All rights reserved.}},
