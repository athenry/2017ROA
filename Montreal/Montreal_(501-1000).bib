@inproceedings{ ISI:000386900400089,
Author = {Gemieux, Michel and Savaria, Yvon and Zhu, Guchuan and Frigon,
   Jean-Francois},
Book-Group-Author = {{IEEE}},
Title = {{Towards LTE Physical Layer Virtualization on a COTS Multicore Platform
   with Efficient Scheduling}},
Booktitle = {{2016 14TH IEEE INTERNATIONAL NEW CIRCUITS AND SYSTEMS CONFERENCE
   (NEWCAS)}},
Series = {{IEEE International New Circuits and Systems Conference}},
