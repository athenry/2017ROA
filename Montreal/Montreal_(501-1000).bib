@article{ ISI:000376107900006,
Author = {Dadgarpour, Abdolmehdi and Zarghooni, Behnam and Denidni, Tayeb A. and
   Kishk, Ahmed A.},
Title = {{Dual-Band Radiation Tilting Endfire Antenna for WLAN Applications}},
Journal = {{IEEE ANTENNAS AND WIRELESS PROPAGATION LETTERS}},
Year = {{2016}},
Volume = {{15}},
Pages = {{1466-1469}},
Abstract = {{A dual-band bow-tie antenna with enhanced tilt angle in the azimuth
   plane at 2.4 and 5.5 GHz is proposed. The proposed antenna consists of
   bow-tie radiators with dual-impedance bandwidth operating at 2.3-2.5 and
   5.2-5.8 GHz. To enhance the tilt angle in the azimuth plane, an array of
   H-shaped meander-line unit cells as electric resonators is designed at
   2.4 and 5.5 GHz with high refractive index property. Integrating the
   proposed array in the off-axis of the bow-tie antenna tilts the main
   beam radiation in the endfire direction by +17 degrees and -20 degrees
   at 2.4 and 5.5 GHz, respectively. When the beam is tilted by 17 degrees,
   there is a gain enhancement of 2.7 dBi at 5.5 GHz compared to the
   unloaded case.}},
