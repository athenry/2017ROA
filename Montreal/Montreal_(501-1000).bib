@article{ ISI:000371883700006,
Author = {Ouafia, N. and Moghrani, H. and Benaouada, N. and Yassaa, N. and Maachi,
   R. and Younsi, R.},
Title = {{MOISTURE SORPTION ISOTHERMS AND HEAT OF SORPTION OF ALGERIAN BAY LEAVES
   (Laurus nobilis)}},
Journal = {{MADERAS-CIENCIA Y TECNOLOGIA}},
Year = {{2015}},
Volume = {{17}},
Number = {{4}},
Pages = {{759-772}},
Month = {{OCT}},
Abstract = {{The moisture sorption isotherms of Algerian bay leaves (Laurus nobilis)
   were determined experimentally in this work. The equilibrium moisture
   contents of the leaves were measured at 40, 50, and 60 degrees C using
   static gravimetric method. Six mathematical models were tested to fit
   the experimental data of sorption isotherms and predict the hygroscopic
   behavior during storage or drying. Peleg model was found to be the best
   fitting model for describing the sorption curves. The net isosteric heat
   of sorption was computed from the equilibrium data at different
   temperatures by applying the integrated form of the Clausius-Clapeyron
   equation.
   The net isosteric heat of sorption is inversely proportional to the
   equilibrium moisture content and is found to be an exponential function
   of moisture content.}},
