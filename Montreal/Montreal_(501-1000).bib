@inproceedings{ ISI:000386842300017,
Author = {Chen, Chao and Santinelli, Luca and Hugues, Jerome and Beltrame,
   Giovanni},
Book-Group-Author = {{IEEE}},
Title = {{Static Probabilistic Timing Analysis in Presence of Faults}},
