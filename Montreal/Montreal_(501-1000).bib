@article{ ISI:000363793500001,
Author = {Peshkov, Ilya and Grmela, Miroslav and Romenski, Evgeniy},
Title = {{Irreversible mechanics and thermodynamics of two-phase continua
   experiencing stress-induced solid-fluid transitions}},
Journal = {{CONTINUUM MECHANICS AND THERMODYNAMICS}},
Year = {{2015}},
Volume = {{27}},
Number = {{6}},
Pages = {{905-940}},
Month = {{NOV}},
Abstract = {{On the example of two-phase continua experiencing stress-induced
   solid-fluid phase transitions, we explore the use of the Euler structure
   in the formulation of the governing equations. The Euler structure
   guarantees that solutions of the time evolution equations possessing it
   are compatible with mechanics and with thermodynamics. The former
   compatibility means that the equations are local conservation laws of
   the Godunov type, and the latter compatibility means that the entropy
   does not decrease during the time evolution. In numerical illustrations,
   in which the one-dimensional Riemann problem is explored, we require
   that the Euler structure is also preserved in the discretization.}},
