@article{ ISI:000362419900002,
Author = {Li, Jingwen and Qu, Hang and Skorobogatiy, Maksim},
Title = {{Simultaneous monitoring the real and imaginary parts of the analyte
   refractive index using liquid-core photonic bandgap Bragg fibers}},
Journal = {{OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{23}},
Number = {{18}},
Pages = {{22963-22976}},
Month = {{SEP 7}},
Abstract = {{We demonstrate simultaneous monitoring of the real and imaginary parts
   of the liquid analyte refractive index by using a hollow-core Bragg
   fiber. We apply this two-channel fiber sensor to monitor concentrations
   of various commercial cooling oils. The sensor operates using spectral
   monitoring of the fiber bandgap center wavelength, as well as monitoring
   of the fiber transmission amplitude at mid-bandgap position. The
   sensitivity of the fiber sensor to changes in the real part of the core
   refractive index is found to be 1460nm/Refractive index unit (RIU). By
   using spectral modality and effective medium theory, we determine the
   concentrations of the two commercial fluids from the measured refractive
   indices with an accuracy of similar to 0.57\% for both low-and high-loss
   oils. Moreover, using an amplitude-based detection modality allows
   determination of the oil concentration with accuracy of similar to
   1.64\% for low-loss oils and similar to 2.81\% for the high-loss oils.
   (C) 2015 Optical Society of America}},
