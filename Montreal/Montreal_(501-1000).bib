@article{ ISI:000352702400030,
Author = {Lopez, Israel and Laurin, Jean-Jacques},
Title = {{Alternative Topologies of Circular Polarization Selective Surfaces Based
   on Modifications of the Pierrot Cell}},
Journal = {{IEEE TRANSACTIONS ON ANTENNAS AND PROPAGATION}},
Year = {{2015}},
Volume = {{63}},
Number = {{4, 1, SI}},
Pages = {{1465-1472}},
Month = {{APR}},
Abstract = {{Circular polarization selective surfaces (CPSS) based on two variants of
   the original Pierrot cell are proposed. For the first variant, which
   uses a load in the middle segment, it is possible to equalize the
   frequencies at which the reflection and transmission performance for the
   two orthogonal circular polarizations is optimal. The second variant is
   a double Pierrot cell having a twofold rotation symmetry, which leads to
   an increased bandwidth and less sensitivity to variations in the angle
   of incidence. Experimental results in good agreement with full wave
   simulations are presented. The printed circuit board (PCB) fabrication
   process used with flexible substrates allows more freedom for adjusting
   the cells in order to improve the performance and the bandwidth of the
   CPSS.}},
