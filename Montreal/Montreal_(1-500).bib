@article{ ISI:000390243100014,
Author = {Grasland-Mongrain, Pol and Lu, Yuankang and Lesage, Frederic and
   Catheline, Stefan and Cloutier, Guy},
Title = {{Generation of shear waves by laser in soft media in the ablative and
   thermoelastic regimes}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2016}},
Volume = {{109}},
Number = {{22}},
Month = {{NOV 28}},
Abstract = {{This article describes the generation of elastic shear waves in a soft
   medium using a laser beam. Our experiments show two different regimes
   depending on laser energy. Physical modeling of the underlying phenomena
   reveals a thermoelastic regime caused by a local dilatation resulting
   from temperature increase and an ablative regime caused by a partial
   vaporization of the medium by the laser. The computed theoretical
   displacements are close to the experimental measurements. A numerical
   study based on the physical modeling gives propagation patterns
   comparable to those generated experimentally. These results provide a
   physical basis for the feasibility of a shear wave elastography
   technique (a technique that measures a soft solid stiffness from shear
   wave propagation) by using a laser beam. Published by AIP Publishing.}},
