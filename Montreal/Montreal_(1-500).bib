@article{ ISI:000370722800020,
Author = {Farah, Rana and Langlois, J. M. Pierre and Bilodeau, Guillaume-Alexandre},
Title = {{Computing a rodent's diary}},
Journal = {{SIGNAL IMAGE AND VIDEO PROCESSING}},
Year = {{2016}},
Volume = {{10}},
Number = {{3, SI}},
Pages = {{567-574}},
Month = {{MAR}},
Abstract = {{Rodent monitoring in biomedical laboratories is a time consuming and
   tedious task. Several automatic solutions that rely on different types
   of sensors have been proposed. Computer vision provides a significantly
   more universal and less intrusive solution. In this article we propose a
   new method to detect and classify three behaviors in rodents: exploring,
   rearing, and static. The method uses motion history images and a
   multiple classifier system to detect the three behaviors under typical
   laboratory conditions. It is independent of the color of the rodent and
   of the background. The method performs equally well on short and long
   video sequences, achieving a success rate of 87 \%.}},
