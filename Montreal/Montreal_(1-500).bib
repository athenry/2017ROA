@article{ ISI:000374598900007,
Author = {Hertz, Alain and Melot, Hadrien},
Title = {{Counting the number of non-equivalent vertex colorings of a graph}},
Journal = {{DISCRETE APPLIED MATHEMATICS}},
Year = {{2016}},
Volume = {{203}},
Pages = {{62-71}},
Month = {{APR 20}},
Abstract = {{We give some extremal properties on the number B(G) of non-equivalent
   ways of coloring a given graph G, also known as the (graphical) Bell
   number of G. In particular, we study bounds on (G) for graphs with a
   maximum degree constraint. First, an upper bound on B(G) is given for
   graphs with fixed order n and maximum degree Delta. Then, we give lower
   bounds on B(G) for fixed order n and maximum degree 1, 2, n - 2 and n -
   1. In each case, the bound is tight and we describe all graphs that
   reach the bound with equality. (C) 2015 Elsevier B.V. All rights
   reserved.}},
