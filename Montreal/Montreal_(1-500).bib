@inproceedings{ ISI:000390094702192,
Author = {Khelifi, Mounir and Massicotte, Daniel and Savaria, Yvon},
Book-Group-Author = {{IEEE}},
Title = {{Towards Efficient and Concurrent FFTs Implementation on Intel Xeon/MIC
   Clusters for LTE and HPC}},
Booktitle = {{2016 IEEE INTERNATIONAL SYMPOSIUM ON CIRCUITS AND SYSTEMS (ISCAS)}},
Series = {{IEEE International Symposium on Circuits and Systems}},
Year = {{2016}},
Pages = {{2611-2614}},
Note = {{IEEE International Symposium on Circuits and Systems (ISCAS), Montreal,
