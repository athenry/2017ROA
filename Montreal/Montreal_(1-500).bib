@article{ ISI:000386213000006,
Author = {Mhamdi, Lotfi and Mhamdi, Nejib and Mhamdi, Naceur and Lejeune, Philippe
   and Jaffrezic, Nicole and Burais, Noel and Scorretti, Riccardo and
   Pokorny, Jiry and Ponsonnet, Laurence},
Title = {{Effect of a static magnetic field on Escherichia coli adhesion and
   orientation}},
Journal = {{CANADIAN JOURNAL OF MICROBIOLOGY}},
Year = {{2016}},
Volume = {{62}},
Number = {{11}},
Pages = {{944-952}},
Month = {{NOV}},
Abstract = {{This preliminary study focused on the effect of exposure to 0.5 T static
   magnetic fields on Escherichia coli adhesion and orientation. We
   investigated the difference in bacterial adhesion on the surface of
   glass and indium tin oxide-coated glass when exposed to a magnetic field
   either perpendicular or parallel to the adhesion surface (vectors of
   magnetic induction are perpendicular or parallel to the adhesion
   surface, respectively). Control cultures were simultaneously grown under
   identical conditions but without exposure to the magnetic field. We
   observed a decrease in cell adhesion after exposure to the magnetic
   field. Orientation of bacteria cells was affected after exposure to a
   parallel magnetic field. On the other hand, no effect on the orientation
   of bacteria cells was observed after exposure to a perpendicular
   magnetic field.}},
