Book-Group-Author = {{IOP}},
Title = {{CFD evaluation of added damping due to fluid flow over a hydroelectric
   turbine blade}},
Booktitle = {{28TH IAHR SYMPOSIUM ON HYDRAULIC MACHINERY AND SYSTEMS (IAHR2016), PTS
   1-12}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
