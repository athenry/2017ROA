@article{ ISI:000392405100026,
Author = {Ma, Tian and Guerboukha, Hichem and Girard, Martin and Squires, Andrew
   D. and Lewis, Roger A. and Skorobogatiy, Maksim},
Title = {{3D Printed Hollow-Core Terahertz Optical Waveguides with Hyperuniform
   Disordered Dielectric Reflectors}},
Journal = {{ADVANCED OPTICAL MATERIALS}},
Year = {{2016}},
Volume = {{4}},
Number = {{12}},
Pages = {{2085-2094}},
Month = {{DEC}},
Abstract = {{Novel hollow-core THz waveguides featuring hyperuniform disordered
   reflectors are proposed, fabricated, and characterized. The reflector
   comprises aperiodically positioned dielectric cylinders connected with
   dielectric bridges. The proposed waveguides are fabricated using a 3D
   MultiJet printer. Optical properties of the fabricated waveguides are
   investigated numerically using finite element method, as well as
   experimentally using terahertz time-domain spectroscopy. The results
   confirm that proposed waveguides exhibit sizable photonic bandgaps
   (21\%) even with relatively low refractive index contrast (resin/air).
   Position of the bandgaps can be easily tuned by varying reflector
   geometrical parameters.}},
