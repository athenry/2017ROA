@article{ ISI:000379230800003,
Author = {Baud-Lavigne, Bertrand and Bassetto, Samuel and Agard, Bruno},
Title = {{A method for a robust optimization of joint product and supply chain
   design}},
Journal = {{JOURNAL OF INTELLIGENT MANUFACTURING}},
Year = {{2016}},
Volume = {{27}},
Number = {{4}},
Pages = {{741-749}},
Month = {{AUG}},
Abstract = {{This paper proposes a method for finding a robust solution to the
   problem of joint product family and supply chain design. Optimizing
   product design and the supply chain network at the same time brings
   substantial benefits. However, this approach involves decisions that can
   generate uncertainties in the long term. The challenge is to come up
   with a method that can adapt to most possible environments without
   straying too far from the optimal solution. Our approach is based on the
   generation of scenarios that correspond to combinations of uncertain
   parameters within the model. The performance of designs resulting from
   these scenario optimizations are compared to the performance of each of
   the other design scenarios, based on their probability of occurrence.
   The proposed methodology will allow practitioners to choose a suitable
   design, from the most profitable to the most reliable.}},
