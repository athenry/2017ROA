@article{ ISI:000378819000002,
Author = {Talebi-Esfandarani, M. and Rousselot, S. and Gauthier, M. and Sauriol,
   P. and Liang, G. and Dolle, M.},
Title = {{LiFePO4 synthesized via melt synthesis using low-cost iron precursors}},
