@article{ ISI:000373410000009,
Author = {Homaie-Shandizi, Amir-Hosein and Nia, Vahid Partovi and Gamache, Michel
   and Agard, Bruno},
Title = {{Flight deck crew reserve: From data to forecasting}},
Journal = {{ENGINEERING APPLICATIONS OF ARTIFICIAL INTELLIGENCE}},
Year = {{2016}},
Volume = {{50}},
Pages = {{106-114}},
Month = {{APR}},
Abstract = {{Airline companies are subject to a considerable amount of disruption
   during operation. It is vital for many industries to predict the source
   of disruption in different levels of management, in order to reduce the
   recovery cost. One of the most important and costly sources of
   disruption in the airline industry is the number of reserve pilots.
   We devise a supervised learning method, based on decision trees, to
   predict total monthly pilot reserve hours. The proposed method uses
   characteristics of the monthly schedule as factors and an iterative
   algorithm to make a prediction. The model is tested on real data and a
   substantial improvement is observed in the results compared to the
   current state of the practice in the domain in which a seasonal linear
   trend decomposition is employed. (C) 2016 Elsevier Ltd. All rights
   reserved.}},
