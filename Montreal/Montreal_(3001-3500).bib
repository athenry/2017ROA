@article{ ISI:000305085000026,
Author = {Zhang, Han and Virally, Stephane and Bao, Qiaoliang and Ping, Loh Kian
   and Massar, Serge and Godbout, Nicolas and Kockaert, Pascal},
Title = {{Z-scan measurement of the nonlinear refractive index of graphene}},
Journal = {{OPTICS LETTERS}},
Year = {{2012}},
Volume = {{37}},
Number = {{11}},
Pages = {{1856-1858}},
Month = {{JUN 1}},
Abstract = {{Under strong laser illumination, few-layer graphene exhibits both a
   transmittance increase due to saturable absorption and a nonlinear phase
   shift. Here, we unambiguously distinguish these two nonlinear optical
   effects and identify both real and imaginary parts of the complex
   nonlinear refractive index of graphene. We show that graphene possesses
   a giant nonlinear refractive index n(2) similar or equal to 10(-7) cm(2)
   W-1, almost 9 orders of magnitude larger than bulk dielectrics. We find
   that the nonlinear refractive index decreases with increasing excitation
   flux but slower than the absorption. This suggests that graphene may be
   a very promising nonlinear medium, paving the way for graphene-based
   nonlinear photonics. (C) 2012 Optical Society of America}},
