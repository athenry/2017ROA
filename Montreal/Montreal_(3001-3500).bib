@article{ ISI:000307367300016,
Author = {Zamyadi, Arash and McQuaid, Natasha and Dorner, Sarah and Bird, David F.
   and Burch, Mike and Baker, Peter and Hobson, Peter and Prevost, Michele},
Title = {{Cyanobacterial detection using in vivo fluorescence-probes: Managing
   interferences for improved decision-making}},
Journal = {{JOURNAL AMERICAN WATER WORKS ASSOCIATION}},
Year = {{2012}},
Volume = {{104}},
Number = {{8}},
Pages = {{37-38}},
Month = {{AUG}},
Abstract = {{The applications of in vivo probes that can detect the fluorescence of
   cyanobacterial phycocyanin are an emerging and widely used technology
   for cyanobacterial detection in source waters. The objectives of this
   project were to study the interference sources involved with the
   readings from five probes using laboratory cultures and field samples.
   To compare the direct readings from different probes, a novel approach
   was used to present the readings in the form of a biovolume equivalent
   of cyanobacteria (CB). Inorganic turbidity and the presence of algal
   biomass interfered with probe readings and influenced application and
   interpretation of the probe readings. A novel correction factor was
   developed for the CB probes using simultaneous chlorophyll a (Chla)
   measurements. The field data demonstrate that the potential
   underestimation of cyanobacterial biomass at lower alert levels is a
   significant issue but that in vivo probes are valuable tools for higher
   alert levels.}},
