@article{ ISI:000300871900003,
Author = {Wang Qian and Ji Shaocheng and Sun Shengsi and Salisbury, Matthew H. and
   Kern, Hartmut},
Title = {{Elastic and Seismic Properties of Dabie-Sulu Ultrahigh Pressure
   Metamorphic Rocks}},
Journal = {{ACTA GEOLOGICA SINICA-ENGLISH EDITION}},
