@article{ ISI:000302617200011,
Author = {Mondal, Priyanka and Guan, Yong Liang and Alphones, Arokiaswami},
Title = {{Coplanar waveguide wideband bandpass filter and its application to
   ultra-wideband pulse generation}},
Journal = {{INTERNATIONAL JOURNAL OF RF AND MICROWAVE COMPUTER-AIDED ENGINEERING}},
Year = {{2012}},
Volume = {{22}},
Number = {{3}},
Pages = {{370-376}},
Month = {{MAY}},
Abstract = {{A technique to design wideband coplanar waveguide bandpass filters is
   reported. The filter is realized by etching a slot on the ground plane
   around a gap on its central conductor and modifying the gap in the form
   of parallel lines. It is shown that the 3-dB fractional bandwidth of the
   filter can be varied from 60 to 110\% by tuning the size of the slot
   aperture and the length of the parallel lines. Equivalent circuit and
   design steps are presented. Implementation area of the filter having
   passband 3.210.5 GHz is 0.90 ?g x 0.26 ?g, ?g being the guided
   wavelength at 6.85 GHz while 20-dB stopband is at least up to 18 GHz.
   Insertion loss is less than 2 dB up to 9 GHz. Area of the filter having
   fractional bandwidth 60\% at 3.85 GHz is 0.67 ?g x 0.11 ?g. Passband
   loss is within 1.5 and 20 dB stopband is at least up to 12 GHz. The
   proposed filter structure is very simple to integrate, and the
   ultra-wideband filter is used to generate an ultra-wideband pulse as
   defined by the US Federal Communication Commission. (C) 2012 Wiley
   Periodicals, Inc. Int J RF and Microwave CAE, 2012.}},
