@article{ ISI:000309588200008,
Author = {Pottier, Pierre and Shi, Lina and Peter, Yves-Alain},
Title = {{Evolution of modes of Fabry-Perot cavity based on photonic crystal
   guided-mode resonance mirrors}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA B-OPTICAL PHYSICS}},
Year = {{2012}},
Volume = {{29}},
Number = {{10}},
Pages = {{2698-2703}},
Month = {{OCT}},
Abstract = {{We describe the modes of a Fabry-Perot cavity made of two-dimensional
   photonic crystal guided-mode resonance mirrors (Si3N4/air), and compare
   it with an ideal Fabry-Perot cavity and a cavity made of Bragg mirrors.
   As the evolution of modes is analyzed, a lower tuning efficiency and a
   larger tuning range are obtained compared to Bragg mirror cavities. New
   behavior also emerges such as the possibility to tune the inner
   interface from being a node to an antinode of the standing wave electric
   field, and therefore the possibility to provide enhancement of emission
   of nanoemitters binding to the inner interfaces of the cavity, such as
   in microfluidic microassay systems. (c) 2012 Optical Society of America}},
