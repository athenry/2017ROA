@inproceedings{ ISI:000320762900115,
Author = {Sounas, Dimitrios L. and Caloz, Christophe},
Book-Group-Author = {{IEEE}},
Title = {{Non-Reciprocal Faraday Rotation in Graphene: Just a Unique Phenomenon or
   Even More?}},
Booktitle = {{2013 IEEE RADIO AND WIRELESS SYMPOSIUM (RWS)}},
Series = {{IEEE Radio and Wireless Symposium}},
Year = {{2013}},
Pages = {{352-354}},
Note = {{IEEE Radio and Wireless Symposium (RWS), Austin, TX, JAN 20-23, 2013}},
Organization = {{IEEE; IEEE Microwave Theory \& Tech Soc (MTT-S); IEEE Antennas \&
   Propagat Soc; IEEE Commun Soc (ComSoc); IEEE Engn Med \& Biol Soc (EMBS)}},
Abstract = {{Faraday rotation observed in magnetically biased graphene is reviewed.
   For the best quality of CVD grown graphene to date and a magnetic field
   of 1 T a Faraday rotation around 10 degrees is estimated. In contrast to
   other gyrotropic materials such as ferrites where rotation direction can
   only be controlled by the magnetic bias field, the rotation direction in
   graphene can be reversed by reversing an electric bias voltage applied
   between graphene and a gate electrode. The rotation angle is
   proportional to the mobility of graphene, which imposes a practical
   challenge in achieving rotation angles greater than 15 degrees. However,
   even angles as low as 10 degrees are enough to design isolators with
   isolation in the order of 20 dB. Due to the conducting nature of
   graphene at microwave frequencies, such devices are always accompanied
   by moderate loss, which however can be considered acceptable accounting
   advantages of graphene-based devices, such as easy tunability and
   invunerability to demagnetization effects present in ferrite-based
   devices.}},
