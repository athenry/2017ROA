@article{ ISI:000312835200009,
Author = {Wolfgramm, Florian and Vitelli, Chiara and Beduini, Federica A. and
   Godbout, Nicolas and Mitchell, Morgan W.},
Title = {{Entanglement-enhanced probing of a delicate material system}},
Journal = {{NATURE PHOTONICS}},
Year = {{2013}},
Volume = {{7}},
Number = {{1}},
Pages = {{28-32}},
Month = {{JAN}},
Abstract = {{Quantum metrology(1) uses entanglement(2-5) and other quantum effects(6)
   to improve the sensitivity of demanding measurements-(7-9). Probing of
   delicate systems demands high sensitivity from limited probe energy and
   has motivated the field's key benchmark-the standard quantum limit(10).
   Here we report the first entanglement-enhanced measurement of a delicate
   material system. We non-destructively probe an atomic spin ensemble by
   means of near-resonant Faraday rotation, a measurement that is limited
   by probe-induced scattering in quantum-memory and spin-squeezing
   applications(6,11-13). We use narrowband, atom-resonant NOON states to
   beat the standard quantum limit of sensitivity by more than five
   standard deviations, both on a per-photon and per-damage basis. This
   demonstrates quantum enhancement with fully realistic loss and noise,
   including variable-loss effects(14-16). The experiment opens the way to
   ultra-gentle probing of single atoms(17), single molecules(18), quantum
   gases(19) and living cells(20).}},
