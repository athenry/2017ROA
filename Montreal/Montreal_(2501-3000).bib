@inproceedings{ ISI:000329309500070,
Author = {Farahani, R. D. and Klemberg-Sapieha, J. E. and Therriault, D.},
Editor = {{Bakis, CE}},
Title = {{Improvement of Electrical Conductivity in Polymer Nanocomposites
   Featuring Heterogeneous Nanofillers Distribution}},
Booktitle = {{PROCEEDINGS OF THE AMERICAN SOCIETY FOR COMPOSITES}},
Year = {{2013}},
Note = {{28th Technical Conference of the American-Society-for-Composites, Penn
   State Univ, Dept Engn Sci \& Mech, State College, PA, SEP 09-11, 2013}},
Organization = {{Amer Soc Composites}},
Abstract = {{This work presents the fabrication of polymer based nanocomposites with
   improved conductivity consisting of heterogeneous distribution of
   nanofillers, either multi-walled carbon nanotubes (MWCNTs) or
   surface-treated silver nanowires (SN) in a thermosetting polymer matrix.
   The heterogeneous distribution was achieved through controlling the
   filler-resin interactions during the nanocomposites curing. As the
   nanocomposites were thermally cured, the nanofillers tended to
   self-assemble into a segregated network and form continuous pathways of
   conductive nanofillers. This rearrangement of the nanofillers resulted
   in a biphasic nanocomposite with filler-rich and resin-rich domains, as
   observed under optical microscopy. At a specific filler concentration,
   the achieved biphasic nanocomposites showed an improved electrical
   conductivity compared to the nanocomposites composed of uniformly
