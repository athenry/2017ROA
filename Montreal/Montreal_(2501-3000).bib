@inproceedings{ ISI:000319023100064,
Author = {Saez, S. and Dufay, B. and Dolabdjian, C. and Yelon, A. and Menard, D.},
Editor = {{Hristoforou, E and Vlachos, DS}},
Title = {{Effect of Parasitic Capacitance on GMI Magnetic Sensor Performance}},
Booktitle = {{MATERIALS AND APPLICATIONS FOR SENSORS AND TRANSDUCERS II}},
Series = {{Key Engineering Materials}},
Year = {{2013}},
Volume = {{543}},
Pages = {{261+}},
Note = {{2nd International Conference on Materials and Applications for Sensors
   and Transducers, IC-MAST, Budapest, HUNGARY, MAY 24-28, 2012}},
Abstract = {{Magnetic sensors based on GMI devices are the subject of intensive
   research, as they appear promising for magnetometry applications.
   Performances of GMT magnetometers are often limited by the noise of the
   electronic setup. Thus, the present challenge is to increase the GMI
   device sensitivity (expressed in V/T) in order to decrease the
   equivalent magnetic noise of the system. In our previous work, we showed
   that the use of a pick-up coil in an off-diagonal configuration improves
   the magnetic sensor sensitivity and offers a promising approach for
   developing an inexpensive magnetometer with sub-pT/root Hz equivalent
   magnetic noise levels. Ideally, the use of a coil increases the
   sensitivity linearly as a function of the number of turns. However, this
   effect is reduced by the parasitic capacitance of the coil. This affects
   the device sensitivity, noise level and system performance. The
   parasitic capacitance can degrade all of these, but also induces a
   resonance effect, which can help to optimize magnetometer sensitivity,
   and thus, its noise level. We analyze the effects of the parasitic
   capacitance on the system (sensitivity and noise) and propose
   optimization routes. We have obtained sensor sensitivity as high as 700
   fT/root Hz.}},
