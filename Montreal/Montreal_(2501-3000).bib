Title = {{Detection of Process Antipatterns: A BPEL Perspective}},
Booktitle = {{17TH IEEE INTERNATIONAL ENTERPRISE DISTRIBUTED OBJECT COMPUTING
   CONFERENCE WORKSHOPS (EDOCW 2013)}},
Series = {{IEEE International Enterprise Distributed Object Computing Conference
