@article{ ISI:000312301800010,
Author = {Qin, Yong and Vogelgesang, Ralf and Esslinger, Moritz and Sigle,
   Wilfried and van Aken, Peter and Moutanabbir, Oussama and Knez, Mato},
Title = {{Bottom-Up Tailoring of Plasmonic Nanopeapods Making Use of the
   Periodical Topography of Carbon Nanocoil Templates}},
Journal = {{ADVANCED FUNCTIONAL MATERIALS}},
Year = {{2012}},
Volume = {{22}},
Number = {{24}},
Pages = {{5157-5165}},
Month = {{DEC 19}},
Abstract = {{Au nanoparticle chains embedded in helical Al2O3 nanotubes (nanopeapods)
   are synthesized by annealing carbon nanocoils coated with Au by
   sputtering and Al2O3 by atomic layer deposition. Regular spacing between
   nanoparticles with a sharp size distribution is achieved by
   fragmentation of the Au coating in agreement with the pitch of the
   nanocoils arising from three-dimensional periodical topography of the
   carbon nanocoil templates. A strong plasmonic resonance behavior of the
   fabricated nanopeapods manifests itself in confocal laser scanning
   microscopy by a clear polarization contrast at red wavelengths, which is
   absent in the blue. Numerical simulations confirm an incisive resonance
   enhancement for longitudinal polarization and suggest the nanopeapods as
   promising candidates for highly efficient, ultrathin waveguides. The
   waveguiding properties of the nanopeapods are investigated by electron
   energy-loss spectroscopy and energy-filtered TEM imaging.}},
