Title = {{Design of Low Power 4-Bit Flash ADC Based on Standard Cells}},
Booktitle = {{2013 IEEE 11TH INTERNATIONAL NEW CIRCUITS AND SYSTEMS CONFERENCE
   (NEWCAS)}},
Series = {{IEEE International New Circuits and Systems Conference}},
Year = {{2013}},
