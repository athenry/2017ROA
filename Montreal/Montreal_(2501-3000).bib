@inproceedings{ ISI:000321127000025,
Author = {German, Daniel M. and Adams, Bram and Hassan, Ahmed E.},
Editor = {{Cleve, A and Ricca, F and Cerioli, M}},
Title = {{The Evolution of the R Software Ecosystem}},
Booktitle = {{PROCEEDINGS OF THE 17TH EUROPEAN CONFERENCE ON SOFTWARE MAINTENANCE AND
