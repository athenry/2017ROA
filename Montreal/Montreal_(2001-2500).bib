Author = {Skaljo, Edvin and Nemsic, Boris and Babic, Slobodan and Mujcic, Aljo},
Title = {{Integration of Mobile Backhaul and Broadband Fixed Access Networks in
   Urban Metropolitan Areas}},
Journal = {{FIBER AND INTEGRATED OPTICS}},
Year = {{2013}},
Volume = {{32}},
Number = {{2, SI}},
Pages = {{105-116}},
Month = {{MAR 1}},
Abstract = {{Integration of mobile backhaul and broadband fixed access networks is a
   practical and inexpensive solution. This study discusses integration in
   urban metropolitan areas when a broadband fixed access network is based
   on gigabit-capable passive optical network and mobile backhaul is
   intended for the 3G global system for mobile communication networks.
   Bandwidth and quality of recovery clock are considered as two main
   parameters for successful integration. Two clock recovery approaches are
   described, and their performances are measured on the real
   gigabit-capable passive optical network system.}},
