@article{ ISI:000326037600038,
Author = {Tousignant, M. and Zabeida, O. and Rudkowska, G. and Yelon, A.},
Title = {{Investigation of surface effect on giant magnetoimpedance in microwires}},
Journal = {{JOURNAL OF MAGNETISM AND MAGNETIC MATERIALS}},
Year = {{2014}},
Volume = {{349}},
Pages = {{232-234}},
Month = {{JAN}},
Abstract = {{Previous work from our group suggested that the giant magnetoimpedance
   (GMI) response of soft ferromagnetic microwires could be improved by
   removing a hypothetical hard magnetic shell on the surface of the
   cylindrical conductors. Therefore, we have been trying to increase the
   sensitivity of our samples by exposing them to a mild plasma etching
   treatment. Although we measured a reduction in diameter of the
   microwires and ensured that their bulk magnetic properties remained
   unchanged, it was found that the surface treatment did not improve the
   GMI behavior of the samples. We thus conclude that the sensitivity of
   ferromagnetic microwires must be limited by bulk inhomogeneities, yet to
   be investigated. (C) 2013 Elsevier B.V. All rights reserved.}},
