@article{ ISI:000321260700015,
Author = {Audet, Charles and Ninin, Jordan},
Title = {{Maximal perimeter, diameter and area of equilateral unit-width convex
   polygons}},
Journal = {{JOURNAL OF GLOBAL OPTIMIZATION}},
Year = {{2013}},
Volume = {{56}},
Number = {{3, SI}},
Pages = {{1007-1016}},
Month = {{JUL}},
Note = {{Global Optimization Workshops (GOWs), Toulouse, FRANCE, AUG 31-SEP 03,
   2010}},
Abstract = {{The paper answers the three distinct questions of maximizing the
   perimeter, diameter and area of equilateral unit-width convex polygons.
   The solution to each of these problems is trivially unbounded when the
   number of sides is even. We show that when this number is odd, the
   optimal solution to these three problems is identical, and arbitrarily
   close to a trapezoid. The paper also considers the maximization of the
   sum of distances between all pairs of vertices of equilateral unit-width
   convex polygons. Based on numerical experiments on the three first open
   cases, it is conjectured that the optimal solution to this fourth
   problem is the same trapezoid as for the three other problems.}},
