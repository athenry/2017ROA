@article{ ISI:000321416800003,
Author = {Lemamou, Eunice Adjarath and Chamberland, Steven and Galinier, Philippe},
Title = {{A reliable model for global planning of mobile networks}},
Journal = {{COMPUTERS \& OPERATIONS RESEARCH}},
Year = {{2013}},
Volume = {{40}},
Number = {{10}},
Pages = {{2270-2282}},
Month = {{OCT}},
Abstract = {{In this paper, we propose a global model for WiMAX networks planning.
   This model represents the network planning problem and helps to solve it
   entirely without dividing it into several subproblems. The objective of
   the model is to minimize the cost of the network while maximizing its
   survivability. The model has been compared to a sequential model with
   the same constraints, which consists in solving the subproblems
   sequentially, and to a global model without reliability constraints. The
   results show that the proposed model performs on an average 25\% better
   than the other models. (C) 2013 Elsevier Ltd. All rights reserved.}},
