@article{ ISI:000327494000123,
Author = {Ahlawat, Meenu and Bostani, Ameneh and Tehranchi, Amirhossein and
   Kashyap, Raman},
Title = {{Tunable single-to-dual channel wavelength conversion in an
   ultra-wideband SC-PPLN}},
Journal = {{OPTICS EXPRESS}},
Year = {{2013}},
Volume = {{21}},
Number = {{23}},
Pages = {{28809-28816}},
Month = {{NOV 18}},
Abstract = {{We experimentally demonstrate tunable dual channel broadcasting of a
   signal over the C-band for wavelength division multiplexed (WDM) optical
   networks. This is based on cascaded chi((2)) nonlinear mixing processes
   in a specially engineered, 20-mm-long step-chirped periodically poled
   lithium niobate with a broad 28-nm second harmonic (SH) bandwidth in the
   1.55-mu m spectral range. A 10-GHz picosecond mode-locked laser was used
   as a signal along with a CW pump to generate two pulsed idlers, which
   are simultaneously tuned across the C-band by detuning of the pump
   wavelength within the broad SH bandwidth. Variable-input,
   variable-output scheme of tuned idlers is successfully achieved by
   tuning the signal wavelength. Pump or signal wavelength tuning of
   similar to 10 nm results in the idlers spreading across 30 nm in the
   C-band. (C) 2013 Optical Society of America}},
