@article{ ISI:000321715900010,
Author = {Firouzbahrami, Majid and Babazadeh, Maryam and Nobakhti, Amin and
   Karimi, Houshang},
Title = {{Improved bounds for the spectrum of interval matrices}},
Journal = {{IET CONTROL THEORY AND APPLICATIONS}},
Year = {{2013}},
Volume = {{7}},
Number = {{7}},
Pages = {{1022-1028}},
Month = {{MAY}},
Abstract = {{This study presents new sufficient conditions for Hurwitz and Schur
   stability of interval matrices. Tight bounds for the spectrum of
   interval matrices are estimated using computationally simple
   optimisation problems. The conservativeness is reduced further by
   application of ordinary similarity transformation. A necessary and
   sufficient vertex based criterion for the stability of a subclass of
   interval systems in continuous and discrete-time cases is also proposed.
   This enables the spectra for this class of interval systems to be
   determined exactly. A selection of various examples adopted from
   existing literature is used to demonstrate the utility of the proposed
   criteria.}},
