@article{ ISI:000319306100002,
Author = {Harvey, Jean-Philippe and Eriksson, Gunnar and Orban, Dominique and
   Chartrand, Patrice},
Title = {{GLOBAL MINIMIZATION OF THE GIBBS ENERGY OF MULTICOMPONENT SYSTEMS
   INVOLVING THE PRESENCE OF ORDER/DISORDER PHASE TRANSITIONS}},
Journal = {{AMERICAN JOURNAL OF SCIENCE}},
Year = {{2013}},
Volume = {{313}},
Number = {{3}},
Pages = {{199-241}},
Month = {{MAR}},
Abstract = {{We present in this paper a robust strategy to determine the equilibrium
   state, in the isobaric-isothermal (NPT) ensemble, of complex
   multicomponent systems in which solid solutions presenting
   order/disorder transitions are stable. The algorithm specifically
   designed to construct the first estimate of the phase assemblage
   describing the equilibrium state of the system is presented in detail in
   this work and tested on different binary and ternary systems in which
   solid solutions are modeled using i) the cluster site approximation or
   the cluster variation method in the tetrahedron approximation for both
   the face-centered and body-centered cubic solutions. The performance of
   the sequential quadratic strategy using an exact Newton method and a
   linesearch method, implemented in this work for the specific resolution
   of Gibbs free energy minimization problems, is compared to the one of
