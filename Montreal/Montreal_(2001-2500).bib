@article{ ISI:000322713300002,
Author = {Mitchell, Denis and Huffman, Sharlie and Tremblay, Robert and
   Saatcioglu, Murat and Palermo, Dan and Tinawi, Rene and Lau, David},
Title = {{Damage to bridges due to the 27 February 2010 Chile earthquake}},
Journal = {{CANADIAN JOURNAL OF CIVIL ENGINEERING}},
Year = {{2013}},
Volume = {{40}},
Number = {{8}},
Pages = {{675-692}},
Month = {{AUG}},
Abstract = {{This paper provides a summary of the damage to bridges in the M-w 8.8
   Chile earthquake of 27 February 2010. Lessons from the different types
   of structural damage observed on concrete and steel bridges are
   discussed. The important roles played by soil liquefaction, settlement
   and embankment failures are highlighted. Aspects such as shear failure
   of steel piles, shear failure of concrete substructure elements,
   failures and severe buckling of steel braces, failures of shear keys and
   restrainers at supports, and damage to girders due to lack of diaphragms
   are described. Many examples of loss of superstructure support are
   presented. Skew supports and multi-span simply supported bridges were
   particularly susceptible to loss of support. Several aspects of the
   Chilean bridge design code are discussed and compared with North
   American codes (CSA S6 and AASHTO).}},
