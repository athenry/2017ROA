@article{ ISI:000320619300075,
Author = {Sadeghi, S. M. and Hatef, A. and Meunier, Michel},
Title = {{Quantum detection and ranging using exciton-plasmon coupling in coherent
   nanoantennas}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{102}},
Number = {{20}},
Month = {{MAY 20}},
Abstract = {{We utilize interaction of a laser field with a quantum dot-metallic
   nanoshell system to investigate nanoscale detection and ranging using
   quantum coherence. We demonstrate that the nanoshell in this system can
   act as a coherent nanoantenna capable of designating each position in
   its range with unique space-time field coordinates. This shows that
   coherent exciton-plasmon coupling in such a system allows the electric
   field of this antenna generates position-dependent dynamics in molecules
   and nanostructures in its vicinity, allowing their remote detection. The
   results are obtained considering the ultrafast polarization dephasing of
   the quantum dot at elevated temperatures. (C) 2013 AIP Publishing LLC.}},
