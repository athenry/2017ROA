@article{ ISI:000345729500032,
Author = {Kizilkale, Arman C. and Dimitrakopoulos, Roussos},
Title = {{Optimizing mining rates under financial uncertainty in global mining
   complexes}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION ECONOMICS}},
Year = {{2014}},
Volume = {{158}},
Pages = {{359-365}},
Month = {{DEC}},
Abstract = {{This paper presents a distributed and dynamic programming framework to
   the mining production rate target tracking of multiple metal mines under
   financial uncertainty. A single mine's target tracking is stated as a
   stochastic optimization problem and the solution is obtained by solving
   the dynamic program which gives the optimal production rate schedule of
   each mine as a Markovian feedback control on the price process. The
   global solution is distributed on multiple mines by a policy iteration
   method, and this iterative method is shown to provide the unique
   equilibrium among Markovian strategies. Numerical results confirm the
   efficacy of the proposed global method when compared to individual
   optimization of mining rate target tracking. (C) 2014 The Authors.
   Published by Elsevier B.V.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
