@inproceedings{ ISI:000357259000060,
Author = {Bian, Siyuan and Loranger, Sebastien and Kashyap, Raman and Strickland,
   Donna},
Editor = {{George, T and Dutta, AK and Islam, MS}},
Title = {{Long wavelength mid-infrared from mixing two colors from a fiber
   amplifier}},
Booktitle = {{MICRO- AND NANOTECHNOLOGY SENSORS, SYSTEMS, AND APPLICATIONS VII}},
