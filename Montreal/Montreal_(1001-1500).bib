Title = {{Low Latency Solver for Linear Equation Systems in Floating Point
   Arithmetic}},
Booktitle = {{2015 INTERNATIONAL CONFERENCE ON RECONFIGURABLE COMPUTING AND FPGAS
   (RECONFIG)}},
Series = {{Proceedings International Conference on Reconfigurable Computing and
   FPGAs}},
