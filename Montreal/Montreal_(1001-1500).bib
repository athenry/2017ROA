@article{ ISI:000370537000008,
Author = {Gagnon, Louis and Sakadzic, Sava and Lesage, Frederic and Mandeville,
   Emiri T. and Fang, Qianqian and Yaseen, Mohammad A. and Boas, David A.},
Title = {{Multimodal reconstruction of microvascular-flow distributions using
   combined two-photon microscopy and Doppler optical coherence tomography}},
Journal = {{NEUROPHOTONICS}},
Year = {{2015}},
Volume = {{2}},
Number = {{1}},
Month = {{JAN-MAR}},
Abstract = {{Computing microvascular cerebral blood flow (mu CBF) in real cortical
   angiograms is challenging. Here, we investigated whether the use of
   Doppler optical coherence tomography (DOCT) flow measurements in
   individual vessel segments can help in reconstructing mu CBF across the
   entire vasculature of a truncated cortical angiogram. A mu CBF
   computational framework integrating DOCT measurements is presented.
   Simulations performed on a synthetic angiogram showed that the addition
   of DOCT measurements, especially close to large inflowing or outflowing
   vessels, reduces the impact of pressure boundary conditions and
   estimated vessel resistances resulting in a more accurate reconstruction
   of mu CBF. Our technique was then applied to reconstruct microvascular
   flow distributions in the mouse cortex down to 660 mu m by combining
   two-photon laser scanning microscopy angiography with DOCT. (C) 2015
   Society of Photo-Optical Instrumentation Engineers (SPIE)}},
