@article{ ISI:000347527800007,
Author = {Laflamme-Mayer, Nicolas and Blaquiere, Yves and Sawan, Mohamad},
Title = {{A configurable analog buffer dedicated to a wafer-scale prototyping
   platform}},
Journal = {{ANALOG INTEGRATED CIRCUITS AND SIGNAL PROCESSING}},
Year = {{2015}},
Volume = {{82}},
Number = {{1, SI}},
Pages = {{57-66}},
Month = {{JAN}},
Note = {{IEEE 4th Latin American Symposium on Circuits and Systems (LASCAS),
   Santiago de Chile, CHILE, FEB 26-28, 2014}},
Organization = {{IEEE}},
Abstract = {{This paper concerns a novel configurable analog buffer dedicated to a
   wafer-scale prototyping platform of electronic systems. The proposed
   architecture uses complementary nMOS and pMOS stage buffers, which are
   built with modified conventional differential pairs used for maximizing
   the output voltage swing. This compact analog buffer offers several
   slew-rate features that range from 66 up to 495 V/A mu s with a
   quasi-unity gain and only uses 21 transistors for a total silicon area
   of 0.001824 mm(2). The bandwidth of this proposed buffer can be
   programmed from 74 up to 194 MHz with response time up to 5.3 ns. This
   overall configurability allows better power management, reduces the
   power-supply noise injection within the wafer-scale platform, and
   diminishes the quiescent current.}},
