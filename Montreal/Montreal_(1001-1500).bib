@incollection{ ISI:000370529900004,
Author = {Farooq, Hamza and Genis, Helen and Alarcon, Joseph and Vuong, Barry and
   Jivraj, Jamil and Yang, Victor X. D. and Cohen-Adad, Julien and
   Fehlings, Michael G. and Cadotte, David W.},
Editor = {{Dancause, N and Nadeau, S and Rossignol, S}},
Title = {{High-resolution imaging of the central nervous system: how novel imaging
   methods combined with navigation strategies will advance patient care}},
Booktitle = {{SENSORIMOTOR REHABILITATION AT THE CROSSROADS OF BASIC AND CLINICAL
   SCIENCES}},
Series = {{Progress in Brain Research}},
Year = {{2015}},
Volume = {{218}},
Pages = {{55-78}},
Abstract = {{This narrative review captures a subset of recent advances in imaging of
   the central nervous system. First, we focus on improvements in the
   spatial and temporal profile afforded by optical coherence tomography,
   fluorescence-guided surgery, and Coherent Anti-Stokes Raman Scattering
   Microscopy. Next, we highlight advances in the generation and uses of
   imaging-based atlases and discuss how this will be applied to specific
   clinical situations. To conclude, we discuss how these and other imaging
   tools will be combined with neuronavigation techniques to guide surgeons
   in the operating room. Collectively, this work aims to highlight
   emerging biomedical imaging strategies that hold potential to be a
   valuable tool for both clinicians and researchers in the years to come.}},
