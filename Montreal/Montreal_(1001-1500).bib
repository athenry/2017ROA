@article{ ISI:000361560800010,
Author = {Mulu, B. G. and Cervantes, M. J. and Devals, C. and Vu, T. C. and
   Guibault, F.},
Title = {{Simulation-based investigation of unsteady flow in near-hub region of a
   Kaplan Turbine with experimental comparison}},
Journal = {{ENGINEERING APPLICATIONS OF COMPUTATIONAL FLUID MECHANICS}},
Year = {{2015}},
Volume = {{9}},
Number = {{1}},
Pages = {{139-156}},
Abstract = {{This paper presents a detailed comparison of steady and unsteady
   turbulent flow simulation results in the U9 Kaplan turbine draft tube
   with experimental velocity and pressure measurements. The computational
   flow domain includes the guide vanes, the runner and the draft tube. A
   number of turbulence models were studied, including the standard k -
   epsilon, RNG k - epsilon, SST and SST-SAS models. Prediction of the flow
   behavior in the conical section of the draft tube directly below the
   runner cone is very sensitive to the prediction of the separation point
   on the runner cone. The results demonstrate a significant increase in
   precision of the flow modeling in the runner cone region by using
   unsteady flow simulations compare to stage simulation. The prediction of
