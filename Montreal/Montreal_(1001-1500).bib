@article{ ISI:000353766400001,
Author = {Brassard, G. and Elias, Y. and Fernandez, J. M. and Gilboa, H. and
   Jones, J. A. and Mor, T. and Weinstein, Y. and Xiao, L.},
Title = {{Experimental heat-bath cooling of spins}},
