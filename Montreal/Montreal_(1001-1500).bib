@article{ ISI:000349394500001,
Author = {Zhang, Tao and Hong, Wei and Wu, Ke},
Title = {{A Low-Profile Triple-Band Triple-Polarization Antenna With Two
   Triangular Rings}},
Journal = {{IEEE ANTENNAS AND WIRELESS PROPAGATION LETTERS}},
Year = {{2015}},
Volume = {{14}},
Pages = {{378-381}},
Abstract = {{In this letter, a low-profile triple-band triple-polarization antenna is
   proposed. The antenna employs two triangular rings (TRGs) with different
   sizes. The two triangular rings each works as a dual-frequency
   dual-polarization radiator. The polarization states at the three bands
   are horizontal polarization, 45 polarization, and vertical polarization,
   which are achieved by the horizontal mode of the larger TRG, the
   superposition of the vertical mode of the larger TRG and horizontal mode
   of the smaller TRG, and the vertical mode of the smaller TRG,
   respectively. The overall size of the proposed antenna on the substrate
   with a low dielectric constant of 2.2 is 0.28 lambda(0) x 0.28 lambda(0)
   x 0.04 lambda(0), where lambda(0) denotes the wavelength in free space
   at 8.38 GHz.}},
