@article{ ISI:000330735200033,
Author = {Harvey, Jean-Philippe and Gheribi, Aimen E.},
Title = {{Process Simulation and Control Optimization of a Blast Furnace Using
   Classical Thermodynamics Combined to a Direct Search Algorithm}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS B-PROCESS METALLURGY AND
   MATERIALS PROCESSING SCIENCE}},
Year = {{2014}},
Volume = {{45}},
Number = {{1}},
Pages = {{307-327}},
