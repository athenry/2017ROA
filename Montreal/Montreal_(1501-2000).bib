@article{ ISI:000337253400019,
Author = {Desjardins, Michele and Berti, Romain and Lefebvre, Joel and Dubeau,
   Simon and Lesage, Frederic},
Title = {{Aging-related differences in cerebral capillary blood flow in
   anesthetized rats}},
Journal = {{NEUROBIOLOGY OF AGING}},
Year = {{2014}},
Volume = {{35}},
Number = {{8}},
Pages = {{1947-1955}},
Month = {{AUG}},
Abstract = {{Age-related decreases in baseline cerebral blood flow have been measured
   with various imaging modalities, however, the contribution of capillary
   flow to this phenomenon remain to elucidate. This study used 2-photon
   laser scanning fluorescence microscopy to measure capillary diameter,
   red blood cell speed, and flux in individual capillaries in the
   sensory-motor cortex of 12 adult (3-month-old) and 12 old (24-month-old)
   male Long-Evans rats under isoflurane anesthesia. The average (+/-
   standard deviation) diameter and speed over 921 capillaries were 6.4 +/-
   1.4 mu m and 1.3 +/- 1.1 mm/s, respectively. Red blood cell speed and
   flux were significantly higher, by 48\% and 15\%, respectively, in old
   compared with young animals (p < 5\%). The diameter also showed a
   similar tendency (7\% higher, p = 5.7\%). Furthermore, capillary
   hematocrit and density were significantly lower in the older group (p <
   5\%), by 32\% and 20\%, respectively. (C) 2014 Elsevier Inc. All rights
   reserved.}},
