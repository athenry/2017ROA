@inproceedings{ ISI:000346657100100,
Author = {Szkopek, Thomas and Caloz, Christophe and Skulason, Helgi Skuli},
Book-Group-Author = {{IEEE}},
Title = {{Large Area Graphene Electromagnetic Devices}},
