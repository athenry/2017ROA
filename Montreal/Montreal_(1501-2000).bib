@article{ ISI:000342382000001,
Author = {Chiganne, Francois and Marche, Claude and Mahdi, Tew-Fik},
Title = {{Probabilistic Modeling of flow breaking flood dam embankment}},
Journal = {{CANADIAN JOURNAL OF CIVIL ENGINEERING}},
Year = {{2014}},
Volume = {{41}},
Number = {{8}},
Pages = {{677-685}},
Month = {{AUG}},
Abstract = {{Earthfill dams, and even all dams, are designed to withstand different
   factors ( meteorological, seismic, and human), which might be
   responsible for their failure. Yet there is no such thing as zero risk,
   and the possibilities of failure exist. With a view to optimize the
   safety of downstream areas, dam failures by overtopping can be studied
   from a probabilistic point of view, in order to go further than a simple
   failure - non failure evaluation. Probability calculation of the
   failure, mainly with flood studies, is the first step. Then, in order to
   improve the prediction of the failure, the failure hydrograph is
   probabilized by its maximum discharge and the time this discharge needs
   to be reached.}},
