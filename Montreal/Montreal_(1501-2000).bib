@article{ ISI:000337882500010,
Author = {Nahas, Nabil and Nourelfath, Mustapha and Gendreau, Michel},
Title = {{Selecting machines and buffers in unreliable assembly/disassembly
   manufacturing networks}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION ECONOMICS}},
Year = {{2014}},
Volume = {{154}},
Pages = {{113-126}},
Month = {{AUG}},
Abstract = {{This paper formulates an optimal design model for assembly/disassembly
   manufacturing networks. The objective is to maximize production rate
   subject to a total cost constraint Machines are chosen from a list of
   products available on the market, and sizes of the buffers are chosen
   within a predetermined range. Each machine type is characterized by its
   total cost of ownership, failure rate, repair rate and processing time.
   The buffers are also characterized by their total cost of ownership
   coefficients associated with the buffer size. To estimate
   assembly/disassembly network performance, a decomposition-type
   approximation is used. The optimal design model is formulated as a
   combinatorial optimization one in which the derision variables are
   buffers and types of machines. A genetic algorithm is proposed as an
   optimization technique. Numerical examples are used to highlight the
   benefit of selecting simultaneously the buffers and the machines. (C)
   2014 Elsevier B.V. All rights reserved.}},
