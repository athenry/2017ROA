@article{ ISI:000342382000010,
Author = {Daneshvar, Poulad and Bouaanani, Najib and Leger, Pierre},
Title = {{Application of conditional mean spectra for evaluation of a building's
   seismic response in Eastern Canada}},
Journal = {{CANADIAN JOURNAL OF CIVIL ENGINEERING}},
Year = {{2014}},
Volume = {{41}},
Number = {{8}},
Pages = {{769-773}},
Month = {{AUG}},
Abstract = {{An existing eight-storey reinforced concrete shear wall building located
   in Montreal is studied to evaluate the impact of applying a conditional
   mean spectrum (CMS) in lieu of the conventional uniform hazard spectrum
   (UHS) to the seismic evaluation of such structures. The construction of
   the CMS is reviewed and adapted to take account of seismic hazard in
   Montreal. The developed CMS and their envelopes were used to conduct
   modal response spectrum analyses of the building and the results are
   compared to those obtained from the NBCC 2005 UHS. Justifications of
   similarities or differences between CMS- and UHS-based results are
   illustrated throughout the paper.}},
