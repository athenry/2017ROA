@article{ ISI:000340098600018,
Author = {Gupta, Shulabh and Jiang, Li Jun and Caloz, Christophe},
Title = {{Magnetoelectric Dipole Antenna Arrays}},
Journal = {{IEEE TRANSACTIONS ON ANTENNAS AND PROPAGATION}},
Year = {{2014}},
Volume = {{62}},
Number = {{7}},
Pages = {{3613-3622}},
Month = {{JUL}},
Abstract = {{A planar magnetoelectric (ME) dipole antenna array is proposed and
   demonstrated by both full-wave analysis and experiments. The proposed
   structure leverages the operation of composite right/left-handed
   transmission lines under balanced conditions to form high-gain magnetic
   radiators, combined with radial conventional electric radiators, where
   the overall structure is excited by a single differential feed. The
   traveling-wave-type nature of the proposed ME-dipole antenna enables the
   formation of directive arrays with high-gain characteristics and
   scanning capability. Peak gains of 10.84 dB and 5.73 dB are demonstrated
   for the electric dipole and magnetic-dipole radiation components,
   respectively.}},
