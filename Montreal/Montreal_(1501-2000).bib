@inproceedings{ ISI:000350044701139,
Author = {Gervais, Thomas and Safavieh, Mohammadali and Qasaimeh, Mohammad A. and
   Juncker, David},
Book-Group-Author = {{IEEE}},
Title = {{Systematic analysis of microfluidic probe design and operation}},
Booktitle = {{2014 36TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
   MEDICINE AND BIOLOGY SOCIETY (EMBC)}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
