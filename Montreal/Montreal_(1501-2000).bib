Title = {{Vt-Conscious Repeater Insertion in Power-Managed VLSI}},
Booktitle = {{2014 14TH INTERNATIONAL SYMPOSIUM ON INTEGRATED CIRCUITS (ISIC)}},
Series = {{International Symposium on Integrated Circuits}},
Year = {{2014}},
