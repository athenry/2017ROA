@article{ ISI:000339820400001,
Author = {Adjengue, Luc and Audet, Charles and Ben Yahia, Imen},
Title = {{A variance-based method to rank input variables of the Mesh Adaptive
   Direct Search algorithm}},
Journal = {{OPTIMIZATION LETTERS}},
Year = {{2014}},
Volume = {{8}},
Number = {{5}},
Pages = {{1599-1610}},
Month = {{JUN}},
Abstract = {{The Mesh Adaptive Direct Search algorithm (MADS) algorithm is designed
   for nonsmooth blackbox optimization problems in which the evaluation of
   the functions defining the problems are expensive to compute. The MADS
   algorithm is not designed for problems with a large number of variables.
   The present paper uses a statistical tool based on variance
   decomposition to rank the relative importance of the variables. This
   statistical method is then coupled with the MADS algorithm so that the
   optimization is performed either in the entire space of variables or in
   sub-spaces associated with statistically important variables. The
   resulting algorithm is called STATS-MADS and is tested on bound
   constrained test problems having up to 500 variables. The numerical
   results show a significant improvement in the objective function value
   after a fixed budget of function evaluations.}},
