@inproceedings{ ISI:000341702106018,
Author = {Ameli, Reza and Mirbozorgi, Abdollah and Neron, Jean-Luc and LeChasseur,
   Yoan and Gosselin, Benoit},
Book-Group-Author = {{IEEE}},
Title = {{A Wireless and Batteryless Neural Headstage with Optical Stimulation and
   Electrophysiological Recording}},
