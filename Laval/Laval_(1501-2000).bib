@inproceedings{ ISI:000323698200001,
Author = {Caron, J. and Rochefort, L.},
Editor = {{Caron, J}},
Title = {{Use of Peat in Growing Media: State of the Art on Industrial and
   Scientific Efforts Envisioning Sustainability}},
Booktitle = {{INTERNATIONAL SYMPOSIUM ON RESPONSIBLE PEATLAND MANAGEMENT AND GROWING
   MEDIA PRODUCTION}},
