@article{ ISI:000328665000008,
Author = {Beaudoin, Georges and Mercier-Langevin, Patrick and Dube, Benoit and
   Taylor, Bruce E.},
Title = {{Low-Temperature Alteration at the World-Class LaRonde Penna Archean
   Au-Rich Volcanogenic Massive Sulfide Deposit, Abitibi Subprovince,
   Quebec, Canada: Evidence from Whole-Rock Oxygen Isotopes}},
Journal = {{ECONOMIC GEOLOGY}},
Year = {{2014}},
Volume = {{109}},
Number = {{1, SI}},
Pages = {{167-182}},
Month = {{JAN-FEB}},
Abstract = {{The oxygen isotope composition of 169 whole-rock samples from the
   world-class LaRonde Penna Au-rich volcanogenic massive sulfide deposit
   range from 6.5 to 22.0 parts per thousand. Fractional crystallization
   from basalt to rhyolite accounts for a variation of approximately 2
   parts per thousand in primary whole-rock delta O-18 values. The
   remaining variance in whole-rock delta O-18 values found at LaRonde
   Penna must therefore be ascribed to water-rock exchange with
   hydrothermal fluids. The metamorphosed, hydrothermally altered host
   rocks have been subdivided according to mineralogical assemblages, which
   display weak covariance with delta O-18 values and whole-rock
   geochemical alteration indices. Water-rock equilibrium exchange models
   indicate that the high delta O-18 values that characterize the LaRonde
   Penna deposit are compatible with low-temperature (similar to 150
   degrees C) hydrothermal alteration at high water-rock oxygen atomic
   ratios (1-50). Zones of lower delta O-18 values stratigraphically
   beneath the principal ore lens (20 North) indicate local zones of higher
   temperature hydrothermal alteration. The oxygen isotope composition of
   the hydrothermal fluid is estimated to have been similar to 5 parts per
   thousand, as a consequence of inking between seawater (delta O-18
   similar to 0 parts per thousand) and a component of magmatic water
   degassed from the volcanic and intrusive rocks associated with the
   LaRonde Penna deposit.}},
