Title = {{The boron isotope composition of tourmaline from the Val-d'Or orogenic
   gold deposits, Quebec, Canada}},
Booktitle = {{MINERAL DEPOSIT RESEARCH FOR A HIGH-TECH WORLD, VOLS. 1-4}},
Year = {{2013}},
Pages = {{1090-1092}},
Note = {{12th Biennial SGA Meeting on Mineral Deposit Research for a High-Tech
