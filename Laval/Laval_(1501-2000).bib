Title = {{Experimental study of the pressure fluctuations on propeller turbine
   runner blades: part 2, transient conditions}},
Booktitle = {{26TH IAHR SYMPOSIUM ON HYDRAULIC MACHINERY AND SYSTEMS, PTS 1-7}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
Year = {{2013}},
Volume = {{15}},
