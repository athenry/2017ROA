@article{ ISI:000312946000005,
Author = {Tougas, B. and Blais, C. and Chagnon, F. and Pelletier, S.},
Title = {{Strain induced martensite formation in PM nickel steels}},
Journal = {{POWDER METALLURGY}},
Year = {{2012}},
Volume = {{55}},
Number = {{5}},
Pages = {{348-353}},
Month = {{DEC}},
Abstract = {{Powder metallurgy (PM) nickel steels are often selected because they
   have high strength, high impact resistance and good abrasion resistance.
   The microstructure of slowly cooled PM nickel steels typically contains
   pearlite, bainite, martensite and a fair volume fraction of retained
   austenite. Since volume diffusion is really low at conventional
   sintering temperatures {[}1120-1150 degrees C (approximate to 2050-2100
   degrees F)], nickel rich areas are usually found where prior admixed
   nickel particles were located, i.e. at the surface of iron particles and
   in sintering necks. Therefore, there is a discrepancy between the rather
   low mechanical properties of austenite and the high strength of PM
   nickel steel. Hence, the hypotheses that stress induced or strain
   induced martensite formation takes place during mechanical testing have
   been investigated. Results show that martensite forms during mechanical
   testing, and its final volume fraction is proportional to nickel
   content.}},
