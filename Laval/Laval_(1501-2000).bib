@article{ ISI:000325908300026,
Author = {Patel, Jayesh D. and Mighri, Frej and Ajji, Abdellah and Chaudhuri,
   Tapas K.},
Title = {{Fatty acid-assisted synthesis of CdS microspheres: physicochemical
   properties and photocatalytic activity}},
Journal = {{MATERIALS LETTERS}},
Year = {{2013}},
Volume = {{110}},
Pages = {{94-97}},
Month = {{NOV 1}},
Abstract = {{This work deals with fatty acid-assisted high yield synthesis of cadmium
   sulfide (CdS) mesoporous microspheres (MSs). The synthesis was conducted
   by solvothermal treatment of thiourea (TU) mixed cadmium-oleate (Cd-O)
   complex precursor at 180 degrees C. The Cd-O complex sulphurized upon
   thermal decomposition of TU and produced OA-capped CdS nanocrystals
   (NCs) composed cauliflower-like mesoporous CdS MSs with an average size
   of 1.5 pm. The as-synthesized CdS MSs exhibit good physicochemical
   properties. Moreover, the as-synthesized CdS MSs showed pronounced
   photocatalytic activity to degrade rhodamine B (RhB) in aqueous medium.
   This suggested that the as-synthesized CdS MSs might be used as a
