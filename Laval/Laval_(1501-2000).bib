@inproceedings{ ISI:000332006802126,
Author = {Yousefian, Alireza and Roy, Sebastien and Gosselin, Benoit},
Book-Group-Author = {{IEEE}},
Title = {{A Low-Power Wireless Multi-Channel Surface EMG Sensor with Simplified
   ADPCM Data Compression}},
