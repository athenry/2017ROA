@article{ ISI:000388212100014,
Author = {Liato, Viacheslav and Labrie, Steve and Benali, Marzouk and Aider,
   Mohammed},
Title = {{Effect of electro-activated brine solution on the migration of metallic
   ions from the cans to the product in sterilized canned sweet corn}},
Journal = {{FOOD SCIENCE \& NUTRITION}},
Year = {{2016}},
Volume = {{4}},
Number = {{6}},
Pages = {{897-905}},
Month = {{NOV}},
Abstract = {{Tinplate cans were used to study if electro-activated brine solution
   (EAS) is more corrosive than conventional one by ICP analysis. The
   results showed different effects of EAS on cans, alone or filled with
   product. Acidic EAS (pH 2-3) and Redox + 900 to + 1200 mV highly reacted
   with the cans. The concentrations of Zn, Fe, and Cu in the solution were
   0.028, 28.81, and 0.022 ppm, respectively. No Sn migration was observed
   in this case. When neutral or acidic chlorine-free EAS was used, no
   significant difference was observed in comparison with the corrosivity
   of standard NaCl brine. Alkaline EAS with pH>10 and negative E (<=-966
   mV) did not affect Zn, Fe, and Cu migration. However, it affected tin
   migration. Nevertheless, it is important to mention that even if some
   corrosion was observed, it was in the limit of the permitted level of
   concentration when the cans were filled with a product.}},
