@article{ ISI:000378369400002,
Author = {Nguyen Thanh-Binh and Dubois, Jean-Luc and Kaliaguine, Serge},
Title = {{Ammoxidation of acrolein to acrylonitrile over bismuth molybdate
   catalysts}},
Journal = {{APPLIED CATALYSIS A-GENERAL}},
Year = {{2016}},
Volume = {{520}},
Pages = {{7-12}},
Month = {{JUN 25}},
Abstract = {{The present work deals with the potentially significant process
   converting acrolein of green origin to acrylonitrile using mesoporous
   bismuth molybdate catalysts. The ammoxidation catalysts were
   characterized by N-2 physisorption, X-ray diffraction, and catalytic
   tests under various conditions at different temperatures, contact times,
   and reactant molar ratios. The results indicated a catalytic activity
   proportional to specific surface area, which depends on bismuth
   molybdate phases, and concentration of oxygen in the gas feed. The
   selectivity of the catalysts only depends on reaction temperature. ACN
   selectivity obtained at 350-400 degrees C was 100\% and reduced to 97\%
   at 450 degrees C. (C)2016 Elsevier B.V. All rights reserved.}},
