@inproceedings{ ISI:000390760100194,
Author = {Baillargeon, Sophie and Halle, Simon and Gagne, Christian},
Editor = {{Kumar, R and Caverlee, J and Tong, H}},
Title = {{Stream Clustering of Tweets}},
Booktitle = {{PROCEEDINGS OF THE 2016 IEEE/ACM INTERNATIONAL CONFERENCE ON ADVANCES IN
   SOCIAL NETWORKS ANALYSIS AND MINING ASONAM 2016}},
Year = {{2016}},
Pages = {{1256-1261}},
Note = {{8th IEEE/ACM International Conference on Advances in Social Networks
   Analysis and Mining (ASONAM), San Francisco, CA, AUG 18-21, 2016}},
Organization = {{IEEE; Assoc Comp Machinery; ACM SIGMOD; IEEE Comp Soc; IEEE TCDE;
   Springer; VEEPIO}},
Abstract = {{This paper proposes an approach to cluster social media posts. It aims
   at taking full advantage of this recent source of newsworthy information
   and at facilitating the work of users who need to monitor public events
   in real-time. The emphasis is on developing a stream clustering
   algorithm able to process incoming tweets. A first implementation of the
   algorithm, focusing on the tweets' text, was tuned and tested on a
   dataset of manually annotated messages. Results show that the algorithm
   produces a partition of tweets similar to the manual partition obtained
   from humans. In future work, we plan to extend this algorithm with
   additional features and integrate the resulting analytical capabilities
   to a real-time social media monitoring platform called CrowdStack.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
