@article{ ISI:000374496500021,
Author = {Yousefian, Hajar and Rodrigue, Denis},
Title = {{Effect of nanocrystalline cellulose on morphological, thermal, and
   mechanical properties of Nylon 6 composites}},
Journal = {{POLYMER COMPOSITES}},
Year = {{2016}},
Volume = {{37}},
Number = {{5}},
Pages = {{1473-1479}},
Month = {{MAY}},
Abstract = {{In this study, nanocomposites based on Nylon 6 and nanocrystalline
   cellulose (NCC) were prepared by melt compounding. Then, morphological,
   thermal, and mechanical properties were analyzed for NCC content between
   0 and 7 wt\%. Morphological analyses showed different roughness in
   fractured surface of neat Nylon and its nanocomposites caused by the
   presence of NCC. Mechanical results showed that the optimum properties
   were obtained at 3\% NCC which could be related to relatively good NCC
   dispersion at low concentrations with good Nylon-NCC bonding. Overall,
   flexural (41\%) and tensile (23\%) moduli, as well as tensile strength
   (11\%) were increased up to 3\% of NCC. However, elongation at break and
   impact strength decreased with NCC addition. Finally, density and
   hardness showed only a small increase of 5 and 3\%, respectively. POLYM.
   COMPOS., 37:1473-1479, 2016. (c) 2014 Society of Plastics Engineers}},
