@article{ ISI:000376096500002,
Author = {Hoevenaars, Antonius G. L. and Gosselin, Clement and Lambert, Patrice
   and Herder, Just L.},
Title = {{Experimental Validation of Jacobian-Based Stiffness Analysis Method for
   Parallel Manipulators With Nonredundant Legs}},
Journal = {{JOURNAL OF MECHANISMS AND ROBOTICS-TRANSACTIONS OF THE ASME}},
Year = {{2016}},
Volume = {{8}},
Number = {{4}},
Month = {{AUG}},
Abstract = {{A complete stiffness analysis of a parallel manipulator considers the
   structural compliance of all elements, both in designed
   degrees-of-freedom (DoFs) and constrained DoFs, and also includes the
   effect of preloading. This paper presents the experimental validation of
   a Jacobian-based stiffness analysis method for parallel manipulators
   with nonredundant legs, which considers all those aspects, and which can
   be applied to limited-DoF parallel manipulators. The experimental
   validation was performed by comparing differential wrench measurements
   with predictions based on stiffness analyses with increasing levels of
   detail. For this purpose, two passive parallel mechanisms were designed,
   namely, a planar 3DoF mechanism and a spatial 1DoF mechanism. For these
   mechanisms, it was shown that a stiffness analysis becomes more accurate
   if preloading and structural compliance are considered.}},
