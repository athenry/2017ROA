@article{ ISI:000379588700027,
Author = {Bilodeau, Jean-Pascal and Plamondon, Claudiane Ouellet and Dore, Guy},
Title = {{Estimation of resilient modulus of unbound granular materials used as
   pavement base: combined effect of grain-size distribution and aggregate
   source frictional properties}},
Journal = {{MATERIALS AND STRUCTURES}},
Year = {{2016}},
Volume = {{49}},
Number = {{10}},
Pages = {{4363-4373}},
Month = {{OCT}},
Abstract = {{The resilient modulus of granular materials used in base layers is
   influenced by numerous factors, such as grain-size distribution and
   aggregate source properties. Resilient modulus tests were performed on
   three aggregate sources for five identical gradations in order to study
   their respective effects on the mechanical response of granular
   materials. To consider these effects, two new parameters associated with
   frictional properties and gradation are introduced. For the proposed
   model, standard grain-size distribution and flow tests are needed to
   estimate of the resilient modulus of granular materials.}},
