@article{ ISI:000384126100008,
Author = {Nejad, Reza Mirzaei and Allahverdyan, Karen and Vaity, Pravin and
   Amiralizadeh, Siamak and Brunet, Charles and Messaddeq, Younes and
   LaRochelle, Sophie and Rusch, Leslie Ann},
Title = {{Mode Division Multiplexing Using Orbital Angular Momentum Modes Over
   1.4-km Ring Core Fiber}},
Journal = {{JOURNAL OF LIGHTWAVE TECHNOLOGY}},
Year = {{2016}},
Volume = {{34}},
Number = {{18}},
Pages = {{4252-4258}},
Month = {{SEP 15}},
Abstract = {{Mode division multiplexing (MDM) systems using orbital angular momentum
   (OAM) modes can recover the data in D different modes without recourse
   to full (2D x 2D) multiple input multiple output (MIMO) processing. One
   of the biggest challenges in OAM-MDM systems is the mode instability
   following fiber propagation. Previously, MIMO-free OAM-MDM data
   transmission with two modes over 1.1 km of vortex fiber was
   demonstrated, where optical polarization demultiplexing was employed in
   the setup. We demonstrate MDM data transmission using two OAM modes over
   1.4 km of a specially designed ring core fiber without using full MIMO
   processing or optical polarization demultiplexing. We demonstrate
   reception with electrical polarization demultiplexing, i.e., minimal 2 x
   2 MIMO, showing the compatibility of OAM-MDM with current polarization
   demultiplexing receivers.}},
