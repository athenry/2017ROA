@article{ ISI:000344992600007,
Author = {Brunet, Charles and Ung, Bora and Belanger, Pierre-Andre and Messaddeq,
   Younes and LaRochelle, Sophie and Rusch, Leslie Ann},
Title = {{Vector Mode Analysis of Ring-Core Fibers: Design Tools for Spatial
   Division Multiplexing}},
Journal = {{JOURNAL OF LIGHTWAVE TECHNOLOGY}},
Year = {{2014}},
Volume = {{32}},
Number = {{23}},
Pages = {{4046-4057}},
Month = {{DEC 1}},
Abstract = {{Design tools have existed for decades for standard step-index fibers,
   with analytical expressions for cutoff conditions as a function of core
   size, refractive indexes, and wavelength. We present analytical
   expressions for cutoff conditions for fibers with a ring-shaped
   propagation region. We validate our analytical expressions against
   numerical solutions, as well as via asymptotic analysis yielding the
   existing solutions for standard step-index fiber. We demonstrate the
   utility of our solutions for optimizing fibers supporting specific
   eigenmode behaviors of interest for spatial division multiplexing. In
   particular, we address large mode separation for orbital angular
   momentum modes and fibers supporting only modes with a single intensity
   ring.}},
