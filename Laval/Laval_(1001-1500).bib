@inproceedings{ ISI:000370971100070,
Author = {Fattahi, Jaouhar and Mejri, Mohamed},
Book-Group-Author = {{IEEE}},
Title = {{Secrecy by Witness-Functions under Equational Theories}},
Booktitle = {{PROCEEDINGS OF THE 2015 7TH INTERNATIONAL CONFERENCE ON ELECTRONICS,
   COMPUTERS AND ARTIFICIAL INTELLIGENCE (ECAI)}},
Series = {{International Conference on Electronics Computers and Artificial
   Intelligence}},
