@article{ ISI:000343832300024,
Author = {Kakroodi, Adel Ramezani and Rodrigue, Denis},
Title = {{Impact Modification of Polypropylene-Based Composites Using
   Surface-Coated Waste Rubber Crumbs}},
Journal = {{POLYMER COMPOSITES}},
Year = {{2014}},
Volume = {{35}},
Number = {{11}},
Pages = {{2280-2289}},
Month = {{NOV}},
Abstract = {{Blends of maleated polypropylene (MAPP) with high contents of waste
   rubber powder, namely ground tire rubber and waste ethylene propylene
   diene monomer (EPDM) powder, were used as impact modifiers for
   polypropylene (PP) based composites with different reinforcements (hemp,
   talc, and milled glass fiber). Adding reinforcements led to increase in
   modulus (tensile, flexural, and torsion moduli) of PP, while its impact
   strength decreased noticeably. Impact modification of PP-based
   composites was successfully performed via inclusion of MAPP/waste rubber
   compounds, especially compounds containing waste EPDM powder. Inclusion
   of such impact modifiers increased impact strength of composites over
   80\%. The effects of impact modification were more significant for
   hemp-and glass-filled composites compared to composites containing talc.
   However, slight decrease in tensile, flexural, and torsion moduli (up to
   30\%) of the composites was also observed after inclusion of impact
   modifiers. (C) 2014 Society of Plastics Engineers}},
