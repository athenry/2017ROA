@article{ ISI:000331343900075,
Author = {Lachance-Quirion, Dany and Tremblay, Samuel and Lamarre, Sebastien A.
   and Methot, Vincent and Gingras, Daniel and Lemyre, Julien Camirand and
   Pioro-Ladriere, Michel and Ni Allen, Claudine},
Title = {{Telegraphic Noise in Transport through Colloidal Quantum Dots}},
Journal = {{NANO LETTERS}},
Year = {{2014}},
Volume = {{14}},
Number = {{2}},
Pages = {{882-887}},
Month = {{FEB}},
Abstract = {{We report measurements of electrical transport through single CdSe/CdS
   core/shell colloidal quantum dots (cQDs) connected to source and drain
   contacts. We observe telegraphic switching noise showing few plateaus at
   room temperature. We model and interpret these results as charge
   trapping of individual trap states, and therefore we resolve individual
   charge defects in these high-quality low-strain cQDs. The small number
   of observed defects quantitatively validates the passivation method
   based on thick CdS shells nearly lattice-matched to CdSe cores first
   developed to suppress photoluminescence blinking. Finally, we introduce
   a figure of merit useful to efficiently distinguish telegraphic noise
   from noise with a Gaussian distribution.}},
