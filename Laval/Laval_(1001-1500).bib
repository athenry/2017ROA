@article{ ISI:000335548200002,
Author = {Gosselin, C. and Lecours, A. and Laliberte, T. and Fortin, M.},
Title = {{Design and experimental validation of planar programmable inertia
   generators}},
Journal = {{INTERNATIONAL JOURNAL OF ROBOTICS RESEARCH}},
Year = {{2014}},
