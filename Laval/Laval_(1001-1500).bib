@article{ ISI:000331864500008,
Author = {Bilodeau, Jean-Pascal and Dore, Guy},
Title = {{Stress distribution experienced under a portable light-weight
   deflectometer loading plate}},
Journal = {{INTERNATIONAL JOURNAL OF PAVEMENT ENGINEERING}},
Year = {{2014}},
Volume = {{15}},
Number = {{6}},
Pages = {{564-575}},
Month = {{JUL 3}},
Abstract = {{In the field of pavement engineering, the need to rapidly and
   economically obtain properties of soils and materials has given rise to
   the development of many non-destructive techniques. Portable
   light-weight deflectometers (PLWD) are relatively new deflection-based
   tools. As a consequence, the main factors influencing the quality of the
   results obtained using such a tool are still being investigated.
   Therefore, through finite element modelling and experimental
   measurements, this research focussed on the stress distribution and
   stress variability under a PLWD loading plate and how it is influenced
   by stress level, plate diameter, rubber mat use and rubber hardness. It
   was shown that the stress distribution factor used for the calculation
   of elastic modulus from deflection testing is significantly influenced
   by the loading plate diameter. Moreover, the use of a harder rubber mat
   was found to be better, but no significant effect was observed for
   bonded rubber mat conditions.}},
