@inproceedings{ ISI:000366400200010,
Author = {Angeloni, L. and Passeri, D. and Reggente, M. and Rossi, M. and
   Mantovani, D. and Lazzaro, L. and Nepi, F. and De Angelis, F. and
   Barteri, M.},
