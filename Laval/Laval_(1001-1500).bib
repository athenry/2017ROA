@article{ ISI:000355977200006,
Author = {Mariotti, Nicolas and Stevanovic, Tatjana and Rodrigue, Denis and Wang,
   Xiang-Ming and Grenier, Daniel},
Title = {{HDPE COMPOSITES CONTAINING BARK AND INDUSTRIAL KRAFT LIGNINS}},
Journal = {{J-FOR-JOURNAL OF SCIENCE \& TECHNOLOGY FOR FOREST PRODUCTS AND PROCESSES}},
Year = {{2014}},
Volume = {{4}},
Number = {{3}},
Pages = {{27-34}},
Abstract = {{Softwood and hardwood Kraft lignins have been precipitated by carbon
   dioxide using black liquor from two pulp mills in Quebec (Domtar Windsor
   and Kruger Wayagamack) following a procedure inspired by the LignoBoost
   protocol (using CO2 to decrease the pH). Commercial Kraft lignin
   (Indulin AT) was also tested for comparison. The valorization of Kraft
   lignin co-products is an example of the integrated forest biorefinery
   concept and contributes to CO2 sequestration. The studied Kraft lignins
   were esterified with butyric anhydride, maleic anhydride, or stearoyl
   chloride to improve their compatibility with hydrophobic high-density
   polyethylene (HDPE) and to test their applicability as coupling agents.
   The properties of HDPE composites containing non-modified and esterified
   Kraft lignins as coupling agents between the matrix and the bark fibres
   were examined in comparison to maleated polyethylene (MAPE). The
   chemical compositions of yellow birch (Betula alleghaniensis) and black
   spruce (Picea mariana) bark fibres were determined. The thermal,
   mechanical (flexural and tensile properties), swelling, and wettability
   (contact angle) properties of the composites containing non-extracted
   and extracted bark fibres and Kraft lignins in HDPE were compared.
   Overall, water extraction of bark fibres does not seem to have a
   significant effect on the tensile properties of biocomposites.}},
