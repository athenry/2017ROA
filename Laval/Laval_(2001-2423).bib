@article{ ISI:000298812200046,
Author = {Gagnon, Jean-Christophe and Theriault, Dany and Guillot, Mathieu and
   Archambault, Louis and Beddar, Sam and Gingras, Luc and Beaulieu, Luc},
Title = {{Dosimetric performance and array assessment of plastic scintillation
