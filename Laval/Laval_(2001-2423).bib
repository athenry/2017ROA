@inproceedings{ ISI:000335932600036,
Author = {Lauzier, Nicolas and Gosselin, Clement},
Book-Group-Author = {{ASME}},
Title = {{TOWARDS WHOLE-ARM STATICS}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
