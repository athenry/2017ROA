@article{ ISI:000302807500025,
Author = {Lecours, Pascale Blais and Veillette, Marc and Marsolais, David and
   Duchaine, Caroline},
Title = {{Characterization of Bioaerosols from Dairy Barns: Reconstructing the
   Puzzle of Occupational Respiratory Diseases by Using Molecular
   Approaches}},
Journal = {{APPLIED AND ENVIRONMENTAL MICROBIOLOGY}},
Year = {{2012}},
Volume = {{78}},
Number = {{9}},
Pages = {{3242-3248}},
Month = {{MAY}},
Abstract = {{To understand the etiology of exposure-related diseases and to establish
   standards for reducing the risks associated with working in contaminated
   environments, the exact nature of the bioaerosol components must be
   defined. Molecular biology tools were used to evaluate airborne
   bacterial and, for the first time, archaeal content of dairy barns.
   Three air samplers were tested in each of the 13 barns sampled. Up to
   106 archaeal and 108 bacterial 16S rRNA genes per m(3) of air were
   detected. Archaeal methanogens, mainly Methanobrevibacter species, were
   represented. Saccharopolyspora rectivirgula, the causative agent of
   farmer's lung, was quantified to up to 107 16S rRNA genes per m(3) of
   air. In addition, a wide variety of bacterial agents were present in our
