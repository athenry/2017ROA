@article{ ISI:000304220700010,
Author = {Mashreghi, Javad and Nasri, Mostafa},
Title = {{HYBRID LAGRANGE MULTIPLIER APPROACHES FOR SOLVING INFINITE DIMENSIONAL
   EQUILIBRIUM PROBLEMS WITH CONE CONSTRAINTS}},
Journal = {{JOURNAL OF NONLINEAR AND CONVEX ANALYSIS}},
Year = {{2012}},
Volume = {{13}},
Number = {{2}},
Pages = {{331-349}},
Abstract = {{We introduce two Lagrange multiplier algorithms which are hybrid types
   for solving equilibrium problems in reflexive Banach spaces whose
   feasible sets are cone constrained. At each iteration of these
   algorithms, first an approximate solution of an unconstrained
   equilibrium problem is found. Then, using information obtained from this
   approximate solution, the next iterate of our algorithms is computed by
   performing either an extragradient-type step with respect to the metric
   induced by a Bregrarn distance or a Bregrnan projection of the current
   iterate onto a certain hyperplane separating the current iterate from
   the solution set of the problem. One of the important features of our
   schemes is that they allow significant relaxation of tolerance
   requirements imposed on the solution of unconstrained subproblems. Under
   appropriate hypotheses on the problem data, we show that the generated
   sequence by each algorithm weakly and globally converges to a solution
   of the problem.}},
