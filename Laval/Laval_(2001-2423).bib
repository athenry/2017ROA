@inproceedings{ ISI:000308517301221,
Author = {El Ouafi, A. and Belanger, R. and Guillot, M.},
Editor = {{Chandra, T and Ionescu, M and Mantovani, D}},
Title = {{Dynamic resistance based model for on-line resistance spot welding
   quality assessment}},
