Author = {Chen, Xiao Yuan and Nik, Omid Ghaffari and Rodrigue, Denis and
   Kaliaguine, Serge},
Title = {{Mixed matrix membranes of aminosilanes grafted FAU/EMT zeolite and
   cross-linked polyimide for CO2/CH4 separation}},
