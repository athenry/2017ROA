@article{ ISI:000304278300004,
Author = {Allahverdyan, Karen and Galstian, Tigran},
Title = {{Accelerating the Cholesteric Helix Restoring by a Dual Frequency
   Compound}},
Journal = {{MOLECULAR CRYSTALS AND LIQUID CRYSTALS}},
Year = {{2012}},
Volume = {{560}},
Number = {{SI}},
Pages = {{35-48}},
Abstract = {{We report the creation and the use of dual frequency cholesteric liquid
   crystal mixtures for the dynamic electrical unwinding and forced
   (accelerated) restoring of their molecular helix. The use of a
   cholesteric in a mixture with a dual frequency nematic shifts the
   resonant wavelength to the near infrared area. We use polarimetric
   transmission measurements (out of resonance) to check the effective
   birefringence of the cell as well as the resonant reflection (at near
   infra red) to characterize the dynamics of unwinding and restoring
   processes. The restoring process is accelerated almost by an order of
