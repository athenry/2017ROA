Author = {Baril, Mathieu and Laliberte, Thierry and Gosselin, Clement and
   Routhier, Francois},
Book-Group-Author = {{ASME}},
Title = {{ON THE DESIGN OF MECHANICALLY PROGRAMMABLE UNDERACTUATED ANTHROPOMORPHIC
   ROBOTIC AND PROSTHETIC GRIPPERS}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
