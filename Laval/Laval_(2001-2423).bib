@article{ ISI:000309913000043,
Author = {Dastmalchi, Mansour and Abtahi, Mohammad and Lemus, David and Rusch,
   Leslie A. and LaRochelle, Sophie},
Title = {{Reconfigurable ultra-wideband waveform generation with simple photonic
   devices}},
Journal = {{OPTICAL ENGINEERING}},
Year = {{2012}},
Volume = {{51}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{We propose and experimentally demonstrate a low cost, low power
   consumption technique for ultra-wideband pulse shaping. Our approach is
   based on thermal apodization of two identical linearly chirped fiber
   Bragg gratings (LCFBG) placed in both arms of a balanced photodetector.
   Resistive heating elements with low electrical power consumption are
   used to tune the LCFBG spectral responses. Using a standard gain
   switched distributed feedback laser as a pulsed optical source and a
   simple energy detector receiver, we measured a bit error rate of 1.5 x
   10(-4) at a data rate of 1 Gb/s after RF transmission over a 1-m link.
   (C) 2012 Society of Photo-Optical Instrumentation Engineers (SPIE).
