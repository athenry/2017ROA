@inproceedings{ ISI:000327979000007,
Author = {Depatie, J. and Bilodeau, J-P and Dore, G.},
Editor = {{Miura, S and Ishikawa, T and Yoshida, N and Hisari, Y and Abe, N}},
Title = {{Evaluation of the mechanical characteristics of recycled base layers
   produced by Full Depth Reclamation (FDR)}},
Booktitle = {{ADVANCES IN TRANSPORTATION GEOTECHNICS II}},
Year = {{2012}},
Pages = {{93-98}},
Note = {{2nd International Conference on Transportation Geotechnics (ICTG),
   JAPAN, SEP 10-12, 2012}},
Organization = {{Int Soc Soil Mech \& Geotechn Engn; Int Soc Soil Mech \& Geotechn Engn,
   Int Tech Comm Transportat Geotechn 202}},
Abstract = {{From the perspective of sustainable development, recycling road
   materials is a possible and suitable waste reduction solution. In fact,
   recycling the asphalt concrete layer within the pavement base by Full
   Depth Reclamation (FDR) reduces the use of non-renewable resources and
   the costs associated with waste management. However, there is still some
   reluctance to reuse these materials as a new road base, mainly due to
   poor knowledge or understanding of their mechanical behavior, especially
   regarding permanent deformation. Thus, to address this problem, the
   research project presented in this paper, done in a laboratory, focused
   on the effect of Recycled Asphalt Pavement (RAP) content on the
   mechanical behavior of a recycled material (0, 30, 50, 60 and 70\% RAP).
   Three main parameters were analyzed: the susceptibility to the
   development of permanent deformation, the resilient modulus and the
   effect of moisture on the resilient modulus. A significant effect of the
   content of RAP on the rate of permanent deformation was observed for
   values higher than 50\% RAP, while this effect on the resilient modulus
   is more limited. It was observed that the mechanical behavior of
   recycled material is less sensitive to variations in the degree of
   saturation than are virgin materials.}},
