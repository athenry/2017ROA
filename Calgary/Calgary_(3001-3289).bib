@article{ ISI:000304524100004,
Author = {Deng, Haisong and Ma, Yizhong and Shao, Wenze and Tu, Yiliu},
Title = {{A Bayesian Meta-Modeling Approach for Gaussian Stochastic Process Models
   Using a Non Informative Prior}},
Journal = {{COMMUNICATIONS IN STATISTICS-THEORY AND METHODS}},
Year = {{2012}},
Volume = {{41}},
Number = {{5}},
Pages = {{829-850}},
Abstract = {{In this article, an efficient Bayesian meta-modeling approach is
   proposed for Gaussian stochastic process models in computer experiments.
   Different prior densities and particularly, a non informative hyper
   prior have been employed on the parameters involved in the correlation
   matrix. And the estimation of related parameters is obtained by the
   expectation-maximization algorithm. Compared with the recent work of Li
   and Sudjianto (2005), the proposed approach is not only of higher
   prediction accuracy but also of lower computational cost, due to the
   utilization of the non informative prior and the absence of tuning
   parameters. Experimental results demonstrate that our approach yields
   state-of-the-art performance.}},
