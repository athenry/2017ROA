@article{ ISI:000298943200007,
Author = {Daneshvar, Somayeh and Salak, Feridoun and Ishii, Takasada and Otsuka,
   Koji},
Title = {{Application of Subcritical Water for Conversion of Macroalgae to
   Value-Added Materials}},
