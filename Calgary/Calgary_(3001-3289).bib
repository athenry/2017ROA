@inproceedings{ ISI:000317865600047,
Author = {He, Zhe and Petovello, Mark and Lachapelle, Gerard},
Book-Group-Author = {{ION}},
Title = {{Modeling HSGPS Doppler Errors in Indoor Environments for Pedestrian
   Dead-Reckoning}},
Booktitle = {{PROCEEDINGS OF THE 2012 INTERNATIONAL TECHNICAL MEETING OF THE INSTITUTE
   OF NAVIGATION}},
Series = {{Proceedings of the International Technical Meeting of the Institute of
   Navigation}},
