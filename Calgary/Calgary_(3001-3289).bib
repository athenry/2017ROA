@inproceedings{ ISI:000320922700017,
Author = {Nassar, Nashaat N. and Husein, Maen M.},
Editor = {{Paul, BK and Moulik, SP}},
Title = {{Maximizing the Uptake of Nickel Oxide Nanoparticles by AOT (W/O)
   Microemulsions}},
Booktitle = {{RECENT TRENDS IN SURFACE AND COLLOID SCIENCE}},
