@inproceedings{ ISI:000327163300049,
Author = {Sadrieh, Seyed Nima and Broumandan, Ali and Lachapelle, Gerard},
Book-Group-Author = {{ION}},
Title = {{Investigating Indoor GPS Doppler and Pseudorange Characteristics}},
Booktitle = {{PROCEEDINGS OF THE 25TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
