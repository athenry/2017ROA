@inproceedings{ ISI:000327163301022,
Author = {Daneshmand, Saeed and Jafarnia-Jahromi, Ali and Broumandon, Ali and
   Lachapelle, Gerard},
Book-Group-Author = {{ION}},
Title = {{A Low-Complexity GPS Anti-Spoofing Method Using a Multi-Antenna Array}},
Booktitle = {{PROCEEDINGS OF THE 25TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
   DIVISION OF THE INSTITUTE OF NAVIGATION (ION GNSS 2012)}},
Series = {{Institute of Navigation Satellite Division Proceedings of the
   International Technical Meeting}},
Year = {{2012}},
Pages = {{1233-1243}},
Note = {{25th International Technical Meeting of the Satellite-Division of the
   Institute-of-Navigation, Nashville, TN, SEP 17-21, 2012}},
Organization = {{Inst Nav, Satellite Div}},
Abstract = {{Spoofing attacks can completely mislead the position solution provided
   by global navigation satellite systems (GNSS). Recently, several
   anti-spoofing techniques have been introduced to combat spoofing
   attacks. However, in most cases the available anti-spoofing techniques
   are computationally complex or limited to a specific spoofing scenario.
   This paper provides a low computational complexity approach to mitigate
   the spoofing signals based on an antenna array processing technique. The
   proposed method is established based on the fact that all spoofing
   pseudo random noise (PRN) codes are transmitted from the same source in
   space. This technique steers a null toward a spatial sector where the
   maximum spatial energy is coming from. To avoid unintentional
   attenuation of authentic signals due to the beam pattern profile, the
   received power of each authentic signal is maximized individually after
   spoofing mitigation. The proposed anti-spoofing technique provides a low
   computational complexity process that performs prior to the acquisition
   stage of GPS receivers. Moreover, this method does not require any
   antenna array calibration process. Therefore, it can be easily employed
   as a stand-alone anti-spoofing module at the receiver input. Simulation
   results show that the proposed anti-spoofing method performs well for a
   wide range of received spoofing power. To verify the applicability of
   this method in real-world scenarios, a real data set has been collected
   and processed without any outdoor propagation. The data processing
   results demonstrate the desired performance of the proposed technique
   for real spoofing scenarios.}},
