@inproceedings{ ISI:000310372500109,
Author = {Taghavi, M. H. and Belostotski, L. and Haslett, J. W.},
Book-Group-Author = {{IEEE}},
Title = {{A Bandwidth Enhancement Technique for CMOS TIAs Driven by Large
   Photodiodes}},
Booktitle = {{2012 IEEE 10TH INTERNATIONAL NEW CIRCUITS AND SYSTEMS CONFERENCE
   (NEWCAS)}},
Series = {{IEEE International New Circuits and Systems Conference}},
Year = {{2012}},
Pages = {{433-436}},
Note = {{10th IEEE International New Circuits and Systems Conference (NEWCAS),
   Montreal, CANADA, JUN 17-20, 2012}},
Organization = {{IEEE; IEEE Circuits \& Syst Soc; CMC Microsyst; UQAM, Dept Informatique;
   Polytechnique Montreal; Regroupement Strategique Microsysteme Quebec}},
Abstract = {{A new method of enhancing the bandwidth of a conventional series-peaked
   transimpedance amplifier (TIA) driven by a large photodiode is
   introduced. It is shown that by using N identical TIAs in parallel the
   circuit bandwidth can be significantly improved. The advantage of the
   proposed technique is in its ability to provide large bandwidth
   enhancements in multi-GHz frequency ranges even when photodiode
   capacitance is large, without a noticeable increase in TIA core circuit
   area. This technique is supported by a design example simulated in a
   0.13 mu m standard CMOS technology. Simulation results show a 3dB
   bandwidth of 26GHz with 0.5pF photodiode capacitance, a transimpedance
   gain of 51dB Omega and group delay of 33.5 +/- 4ps. The proposed
   technique shows an overall bandwidth enhancement ratio of 3.25 with less
   than 0.1dB gain ripple resulting in higher bandwidth enhancement than
   previously reported for large photodiode capacitances.}},
