Language = {{English}},
Affiliation = {{Kwak, E (Reprint Author), Univ Calgary, Dept Geomat Engn, Calgary, AB, Canada.
   Kwak, Eunju; Habib, Ayman, Univ Calgary, Dept Geomat Engn, Calgary, AB, Canada.}},
ISBN = {{978-1-4673-1274-5}},
Keywords = {{DBM; photogrammetry; LiDAR; qualtiy analysis}},
