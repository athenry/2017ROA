@article{ ISI:000327840700002,
Author = {Shahamiri, Seyed Ali and Wierzba, Ida},
Title = {{CATALYTIC OXIDATION OF LEAN CH4-CO2 -AIR MIXTURES}},
Journal = {{INTERNATIONAL JOURNAL OF GREEN ENERGY}},
Year = {{2014}},
Volume = {{11}},
Number = {{1}},
Pages = {{12-27}},
Month = {{JAN 2}},
Abstract = {{The presence of significant concentrations of CO2 with methane,
   especially for very lean mixtures in air affects adversely the
   unanalyzed combustion processes of such fuels in common conventional
   devices. The catalytic oxidation of lean homogeneous CH4-CO2-air
   mixtures on a non-adiabatic lab-scale Pt/Al2O3 packed bed reactor was
   investigated numerically. The model employed a two-phase treatment with
   volume averaging for the packed bed while accounting for the effects of
   mass and heat transfers between the solid and gas phases. All three
   modes of heat transfer as well as detailed surface and gas chemical
   kinetics were incorporated. The model was applied to simulate fuel
   oxidation at atmospheric pressure within the reactor that was fed with
   preheated homogeneous lean fuel-air mixtures. The effects of changes in
   the value of inlet feed temperature (600 T-inlet 900 K), equivalence
   ratio (0.15--0.55), and fuel composition (0\%--90\% CO2) were
   investigated. Based on the simulation results, the methane oxidation
   rate was significantly reduced only when the concentration of CO2 in the
   fuel mixture exceeded approximate to 70\%. At very low values of feed
   equivalence ratio or relatively low feed temperatures, the energy
   released by the reaction can become less than that needed to provide the
   energy for preheating the feed. The corresponding values for these
   limiting equivalence ratios were identified for different CO2
   concentrations in the fuel mixture and feed inlet temperatures. For the
   conditions considered the gas phase reactions were predicted to be
   relatively small in comparison to those for the solid phase.}},
