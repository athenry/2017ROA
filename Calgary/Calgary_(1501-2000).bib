@article{ ISI:000333827900014,
Author = {Peng, X. Y. and Cheng, Y. F.},
Title = {{A comparison of hydrogen permeation and the resulting corrosion
   enhancement of X65 and X80 pipeline steels}},
Journal = {{CANADIAN METALLURGICAL QUARTERLY}},
Year = {{2014}},
Volume = {{53}},
Number = {{1}},
Pages = {{107-111}},
Month = {{JAN}},
Abstract = {{In this work, corrosion and electrochemical hydrogen permeation
   behaviour of two grades of pipeline steel, X65 and X80 steels, were
   investigated by hydrogen-charging, electrochemical impedance
   spectroscopy measurements and metallographic observation. It was found
   that the corrosion and hydrogen permeation behaviour of steels is
   affected by their metallurgical features. Upon hydrogen-charging, both
   high grade of X80 pipeline steel and low grade of X65 steel show
   enhanced corrosion activity. The electrochemical hydrogen permeation
   current measurements and calculations show that the X80 steel contains a
   higher density of hydrogen traps than X65 steel, which may potentially
   result in the increased susceptibility of X80 steel to hydrogen-induced
   cracking (HIC).}},
