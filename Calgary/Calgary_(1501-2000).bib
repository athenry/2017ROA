@inproceedings{ ISI:000356331203016,
Author = {Krasovski, Sergey and Petovello, Mark and Lachapelle, Gerard},
Book-Group-Author = {{Inst Navigat}},
Title = {{Ultra-tight GPS/INS Receiver Performance in the Presence of Jamming
   Signals}},
Booktitle = {{PROCEEDINGS OF THE 27TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
   DIVISION OF THE INSTITUTE OF NAVIGATION (ION GNSS 2014)}},
