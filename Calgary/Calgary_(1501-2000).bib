Title = {{Characterizing Coherent Wind Structures using Large-Scale Particle
   Tracking Velocimetry: A Proof-of-Principle Study}},
Booktitle = {{SCIENCE OF MAKING TORQUE FROM WIND 2012}},
Series = {{Journal of Physics Conference Series}},
