@article{ ISI:000330018200072,
Author = {Alkhaldi, Salman and Husein, Maen M.},
Title = {{Hydrocracking of Heavy Oil by Means of In Situ Prepared Ultradispersed
   Nickel Nanocatalyst}},
Journal = {{ENERGY \& FUELS}},
Year = {{2014}},
Volume = {{28}},
