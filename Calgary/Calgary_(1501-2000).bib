@article{ ISI:000332812300028,
Author = {El Sawy, Ehab N. and Molero, Hebert M. and Birss, Viola I.},
Title = {{Methanol Oxidation at Porous Co-Electrodeposited Pt-Ir Thin Films}},
Journal = {{ELECTROCHIMICA ACTA}},
Year = {{2014}},
Volume = {{117}},
Pages = {{202-210}},
Month = {{JAN 20}},
Abstract = {{Pt-Ir films, formed by the mixing of Pt and Ir sols together, were shown
   earlier to be catalytic towards the methanol oxidation reaction (MOR),
   although neither the composition nor the microstructure of these films
   were reproducible. In the present work, thin and porous Pt-Jr films,
   with controlled compositions, were electrodeposited on Au at a charge
   efficiency of close to 100\% (as shown by in situ mass measurements).
   X-ray photoelectron spectroscopy demonstrated that the Pt-Ir film
   composition is homogeneous in all dimensions and closely matches that of
