@article{ ISI:000332053500010,
Author = {Zerpa, Sergio and Satyro, Marco and Clarke, Matthew A.},
Title = {{A short note on the use of an equation of state (EOS) based approach to
   modelling the thermodynamics of biodiesel systems}},
Journal = {{FUEL PROCESSING TECHNOLOGY}},
Year = {{2014}},
Volume = {{121}},
Pages = {{70-75}},
Month = {{MAY}},
Abstract = {{An initial attempt to adapt the Peng-Robinson EOS, with Huron-Vidal
   mixing rules, for the prediction of phase equilibrium in the production
   of biodiesel is described in this work. Six vegetable oils and methanol
   were chosen to constitute the feedstock. In order to describe the
   chemical composition of the vegetable oils, a fuel surrogate technique
   was employed. In regressing the binary interaction parameters for the
   equation of state, it was found that the necessary binary phase
   equilibrium data were seldom available in the literature. Thus, the EOS
   was formulated as a predictive model in which the KT-UNIFAC method was
   used to assist in generating binary interaction parameters. The EOS was
   subsequently tested by computing phase equilibrium and then comparing
   the results to the small amount of available experimental data. It was
   found that the predictions ranged from less than satisfactory to very
   good. (C) 2014 Elsevier B.V. All rights reserved.}},
