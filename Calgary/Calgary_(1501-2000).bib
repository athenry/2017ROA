@inproceedings{ ISI:000356142700047,
Author = {Amannejad, Yasaman and Garousi, Vahid and Irving, Rob and Sahaf, Zahra},
Book-Group-Author = {{IEEE}},
Title = {{A Search-based Approach for Cost-Effective Software Test Automation
   Decision Support and an Industrial Case Study}},
Booktitle = {{2014 SEVENTH IEEE INTERNATIONAL CONFERENCE ON SOFTWARE TESTING,
   VERIFICATION AND VALIDATION WORKSHOPS (ICSTW 2014)}},
