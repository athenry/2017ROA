Author = {Al-Hamad, A. and Moussa, A. and El-Sheimy, N.},
Editor = {{Toth, C and Holm, T and Jutzi, B}},
Title = {{VIDEO BASED MOBILE MAPPING SYSTEM USING SMARTPHONES}},
