@article{ ISI:000323591200010,
Author = {Rosi, G. A. and Martinuzzi, R. J. and Rival, D. E.},
Title = {{A conditional analysis of spanwise vortices within the lower atmospheric
   log layer}},
Journal = {{JOURNAL OF WIND ENGINEERING AND INDUSTRIAL AERODYNAMICS}},
Year = {{2013}},
Volume = {{119}},
Pages = {{89-101}},
Month = {{AUG}},
Abstract = {{The current study is an initial step towards the full characterization
   of coherent structures within the turbulent atmospheric boundary layer
   (ABL). A conditional-analysis technique to identify spanwise vortices is
   applied on data acquired throughout January-August 2012. This study
   begins with an approximation of the ABL stability and thickness (delta).
   Subsequently the measured Reynolds stresses are compared to those
   measured by Klebanoff (1955) over a flat plate and similarity
   formulations developed by Marusic and Kunkel (2003). The ABL data is
   shown to be neutral with a thickness of approximately 1000 m. The
   statistical descriptors of the turbulent fluctuations are found to
   qualitatively agree with Klebanoff (1955) and Marusic and Kunkel (2003).
   From the conditional analysis, it is found that the height of spanwise
   vortices follows a Rayleigh-like distribution with a mean and standard
   deviation of 0.065 delta and 0.038 delta, respectively. The circulation
   distribution of spanwise vortices is found to be bimodal about Gamma/U-0
   delta=0 with a standard deviation of 0.036U(0)delta. The magnitude of
   the positive peak of the bimodal distribution tends to be 80-100\% that
   of the negative peak, which demonstrates that prograde vortices are more
   frequent than retrograde vortices. (C) 2013 Elsevier Ltd. All rights
   reserved.}},
