@inproceedings{ ISI:000370963700015,
Author = {Sultana, Madeena and Gavrilova, Marina and Yanushkevich, Svetlana},
Book-Group-Author = {{IEEE}},
Title = {{Multi-Resolution Fusion of DTCWT and DCT for Shift Invariant Face
   Recognition}},
Booktitle = {{2014 IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN AND CYBERNETICS (SMC)}},
Series = {{IEEE International Conference on Systems Man and Cybernetics Conference
   Proceedings}},
Year = {{2014}},
Pages = {{80-85}},
Note = {{IEEE International Conference on Systems, Man, and Cybernetics (SMC),
   San Diego, CA, OCT 05-08, 2014}},
Organization = {{IEEE}},
Abstract = {{A novel Multi-Resolution Fusion (MRF) of Dual-Tree Complex Wavelet
   Transform (DTCWT) and Discrete Cosine Transform (DCT) is introduced in
   this paper. Shift invariant multi-scale feature set is obtained using 2D
   DTCWT. Subsequently, discriminant DCT coefficients are extracted to map
   the high dimensional features into low dimensional subspace. The
   resulting feature vector contains non-redundant discriminative
   information and is small in size. Therefore, the proposed face
   recognition technique exhibits computational efficiency, low storage
   requirement along with high recognition rate under varying shift
   conditions. It also provides robustness to expression and illumination
