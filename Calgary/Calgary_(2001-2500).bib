@inproceedings{ ISI:000343674300046,
Author = {Kondapalli, Soumya and Madanayake, Arjuna and Bruton, Len},
Book-Group-Author = {{IEEE}},
Title = {{Systolic Architectures for RF Antenna Apertures using Integral-form 2-D
   IIR Beam Filters}},
Booktitle = {{2013 IEEE PACIFIC RIM CONFERENCE ON COMMUNICATIONS, COMPUTERS AND SIGNAL
   PROCESSING (PACRIM)}},
