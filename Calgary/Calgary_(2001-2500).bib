@article{ ISI:000327952100057,
Author = {Younes, Mayada and Kwan, Andrew and Rawat, Meenakshi and Ghannouchi,
   Fadhel M.},
Title = {{Linearization of Concurrent Tri-Band Transmitters Using 3-D
   Phase-Aligned Pruned Volterra Model}},
Journal = {{IEEE TRANSACTIONS ON MICROWAVE THEORY AND TECHNIQUES}},
Year = {{2013}},
Volume = {{61}},
