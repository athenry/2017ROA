@article{ ISI:000313590600014,
Author = {Al-Saleh, Mohammed H. and Sundararaj, Uttandaraman},
Title = {{X-band EMI shielding mechanisms and shielding effectiveness of high
   structure carbon black/polypropylene composites}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{2013}},
Volume = {{46}},
Number = {{3}},
Month = {{JAN 23}},
Abstract = {{The electromagnetic interference (EMI) shielding effectiveness (SE) and
   EMI shielding mechanisms of high structure carbon black
   (HS-CB)/polypropylene (PP) composites in the X-band frequency range were
   studied. Composite plates with three different thicknesses and five
   different electrical conductivities were studied. The reflection loss
   and absorption loss of the composites were quantified based on the
   electromagnetic radiation power balance. The results showed that for
   HS-CB/PP composites, absorption loss contribution to the overall
   attenuation is more than the contribution of the reflection loss. The
   ability of the theoretical model to predict the EMI shielding by
   reflection and absorption was found to be a function of the shielding
   plate thickness and conductivity.}},
