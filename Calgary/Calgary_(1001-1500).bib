@inproceedings{ ISI:000365398300013,
Author = {Li, Yihe and Nie, Zhixi and Chen, Shaohua and Gao, Yang},
Book-Group-Author = {{Inst Navigat}},
Title = {{Multiple Constellation Navigation Performance Using Long-Term Ephemeris
   Extension with Backward Error Representation}},
Booktitle = {{PROCEEDINGS OF THE ION 2015 PACIFIC PNT MEETING}},
Series = {{Proceedings of the Institute of Navigation Pacific PNT Meeting}},
Year = {{2015}},
Pages = {{117-130}},
Note = {{Pacific PNT Meeting, Honolulu, HI, APR 20-23, 2015}},
Abstract = {{Long-term ephemeris extension is needed in challenging environments in
   order to reduce the Time-To-First-Fixed (TTFF). It makes quick position
   fix possible for mobile devices while they are still acquiring the
   broadcast ephemeris. Based on the experiment results described in this
   paper, the median User Range Error (URE) for the extended ephemeris
   after 3 and 7 days are 2.14m and 7.36m, respectively. Both of the
   multi-constellation extended ephemeris and its accuracy represented with
   backward errors are used for urban navigation. Simulated signal
   obstruction experiments are also conducted to assess the position fix
   accuracy, availability and reliability based on different mean square
   positional error (MSPE) thresholds, prediction durations and obstruction
   scenarios. The results have demonstrated superior performance for
   multiple constellation navigation system with backward error
   representation method compared to the GPS only system without backward
   error representation. With the aid of multiple constellation, the
   obtained average navigation availability and reliability improve from
   67.7\% to 93.4\% while the reliability also improve from 88.1\% to
   89.5\% using a user-defined MSPE of 15m after 3 days' prediction. Both
   of the average availability and reliability can further increase to
   97.1\% and 94.8\% after applying the revised strategy considering the
   ephemeris extension errors with backward representation method.}},
