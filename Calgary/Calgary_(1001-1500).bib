@article{ ISI:000352848600009,
Author = {Nazari, M. and Shokri, H. and Mohamad, A. A.},
Title = {{Lattice Boltzmann simulation of natural convection in open end cavity
   with inclined hot wall}},
Journal = {{APPLIED MATHEMATICS AND MECHANICS-ENGLISH EDITION}},
Year = {{2015}},
Volume = {{36}},
Number = {{4}},
Pages = {{523-540}},
Month = {{APR}},
Abstract = {{Natural convection in an open end cavity with a hot inclined wall is
   simulated based on the lattice Boltzmann method (LBM). The physics of
   flow and energy transfer in open end cavities are addressed when the hot
   wall is inclined. The combination of the two topics (open cavity and
   inclined walls) is the main novelty of the present study. The effects of
   the angle of the hot inclined wall on the flow field and heat transfer
   are thoroughly investigated. The Prandtl number is fixed to 0.71 (air).
   The Rayleigh number and the angle of the hot inclined wall are varied in
   the range of 10(4) to 10(6) and 60A degrees to 85A degrees,
   respectively. The results are presented for two different aspect ratios,
   i.e., A = 1 and 2. The results obtained with the LBM are also compared
   with those of the finite volume method (FVM). The predicted results of
   the LBM conform to those of the FVM. The results show that by increasing
   the angle of the hot inclined wall and the aspect ratio of the cavity,
   the average Nusselt number decreases. The trend of the local Nusselt
   number on the inclined wall is also discussed.}},
