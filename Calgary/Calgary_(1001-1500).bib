@article{ ISI:000355158200036,
Author = {Sanchez-Lemus, M. C. and Okafor, J. C. and Ortiz, D. P. and Schoeggl, F.
   F. and Taylor, S. D. and van den Berg, F. G. A. and Yarranton, H. W.},
Title = {{Improved Density Prediction for Mixtures of Native and Refined Heavy Oil
   with Solvents}},
