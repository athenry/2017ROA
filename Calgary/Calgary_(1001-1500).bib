@inproceedings{ ISI:000380397900203,
Author = {Frighetto-Pereira, L. and Menezes-Reis, R. and Metzner, G. A. and
   Rangayyan, R. M. and Nogueira-Barbosa, M. H. and Azevedo-Marques, P. M.},
