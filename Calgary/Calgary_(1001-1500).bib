@article{ ISI:000352232600009,
Author = {Zhuang, Y. and Syed, Z. and Georgy, J. and El-Sheimy, N.},
Title = {{Autonomous smartphone-based WiFi positioning system by using access
   points localization and crowdsourcing}},
Journal = {{PERVASIVE AND MOBILE COMPUTING}},
Year = {{2015}},
Volume = {{18}},
Pages = {{118-136}},
Month = {{APR}},
Abstract = {{The survey of WiFi access points (APs) locations and their propagation
   parameters (PPs) is a time and labour consuming process, which makes
   WiFi positioning impractical. In this paper, a novel crowdsourcing
   method is introduced. The proposed method is used for automatic AP
   localization and PPs estimation by employing an inertial navigation
   solution, such as Trusted Portable Navigator (T-PN). The proposed system
   runs on smartphones, from which it builds and updates the database
   autonomously and adaptively to account for the dynamic environment. A
   WiFi positioning method, based on the generated database, is also
   discussed. The proposed system is validated by both simulated and
   experimental tests. (C) 2015 Elsevier B.V. All rights reserved.}},
