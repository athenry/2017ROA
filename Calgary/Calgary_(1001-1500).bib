@inproceedings{ ISI:000368929600071,
Author = {Darav, Nima Karimpour and Kennings, Andrew and Westwick, David and
   Behjat, Laleh},
Book-Group-Author = {{IEEE}},
Title = {{High Performance Global Placement and Legalization Accounting for Fence
   Regions}},
