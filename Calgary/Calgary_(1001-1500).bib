@inproceedings{ ISI:000380482500010,
Author = {Adegboyega, Abiola},
Book-Group-Author = {{IEEE}},
Title = {{An Adaptive Reservation Scheme for Effective QoS Provisioning in the
   Cloud SDN Stack}},
