@inproceedings{ ISI:000354516300028,
Author = {Salari, Alinaghi and Dalton, Colin},
Editor = {{Gray, BL and Becker, H}},
Title = {{A NOVEL AC ELECTROTHERMAL MICROPUMP FOR BIOFLUID TRANSPORT USING
   CIRCULAR INTERDIGITATED MICROELECTRODE ARRAY}},
Booktitle = {{MICROFLUIDICS, BIOMEMS, AND MEDICAL MICROSYSTEMS XIII}},
