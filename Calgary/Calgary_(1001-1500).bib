@inproceedings{ ISI:000375211603068,
Author = {Xiang, Yan and Gao, Yang and Chen, Xingyu and Yang, Hongzhou},
Book-Group-Author = {{Inst Navigat}},
Title = {{TEC extraction with different schemes to model DCB under various
   ionosphere conditions}},
Booktitle = {{PROCEEDINGS OF THE 28TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
   DIVISION OF THE INSTITUTE OF NAVIGATION (ION GNSS+ 2015)}},
Series = {{Institute of Navigation Satellite Division Proceedings of the
   International Technical Meeting}},
Year = {{2015}},
Pages = {{3608-3614}},
Note = {{28th International Technical Meeting of The
   Satellite-Division-of-the-Institute-of-Navigation (ION GNSS+), Tampa,
   FL, SEP 14-18, 2015}},
Organization = {{Inst Navigat, Satellite Div}},
Abstract = {{Difference code biased (DCB) is one of the main errors when extracting
   total electron content (TEC) from the ionosphere observations. DCB is
   often assumed to be constant over time, e.g. over a period of two-hours
   or a day. Considering the variation of DCB during a day, the paper
   estimates and compare the ionosphere TEC estimates by modeling receiver
   DCB as a constant during a day and as a Gauss-Markov process under both
   quiet and disturbed ionospheric conditions. The results show that the
   DCB estimate can quickly converge to a relative stable value within five
   hours. Although the residuals under the disturbed ionosphere conditions
   are larger than under peace conditions, the DCB is little affected. The
   standard deviation of DCB estimates by modeling DCB as Gauss-Markov is
   smaller than that by modeling DCB as a constant.}},
