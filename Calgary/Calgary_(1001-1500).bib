@article{ ISI:000361559700076,
Author = {Lee, Chulhee and Kim, Chuntae and Jeong, Minseok and Kim, Jeonghyo and
   Lee, Jaewook and Oh, Jin-Woo and Lee, Jaebeom and Kim, Soo Hyung and
   Park, Simon S. and Kim, Jong-Man},
Title = {{Highly flexible and transparent metal grids made of metal nanowire
   networks}},
Journal = {{RSC ADVANCES}},
Year = {{2015}},
Volume = {{5}},
Number = {{94}},
Pages = {{77288-77295}},
Abstract = {{This work reports a new flexible transparent conductive electrode
   architecture based on solution-processed silver nanowire percolation
   metal grids (AgNW MGs). The AgNW MGs are stably fabricated by a
   well-established wet coating method and subsequent selective chemical
   etching of the AgNWs. The fabricated AgNW MGs exhibit good
   optoelectronic performance with sheet resistances of 9.9 Omega sq(-1)
   and 14.7 Omega sq(-1) at transmittances of 84.7\% and 88.7\%,
   respectively. The integration of percolated NW networks into the
   micro-grid structures also enables reproducible fabrication and
   controllable and reversible performance. The electrical robustness is
   greatly improved by incorporating a conductive polymer over-coating.}},
