@article{ ISI:000376148500007,
Author = {Arjmand, Mohammad and Ameli, Amir and Sundararaj, Uttandaraman},
Title = {{Employing Nitrogen Doping as Innovative Technique to Improve Broadband
   Dielectric Properties of Carbon Nanotube/Polymer Nanocomposites}},
Journal = {{MACROMOLECULAR MATERIALS AND ENGINEERING}},
Year = {{2016}},
Volume = {{301}},
Number = {{5}},
Pages = {{555-565}},
Month = {{MAY}},
Abstract = {{Undoped carbon nanotubes (CNTs) and N-CNTs are synthesized by chemical
   vapor deposition using Fe catalyst, and then melt-mixed in an APAM mixer
   with polyvinylidene fluoride (PVDF) to prepare the nanocomposites. The
   morphology, crystallinity, aspect ratio, nitrogen content, and nitrogen
   bonding type of CNTs, and the broadband dielectric properties of undoped
   CNT/PVDF and N-CNT/PVDF nanocomposites are analyzed. The results show
   that while undoped CNTs present a crystalline structure with open
   channels, doping with nitrogen results in CNTs with a bamboo-like
   configuration, inferior crystallinity, smaller length, and larger
   diameter. The N-CNT/PVDF nanocomposites, thus, have a higher percolation
   threshold (approximate to 3.5 wt\%) compared to that of the undoped
   CNT/PVDF nanocomposites (approximate to 0.5 wt\%). Comparison of the
