@article{ ISI:000390668900052,
Author = {Dey, Indrakshi and Tsiftsis, Theodoros A. and Rowell, Corbett},
Title = {{Achievable Channel Cutoff Rate and Bandwidth Efficiency in Indoor
   Wireless Environments}},
Journal = {{IEEE TRANSACTIONS ON VEHICULAR TECHNOLOGY}},
Year = {{2016}},
Volume = {{65}},
Number = {{12}},
Pages = {{10074-10079}},
Month = {{DEC}},
Abstract = {{In this paper, analytical expressions for the channel capacity, channel
   cutoff rate, and achievable bandwidth efficiency associated with M-ary
   signaling and Joint Fading Two-path Shadowing (JFTS) fading/shadowing
   channel model that characterize mobility-constrained indoor propagation
   scenarios are derived. Expressions for the bandwidth efficiency of a
   JFTS fading channel involve infinite-series summation since the
   probability density function of the JFTS signal-to-noise ratio (SNR)
   contains infinite series. We derive lower and upper bounds on the errors
   resulting from truncating the infinite series in the bandwidth
   efficiency expression. The cutoff rate and bandwidth efficiency analysis
   can be used to demonstrate how much throughput can be practically
   achieved when M-ary signaling techniques are implemented over indoor
   composite fading/shadowing channel conditions. Simulation results show
   that in order to achieve 3 bits/symbol, the average SNR for a
   16-quadrature-amplitude-modulation (16-QAM) system requires almost 10 dB
   more for a JFTS channel model compared with a traditional Rayleigh
   channel model, thus reinforcing the necessity of using proper channel
   models for indoor communication systems.}},
