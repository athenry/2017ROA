@article{ ISI:000372841200007,
Author = {Eastwood, Shawn C. and Shmerko, V. P. and Yanushkevich, S. N. and
   Drahansky, M. and Gorodnichy, D. O.},
Title = {{Biometric-Enabled Authentication Machines: A Survey of Open-Set
   Real-World Applications}},
