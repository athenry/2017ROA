@article{ ISI:000386949300021,
Author = {Surrao, Denver C. and Boon, Kathryn and Borys, Breanna and Sinha,
   Sarthak and Kumar, Ranjan and Biernaskie, Jeff and Kallos, Michael S.},
Title = {{Large-scale expansion of human skin-derived precursor cells (hSKPs) in
   stirred suspension bioreactors}},
