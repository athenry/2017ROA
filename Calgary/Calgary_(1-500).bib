@article{ ISI:000383705700005,
Author = {Mostafavi, Ehsan and Pauls, Jennifer H. and Lim, C. Jim and Mahinpey,
   Nader},
Title = {{SIMULATION OF HIGH-TEMPERATURE STEAM-ONLY GASIFICATION OF WOODY BIOMASS
   WITH DRY-SORPTION CO2 CAPTURE}},
