@article{ ISI:000372167200014,
Author = {Li, Qiang and Cheng, Y. Frank},
Title = {{Nonuniform Corrosion of Steel Pipe with a Fluid Flow Containing Carbon
   Dioxide}},
Journal = {{MATERIALS PERFORMANCE}},
Year = {{2016}},
Volume = {{55}},
Number = {{3}},
Pages = {{58-62}},
Month = {{MAR}},
Abstract = {{This work presents results of a corrosion investigation of X65 (UNS
   K03014) steel pipe conveying carbon dioxide (CO2)-saturated,
   single-phase brine water in an in-house designed fow loop. Steel
   electrodes installed at the top and bottom of a testing unit, which is
   part of the fow pipe, exhibited different corrosion activities. The
   electrode at the bottom of the pipe experienced greater corrosion than
   the one at the top of the pipe. Thus, corrosion of the fow pipe is
   nonuniform at individual cross sections. Fluid dynamics do not
   contribute to the nonuniformity of corrosion; instead, the greater
   hydrostatic pressure at the bottom of the pipe appears to play a key
   role.}},
