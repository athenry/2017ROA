@article{ ISI:000382410100095,
Author = {Barani, Alireza and Paktinat, Hossein and Janmaleki, Mohsen and
   Mohammadi, Aminollah and Mosaddegh, Peiman and Fadaei-Tehrani, Alireza
   and Sanati-Nezhad, Amir},
Title = {{Microfluidic integrated acoustic waving for manipulation of cells and
   molecules}},
Journal = {{BIOSENSORS \& BIOELECTRONICS}},
Year = {{2016}},
Volume = {{85}},
Pages = {{714-725}},
Month = {{NOV 15}},
Abstract = {{Acoustophoresis with its simple and low-cost fabrication, rapid and
   localized fluid actuation, compatibility with microfluidic components,
   and biocompatibility for cellular studies, has been extensively
   integrated into microfluidics to provide on-chip microdevices for a
   variety of applications in biology, bioengineering and chemistry. Among
   different applications, noninvasive manipulation of cells and
   biomolecules are significantly important, which are addressed by
   acoustic-based microfluidics. Here in this paper, we briefly explain the
   principles and different configurations of acoustic wave and acoustic
   streaming for the manipulation of cells washing molecules and overview
   its applications for single cell isolation, cell focusing and sorting,
   cell washing and patterning, cell-cell fusion and communication, and
   tissue engineering. We further discuss the application of acoustic based
   microfluidic systems for the mixing and transport of liquids,
   manipulation of deoxyribonucleic acid (DNA) and ribonucleic acid (RNA)
   molecules, followed by explanation on the present challenges of
   acoustic-based microfluidics for the handling of cells and molecules,
   and highlighting the future directions. Crown Copyright (C) 2016
