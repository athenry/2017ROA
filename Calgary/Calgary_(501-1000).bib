Author = {Frighetto-Pereira, L. and Metzner, G. A. and Azevedo-Marques, P. M. and
   Nogueira-Barbosa, M. H. and Oloumi, F. and Rangayyan, R. M.},
Book-Group-Author = {{IEEE}},
Title = {{Recognition of vertebral compression fractures in magnetic resonance
   images using statistics of height and width}},
Booktitle = {{2016 IEEE INTERNATIONAL SYMPOSIUM ON MEDICAL MEASUREMENTS AND
   APPLICATIONS (MEMEA)}},
Series = {{IEEE International Symposium on Medical Measurements and Applications
   Proceedings-MeMeA}},
