@article{ ISI:000354571400014,
Author = {Nassar, Nashaat N. and Franco, Camilo A. and Montoya, Tatiana and
   Cortes, Farid B. and Hassan, Azfar},
Title = {{Effect of oxide support on Ni-Pd bimetallic nanocatalysts for steam
   gasification of n-C-7 asphaltenes}},
Journal = {{FUEL}},
Year = {{2015}},
Volume = {{156}},
Pages = {{110-120}},
Month = {{SEP 15}},
Abstract = {{In a previous study, we showed that Ni-Pd bimetallic nanocatalysts
   supported on fumed silica had better adsorption affinity and catalytic
   activity toward adsorption and post-adsorption decomposition of
   asphaltenes compared to the support without functionalization. In this
   study, the effect of oxide support types on the adsorption and
   subsequent steam gasification of adsorbed asphaltenes was studied over
   Ni-Pd bimetallic nanocatalysts supported on TiO2 or gamma-Al2O3
   nanoparticles. X-ray diffraction (XRD), field emission scanning electron
   microscopy (FESEM) and N-2 physisorption at 77 K were used to
   characterize the crystal size and surface area of NiO and/or
   PdO-supported-on-TiO2 or gamma-Al2O3 nanoparticles, respectively. The
   type of support was shown to affect the adsorption affinity and
   catalytic activity of the particles; Ni-Pd/TiO2 nanoparticles showed
   higher adsorption capacity and catalytic activity than
   Ni-Pd/gamma-Al2O3, presumably due to different metal-support interaction
   and geometric and electronic effects on the surface. Bimetallic
   nanoparticles appeared to have better adsorption and catalytic behavior
   than monometallic nanoparticles regardless of the oxide support used,
   confirming their synergistic effect. (C) 2015 Elsevier Ltd. All rights
   reserved.}},
