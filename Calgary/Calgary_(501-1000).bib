@inproceedings{ ISI:000388480401019,
Author = {Mollasalehi, Ehsan and Wood, David and Sun, Qiao},
Editor = {{Vogiatzis, K and Kouroussis, G and Crocker, M and Pawelczyk, M}},
Title = {{BEARING FAULT DIAGNOSIS USING EMPIRICAL MODE DECOMPOSITION BASED ORDER
   TRACKING - WIND TURBINE GENERATOR APPLICATION}},
Booktitle = {{PROCEEDINGS OF THE 23RD INTERNATIONAL CONGRESS ON SOUND AND VIBRATION:
   FROM ANCIENT TO MODERN ACOUSTICS}},
Series = {{Proceedings of the International Congress on Sound and Vibration}},
Year = {{2016}},
Note = {{23rd International Congress on Sound and Vibration (ICSV), Athens,
   GREECE, JUL 10-14, 2016}},
Organization = {{Hellenic Assoc Toll Road Network; Dynam Acoust S A; Traffic Transportat
   \& Environm Consultants S A; ACCON GmbH; T \& S Technologies Grp; OROS;
   PEMARD; Vibro Acoust; AKTOR; APION KLEOS CJV; TERNA}},
Abstract = {{In many applications of roller bearing fault diagnosis, such as in wind
   turbine, shaft rotational speed varies with respect to time, and
   consequently the normal frequency domain analysis, which is valid for
   constant speed condition, cannot be used in principal. Alternatively,
   order tracking in conjunction with phase demodulation has been used to
   estimate the shaft speed and resample the time domain signal into
   angular domain. Speed estimation using phase demodulation has been
   mainly used for gearbox application where a reference signal or a shaft
   harmonic is well defined. In this paper, an Empirical Mode Decomposition
   based approach for shaft speed estimation is examined. The estimated
   instantaneous shaft phase is then used for order tracking to capture the
   bearing fault diagnosis, under variable speed profile. The approach is
   applied on a simulated bearing signal having an outer race fault, and
   then on operational low signal noise ratio vibration data of 1.5 MW wind
   turbine. The most suitable decomposed mode is selected to eliminate the
   effect of unwanted components from other drive-train rotating elements
   as well as noise from the acquired vibration signal.}},
