@article{ ISI:000382513200007,
Author = {Mohamad, A. A. and Tarokh, A. and Alansary, H.},
Title = {{Heat transfer enhancement of laminar forced convection in a channel by
   von-Karman vortex generator}},
Journal = {{PROGRESS IN COMPUTATIONAL FLUID DYNAMICS}},
Year = {{2016}},
Volume = {{16}},
Number = {{5}},
Pages = {{334-338}},
Abstract = {{The work deals with a method of enhancing laminar, forced convection in
   channels. It is well-known that the inadequate flow mixing process in
   laminar flow is the limiting factor for heat transfer compared with the
   turbulent flow. In this work, vortex shedding (von-Karman vortex)
   technique is used to enhance the rate of heat transfer in a channel
   fitted with off-centre obstacles. Vortex formations at the downstream of
   the obstacles enhance the fluid mixing process; consequently, enhancing
   the rate of heat transfer. The distance between obstacles is selected
   based on fact that at certain the downstream distance of the obstacle,
   the strength of the vortex decays. Lattice Boltzmann method is used to
   solve the governing equations.}},
