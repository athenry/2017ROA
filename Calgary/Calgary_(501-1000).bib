@article{ ISI:000360067600013,
Author = {Gomaa, Hazem and Messier, Geoffrey G. and Davies, Robert},
Title = {{Hierarchical Cache Performance Analysis Under TTL-Based Consistency}},
Journal = {{IEEE-ACM TRANSACTIONS ON NETWORKING}},
Year = {{2015}},
Volume = {{23}},
Number = {{4}},
Pages = {{1190-1201}},
Month = {{AUG}},
Abstract = {{This paper introduces an analytical model for characterizing the
   instantaneous hit ratio and instantaneous average hit distance of a
   traditional least recently used (LRU) cache hierarchy. The analysis
   accounts for the use of two variants of the Time-to-Live (TTL) weak
   consistency mechanism. The first is the typical TTL scheme (TTL-T) used
   in the HTTP/1.1 protocol where expired objects are refreshed using
   conditional GET requests. The second is TTL immediate ejection (TTL-IE)
   where objects are ejected as soon as they expire. The analysis also
   accounts for two sharing protocols: Leave Copy Everywhere (LCE) and
   Promote Cached Objects (PCO). PCO is a new sharing protocol introduced
   in this paper that decreases the user's perceived latency and is robust
   under nonstationary access patterns.}},
