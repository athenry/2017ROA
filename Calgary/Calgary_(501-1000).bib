@article{ ISI:000365112500011,
Author = {Arisman, C. J. and Johansen, C. T.},
Title = {{Nitric Oxide Chemistry Effects in Hypersonic Boundary Layers}},
Journal = {{AIAA JOURNAL}},
Year = {{2015}},
Volume = {{53}},
