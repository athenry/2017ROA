@inproceedings{ ISI:000402634701010,
Author = {Trejo, Fernando and Hu, Yaoping},
Book-Group-Author = {{IEEE}},
Title = {{Towards an Analytic Haptic Model for Force Rendering of Soft-Tissue
   Dissection}},
Booktitle = {{2016 IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN, AND CYBERNETICS
   (SMC)}},
Series = {{IEEE International Conference on Systems Man and Cybernetics Conference
   Proceedings}},
Year = {{2016}},
Pages = {{1098-1103}},
Note = {{IEEE International Conference on Systems, Man, and Cybernetics (SMC),
   Budapest, HUNGARY, OCT 09-12, 2016}},
Organization = {{IEEE}},
Abstract = {{Both surgical simulation and robot-assisted surgery require haptic
   models of tool-tissue interaction for force rendering. Most efforts of
   haptic modeling have focused on characterizing tool-tissue interaction
   of soft-tissue indentation, insertion and cutting. Less attention has
   been devoted to soft-tissue dissection however. For the dissection,
   haptic models remain elusive to meet two requirements as: to represent
   nonlinearity of soft-tissue responses and to comply with the time
   constraint of 1 ms for force rendering. Hence, this paper presents a
   modeling framework towards developing an analytic haptic model for force
   rendering of the dissection. Based on estimation theories, the framework
   devises an analytic model to approximate an empirical force-distance
   profile of the dissection. Applying the framework to 2 different
   empirical profiles as use cases, the derived models estimated about 72
   \% and 91\% of the empirical data, respectively. Algorithm
   implementation of these models in Matlab yielded a computational time of
   about 24 Its, much less than 1 ms. The outcomes indicate a potential of
   using the framework to develop an analytic haptic model for force
   rendering of soft-tissue dissection.}},
