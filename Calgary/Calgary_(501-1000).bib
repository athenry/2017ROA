@article{ ISI:000364015400004,
Author = {Khosla, Ajit and Kassegne, Sam},
Title = {{Fabrication of NdFeB-based permanent rare-earth micromagnets by novel
   hybrid micromolding process}},
Journal = {{MICROSYSTEM TECHNOLOGIES-MICRO-AND NANOSYSTEMS-INFORMATION STORAGE AND
   PROCESSING SYSTEMS}},
Year = {{2015}},
Volume = {{21}},
Number = {{11}},
Pages = {{2315-2320}},
Month = {{NOV}},
Abstract = {{In this paper, we present preparation and magnetic characterization of
   an isotropic permanent magnetic Neodymium Iron Boron (NdFeB) based
   polymer composite, which can be micromolded on flexible PDMS substrate,
   via a novel hybrid soft lithographic technique. The NdFeB isotropic
   magnetic powder was homogeneously dispersed by solvent assisted
   ultrasonic and shear mixing in poly-dimethylsiloxne matrix (PDMS). The
   prepared composite was micromolded against an SU-8 master micromold. The
   resultant micromagnets had a diameter of 50 mu m and height 30 mu m.
   These magnets, which were tested using magnetometer showed a remanent
   magnetization (M-r) of 60.10 emu/g and Coercivity (H-c) of 5,260 G at 75
   weight percentage of isotropic magnetic powder in PDMS polymer matrix.
   Furthermore, dispersion analysis of isotropic NdFeB magnetic powder
   through SEM analysis showed homogenous dispersion of the magnetic
   particles-which had an average particle size of 2.5 mu m-in PDMS polymer
   matrix.}},
