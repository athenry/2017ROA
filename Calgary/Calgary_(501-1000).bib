@article{ ISI:000385929800011,
Author = {Dong, Xiaohu and Liu, Huiqing and Chen, Zhangxin},
Title = {{The effect of capillary condensation on the phase behavior of
   hydrocarbon mixtures in the organic nanopores}},
Journal = {{PETROLEUM SCIENCE AND TECHNOLOGY}},
Year = {{2016}},
Volume = {{34}},
Number = {{17-18}},
Pages = {{1581-1588}},
Abstract = {{In nanoscale pores, the adsorption of gas is a multilayer adsorption
   process, and the conventional Langmuir model is no longer valid. In
   particular for the unconventional gas condensate reservoir, the adsorbed
   gas will become condensate once the pressure is above the critical
   condensate pressure at pore scale. In this study, considering the effect
   of adsorption (wetting) film, the multicomponent Kelvin equation is
   modified to computing the isotherm of capillary condensation. Then it is
   coupled with the multispace adsorption model and Peng-Robinson equation
   of state to investigate and represent the phase behavior of hydrocarbons
   in organic nanopores. Then, a prediction process for the behavior of a
   four-component mixture of methane, n-butane, n-pentane, n-hexane are
   performed. The actual Marcellus shale gas is also used to examine the
   performance of this model.}},
