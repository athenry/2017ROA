@article{ ISI:000366781500016,
Author = {Maundy, B. J. and Elwakil, A. S. and Allagui, A.},
Title = {{Extracting the parameters of the single-dispersion Cole bioimpedance
   model using a magnitude-only method}},
Journal = {{COMPUTERS AND ELECTRONICS IN AGRICULTURE}},
Year = {{2015}},
Volume = {{119}},
Pages = {{153-157}},
Month = {{NOV}},
Abstract = {{Affordable and portable indirect impedance measurement techniques with
   minimum hardware and post-processing requirements are necessary for
   various applications, and in particular for bioimpedance measurements.
   In this paper, a new method to extract the single-dispersion Cole
   bioimpedance model parameters is presented using a simple circuit
   consisting of a operational amplifier and a set of resistors. We examine
   the theory behind this magnitude-only method and demonstrate its
   application for the extraction of impedance data from a number of
   different fruit samples. Three means to compute the single-dispersion
   Cole model parameters are given and discussed under different
   measurement scenarios, and compared with experimental results. (C) 2015
   Elsevier B.V. All rights reserved.}},
