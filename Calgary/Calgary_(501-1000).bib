@article{ ISI:000375206200034,
Author = {Dey, I. and Messier, G. G. and Magierowski, S.},
Title = {{On the Capacity of Joint Fading and Two-Path Shadowing Channels}},
Journal = {{IEEE TRANSACTIONS ON VEHICULAR TECHNOLOGY}},
Year = {{2016}},
Volume = {{65}},
Number = {{1}},
Pages = {{403+}},
Month = {{JAN}},
Abstract = {{The ergodic and outage channel capacity of different optimal and
   suboptimal combinations of transmit power and modulation rate adaptation
   strategies over a joint fading and two-path shadowing (JFTS)
   fading/shadowing channel is studied in this paper. Analytically
   tractable expressions for channel capacity are obtained, assuming
   perfect channel side information (CSI) at the receiver and/or the
   transmitter with negligible feedback delay. Furthermore, the impacts of
