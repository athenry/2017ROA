@article{ ISI:000365871500033,
Author = {Raad, Seyed Mostafa Jafari and Hassanzadeh, Hassan},
Title = {{Onset of dissolution-driven instabilities in fluids with nonmonotonic
   density profile}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2015}},
Volume = {{92}},
Number = {{5}},
Month = {{NOV 30}},
Abstract = {{Analog systems have recently been used in several experiments in the
   context of convective mixing of CO2. We generalize the nonmonotonic
   density dependence of the growth of instabilities and provide a scaling
   relation for the onset of instability. The results of linear stability
   analysis and direct numerical simulations show that these fluids do not
   resemble the dynamics of CO2-water convective instabilities. A typical
   analog system, such as water-propylene glycol, is found to be less
   unstable than CO2-water. These results provide a basis for further
   research and proper selection of analog systems and are essential to the
   interpretation of experiments.}},
