@article{ ISI:000368999100006,
Author = {Elwakil, A. S. and Maundy, B. J.},
Title = {{Calculating output impedance in linear networks without source nulling
   or load disconnect: the instantaneous output impedance}},
Journal = {{INTERNATIONAL JOURNAL OF CIRCUIT THEORY AND APPLICATIONS}},
Year = {{2016}},
Volume = {{44}},
Number = {{1}},
Pages = {{98-108}},
Month = {{JAN}},
Abstract = {{This paper derives exact expressions for the output impedance of a
   voltage-excited (current-excited) linear network without having to
   nullify (kill) the exciting source or disconnect the load. The derived
   expressions show that the instantaneous output impedance is both source
   and load dependent, but when the source is nullified, the output
   impedance becomes independent of the load, as expected. For some special
   networks, the output impedance is also independent of the load. The
   expressions are verified for a number of known circuit setups and
   experimentally on a gyrator-based linear network, which has an output
   impedance that can switch between positive and negative values.
