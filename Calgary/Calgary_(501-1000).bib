@article{ ISI:000361385900012,
Author = {Gildenhuys, Anne and Zandiyeh, Payam and Kuntze, Gregor and Goldsmith,
   Peter and Ronsky, Janet L.},
Title = {{Biomechanical Analysis of a Dynamic Stability Test System to Evoke Sway
   and Step Recovery}},
Journal = {{JOURNAL OF BIOMECHANICAL ENGINEERING-TRANSACTIONS OF THE ASME}},
Year = {{2015}},
Volume = {{137}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{This paper reports on the dynamic analysis and experimental validation
   of a method to perturb the balance of subjects in quiet standing.
   Electronically released weights pull the subject's waist through a
   specified displacement sensed by a photoelectric sensor. A dynamic model
   is derived that computes the force applied to the subject as a function
   of waist acceleration. This model accurately predicts the acceleration
   of mock subjects (suspended masses) with high repeatability. The
   validity and simplicity of this model suggest that this method can
   provide a standard for provocation testing on stable surfaces.
   Proof-of-concept trials on human subjects demonstrate that the device
   can be used with a force platform and motion tracking and that the
   device can induce both sway and step recoveries in healthy male adults.}},
