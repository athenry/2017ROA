@article{ ISI:000365308500006,
Author = {Wang, Yingxu},
Title = {{Fuzzy Probability Algebra (FPA): A Theory of Fuzzy Probability for Fuzzy
   Inference and Computational Intelligence}},
Journal = {{JOURNAL OF ADVANCED MATHEMATICS AND APPLICATIONS}},
Year = {{2015}},
Volume = {{4}},
Number = {{1}},
Pages = {{37-54}},
Month = {{JUN}},
Abstract = {{Probability is an extrapolative estimation and predication of uncertain
   outcomes of events based on certain norms in a predetermined probability
   space. The classic probability theory is cyclically defined among a set
   of highly coupled operations. A theory of fuzzy probability is
   introduced in order to extend the classic probability theory to deal
   with complex problems in the fuzzy probability space. The fuzzy
   probability theory is presented in a mathematical structure of fuzzy
   probability algebra (FPA). In FPA the mathematical model of fuzzy
   probability is created on the basis of fuzzy sets. Rigorous
   manipulations on fuzzy probabilities are formally described as a set of
   independent algebraic operators. Typical applications of FPA are
   illustrated for dealing with uncertain events and fuzzy causations in
   fuzzy inference, qualification, quantification, and semantic analysis.}},
