@article{ ISI:000362619900023,
Author = {Kuang, Da and Cheng, Y. Frank},
Title = {{Study of cathodic protection shielding under coating disbondment on
   pipelines}},
Journal = {{CORROSION SCIENCE}},
Year = {{2015}},
Volume = {{99}},
Pages = {{249-257}},
Month = {{OCT}},
Abstract = {{In this work, the cathodic protection (CP) shielding behavior of
   pipeline coatings, i.e., high density polyethylene (HDPE) and fusion
   bonded epoxy (FBE), was investigated. While the HDPE is a CP-shielding
   coating, the FBE is permeable to CP. The structure of HOPE does not
   change upon CP permeating test, and water molecules are not absorbed
   into the coating. Conversely, there are obvious changes of the
   functional groups in FBE. Water uptakes into the coating occur
   continuously with time. The CP permeation through FBE coating is time
   dependent. A more negative CP potential level enhances the CP
   permeation. (C) 2015 Elsevier Ltd. All rights reserved.}},
