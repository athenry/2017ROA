@inproceedings{ ISI:000375213003013,
Author = {Bhandari, V. and O'Keefe, K. and Lachapelle, G.},
Book-Group-Author = {{Inst Navigat}},
Title = {{Assessment of multi-constellation RTK solutions during differential
   correction data outages}},
Booktitle = {{PROCEEDINGS OF THE 26TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
   DIVISION OF THE INSTITUTE OF NAVIGATION (ION GNSS 2013)}},
