@inproceedings{ ISI:000327064500055,
Author = {Du, Shuang and Huang, Bei and Gao, Yang},
Book-Group-Author = {{ION}},
Title = {{Integration of Floor Plan, Vision and Inertial Sensors for Pedestrian
   Navigation in Indoor Environments}},
Booktitle = {{PROCEEDINGS OF THE ION 2013 PACIFIC PNT MEETING}},
Series = {{Proceedings of the Institute of Navigation Pacific PNT Meeting}},
Year = {{2013}},
Pages = {{526-531}},
Note = {{ION Pacific PNT Meeting, Honolulu, HI, APR 23-25, 2013}},
Organization = {{Inst Nav}},
Abstract = {{The foot mounted inertial measurement unit (IMU) has been used for
   inertial-based pedestrian navigation in recent literatures, where zero
   velocity updates (ZUPT) are applied each time the user takes a step. As
   only the velocity error is directly observed through ZUPT, the position
   error still accumulates with time, which limits the wide use of this
   method. According to previous research, the position error exceeds 10 m
   within about 10 minutes.
   In this research, a floor plan-based vision aiding method is proposed,
   from which the position update can be derived, to correct the position
   errors in the inertial-based pedestrian navigation. The proposed
   algorithm is designed to integrate two-dimensional camera image with the
   geodetic position and real scale of floor plan to derive
   three-dimensional camera position and orientation, hence to limit the
   accumulative error in inertial-based pedestrian navigation. The indoor
   field test results indicate that the pedestrian position solution is
   significantly improved with the vision aiding and the maximum error is
   reduced from over 15m to less than 4m in about 14 minutes.}},
