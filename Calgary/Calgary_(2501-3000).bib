@article{ ISI:000314021300019,
Author = {Collins, Michael J. and Denbina, Michael and Atteia, Ghada},
Title = {{On the Reconstruction of Quad-Pol SAR Data From Compact Polarimetry Data
   For Ocean Target Detection}},
Journal = {{IEEE TRANSACTIONS ON GEOSCIENCE AND REMOTE SENSING}},
Year = {{2013}},
Volume = {{51}},
Number = {{1, 2}},
Pages = {{591-600}},
Month = {{JAN}},
Abstract = {{Circular-transmit/linear-receive compact polarimetry synthetic aperture
   radar systems combine coherent dual polarization with wide-swath
   imaging. The polarization information in these data may be represented
   as a Stokes vector, or one can reconstruct several quadpolarized
   covariance elements. Two reconstruction algorithms have been published
   in the literature: one by Souyris et al. and a refined algorithm by Nord
   et al. We investigated the application of these two algorithms for
   reconstructing ocean clutter for the purpose of detecting targets. We
   tested the assumptions underlying these algorithms and found that they
   were not valid for ocean scenes. We present a simple empirical
   reconstruction model whose reconstruction and target detection
   performance is superior to the two published algorithms.}},
