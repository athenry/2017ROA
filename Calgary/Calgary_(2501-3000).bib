@inproceedings{ ISI:000346481203033,
Author = {Rao, Jaya and Fapojuwo, Abraham O.},
Book-Group-Author = {{IEEE}},
Title = {{Analysis of Load Dependent Energy Efficiency of Two-Tier heterogeneous
   Cellular Networks}},
Booktitle = {{2013 IEEE 24TH INTERNATIONAL SYMPOSIUM ON PERSONAL, INDOOR, AND MOBILE
   RADIO COMMUNICATIONS (PIMRC)}},
Series = {{IEEE International Symposium on Personal Indoor and Mobile Radio
   Communications Workshops-PIMRC}},
Year = {{2013}},
Pages = {{3095-3099}},
Note = {{IEEE 24th International Symposium on Personal, Indoor and Mobile Radio
   Communications (PIMRC), London, ENGLAND, SEP 08-11, 2013}},
Organization = {{IEEE}},
Abstract = {{In this paper, a tractable analytical framework for investigating the
   energy efficiency (EE) of a 2-tier heterogeneous cellular network
   consisting of macro and picocell base stations (BS) is developed. The
   BSs, which are randomly distributed, serve a number of users in the
   downlink under shared spectrum operation. By incorporating a tunable
   downlink power control mechanism, the successful transmission
   probability and the average throughput achievable in both tiers are
   analytically derived. Since varying load conditions has an important
   implication on the network performance, the EE is investigated as a
   function of the load level, considering the ability to transfer the load
   between the 2 tiers. An optimization problem that maximizes the
   throughput constrained 2-tier EE is formulated and solved to determine
   the optimal network operational parameters. In addition to providing
   valuable design insights, the findings reveal that, for different load
   conditions, it is feasible to significantly enhance the EE performance
   by optimally adapting the network configuration.}},
