@inproceedings{ ISI:000375213002086,
Author = {Dehghanian, Vahid and Nielsen, John},
Book-Group-Author = {{Inst Navigat}},
Title = {{GNSS Spoofing Detection based on a Sequence of RSS Measurements}},
Booktitle = {{PROCEEDINGS OF THE 26TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
   DIVISION OF THE INSTITUTE OF NAVIGATION (ION GNSS 2013)}},
Series = {{Institute of Navigation Satellite Division Proceedings of the
   International Technical Meeting}},
Year = {{2013}},
Pages = {{2931-2935}},
Note = {{26th International Technical Meeting of The
   Satellite-Division-of-the-Institute-of-Navigation (ION GNSS), Nashville,
   TN, SEP 16-20, 2013}},
Organization = {{Inst Navigat, Satellite Div}},
Abstract = {{The GNSS spoofer is a potential threat that can cause significant damage
   by transmitting counterfeit GNSS signals that result in an erroneous
   navigation solution. In this paper, an effective and yet inexpensive
   spoofer detection technique is developed based on measuring the received
   signal strength (RSS) and monitoring its variation arising from
   arbitrary motion of a GNSS receiver. Analytical and simulation results
   show this to be an effective counter to spoofing as well as being
   implementable in a handset based GNSS receiver with minimal firmware
   modifications.}},
