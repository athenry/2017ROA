Title = {{Generic Bandwidth Scalable Behavioral Models for RF Power Amplifiers
   with Memory Effects}},
Booktitle = {{2013 IEEE INTERNATIONAL RF AND MICROWAVE CONFERENCE (RFM)}},
Year = {{2013}},
