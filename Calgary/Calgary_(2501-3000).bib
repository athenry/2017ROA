@article{ ISI:000311510500038,
Author = {Nassar, Nashaat N. and Hassan, Azfar and Pereira-Almao, Pedro},
Title = {{Thermogravimetric studies on catalytic effect of metal oxide
   nanoparticles on asphaltene pyrolysis under inert conditions}},
Journal = {{JOURNAL OF THERMAL ANALYSIS AND CALORIMETRY}},
Year = {{2012}},
Volume = {{110}},
Number = {{3}},
Pages = {{1327-1332}},
Month = {{DEC}},
Abstract = {{This study investigated the catalytic effect of NiO, Co3O4 and Fe3O4
   nanoparticles toward asphaltene thermal decomposition (pyrolysis) under
   inert conditions. Asphaltene adsorbed onto the selected nanoparticles
   were subjected to thermal decomposition up to 800 A degrees C in a
   thermogravimetric analyzer. The presence of nanoparticles caused a
   significant decrease in the asphaltene decomposition temperature and
   activation energy. Activation energies for the process were calculated
   using the Ozawa-Flynn-Wall method. All the selected metal oxide
   nanoparticles showed high catalytic activity toward asphaltene
   decomposition in the following order NiO > Co3O4 > Fe3O4. This study
   confirms that metal oxide nanoparticles can significantly enhance the
   thermal decomposition of heavy hydrocarbons, like asphaltenes.}},
