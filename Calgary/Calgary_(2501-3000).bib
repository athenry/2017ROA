Author = {Harthong, Barthelemy and Wan, Richard G.},
Editor = {{Yu, A and Dong, K and Yang, R and Luding, S}},
Title = {{Directional Plastic Flow and Fabric Dependencies in Granular Materials}},
Booktitle = {{POWDERS AND GRAINS 2013}},
Series = {{AIP Conference Proceedings}},
Year = {{2013}},
Volume = {{1542}},
Pages = {{193-196}},
