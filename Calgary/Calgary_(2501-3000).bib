@article{ ISI:000302981100008,
Author = {Kim, C. I. and Ru, C-Q. and Sudak, L. J. and Schiavone, P.},
Title = {{Analysis of local singular fields near the corner of a quarter-plane
   with mixed boundary conditions in finite plane elastostatics}},
Journal = {{INTERNATIONAL JOURNAL OF NON-LINEAR MECHANICS}},
Year = {{2012}},
Volume = {{47}},
Number = {{2, SI}},
Pages = {{151-155}},
Month = {{MAR}},
Abstract = {{We consider a quarter-plane of compressible hyperelastic material of
   harmonic-type undergoing finite plane deformations. The plane is
   subjected to mixed (free-fixed) boundary conditions. In contrast to the
   analogous case from classical linear elasticity, we find that the
   deformation field is smooth in the vicinity of the vertex and is
   actually bounded at the vertex itself. In particular, the normal
   displacement remains positive eliminating the possibility of material
   interpenetration. Finally, explicit expressions for Cauchy and Piola
   stress distributions are obtained in the vicinity of the vertex. (C)
   2011 Elsevier Ltd. All rights reserved.}},
