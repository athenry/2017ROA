@article{ ISI:000310389000060,
Author = {MacCormack, J. and Zareipour, H. and Rosehart, W. D.},
Title = {{Long-Term Market Equilibrium Model With Strategic, Competitive, and
   Inflexible Generation}},
Journal = {{IEEE TRANSACTIONS ON POWER SYSTEMS}},
Year = {{2012}},
Volume = {{27}},
Number = {{4}},
Pages = {{2291-2292}},
Month = {{NOV}},
Abstract = {{A general model of long-term equilibrium in energy only electricity
