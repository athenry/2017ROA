@inproceedings{ ISI:000375213000063,
Author = {Zhuang, Y. and Syed, Z. and Goodall, C. and Iqbal, U. and El-Sheimy, N.},
Book-Group-Author = {{Inst Navigat}},
Title = {{Automated Estimation and Mitigation of Wireless Time-Delays in
   Smartphones for a Robust Integrated Navigation Solution}},
Booktitle = {{PROCEEDINGS OF THE 26TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
   DIVISION OF THE INSTITUTE OF NAVIGATION (ION GNSS 2013)}},
Series = {{Institute of Navigation Satellite Division Proceedings of the
   International Technical Meeting}},
Year = {{2013}},
Pages = {{667-676}},
Note = {{26th International Technical Meeting of The
   Satellite-Division-of-the-Institute-of-Navigation (ION GNSS), Nashville,
   TN, SEP 16-20, 2013}},
Organization = {{Inst Navigat, Satellite Div}},
Abstract = {{Smartphones not only provide communication and entertainment to the
   users but they are important sources to acquire positioning information.
   A coarse positioning solution can be obtained using the cell phone
   towers `seen' by the phone when the phone is turned on and sometimes
   this coarse navigation solution can be the only solution. In addition,
   smartphones come equipped with Assisted Global Positioning System (AGPS)
   chipset to provide absolute and accurate positioning solution. AGPS
   positioning requires locking to at least four satellites which is
   impossible in deep indoor environments despite specialized techniques to
   acquire weak satellite signals. Smartphones also have capabilities to
   receive WiFi signals and the positions based on these signals. These
   positions are provided for public places by different service providers
   and the accuracy of such positions can be better than 50m in deep indoor
   environments. Also available on the smartphones are accelerometers,
   gyros, magnetometers and barometer sensors. Trusted Positioning uses
   these sensors to provide a complete navigation solution. This solution
   is capable of taking any absolute positioning updates to ensure the most
   accurate navigation for the user. When the absolute updates from cell
   tower, AGPS or WiFi become available to the Trusted Portable Navigator
   (T-PN) engine, they can be delayed when compared to the motion
   measurements made by the onboard sensors. It is important to take care
   of the delay of these measurements before using them as updates. If a
   delayed signal is used without proper mitigation, the overall solution
   will look as if it is pulled backwards for straight line sections of the
   trajectory. If a delayed measurement is applied during a turn, the
   heading can show an error of up to 180 degrees which can cause
   instability in the solution or completely wrong heading of the overall
   solution. This paper will provide methods to estimate the delays using
   offline processing methods and the algorithm for mitigation of the
   delays during real time processing.}},
