@inproceedings{ ISI:000375213000035,
Author = {Winit, Rasika and O'Keefe, Kyle},
Book-Group-Author = {{Inst Navigat}},
Title = {{Four-constellation Reliability in Challenging GNSS Signal Environments
   and the Estimation of Inter-system Time-offsets}},
Booktitle = {{PROCEEDINGS OF THE 26TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
