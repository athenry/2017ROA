@inproceedings{ ISI:000375213000039,
Author = {Li, Bo and O'Keefe, Kyle},
Book-Group-Author = {{Inst Navigat}},
Title = {{WLAN TOA Ranging and GPS Hybrid System for Indoor Navigation}},
Booktitle = {{PROCEEDINGS OF THE 26TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
