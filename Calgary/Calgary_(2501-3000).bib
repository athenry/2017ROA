@article{ ISI:000305803200006,
Author = {Ahmadi, P. and Maundy, B. and Elwakil, A. S. and Belostotski, L.},
Title = {{High-quality factor asymmetric-slope band-pass filters: a
   fractional-order capacitor approach}},
Journal = {{IET CIRCUITS DEVICES \& SYSTEMS}},
Year = {{2012}},
Volume = {{6}},
Number = {{3}},
Pages = {{187-197}},
Month = {{MAY}},
Abstract = {{This study presents new techniques for implementing continuous-time
   second-order band-pass filters with high-quality factors and asymmetric
   slopes. The techniques are centred around the realisation of two
   non-conventional transfer functions which include the non-integer-order
   Laplacian operator s(alpha); 0 < alpha < 1. Four main possible circuit
   realisations; one based on a frequency-dependent negative resistor
   (FDNR), another based on an inductor and two based on multiple amplifier
   biquads (MABs) are given and verified using Spice and experimentally for
   both transfer functions. In addition, a field programmable analogue
   array (FPAA) realisation is tested and verified. Last but not least, a
   possible realisation using current conveyors is also given, tested and
   verified.}},
