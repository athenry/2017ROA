Book-Group-Author = {{IEEE}},
Title = {{Remanufacturing of commercial returns in a market with a variable
   quality tolerance}},
Booktitle = {{2015 INTERNATIONAL CONFERENCE ON INDUSTRIAL ENGINEERING AND OPERATIONS
