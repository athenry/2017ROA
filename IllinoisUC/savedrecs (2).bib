@inproceedings{ ISI:000365985000030,
Author = {Yao, Yuan and Nuzzo, Ralph G.},
Editor = {{Scholl, MS and Paez, G}},
Title = {{Nanomembranes and soft fabrication methods for high performance, low
   cost energy technologies}},
Booktitle = {{INFRARED REMOTE SENSING AND INSTRUMENTATION XXIII}},
