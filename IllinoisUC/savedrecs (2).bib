Book-Group-Author = {{IEEE}},
Title = {{MICROCANTILEVER SYSTEM INCORPORATING INTERNAL RESONANCE FOR
   MULTI-HARMONIC ATOMIC FORCE MICROSCOPY}},
Booktitle = {{2015 28TH IEEE INTERNATIONAL CONFERENCE ON MICRO ELECTRO MECHANICAL
   SYSTEMS (MEMS 2015)}},
