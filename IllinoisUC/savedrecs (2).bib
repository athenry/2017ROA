@inproceedings{ ISI:000373517400036,
Author = {Huang, Wen and Li, Moyang and Gong, Songbin and Li, Xiuling},
Book-Group-Author = {{ASME}},
Title = {{RFIC TRANSFORMER WITH 12X SIZE REDUCTION AND 15X PERFORMANCE ENHANCEMENT
   BY SELF-ROLLED-UP MEMBRANE NANOTECHNOLOGY}},
Booktitle = {{INTERNATIONAL TECHNICAL CONFERENCE AND EXHIBITION ON PACKAGING AND
   INTEGRATION OF ELECTRONIC AND PHOTONIC MICROSYSTEMS, 2015, VOL 3}},
Year = {{2015}},
Note = {{ASME International Technical Conference and Exhibition on Packaging and
   Integration of Electronic and Photonic Microsystems (InterPACK), San
   Francisco, CA, JUL 06-09, 2015}},
Organization = {{ASME}},
Abstract = {{Two types of on-chip RFIC transformers based on CMOS compatible
   strain-induced self-rolled-up membrane (S-RuM) nanotechnology, with
   extremely small footprint, are demonstrated. The rolled-up transformers,
   with their 3D tubular form factors, dramatically reduce the substrate
   parasitic effects and push the maximum working frequency into millimeter
   wave bands with a coupling coefficient, k, as high as 0.92. The 3D
   stand-up nature also allows the tube transformers to be less susceptible
   to residue stress in the substrate and thus compatible with flexible
   platforms for wearable RF applications. The demonstrated samples with a
   turn ratio, n, of 5.5:1 only occupies 805 mu m(2) on-chip area (s) which
   is 12x smaller than that of the best planar transformer with the same
   turn ratio, and its figure of merit n.k/s, is therefore similar to
   6046/mm(2), enhanced by 15x.}},
Publisher = {{AMER SOC MECHANICAL ENGINEERS}},
Address = {{THREE PARK AVENUE, NEW YORK, NY 10016-5990 USA}},
