@article{ ISI:000366231700016,
Author = {Dhasmana, Heena and Ozer, Hasan and Al-Qadi, Mad L. and Zhang, Yuanhui
   and Schideman, Lance and Sharma, Brijendra Kumar and Chen, Wan-Ting and
   Minarick, Mitchell James and Zhang, Peng},
Title = {{Rheological and Chemical Characterization of Biobinders from Different
   Biomass Resources}},
Journal = {{TRANSPORTATION RESEARCH RECORD}},
Year = {{2015}},
Number = {{2505}},
Pages = {{121-129}},
Abstract = {{The increasing costs and strong worldwide demand for petroleum and the
   adverse enviromnental impact of the consumption of nonrenewable energy
   sources have encouraged the development of alternative sources of
   renewable energy. One source of renewable energy can be developed in
   biorefineries, where biomass feedstocks can be converted to biocrude oil
   through thermochemical processes. Biocrude oil can replace
   petroleum-based transportation fuels and can be used to build and
   maintain transportation infrastructure, which requires an
   energy-intensive process that consumes natural resources, including
   mineral aggregates, steel, cement, and petroleum-based binder. This
   study aimed to characterize biocrude oil as an alternative binder
   material, which is referred to here as biobinder. The hydrothermal
   liquefaction technique was used to produce biobinder from spirulina
   algae (microalgae), swine manure, and nanoalgae. A chemical analysis was
   performed with the saturates, aromatics, resins, and asphaltenes
   technique to characterize the percentage of different components present
   in the biobinder. A theological characterization of biobinders was
   conducted to evaluate their feasibility for use in pavement construction
   and to predict their performance during the service life of a pavement.
   Surface free energy properties of biobinders also were determined with
   the use of a sessile drop device to characterize adhesion properties.
   The results indicated that biobinder had significantly different
   rheological and chemical properties than conventional asphalt binder.
   When blended with conventional binder, biobinder showed the potential to
   reduce the stiffness of original binder. As a result, it might be used
   to rejuvenate mixes with recycled asphalt materials or find an
   application under low-temperature conditions.}},
Publisher = {{NATL ACAD SCIENCES}},
Address = {{2101 CONSTITUTION AVE NW, WASHINGTON, DC 20418 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Ozer, H (Reprint Author), Univ Illinois, Dept Civil \& Environm Engn, MC-250,205 North Mathews Ave, Urbana, IL 61801 USA.
   Dhasmana, Heena; Ozer, Hasan; Al-Qadi, Mad L., Univ Illinois, Dept Civil \& Environm Engn, Urbana, IL 61801 USA.
