@inproceedings{ ISI:000370259203045,
Author = {Zhang, Chi and Mehta, Prashant G.},
Book-Group-Author = {{IEEE}},
Title = {{A Comparative Study of Cooperative Localization Techniques for Sensor
   Networks}},
