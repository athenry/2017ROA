Author = {Park, Hyongju and Hutchinson, Seth},
Book-Group-Author = {{IEEE}},
Title = {{A Distributed Robust Convergence Algorithm for Multi-robot Systems in
   the Presence of Faulty Robots}},
Booktitle = {{2015 IEEE/RSJ INTERNATIONAL CONFERENCE ON INTELLIGENT ROBOTS AND SYSTEMS
