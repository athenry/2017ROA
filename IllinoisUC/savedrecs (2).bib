@inproceedings{ ISI:000381554506067,
Author = {Aguilar, Luis T. and Ponce, Israel U. and Orlov, Yury and Bentsman,
   Joseph},
Book-Group-Author = {{IEEE}},
Title = {{Generic Nonsmooth H-infinity Output Synthesis: Tracking Control with
   Application to a Coal-Fired Boiler/Turbine Unit with Input Dead Zone}},
Booktitle = {{2015 54TH IEEE CONFERENCE ON DECISION AND CONTROL (CDC)}},
Year = {{2015}},
