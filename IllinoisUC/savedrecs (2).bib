@inproceedings{ ISI:000380503400197,
Author = {Resu, Swetha and Pavani, G. and Krishnan, Badri and Sailaja, B.},
Book-Group-Author = {{IEEE}},
Title = {{End-to-End Congestion Control in Internet Using Multi-Valued Logic}},
Booktitle = {{2015 1st International Conference on Next Generation Computing
   Technologies (NGCT)}},
Year = {{2015}},
Pages = {{981-984}},
Note = {{1st International Conference on Next Generation Computing Technologies
   (NGCT), Dehradun, INDIA, SEP 04-05, 2015}},
Organization = {{DEFENCE R\&D ORG, MINIST DEFENCE; DEPT SCI \& TECH, GOVT INDIA; SCI \&
   TECH COUNCIL, UTTARAKHAND; ONGC; IEEE UP SEC, INDIA}},
Abstract = {{Congestion has been the major root cause for the loss of data during its
   transit. Many researchers have been working on how to control congestion
   in end-to-end communication network. Yet, much progress was not achieved
   and the result was unsatisfied. Fuzzy techniques were employed that use
   either delay or packet loss. Combination of these two parameters rarely
   exists. This paper considers Queuing Delay (QD), Processing Delay
   (Dproc) and Loss Rate Per Second (LRPS) as input parameters at Fuzzy
   Logic Controller. Making use of ternary valued and penta valued logics
   as key technique, reduces delay, packet loss and improves throughput in
   the network.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
