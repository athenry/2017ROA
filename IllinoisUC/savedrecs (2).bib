@inproceedings{ ISI:000381554504059,
Author = {Heemels, W. P. M. H. and Dullerud, G. E. and Teel, A. R.},
Book-Group-Author = {{IEEE}},
Title = {{A lifting approach to L-2-gain analysis of periodic event-triggered and
