@article{ ISI:000332210700007,
Author = {Fung, Kin Hung and Kumar, Anil and Fang, Nicholas X.},
Title = {{Electron-photon scattering mediated by localized plasmons: A
   quantitative analysis by eigen-response theory}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2014}},
Volume = {{89}},
Number = {{4}},
Month = {{JAN 13}},
Abstract = {{We show that the scattering interaction between a high energy electron
   and a photon can be strongly enhanced by different types of localized
   plasmons in a nontrivial way. The scattering interaction is predicted by
   an eigen-response theory, numerically verified by
   finite-difference-time-domain simulation, and experimentally verified by
   cathodoluminescence spectroscopy. We find that the scattering
   interaction associated with dark plasmons can be as strong as that of
   bright plasmons. Such a strong interaction may offer new opportunities
   to improve single-plasmon detection and high-resolution characterization
   techniques for high quality plasmonic materials.}},
