@inproceedings{ ISI:000356331204004,
Author = {Heng, Liang and Chou, Daniel and Gao, Grace Xingxin},
Book-Group-Author = {{Inst Navigat}},
Title = {{Cooperative GPS Signal Authentication from Unreliable Peers}},
Booktitle = {{PROCEEDINGS OF THE 27TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
