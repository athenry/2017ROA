@incollection{ ISI:000348434900013,
Author = {Leckband, D. E. and de Rooij, J.},
Editor = {{Schekman, R and Lehmann, R}},
Title = {{Cadherin Adhesion and Mechanotransduction}},
Booktitle = {{ANNUAL REVIEW OF CELL AND DEVELOPMENTAL BIOLOGY, VOL 30}},
Series = {{Annual Review of Cell and Developmental Biology}},
Year = {{2014}},
Volume = {{30}},
Pages = {{291-315}},
Abstract = {{Cadherins are the principal adhesion proteins at intercellular junctions
   and function as the biochemical Velcro that binds cells together.
   Besides this mechanical function, cadherin complexes are also
   mechanotransducers that sense changes in tension and trigger adaptive
   reinforcement of intercellular junctions. The assembly and regulation of
   cadherin adhesions are central to their mechanical functions, and new
   evidence is presented for a comprehensive model of cadherin adhesion,
   which is surprisingly more complex than previously appreciated. Recent
   findings also shed new light on mechanisms that regulate cadherin
   junction assembly, adhesion, and mechanotransduction. We further
   describe recent evidence for cadherin-based mechanotransduction, and the
   rudiments of the molecular mechanism, which involves a-catenin and
   vinculin as key elements. Potential roles of a broader cast of possible
   force-sensitive partners are considered, as well as known and
   speculative biological consequences of adhesion and force transduction
