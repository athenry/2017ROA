@inproceedings{ ISI:000345594300012,
Author = {Gupta, Abhishek and Basar, Tamer and Schwartz, Galina A.},
Editor = {{Poovendran, R and Saad, W}},
Title = {{A Three-Stage Colonel Blotto Game: When to Provide More Information to
   an Adversary}},
