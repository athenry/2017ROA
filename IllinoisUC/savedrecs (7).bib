@inproceedings{ ISI:000362380000018,
Author = {Herber, Daniel R. and Allison, James T.},
Book-Group-Author = {{ASME}},
Title = {{WAVE ENERGY EXTRACTION MAXIMIZATION IN IRREGULAR OCEAN WAVES USING
   PSEUDOSPECTRAL METHODS}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
   CONFERENCES AND COMPUTERS AND INFORMATION IN ENGINEERING CONFERENCE,
   2013, VOL 3A}},
