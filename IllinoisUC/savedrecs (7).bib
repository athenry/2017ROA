@inproceedings{ ISI:000362796100095,
Author = {AL-Shudeifat, Mohammad A. and Bergman, Lawrence A. and Vakakis,
   Alexander F.},
Book-Group-Author = {{ASME}},
Title = {{NUMERICAL INVESTIGATION OF ROTATING NONLINEAR ENERGY SINK FOR SHOCK
