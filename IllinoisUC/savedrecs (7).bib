@inproceedings{ ISI:000361249700029,
Author = {Mujumdar, Soham S. and Curreli, Davide and Kapoor, Shiv G. and Ruzic,
   David},
Book-Group-Author = {{ASME}},
Title = {{MODELING OF MELT-POOL FORMATION AND MATERIAL REMOVAL IN MICRO
   ELECTRO-DISCHARGE MACHINING}},
Booktitle = {{PROCEEDINGS OF THE ASME 9TH INTERNATIONAL MANUFACTURING SCIENCE AND
