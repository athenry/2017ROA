Title = {{OIL EFFECTS ON IN-TUBE BOILING OF CO2}},
Booktitle = {{11TH IIR GUSTAV LORENTZEN CONFERENCE ON NATURAL REFRIGERANTS (2014):
   NATURAL REFRIGERANTS AND ENVIRONMENTAL PROTECTION}},
Year = {{2014}},
Pages = {{510-518}},
