@article{ ISI:000332668300022,
Author = {Xu, Lizhi and Gutbrod, Sarah R. and Bonifas, Andrew P. and Su, Yewang
   and Sulkin, Matthew S. and Lu, Nanshu and Chung, Hyun-Joong and Jang,
   Kyung-In and Liu, Zhuangjian and Ying, Ming and Lu, Chi and Webb, R.
