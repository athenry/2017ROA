@inproceedings{ ISI:000343655306191,
Author = {Pfister, Luke and Bresler, Yoram},
Book-Group-Author = {{IEEE}},
Title = {{TOMOGRAPHIC RECONSTRUCTION WITH ADAPTIVE SPARSIFYING TRANSFORMS}},
