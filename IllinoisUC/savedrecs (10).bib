@article{ ISI:000323825100003,
Author = {Xie, Weijun and Ouyang, Yanfeng},
Title = {{Dynamic Planning of Facility Locations with Benefits from Multitype
   Facility Colocation}},
Journal = {{COMPUTER-AIDED CIVIL AND INFRASTRUCTURE ENGINEERING}},
Year = {{2013}},
Volume = {{28}},
Number = {{9}},
Pages = {{666-678}},
Month = {{OCT}},
Abstract = {{This article presents a mixed-integer programming model for a multitype
   facility colocation problem with capacity expansion over a multiperiod
   horizon. This problem is motivated by the emerging U.S. biofuel industry
   that is rapidly expanding its infrastructure facilities to produce
   bioethanol from agricultural crops. Each type of facility (e.g., corn-
   or cellulose-based biorefinery) requires a different type of raw
   material (e.g., corn or perennial grass), but they produce the same
   final product (e.g., bioethanol), and colocation of multiple types of
   facilities results in cost-saving benefits due to complementary
   production process and byproduct recycling. Multiple solution approaches
   (i.e., Lagrangian relaxation, Benders decomposition, and accelerated
   Benders decomposition) are proposed to solve this problem. Numerical
   experiments show that accelerated Benders decomposition most effectively
   solves large-sized problems in a short amount of time. Various
   managerial insights are also drawn from the computational results.}},
