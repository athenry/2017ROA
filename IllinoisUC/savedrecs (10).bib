@article{ ISI:000324389700049,
Author = {Feser, Joseph P. and Xu, Dongyan and Lu, Hong and Zhao, Yang and
   Shakouri, Ali and Gossard, Arthur C. and Majumdar, Arun},
Title = {{Reduced thermal conductivity in Er-doped epitaxial InxGa1-xSb alloys}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{103}},
Number = {{10}},
Month = {{SEP 2}},
Abstract = {{The thermal conductivity of epitaxially grown semiconducting alloys of
   InxGa1-xSb is studied and compared to Er doped alloys. At concentrations
   as low as similar to 0.4\% vol. Er, the Er coalesces to form epitaxially
   embedded ErSb nanocrystals which reduce the thermal conductivity
   compared to undoped alloys. The thermomechanical properties of epitaxial
   ErSb are studied for comparison. ErSb is found to be metallic with
   significantly higher thermal conductivity and stiffness than InxGa1-xSb
   alloys. The stiffness and density contrast from ErSb nanocrystals helps
   explain its high effectiveness as a phonon scattering center, as they
   are the highest of any known lattice matched material pair. (C) 2013 AIP
   Publishing LLC.}},
