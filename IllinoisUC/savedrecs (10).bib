@article{ ISI:000326226300002,
Author = {Wang, Shaowen and Anselin, Luc and Bhaduri, Budhendra and Crosby,
   Christopher and Goodchild, Michael F. and Liu, Yan and Nyerges, Timothy
   L.},
Title = {{CyberGIS software: a synthetic review and integration roadmap}},
