@article{ ISI:000325780400048,
Author = {Hashemi, Roohollah and Avazmohammadi, Reza},
Title = {{Surface effects on stability of nanorings under uniform pressure}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2013}},
Volume = {{114}},
Number = {{14}},
Month = {{OCT 14}},
Abstract = {{In this work, we study the effect of surface stress on the stability
   behavior of a circular ring with nano dimensions under uniform pressure.
   It is known that the surface elasticity and residual surface tension can
   have significant effects on the mechanical behavior of the nano-sized
   structures. Taking both effects into account, we present an analytical
   expression for the smallest critical radial load acting on the nanoring.
   It is shown that, depending on surface mechanical properties of the
   nanoring, these effects can stabilize or destabilize the nanoring under
   uniform pressure. The results, in this work, are expected to provide
   informative predictions on characterizing the stability behavior of nano
   elements used in nanostructured devices. (C) 2013 AIP Publishing LLC.}},
