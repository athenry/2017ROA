@article{ ISI:000325447600089,
Author = {Mo, Kun and Lovicu, Gianfranco and Chen, Xiang and Tung, Hsiao-Ming and
   Hansen, Jon B. and Stubbins, James F.},
Title = {{Mechanism of plastic deformation of a Ni-based superalloy for VHTR
   applications}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2013}},
Volume = {{441}},
