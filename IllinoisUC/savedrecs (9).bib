@article{ ISI:000328525300018,
Author = {Mai Pham and Schideman, Lance and Sharma, Brajendra K. and Zhang,
   Yuanhui and Chen, Wan-Ting},
Title = {{Effects of hydrothermal liquefaction on the fate of bioactive
   contaminants in manure and algal feedstocks}},
Journal = {{BIORESOURCE TECHNOLOGY}},
Year = {{2013}},
Volume = {{149}},
Pages = {{126-135}},
Month = {{DEC}},
Abstract = {{This study investigated the effects of hydrothermal liquefaction (HTL)
   on the fate of bioactive compounds (BACs) often present with wet
   biosolids from wastewater, manure, or algae. Tracking radiolabeled C-14
   for two BACs showed that 60-79\% of the carbon was transferred to the
   HTL raw oil product, and most of the rest was found in the aqueous
   product. In the presence of both swine manure and Spirulina biomass
   feedstocks, HTL provided essentially complete removal of three BACs when
   operated at 300 degrees C for >= 30 min. Experiments with both natural
   transformation and high-efficiency transformation showed that HTL
   provided complete deactivation of antibiotic resistant genes for all
   tested HTL conditions (250-300 degrees C, 15-60 min reaction time).
   Thus, incorporating HTL into wastewater treatment systems can
   simultaneously produce valuable bio-crude oil, provide effective removal
   of BACs and disrupt the natural pathways for antibiotic resistant gene
   transfer from manure and wastewater biosolids to the environment.
   Published by Elsevier Ltd.}},
