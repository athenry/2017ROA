@article{ ISI:000327504000027,
Author = {Tang, Zhi and Gao, Michael C. and Diao, Haoyan and Yang, Tengfei and
   Liu, Junpeng and Zuo, Tingting and Zhang, Yong and Lu, Zhaoping and
   Cheng, Yongqiang and Zhang, Yanwen and Dahmen, Karin A. and Liaw, Peter
   K. and Egami, Takeshi},
Title = {{Aluminum Alloying Effects on Lattice Types, Microstructures, and
   Mechanical Behavior of High-Entropy Alloys Systems}},
