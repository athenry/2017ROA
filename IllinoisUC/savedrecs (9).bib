@article{ ISI:000324227100024,
Author = {Banerjee, Subhabrata and Jacobi, Anthony M.},
Title = {{Transmission loss analysis of single-inlet/double-outlet (SIDO) and
   double-inlet/single-outlet (DISO) circular chamber mufflers by using
   Green's function method}},
Journal = {{APPLIED ACOUSTICS}},
Year = {{2013}},
Volume = {{74}},
Number = {{12}},
Pages = {{1499-1510}},
Month = {{DEC}},
Abstract = {{A Green's function solution method is implemented to study sound
   attenuation in single-inlet/double-outlet (SIDO) and
   double-inlet/single-outlet (DISO) circular chamber mufflers. The
   mufflers are modeled as piston driven rigid circular chambers containing
   a stationary fluid. The pistons are assumed to perform simple harmonic
   motion with uniform velocities. Velocity potential in the chamber is
   derived as a superposition of three dimensional velocity potential due
   to each piston. Pressure field in the chamber is calculated from the
   velocity potential through conservation of linear momentum equation.
   Acoustic pressure acting on each piston is calculated by averaging over
   the surface of the piston. Transmission loss (TL) is evaluated from
   incident and transmitted acoustic energy. TL curves for various
   inlet/outlet orientations derived from this method is validated with
   results obtained from the literature. The effect of locations of
   inlet/outlet on TL is studied. (c) 2013 Elsevier Ltd. All rights
   reserved.}},
