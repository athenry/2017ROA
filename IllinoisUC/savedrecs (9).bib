@article{ ISI:000325234500039,
Author = {Huang, Bo and Tai, Kaiping and Dillon, Shen J.},
Title = {{Structural evolution of alpha-Fe2O3 nanowires during lithiation and
   delithiation}},
Journal = {{JOURNAL OF POWER SOURCES}},
Year = {{2014}},
Volume = {{245}},
Pages = {{308-314}},
Month = {{JAN 1}},
Abstract = {{This work describes the evolution of alpha-Fe2O3 nanowires during the
   conversion reaction associated with lithium intercalation. An electrical
   resistive heating method synthesizes single crystal hematite
   (alpha-Fe2O3) nanowires. Transmission electron microscopy characterizes
   this anode material at various stages of lithiation and delithiation.
   The process initiates through the reduction of alpha-Fe2O3 to
   nanocrystalline Fe3O4. Further lithiation converts all of the iron oxide
   to nanocrystalline BCC Fe. During delithiation, the reactions proceed
   via conversion of Fe to Fe2O3. However, a nanocrystalline structure
   replaces the initial single crystalline alpha-Fe2O3 after the first
   circle. This polycrystalline nanowire structure persists for over 30
   cycles. (C) 2013 Elsevier B.V. All rights reserved.}},
