@inproceedings{ ISI:000377326600191,
Author = {Ellison, Donald H. and Englander, Jacob A. and Conway, Bruce A.},
Editor = {{Broschart, SB and Turner, JD and Howell, KC and Hoots, FR}},
Title = {{ROBUST GLOBAL OPTIMIZATION OF LOW-THRUST, MULTIPLE-FLYBY TRAJECTORIES}},
