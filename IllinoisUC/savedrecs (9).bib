@article{ ISI:000341569200018,
Author = {Liu, Yang and Li, Qi and Gao, Shian and Shang, Jian Ku},
Title = {{Template-free solvothermal synthesis of WO3/WO3 center dot H2O hollow
   spheres and their enhanced photocatalytic activity from the mixture
   phase effect}},
Journal = {{CRYSTENGCOMM}},
Year = {{2014}},
Volume = {{16}},
Number = {{32}},
Pages = {{7493-7501}},
Abstract = {{Well-defined WO3 center dot H2O hollow spheres composed of nanoflakes
   were successfully synthesized by a template-free solvothermal process
   with i-PrOH-H2O mixture solvent. In this process, the tungsten precursor
   was firstly hydrolyzed to form solid spheres composed of both WO3 center
   dot 2H(2)O and WO3 center dot H2O phases. Then, these solid spheres
   underwent an Ostwald ripening process and the WO3 center dot 2H(2)O
   phase was dehydrated to WO3 center dot H2O at the same time to form
   hollow spheres with a pure WO3 center dot H2O phase. With appropriate
   calcination temperature, hollow spheres with WO3 (major) and WO3 center
   dot H2O (minor) mixture phases were created. These hollow spheres with
   WO3 and WO3 center dot H2O mixture phases demonstrated a largely
   enhanced photocatalytic activity for RhB degradation under visible light
   irradiation compared with either pure WO3 center dot H2O hollow spheres
   or pure WO3 hollow spheres which could be attributed to the matched band
   structure between the WO3 and WO3 center dot H2O phases. Thus, an
   effective charge carrier separation can occur between the WO3 and WO3
   center dot H2O phases of these hollow spheres under visible light
   irradiation, contributing to the observed largely enhanced
   photocatalytic performance.}},
