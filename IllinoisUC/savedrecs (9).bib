Title = {{Integrated CUDA-to-FPGA Synthesis with Network-on-Chip}},
Booktitle = {{2014 IEEE 22ND ANNUAL INTERNATIONAL SYMPOSIUM ON FIELD-PROGRAMMABLE
   CUSTOM COMPUTING MACHINES (FCCM 2014)}},
Year = {{2014}},
Pages = {{21-24}},
