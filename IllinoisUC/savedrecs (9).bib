@inproceedings{ ISI:000398274600269,
Author = {Nagao, Akihide and Smith, Cynthia D. and Dadfarnia, Mohsen and Sofronis,
   Petros and Robertson, Ian M.},
Editor = {{Zhang, Z and Skallerud, B and Thaulow, C and Ostby, E and He, J}},
Title = {{Interpretation of hydrogen-induced fracture surface morphologies for
   lath martensitic steel}},
Booktitle = {{20TH EUROPEAN CONFERENCE ON FRACTURE}},
Series = {{Procedia Materials Science}},
Year = {{2014}},
Volume = {{3}},
Pages = {{1700-1705}},
Note = {{20th European Conference on Fracture (ECF), Norwegian Univ Sci Technol,
   Trondheim, NORWAY, JUN 30-JUL 04, 2014}},
Organization = {{European Struct Integr Soc}},
Abstract = {{The mechanism by which fracture partitions between intergranular and
   ``quasi-cleavage{''} in a hydrogen embrittled lath martensitic steel has
   been examined. Intense plasticity is found under both intergranular and
   ``quasi-cleavage{''} fracture surfaces with differencing slip band
   orientation relative to lath structures. In the intergranular fracture
   case, slip bands are parallel to the longitudinal direction of laths and
   the intersection between the slip bands and the interface is inclined to
   the prior austenite grain boundary, whereas in the ``quasi-cleavage{''}
