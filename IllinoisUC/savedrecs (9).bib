@article{ ISI:000341635400002,
Author = {Abdelnaby, Adel E. and Elnashai, Amr S.},
Title = {{Performance of Degrading Reinforced Concrete Frame Systems Under the
   Tohoku and Christchurch Earthquake Sequences}},
Journal = {{JOURNAL OF EARTHQUAKE ENGINEERING}},
Year = {{2014}},
Volume = {{18}},
Number = {{7}},
Pages = {{1009-1036}},
Abstract = {{Field investigations after the recent Tohoku and Christchurch
   earthquakes reported failure of structural systems due to multiple
   earthquakes. In most failure cases the reported damage was mainly due to
   dramatic loss of stiffness and strength of structural elements as a
   result of material deterioration due to repeated earthquake loading.
   This study aims to investigate the degrading behavior of reinforced
   concrete frame systems subjected to Tohoku and Christchurch earthquake
   sequences. Numerical models of RC frames that incorporate damage
   features are established and inelastic response history analyses are
   conducted. The results presented in this study indicate that multiple
   earthquake effects are significant.}},
