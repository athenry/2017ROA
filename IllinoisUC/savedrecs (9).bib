@inproceedings{ ISI:000369428200093,
Author = {Rahman, Md. Ashiqur and Jacobi, Anthony M.},
Editor = {{AlNur, M and Ali, M and Ali, A}},
Title = {{Experimental investigation of wetting anisotropy on microgrooved brass
   surfaces}},
Booktitle = {{10TH INTERNATIONAL CONFERENCE ON MECHANICAL ENGINEERING (ICME 2013)}},
Series = {{Procedia Engineering}},
Year = {{2014}},
Volume = {{90}},
Pages = {{611-617}},
Note = {{10th International Conference on Mechanical Engineering (ICME), BUET,
   Dhaka, BANGLADESH, DEC 20-22, 2013}},
Abstract = {{Through a systematic study of microgrooved brass surfaces, the effect of
   parallel, periodic microgroove geometry on the wettability is examined
   experimentally and compared to that of flat surfaces, where the drop
   size is comparable to the groove dimensions. A total of 18 brass samples
   are fabricated by micromachining without any chemical modification of
   the surface The substrates have groove depths in the range of 26 to 122
   mu m, groove widths of 27 to 187 mu m, and are about 45 mm x 45 mm in
   size, with a thickness about 3 mm. A high degree of wetting anisotropy
   is observed on most of the microgrooved surfaces. A significantly higher
   value of static contact angle (SCA) in the orthogonal direction (across
   the grooves) of the grooves is observed on the microgrooved surfaces
   compared to the contact angle on the flat baseline surfaces which are
   hydrophilic in nature. The contact angle also shows a significant
   dependence on droplet volume. The observed wetting behavior is
   attributed to the manner in which droplets rest on these surfaces,
   contact line pinning by the groove edges and texture and local variation
   of roughness of the micromachined brass surfaces. (C) 2014 The Authors.
   Published by Elsevier Ltd.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{SARA BURGERHARTSTRAAT 25, PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Proceedings Paper}},
Language = {{English}},
Affiliation = {{Rahman, MA (Reprint Author), Univ Illinois, Dept Mech Sci \& Engn, Urbana, IL 61801 USA.
