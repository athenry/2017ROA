@inproceedings{ ISI:000337270600068,
Author = {Stinean, Alexandra-Iulia and Preitl, Stefan and Precup, Radu-Emil and
   Petriu, Emil M. and Dragos, Claudia-Adina and Radac, Mircea-Bogdan},
Book-Group-Author = {{IEEE}},
Title = {{2-DOF PI(D) Takagi-Sugeno and Sliding Mode Controllers for BLDC Drives}},
Booktitle = {{2012 15TH INTERNATIONAL POWER ELECTRONICS AND MOTION CONTROL CONFERENCE
   (EPE/PEMC)}},
Year = {{2012}},
Note = {{15th International Power Electronics and Motion Control Conference and
   Exposition (EPE-PEMC ECCE Europe), Novi Sad, SERBIA, SEP 04-06, 2012}},
Organization = {{Univ Novi Sad, Fac Tech Sci; Prov Secretariat Sci \& Technol Dev \& High
   Educ AP Vojvodina; Minist Educ Sci \& Technol Dev Repub Serbia; IEEE
   PELS}},
Abstract = {{This paper gives a new 2-DOF PI(D) controller structure based on
   Takagi-Sugeno fuzzy controller and two types of sliding mode controllers
   for speed and position control of Brushless Direct Current (BLDC)
   drives. These structures aim optimized set-point responses, good
   disturbance control behaviors and good robustness with respect to model
   uncertainties. Simplified models of BLDC drives are derived for
   controller designs. The simplified models are characterized by a
   second-order with lag dynamics without or with an integral component and
   generally - with variable parameters. A comparative case study based on
   digital simulation results for the detailed BLDC drive model is included
   to exemplify the efficiency of our controllers.}},
