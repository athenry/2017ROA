@inproceedings{ ISI:000335090900091,
Author = {Pitts, Katie L. and Fenech, Marianne},
Book-Group-Author = {{ASME}},
Title = {{BLOOD VELOCITY PROFILE MEASUREMENTS IN MICROCHANNELS USING
   MICRO-PARTICLE IMAGE VELOCIMETRY}},
Booktitle = {{PROCEEDINGS OF THE ASME 10TH INTERNATIONAL CONFERENCE ON NANOCHANNELS,
   MICROCHANNELS AND MINICHANNELS 2012}},
