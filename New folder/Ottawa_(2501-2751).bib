@inproceedings{ ISI:000331660100011,
Author = {Krupin, O. and Berini, P.},
Editor = {{Kieffer, JC}},
Title = {{Biosensing using plasmonic waveguides embedded in CYTOP}},
Booktitle = {{PHOTONICS NORTH 2012}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8412}},
Note = {{Conference on Photonics North, Montreal, CANADA, JUN 06-08, 2012}},
