@inproceedings{ ISI:000302560100050,
Author = {Sina, M. I. and Cretu, A. -M. and Payeur, P.},
Editor = {{Rogowitz, BE and Pappas, TN and DeRidder, H}},
Title = {{Biological Visual Attention Guided Automatic Image Segmentation with
   Application in Satellite Imaging}},
