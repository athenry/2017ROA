@article{ ISI:000301301300012,
Author = {Feng, Chaoyang and Khulbe, K. C. and Tabe, Shahram},
Title = {{Volatile organic compound removal by membrane gas stripping using
   electro-spun nanofiber membrane}},
Journal = {{DESALINATION}},
Year = {{2012}},
Volume = {{287}},
Number = {{SI}},
Pages = {{98-102}},
Month = {{FEB 15}},
Abstract = {{Removal of volatile organic compound (chloroform) from water was carried
   out by membrane gas stripping using electro-spun poly(vinylidene
   fluoride) (PVDF) nanofiber membrane. It was shown that the process was
   feasible. The overall mass transfer coefficient of chloroform through
   the nanofiber membrane was found to be 2 x 10(-5) m/s at room
   temperature, which was more than the highest value obtained earlier for
   a hollow fiber air-stripping system. The activation energy of the mass
   transfer coefficient was almost the same as that of the Henry constant
   for chloroform absorption in water. It was therefore concluded that the
   chloroform desorption from water was the governing factor of the system.
   Crown Copyright (C) 2011 Published by Elsevier B.V. All rights reserved.}},
