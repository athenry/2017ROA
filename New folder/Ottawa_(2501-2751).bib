@inproceedings{ ISI:000331660100036,
Author = {Fong, Norman R. and Berini, Pierre and Tait, R. Niall},
Editor = {{Kieffer, JC}},
Title = {{Grating Coupler Excitation of Membrane Supported Long Range Surface
   Plasmons}},
Booktitle = {{PHOTONICS NORTH 2012}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8412}},
Note = {{Conference on Photonics North, Montreal, CANADA, JUN 06-08, 2012}},
Organization = {{Univ Laval, Ctr Optique, Photonique \& Laser; Canadian Photon
   Consortium; Inst Natl Optique; Quebec Photon Network; Canadian Inst
   Photon Innovat; CMC Microsystems; Ecole Technologie Superieure; INRS}},
Abstract = {{The design of gratings for broadside coupling of a Gaussian beam to a
   long-range surface plasmon polariton (LRSPP) waveguide is explored. The
   waveguide is a gold (Au) slab supported by a thin Cytop membrane bounded
   by air and forms a waveguide structure for potential use as a gas
   sensor. Grating coupler designs are proposed and modeled in two
   dimensions using the finite element method (FEM). A large design space
   of varying grating dimensions and input positions is examined and the
   resulting simulations predict an input coupling efficiency of
   approximately 29\%. Fabrication of these gratings is also examined
   through standard optical lithography.}},
