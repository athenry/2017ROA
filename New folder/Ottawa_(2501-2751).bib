@inproceedings{ ISI:000316692900030,
Author = {Dole, Holly A. E. and Kim, Jong Min and Lizarraga, Leonardo and Vernoux,
   Philippe and Baranova, Elena A.},
Editor = {{Mogensen, MB and Gur, TM and Zhou, XD and Armstrong, T and Yokokawa, H}},
Title = {{Catalytic CO Oxidation over Au Nanoparticles Supported on
   Yttria-Stabilized Zirconia}},
Booktitle = {{IONIC AND MIXED CONDUCTING CERAMICS 8}},
Series = {{ECS Transactions}},
Year = {{2012}},
Volume = {{45}},
Number = {{1}},
Pages = {{265-274}},
Note = {{Symposium on Ionic and Mixed Conducting Ceramics 8 held during the 221st
   Meeting of the Electrochemical-Society, Seattle, WA, MAY 06-10, 2012}},
Organization = {{Electrochem Soc (ECS); High Temp Mat Div}},
Abstract = {{Gold nanoparticles supported on yttria- stabilized zirconia (YSZ) were
   synthesized using a modified alcohol method which involves the
   stabilizing agent, polyvinylpyrrolidone (PVP). Two different average
   sizes (13.1 and 17.1 nm) of Au nanoparticles were synthesized. These
   catalysts were tested for gas phase CO oxidation. Since PVP is known to
   block the active Au sites, the catalysts were calcined at 300 degrees C
   and 600 degrees C in order to remove the PVP. Overall, higher catalytic
   activity was found for the smaller Au nanoparticles. It was also found
   that calcination is required in order to achieve activity, even though
   the particle size increases with this treatment. The effect of
