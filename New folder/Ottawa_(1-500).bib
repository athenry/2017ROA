Author = {Israr, Junaid and Gahi, Youssef and Guennoun, Mouheine and Mouftah,
   Hussein T.},
Book-Group-Author = {{IEEE}},
Title = {{Security Analysis of C-BGP: A Light Alternative to S-BGP}},
