Title = {{Investigation of Heterogeneous Catalysts by an Electrochemical Method:
   Ceria and Titania Supported Iridium Nanoparticles for Ethylene Oxidation}},
Booktitle = {{IONIC AND MIXED CONDUCTING CERAMICS 10}},
Series = {{ECS Transactions}},
