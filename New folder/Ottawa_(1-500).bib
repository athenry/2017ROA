@inproceedings{ ISI:000402610200387,
Author = {de Souza, Allan M. and Boukerche, Azzedine and Maia, Guilherme and
   Cerqueira, Eduardo and Loureiro, Antonio A. F. and Villas, Leandro A.},
Book-Group-Author = {{IEEE}},
Title = {{SPARTAN: A Solution to Prevent Traffic Jam with Real-Time Alert and
   Re-routing for Smart City}},
Booktitle = {{2016 IEEE 84TH VEHICULAR TECHNOLOGY CONFERENCE (VTC FALL)}},
Series = {{IEEE Vehicular Technology Conference Proceedings}},
Year = {{2016}},
Note = {{84th IEEE Vehicular Technology Conference (VTC-Fall), Montreal, CANADA,
   SEP 18-21, 2016}},
Organization = {{IEEE; TELUS}},
Abstract = {{As an important component of Smart Cities, transportation system plays a
   critical role to address the sustainability and mobility of the society.
   One key concern is that the number of vehicles continuously increases
   faster than the available infrastructure, as well as the traffic
   congestion is a difficult issue to deal with. Several solutions to
   Intelligent Transportation Systems (ITS) have been proposed to identify
   congestion and re-route the vehicles afterwards. In this direction, this
   work introduces SPARTAN, a fully distributed ITS solution, which
   notifies drivers about congested areas through Vehicle-to-Vehicle
