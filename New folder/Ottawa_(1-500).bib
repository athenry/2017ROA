@inproceedings{ ISI:000401963304139,
Author = {Meneguette, Rodolfo I. and Boukerche, Azzedine and de Grande, Robson},
Book-Group-Author = {{IEEE}},
Title = {{SMART: An Efficient Resource Search and Management Scheme for Vehicular
   Cloud-connected System}},
Booktitle = {{2016 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
Series = {{IEEE Global Communications Conference}},
