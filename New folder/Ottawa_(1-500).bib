@inproceedings{ ISI:000390300400051,
Author = {Abolarin, David and Forouzanfar, Mohamad and Groza, Voicu Z. and Rajan,
   Sreeraman and Dajani, Hilmi R. and Petriu, Emil M.},
Book-Group-Author = {{IEEE}},
Title = {{Model-Based Filtering and Compression of Oscillometric Blood Pressure
   Pulses}},
