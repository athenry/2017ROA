@article{ ISI:000393321000028,
Author = {Zare, Soheil Ghareh Aghaji and Moore, Stephanie A. and Rennie, Colin D.
   and Seidou, Ousmane and Ahmari, Habib and Malenchak, Jarrod},
Title = {{Reply to comment by Spyros Beltaos on ``Estimation of composite
   hydraulic resistance in ice-covered alluvial streams''}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2016}},
Volume = {{52}},
Number = {{12}},
Pages = {{9665-9670}},
Month = {{DEC}},
Abstract = {{Previously utilized techniques for analysis of flow velocity in
   ice-controlled zone are confirmed to be valid. Potential sources of
   error regarding the estimation of energy grade line slope are corrected
   considering the comments by the discussant. A modified version of the
   equation for composite roughness calculation originally introduced in
   the paper is presented. The revised new method then is tested against
   the other available methods and its accuracy is evaluated. Other
   assumptions and analyses presented in the paper are also tested and
