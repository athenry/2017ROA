@article{ ISI:000377202800004,
Author = {Young, Bradley and Kennedy, Kevin and Delatolla, Robert and Sherif,
   Ranya},
Title = {{Semi-continuous mesophilic anaerobic co-digestion of thermally
   pretreated scum}},
Journal = {{WATER QUALITY RESEARCH JOURNAL OF CANADA}},
Year = {{2016}},
Volume = {{51}},
Number = {{2}},
Pages = {{117-127}},
Month = {{MAY}},
Abstract = {{The objective of this study is to investigate the mesophilic, anaerobic,
   co-digestion of scum with thickened waste activated sludge (TWAS) and
   primary sludge (PS). Observations of the reactor waste show that higher
   pretreatment temperatures assist in scum integration and appear to limit
   coagulation and separation effects associated with digesting fats, oils
   and greases. The 0.40 kg VS/m(3) . d scum loaded reactor with scum
   pretreated at 70 degrees C achieved the greatest improvement in biogas
   production over PS and TWAS co-digestion without scum addition. Based on
   cell viability analysis of the reactor waste, scum-containing reactors
   were shown to produce 48 and 39\% more biogas per m(3) of live cells;
   signifying that scum addition increased the activity of the microbial
   population embedded in the waste or caused a microbial shift in the
   waste towards more active communities.}},
