@inproceedings{ ISI:000391421300043,
Author = {Singh, Shailendra and Yassine, Abdulsalam and Shirmohammadi, Shervin},
Book-Group-Author = {{IEEE}},
Title = {{Incremental Mining of Frequent Power Consumption Patterns from Smart
   Meters Big Data}},
Booktitle = {{2016 IEEE ELECTRICAL POWER AND ENERGY CONFERENCE (EPEC)}},
Series = {{IEEE Electrical Power and Energy Conference}},
Year = {{2016}},
Note = {{IEEE Electrical Power and Energy Conference (EPEC), Ottawa, CANADA, OCT
   12-14, 2016}},
Organization = {{IEEE}},
Abstract = {{The key elements for understanding power consumption of a typical home
   are related to the activities that users are performing, the time at
   which appliances are used, and the interdependencies with other
   appliances that may be used concurrently. This information can be
   extracted from context rich smart meters big data. However, the main
   challenge is how to mine complex interdependencies among different
   appliances usage within a home where multiple concurrent data streams
   are occurring. Furthermore, generation of energy consumption data from a
   smart meter is an ongoing continuous process and over period of time
   inter-appliance associations can change or new ones can establish. In
   this paper, we propose incremental mining of frequent power consumption
   patterns from smart meters big data. Our model exploits the benefits of
   pattern growth strategy and mine in quantum of 24 hour period, i.e.
   frequent patterns are extracted from data comprising of appliance usage
   tuples for 24 hours period, in a progressive manner. The details and the
   results of evaluating the proposed mechanism using real smart meters
   dataset are presented in this paper.}},
