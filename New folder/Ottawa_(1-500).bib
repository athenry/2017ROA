@article{ ISI:000373062500029,
Author = {Robinson, T. and Bronson, B. and Gogolek, P. and Mehrani, P.},
Title = {{Comparison of the air-blown bubbling fluidized bed gasification of wood
   and wood-PET pellets}},
Journal = {{FUEL}},
Year = {{2016}},
Volume = {{178}},
Pages = {{263-271}},
Month = {{AUG 15}},
Abstract = {{Air-blown bubbling fluidized bed gasification of wood pellets and
   wood-PET (polyethylene terephthalate) pellets was conducted at 725
   degrees C, 800 degrees C, and 875 degrees C. Gasification of mixtures of
   wood and PET pellets was attempted; however, rapid formation of coke
   above the fluidized bed prevented steady state operation of the
   gasifier. Coking was prevented when wood and PET were formed into
   composite wood-PET pellets. The performance of wood pellets was better
   than wood-PET pellets. Tar concentrations were lower, while, heating
   values, thermal outputs, and efficiencies tended to be higher for wood
   pellets than wood-PET pellets. The addition of secondary air above the
   fluidized bed was explored as a means of reducing the tar content of the
   produced gases. The influence of the produced gas tar concentrations on
   down-stream tar removal units was considered. Crown Copyright (C) 2016
   Published by Elsevier Ltd. All rights reserved.}},
