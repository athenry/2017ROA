@inproceedings{ ISI:000405491400006,
Author = {Li, Chao and Yongacoglu, Abbas and D'Amours, Claude},
Book-Group-Author = {{IEEE}},
Title = {{Heterogeneous Cellular Network User Distribution Model}},
Booktitle = {{2016 8TH IEEE LATIN-AMERICAN CONFERENCE ON COMMUNICATIONS (LATINCOM)}},
Series = {{IEEE Latin American Conference on Communications}},
Year = {{2016}},
Note = {{8th IEEE Latin-American Conference on Communications (LATINCOM),
   Medellin, COLOMBIA, NOV 15-17, 2016}},
Organization = {{IEEE}},
Abstract = {{In heterogeneous networks (HetNets) users are usually distributed
   non-uniformly. Analysis of the impact of non-uniformity of user
   distribution on HetNets is essential for designing efficient networks.
   This paper proposes a non-uniform user distribution model in which the
   user density depends on the distance to the associated base stations
   (BSs). Moreover, different tiers, such as macro cells, pico cells or
   femto cells, may have different user densities with different level of
   attraction to their associated BSs. Our proposed non-uniform user
   distribution model covers many possible combinations. We give an
   accurate performance analysis of the downlink coverage probability. The
   numerical evaluations show that our analysis can produce results that
   closely match the simulation results.}},
