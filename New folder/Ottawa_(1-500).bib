@article{ ISI:000370196100006,
Author = {Peng, Licheng and Lan, Christopher Q. and Zhang, Zisheng},
Title = {{Cultivation of freshwater green alga Neochloris oleoabundans in
   non-sterile media co-inoculated with protozoa}},
Journal = {{CANADIAN JOURNAL OF CHEMICAL ENGINEERING}},
Year = {{2016}},
Volume = {{94}},
Number = {{3}},
Pages = {{439-445}},
Month = {{MAR}},
Abstract = {{The effects of sodium bicarbonate on freshwater green alga Neochloris
   oleoabundans were evaluated with respect to cell growth, cell
   morphology, and control of protozoa contamination under non-sterile
   conditions co-inoculated with protozoa pre-cultures. While culture
   collapse occurred in the control and the cultures containing 40mmol/L
   NaHCO3 due to protozoa contamination, the addition of 160 or 200mmol/L
   NaHCO3 could completely inhibit the growth of microalgae-preying
   protozoa and allow safe cultivation of N. oleoabundans for co-incubation
