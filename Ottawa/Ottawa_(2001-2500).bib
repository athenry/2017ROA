@article{ ISI:000316670400012,
Author = {Rokkam, R. G. and Sowinski, A. and Fox, R. O. and Mehrani, P. and Muhle,
   M. E.},
Title = {{Computational and experimental study of electrostatics in gas-solid
   polymerization fluidized beds}},
Journal = {{CHEMICAL ENGINEERING SCIENCE}},
Year = {{2013}},
Volume = {{92}},
Pages = {{146-156}},
Month = {{APR 5}},
Abstract = {{Gas-particle flows are present in many industrial applications such as
   polymerization, fluid catalytic cracking, chemical vapor deposition,
   combustion and drying. Particle-particle, particle-wall and gas-particle
   interactions cause electrostatic charge to form on particles. The motion
   of charged particles creates an electric field, affecting the
   hydrodynamics in reactors such as polymerization fluidized beds and
   fluid catalytic crackers (Hendrickson, 2006). In this work, a combined
   multi-fluid and electrostatic model previously developed in Rokkam et
   al. (2010) is used to simulate laboratory-scale experiments on
   electrostatics in gas-solid fluidized beds conducted by Sowinski et al.
