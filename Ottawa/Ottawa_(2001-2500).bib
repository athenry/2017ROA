Author = {Herbert-Copley, Andrew G. and Sinitski, Emily H. and Lemaire, Edward D.
   and Baddour, Natalie},
Book-Group-Author = {{IEEE}},
Title = {{Temperature and Measurement Changes over Time for F-Scan Sensors}},
Booktitle = {{2013 IEEE INTERNATIONAL SYMPOSIUM ON MEDICAL MEASUREMENTS AND
