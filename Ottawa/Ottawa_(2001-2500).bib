@inproceedings{ ISI:000333178300006,
Author = {Kantarci, Burak and Mouftah, Hussein T.},
Book-Group-Author = {{IEEE}},
Title = {{Economizing the Operational Costs of Cloud Services in an Optical
   Transport Network}},
Booktitle = {{2013 15TH INTERNATIONAL CONFERENCE ON TRANSPARENT OPTICAL NETWORKS
   (ICTON 2013)}},
Series = {{International Conference on Transparent Optical Networks-ICTON}},
Year = {{2013}},
Note = {{15th International Conference on Transparent Optical Networks (ICTON),
