@article{ ISI:000310052800034,
Author = {Meunier, Trevor and Villafranca, Ana B. and Bhardwaj, Ravi and Weck,
   Arnaud},
Title = {{Fabrication of microlens arrays in polycarbonate with nanojoule energy
   femtosecond laser pulses}},
Journal = {{OPTICS LETTERS}},
Year = {{2012}},
Volume = {{37}},
Number = {{20}},
Pages = {{4266-4268}},
Month = {{OCT 15}},
Abstract = {{We report fabrication of smooth microlens arrays by focusing a nanojoule
   energy, high repetition rate femtosecond laser oscillator inside a
   polycarbonate sample. Heat accumulation at the laser focal point and
   subsequent material expansion leads to localized swelling at the sample
   surface that behaves as a microlens. By changing the depth of the laser
   focus in the sample, the focal length of the microlens can be controlled
   and varies from 40 to 80 mu m while maintaining a high numerical
   aperture of similar to 0.6. This fabrication technique is a single step,
   controllable, and economical process that can produce arrays of
   optically smooth microlenses. (C) 2012 Optical Society of America}},
