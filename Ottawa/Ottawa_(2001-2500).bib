@inproceedings{ ISI:000346481000172,
Author = {Rezaei, F. and Naik, K. and Nayak, A. and Yousefi, S.},
Book-Group-Author = {{IEEE}},
Title = {{Effective Warning Data Dissemination Scheme in Vehicular Networks for
   Intelligent Transportation System Applications}},
