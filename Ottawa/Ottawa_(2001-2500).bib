@article{ ISI:000304826700003,
Author = {Zhang, Weifeng and Yao, Jianping},
Title = {{Photonic Generation of Millimeter-Wave Signals With Tunable Phase Shift}},
Journal = {{IEEE PHOTONICS JOURNAL}},
Year = {{2012}},
Volume = {{4}},
Number = {{3}},
Pages = {{889-894}},
Month = {{JUN}},
Abstract = {{A novel photonic approach to generating a frequency-quadrupled
   millimeter-wave (mm-wave) signal with a tunable phase shift is proposed
   and demonstrated. Two second-order optical sidebands are generated by
   using a Mach-Zehnder modulator (MZM) that is biased at the maximum
   transmission point and an optical notch filter. A
   polarization-maintaining fiber Bragg grating (PM-FBG) is then utilized
   to make the two sidebands orthogonally polarized, which are then sent to
   a polarization modulator (PolM). By aligning the two orthogonally
   polarized sidebands with the two principal axes of the PolM,
   complementary phase modulation is thus achieved. By beating the two
   phase-modulated sidebands at a photodetector, an mm-wave signal is
   generated and its phase is continuously tunable by tuning the bias
   voltage to the PolM. An experiment is performed. An mm-wave signal with
   a frequency tunable from 36 to 52 GHz is generated and its phase is
   tunable over 360 degrees by tuning the bias voltage.}},
