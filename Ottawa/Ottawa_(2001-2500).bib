@article{ ISI:000310592000008,
Author = {Plouffe, Patrick and Macchi, Arturo and Donaldson, Adam A.},
Title = {{Enhancement of Interphase Transport in Mini-/Microscale Applications
   Using Passive Mixing}},
Journal = {{HEAT TRANSFER ENGINEERING}},
Year = {{2013}},
Volume = {{34}},
Number = {{2-3}},
Pages = {{159-168}},
Note = {{9th International Conference on Nanochannels, Microchannels and
   Minichannels, Univ Alberta Edmonton, Edmonton, CANADA, JUN 19-22, 2011}},
Organization = {{Amer Soc Mech Engineers}},
Abstract = {{Structured mini-/microscale reactors continue to receive attention from
   both industry and academia due to their low pressure drop, high heat and
   mass transfer rates, and ease of scale-up relative to conventional
   reactor technology. Commonly considered for reactions such as
   hydrogenations, hydrodesulfurization, oxidations, and Fischer-Tropsch
   synthesis, the performance of these systems is highly dependent on
   mixing and the interfacial area between phases. While existing
   literature describes the initial flow patterns generated by a broad
   range of two-phase contactors, few studies explore the dynamic impacts
   of downstream passive mixing elements. Experimental and computational
   methodologies for characterizing two-phase flow pattern transitions,
   pressure drop, and heat and mass transfer are discussed, with relevant
   examples for serpentine and Venturi-based passive mixing designs. The
   efficacies of these two configurations are explored in the context of
   pressure drop, conditions leading to significant interface renewal, and
   design considerations for optimizing mass transfer. Challenges associate
   with the characterization of multiphase flow through these systems are
   highlighted, and strategies suggested for both experimental and
   computational analysis of dynamic flow patterns and fluid-fluid
   interactions.}},
