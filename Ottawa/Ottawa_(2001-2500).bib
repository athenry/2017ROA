@inproceedings{ ISI:000333734900265,
Author = {Yacoub, Mostafa I. and Necsulescu, Dan S. and Sasiadek, Jurek Z.},
Book-Group-Author = {{IEEE}},
Title = {{Energy Consumption Optimization for Mobile Robots in Three-dimension
   Motion Using Predictive Control}},
Booktitle = {{2013 9TH ASIAN CONTROL CONFERENCE (ASCC)}},
Year = {{2013}},
Note = {{9th Asian Control Conference (ASCC), Istanbul, TURKEY, JUN 23-26, 2013}},
Abstract = {{As the demand for field mobile robots in off-road operations increased,
   the need to investigate the 3D motion for mobile robots became
   important. One of the main difficulties in the 3D motion of a mobile
   robot is the torque saturation of the DC motors of the wheels that
   occurs while climbing hills. In the present work, off-road conditions
   are utilized to benefit by avoiding torque saturation. Energy
   optimization algorithm using predictive control is implemented on a
   two-DC motor-driven wheels mobile robot while crossing a ditch. The
   developed algorithm is simulated and compared with the PID control and
   the open-loop control. The predictive control showed more capability to
   avoid torque saturation and noticeable reduction in the energy
   consumption. Furthermore, using the wheels motors armature current
   instead of the supply voltage as control variable in the predictive
   control showed more robust speed control. Simulation results showed that
   in case of knowing the ditch dimensions ahead of time, the developed
   algorithm is feasible.}},
