@article{ ISI:000305983100008,
Author = {Mulgundmath, V. P. and Tezel, F. H. and Hou, F. and Golden, T. C.},
Title = {{Binary adsorption behaviour of methane and nitrogen gases}},
Journal = {{JOURNAL OF POROUS MATERIALS}},
Year = {{2012}},
Volume = {{19}},
Number = {{4}},
Pages = {{455-464}},
Month = {{AUG}},
Abstract = {{Separation of methane and nitrogen gases is critical in the upgrading of
   LFG (Landfill gas), natural gas and coal bed gas in order to have a
   commercial heating value for methane. From an environmental point of
   view, methane capture from landfill gas is essential to prevent
   greenhouse gas emissions. Adsorption could be a beneficial process to
   capture low purity methane from a landfill site that is nearing the end
   of its lifecycle and produce high purity methane. In this work, Ceca 13X
   zeolite and Alcan Activated Alumina AA 320-AP have been studied for
   their potential for this separation and compared with Silicalite in
   literature. Pure and mixture adsorption isotherms were determined at 40
   and 100 A degrees C for these adsorbents by constant volume method and
   concentration pulse chromatographic technique, respectively. Mixture
   adsorption isotherms for the binary system of methane and nitrogen gases
   at 40 and 100 A degrees C and 1 atmosphere total pressure have been
   determined by VV-CPM (Van der Vlist and Van der Meijden Concentration
   Pulse Method). The application of Extended Langmuir model for this
   binary system have also been discussed and compared to the experimental
   results. Results show that equilibrium separation factor for silicalite
   is larger than zeolite Ceca 13X and Alcan activated alumina AA320-AP.
   Both Silicalite and Ceca 13X find application in the bulk separation of
   methane from nitrogen when y (CH4) > 0.4, especially in LFG, coal bed
   gas and natural gas.}},
