@inproceedings{ ISI:000335674700006,
Author = {Toboso, Alain U. and Loyka, Sergey and Gagnon, Francois},
Book-Group-Author = {{IEEE}},
Title = {{On Optimal Detection Ordering for Coded V-BLAST}},
Booktitle = {{2013 13TH CANADIAN WORKSHOP ON INFORMATION THEORY (CWIT)}},
Year = {{2013}},
Pages = {{28-32}},
Note = {{13th Canadian Workshop on Information Theory (CWIT), Toronto, CANADA,
   JUN 18-21, 2013}},
Abstract = {{Optimum ordering strategies for the coded V-BLAST with capacity
   achieving temporal codes on each stream are analytically studied in this
   paper, including optimal power and/or rate allocations among data
   streams. A compact closed-form solution is obtained for the case of two
   transmit antennas and necessary optimality conditions are found for the
   general case. The optimal rate allocation is shown to have a major
   impact (stronger streams are detected last) while the optimal power
   allocation does not alter the original Foschini ordering (stronger
   streams are detected first). A sub-optimal ordering is proposed based on
   the necessary optimality conditions, which performs very close to the
   optimal one but has much smaller computational complexity. An SNR gain
   of ordering is introduced and studied. All the results also apply to a
   multiple-access channel under successive interference cancelation.}},
