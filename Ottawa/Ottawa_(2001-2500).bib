@article{ ISI:000309423200173,
Author = {Meunier, Trevor and Weck, Arnaud},
Title = {{Interference rings formation inside cellulose from a back-reflected
   femtosecond laser pulse}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2012}},
Volume = {{112}},
Number = {{6}},
Month = {{SEP 15}},
Abstract = {{We report on the formation of concentric rings within the bulk of
   cellulose films after laser machining with high energy femtosecond laser
   pulses. The rings originate from an interference effect, where the front
   of the pulse is reflected from the back surface of the sample and
   interferes with its tail. Ray tracing simulations confirmed the
   interference and rings formation. Modification at the back surface of
   materials may go unnoticed and lead to undesired properties. These
   findings are therefore important for the ultrafast laser community
   interested in machining of transparent materials. (C) 2012 American
   Institute of Physics. {[}http://dx.doi.org/10.1063/1.4751359]}},
