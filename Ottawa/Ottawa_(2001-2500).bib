@inproceedings{ ISI:000348913400009,
Author = {Loyka, Sergey and Kostina, Victoria and Gagnon, Francois},
Book-Group-Author = {{IEEE}},
Title = {{Convexity of Error Rates in Digital Communications Under Non-Gaussian
   Noise}},
Booktitle = {{2013 IEEE INTERNATIONAL SYMPOSIUM ON INFORMATION THEORY PROCEEDINGS
   (ISIT)}},
Series = {{IEEE International Symposium on Information Theory}},
Year = {{2013}},
Pages = {{41+}},
Note = {{IEEE International Symposium on Information Theory (ISIT), Istanbul,
   TURKEY, JUL 07-12, 2013}},
Organization = {{IEEE; IEEE Informat Theory Soc}},
Abstract = {{Convexity properties of error rates of a class of decoders, including
   the ML/min-distance one as a special case, are studied for arbitrary
   constellations. Earlier results obtained for the AWGN channel are
   extended to a wide class of (non-Gaussian) noise densities, including
   unimodal and spherically-invariant noise. Under these broad conditions,
   symbol error rates are shown to be convex functions of the SNR in the
   high-SNR regime with an explicitly-determined threshold, which depends
   only on the constellation dimensionality and minimum distance, thus
   enabling an application of the powerful tools of convex optimization to
   such digital communication systems in a rigorous way. It is the
   decreasing nature of the noise power density around the decision region
   boundaries that insures the convexity of symbol error rates in the
   general case. The known high/low SNR bounds of the convexity/concavity
   regions are tightened and no further improvement is shown to be possible
   in general.}},
