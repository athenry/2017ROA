@article{ ISI:000368568700005,
Author = {Dole, Holly A. E. and Costa, Ana C. G. S. A. and Couillard, Martin and
   Baranova, Elena A.},
Title = {{Quantifying metal support interaction in ceria-supported Pt, PtSn and Ru
   nanoparticles using electrochemical technique}},
Journal = {{JOURNAL OF CATALYSIS}},
Year = {{2016}},
Volume = {{333}},
Pages = {{40-50}},
Month = {{JAN}},
Abstract = {{Electrochemical promotion and metal-support interaction (MSI) have long
   been viewed as two separate phenomena; however, more recently it has
   been shown that, functionally, for ionic and mixed ionic-electronic
   conducting materials, both are linked by the transfer of ionic species
   at the interfaces. The purpose of this study was to exhibit a
   relationship between these two concepts through the use of an
