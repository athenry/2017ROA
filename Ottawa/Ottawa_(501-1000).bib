@inproceedings{ ISI:000386979000157,
Author = {Nasrallah, Yamen Y. and Al-Anbagi, Irfan and Mouftah, Hussein T.},
Book-Group-Author = {{IEEE}},
Title = {{QoS-based Distributed Time Synchronization Mechanism for High Intensity
   Vehicular Networks}},
