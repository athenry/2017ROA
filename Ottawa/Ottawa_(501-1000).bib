@inproceedings{ ISI:000382523600176,
Author = {Chen, Lei and Zhao, Jiying},
Book-Group-Author = {{IEEE}},
Title = {{Adaptive contourlet-based image watermarking robust to geometric
   transformations and image compression}},
Booktitle = {{2016 IEEE INTERNATIONAL INSTRUMENTATION AND MEASUREMENT TECHNOLOGY
   CONFERENCE PROCEEDINGS}},
