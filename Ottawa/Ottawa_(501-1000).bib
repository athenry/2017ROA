@article{ ISI:000358574700011,
Author = {Wong, Wei Ru and Krupin, Oleksiy and Adikan, Faisal Rafiq Mahamd and
   Berini, Pierre},
Title = {{Optimization of Long-Range Surface Plasmon Waveguides for
   Attenuation-Based Biosensing}},
