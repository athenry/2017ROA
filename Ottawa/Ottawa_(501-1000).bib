@inproceedings{ ISI:000378996100012,
Author = {Venkatesh, Venkataramanan and Nayak, Amiya},
Editor = {{Huang, X and Xiang, Y and Li, KC}},
Title = {{Optimizing I/O Intensive Domain Handling in Xen Hypervisor for
   Consolidated Server Environments}},
