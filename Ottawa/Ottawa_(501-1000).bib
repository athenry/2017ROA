@inproceedings{ ISI:000385791100034,
Author = {Han, Z. and Vanapalli, S. K.},
Editor = {{Chen, Z and Wei, C and Sun, D and Xu, Y}},
Title = {{Resilient modulus - soil suction correlations for two subgrade soils
   from China}},
Booktitle = {{UNSATURATED SOIL MECHANICS-FROM THEORY TO PRACTICE}},
Year = {{2016}},
Pages = {{247-252}},
Note = {{6th Asia-Pacific Conference on Unsaturated Soils, Guilin, PEOPLES R
   CHINA, OCT 23-26, 2015}},
Organization = {{Int Soc Soil Mech \& Geotechn Engn, Tech Comm Unsaturated Soils; Chinese
   Soc Soil Mech \& Geotechn Engn}},
Abstract = {{In recent years, determination and prediction of the resilient modulus
   (MR) soil suction (s) correlations have been encouraged worldwide for
   reliable pavement designs extending mechanistic design methods taking
   account the influence of environmental factors. In this study, the
   constitutive model proposed by Han \& Vanapalli (2015a) is extended to
   predict the non-linear M-R - s correlations for two different subgrade
   soils from China. This constitutive model is formed with only one
   constant model parameter that does not need calibration. In addition,
   all information required for using this model can be determined using
   conventional laboratory testing equipment. There is good agreement
   between the measurements and the predictions of the M-R - s correlations
   for the two soils investigated. The model introduced in this paper is
   simple and can be used in the prediction of the M-R - s correlations for
   compacted cohesive subgrade soils for the rational design of pavements.}},
