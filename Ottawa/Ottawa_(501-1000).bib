@inproceedings{ ISI:000382389302089,
Author = {Xie, Hengheng and Boukerche, Azzedine},
Book-Group-Author = {{IEEE}},
Title = {{Cloud-MERVS: An IoT Cloud-based Error Recovery Video Streaming Scheme}},
Booktitle = {{2015 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
