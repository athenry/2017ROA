@inproceedings{ ISI:000361907500030,
Author = {Bigdeli, Elnaz and Mohammadi, Mandi and Raahemi, Bijan and Matwin, Stan},
Editor = {{Barbosa, D and Milios, E}},
Title = {{Incremental Cluster Updating Using Gaussian Mixture Model}},
Booktitle = {{ADVANCES IN ARTIFICIAL INTELLIGENCE (AI 2015)}},
Series = {{Lecture Notes in Artificial Intelligence}},
Year = {{2015}},
Volume = {{9091}},
