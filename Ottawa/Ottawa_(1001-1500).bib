@inproceedings{ ISI:000380428200046,
Author = {Stinean, Alexandra-Iulia and Bojan-Dragos, Claudia-Adina and Precup,
   Radu-Emil and Preitl, Stefan and Petriu, Emil M.},
Book-Group-Author = {{IEEE}},
Title = {{Takagi-Sugeno PD plus I Fuzzy Control of Processes with Variable Moment
   of Inertia}},
Booktitle = {{2015 INTERNATIONAL SYMPOSIUM ON INNOVATIONS IN INTELLIGENT SYSTEMS AND
   APPLICATIONS (INISTA) PROCEEDINGS}},
Year = {{2015}},
