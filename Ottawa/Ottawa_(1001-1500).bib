@article{ ISI:000347527800022,
Author = {Akbari, Meysam and Biabanifard, Sadegh and Asadi, Shahrouz and Yagoub,
   Mustapha C. E.},
Title = {{High performance folded cascode OTA using positive feedback and
   recycling structure}},
Journal = {{ANALOG INTEGRATED CIRCUITS AND SIGNAL PROCESSING}},
Year = {{2015}},
Volume = {{82}},
Number = {{1, SI}},
Pages = {{217-227}},
Month = {{JAN}},
Note = {{IEEE 4th Latin American Symposium on Circuits and Systems (LASCAS),
   Santiago de Chile, CHILE, FEB 26-28, 2014}},
Organization = {{IEEE}},
Abstract = {{In this paper, a new technique for enhancing the transconductance and
   low frequency output impedance of recycling folded cascode amplifiers is
   presented. These improvements were achieved by using a positive feedback
   and upgrading the recycling structure. The new structure benifits from
   better transconductance, slew rate, and DC gain in comparison to
   conventional folded cascode (FC) amplifier. Moreover, the input referred
   noise and input offset are reduced and the phase-margin is improved. The
   improved amplifier is simulated in 0.18 A mu m CMOS technology,
   demonstrates a DC gain enhancement of 16.4 dB as well as 117.5 MHz
   increase in gain bandwidth compared with conventional FC configuration.
   The amplifier consumes 360 A mu W @ 1.2 V which makes it appropriate for
   low-voltage applications.}},
