@article{ ISI:000340838300005,
Author = {Vanderwel, Christina and Tavoularis, Stavros},
Title = {{On the accuracy of PLIF measurements in slender plumes}},
Journal = {{EXPERIMENTS IN FLUIDS}},
Year = {{2014}},
Volume = {{55}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{The purpose of this article was to assess the measurement uncertainty of
   the planar laser-induced fluorescence (PLIF) method and, as much as
   possible, to devise corrections for predictable biases. More
   specifically, we considered the measurement of concentration maps in
   cross sections parallel to and normal to the axis of a slender plume
   containing Rhodamine 6G as a passive scalar tracer and transported by a
   turbulent shear flow. In addition to previously examined sources of
   error related to PLIF, we also investigated several unexplored ones.
   First, we demonstrated that errors would arise if the laser sheet
   thickness was comparable to or larger than the thickness of the
   instantaneous plume. We then investigated the effect of secondary
   fluorescence, which was attributed to absorption and re-emission of
   primary fluorescence by dye both within and outside the laser sheet. We
   found that, if uncorrected, this effect would contaminate the
   calibration as well as the instantaneous concentration measurements of
   the plume, and proposed methods for the correction of these errors and
   for identifying the instantaneous boundaries of the in-sheet dye
   regions.}},
