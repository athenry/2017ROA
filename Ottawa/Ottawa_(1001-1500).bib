@inproceedings{ ISI:000373628600008,
Author = {Dujmovic, Vida and Eppstein, David and Wood, David R.},
Editor = {{DiGiacomo, E and Lubiw, A}},
Title = {{Genus, Treewidth, and Local Crossing Number}},
Booktitle = {{GRAPH DRAWING AND NETWORK VISUALIZATION, GD 2015}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2015}},
Volume = {{9411}},
