@inproceedings{ ISI:000377207000057,
Author = {Agarwal, Radhika and Nayak, Amiya},
Book-Group-Author = {{IEEE}},
Title = {{DRAP: A Decentralized Public Resourced Cloudlet for Ad-hoc Networks}},
