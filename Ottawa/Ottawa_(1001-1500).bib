@inproceedings{ ISI:000374240900018,
Author = {Stepien, Bernard and Mallur, Kavya and Peyton, Liam},
Editor = {{Fischer, J and Scheidgen, M and Schieferdecker, I and Reed, R}},
Title = {{Testing Business Processes Using TTCN-3}},
