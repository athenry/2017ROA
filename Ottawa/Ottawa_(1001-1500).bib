@article{ ISI:000335625600007,
Author = {Shirazi, Mohammad Mahdi A. and Kargari, Ali and Tabatabaei, Meisam and
   Ismail, Ahmad Fauzi and Matsuura, Takeshi},
Title = {{Concentration of glycerol from dilute glycerol wastewater using sweeping
   gas membrane distillation}},
Journal = {{CHEMICAL ENGINEERING AND PROCESSING}},
Year = {{2014}},
Volume = {{78}},
Pages = {{58-66}},
Month = {{APR}},
Abstract = {{In this work, experimental results for the concentration of dilute
   glycerol wastewater using membrane distillation (MD) with a microporous
   hydrophobic flat-sheet FTFE membrane are reported. Experiments were
   performed using the sweeping gas mode of the MD (SGMD) process. The
   effects of various operating variables, such as feed temperature,
   glycerol concentration in aqueous phase, feed flow rate and sweeping gas
   flow rate were studied. A Taguchi analysis has been performed on the
   experimental results which determined the effects and contribution of
   each of the factors on the distillate flux and the interactions between
   the operating variables. Results showed that the most influential factor
   was feed temperature. The second significant contribution was observed
   for the sweeping gas flow rate. Feed concentration had a negative effect
   on the distillate flux. At optimum conditions (i.e. 65 degrees C, 400
   mL/min, 1 mass\%, and 0.453 Nm(3)/h), the Taguchi model predicted the
   value of the response (the distillate flux) as 20.93 L/m(2) h, which had
   good agreement with the experimental results. (C) 2014 Elsevier B.V. All
   rights reserved.}},
