Author = {Zhou, Xiaoli and Boukerche, Azzedine},
Book-Group-Author = {{IEEE}},
Title = {{An Adaptive Energy-aware MAC Frame Size Scheme in Wireless
   Delay-Tolerant Sensor Networks}},
