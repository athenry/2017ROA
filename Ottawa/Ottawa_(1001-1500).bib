@article{ ISI:000336357400004,
Author = {Yildiz, S. and Groeneveld, D. C.},
Title = {{Diameter effect on supercritical heat transfer}},
Journal = {{INTERNATIONAL COMMUNICATIONS IN HEAT AND MASS TRANSFER}},
Year = {{2014}},
Volume = {{54}},
Pages = {{27-32}},
Month = {{MAY}},
Abstract = {{The objective of this review is to assess and analyze the literature on
   the effect of tube diameter on heat transfer at super-critical (SC)
   pressures. The review is based on SC heat transfer data obtained in
   tubes with a diameter range of 3.18 to 38.1 mm, cooled by carbon
   dioxide, water, R-22, and R-12. The majority of experimental studies
   show that, for the same flow conditions, the heat transfer coefficient
   (HTC) in the `normal' heat transfer mode increases with a decrease in
   tube diameter. Furthermore, it was found that at SC pressures, heat
   transfer is more prone to deteriorate in large tube diameters. In the
   ``deteriorated{''} heat transfer mode, the HTC also appears to decrease
   with an increase in tube diameter. (C) 2014 Elsevier Ltd. All rights
   reserved.}},
