@article{ ISI:000359041700001,
Author = {Baddour, Natalie},
Title = {{Application of the generalized shift operator to the Hankel transform}},
Journal = {{SPRINGERPLUS}},
Year = {{2014}},
Volume = {{3}},
Month = {{MAY 14}},
Abstract = {{It is well known that the Hankel transform possesses neither a
   shift-modulation nor a convolution-multiplication rule, both of which
   have found many uses when used with other integral transforms. In this
   paper, the generalized shift operator, as defined by Levitan, is applied
   to the Hankel transform. It is shown that under this generalized
   definition of shift, both convolution and shift theorems now apply to
   the Hankel transform. The operation of a generalized shift is compared
   to that of a simple shift via example.}},
