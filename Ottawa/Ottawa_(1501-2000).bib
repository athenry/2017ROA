@inproceedings{ ISI:000364200500003,
Author = {Abid, Muhammad R. and Meszaros, Philippe E. and Silva, Ricardo F. D. and
   Petriu, Emil M.},
Book-Group-Author = {{IEEE}},
Title = {{Dynamic Hand Gesture Recognition for Human-Robot and Inter-Robot
   Communication}},
Booktitle = {{2014 IEEE INTERNATIONAL CONFERENCE ON COMPUTATIONAL INTELLIGENCE AND
