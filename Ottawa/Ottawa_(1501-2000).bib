@inproceedings{ ISI:000346516400183,
Author = {Kantarci, Burak and Mouftah, Hussein T.},
Editor = {{Jaworski, M and Marciniak, M}},
Title = {{Resilient Optical Inter-Data-Center Network Design}},
Booktitle = {{2014 16TH INTERNATIONAL CONFERENCE ON TRANSPARENT OPTICAL NETWORKS
   (ICTON)}},
