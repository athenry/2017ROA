@inproceedings{ ISI:000346516400125,
Author = {Mouftah, Hussein T. and Kantarci, Burak},
Editor = {{Jaworski, M and Marciniak, M}},
Title = {{Optical Inter-Data-Center Network Design under Resilience Requirements
   and Dynamic Electricity Pricing}},
Booktitle = {{2014 16TH INTERNATIONAL CONFERENCE ON TRANSPARENT OPTICAL NETWORKS
   (ICTON)}},
Series = {{International Conference on Transparent Optical Networks-ICTON}},
Year = {{2014}},
