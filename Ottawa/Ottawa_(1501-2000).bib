@inproceedings{ ISI:000342631000019,
Author = {Razavi, Amir H. and Inkpen, Diana},
Editor = {{Sokolova, M and VanBeek, P}},
Title = {{Text Representation Using Multi-level Latent Dirichlet Allocation}},
Booktitle = {{ADVANCES IN ARTIFICIAL INTELLIGENCE, CANADIAN AI 2014}},
