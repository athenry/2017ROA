@inproceedings{ ISI:000346083400067,
Author = {Sirmas, Nick and Radulescu, Matei},
Editor = {{Fan, J}},
Title = {{Evolution of Shock Instability in Granular Gases with Viscoelastic
   Collisions}},
Booktitle = {{PROCEEDINGS OF THE 29TH INTERNATIONAL SYMPOSIUM ON RAREFIED GAS DYNAMICS}},
Series = {{AIP Conference Proceedings}},
Year = {{2014}},
Volume = {{1628}},
Pages = {{502-509}},
Note = {{29th International Symposium on Rarefi ed Gas Dynamics (RGD), Xian,
   PEOPLES R CHINA, JUL 13-18, 2014}},
Organization = {{Chinese Acad Sci; Inst Mech, Chinese Acad Sci; State Key Lab High
   temperature Gas Dynam, Minist Sci \& Technol Peoples Republic China;
   Natl Nat Sci Fdn China; Chinese Soc Theoret \& Appl Mech}},
Abstract = {{Shocks in granular media have been shown to develop instabilities. We
   address the role that early stages of shock development have on this
   type of instability. We look at the evolution of shock waves driven by a
   piston in a dilute system of smooth inelastic disks, using both discrete
   particle and continuum modelling. To mimic a realistic granular gas,
   viscoelastic collisions are approximated with an impact velocity
   threshold re needed for inelastic collisions to occur. We show that
   behaviour of the shock evolution is dependent on the ratio of piston
   velocity to impact velocity threshold u(p)/u{*}, and the coefficient of
   restitution epsilon For u(p)/u{*} = 2.0, we recover shock evolution
   behaving similar to that observed in purely inelastic media. This is
   characterized by a short period where the shock front pulls towards the
   piston before attaining a developed structure. No pullback is seen for
   u(p)/u{*} = 1.0. Results show the onset of instability for these
   stronger shocks during this evolving stage. These results suggest that
   the early stages of shock evolution play an important role in the shock
   instability.}},
