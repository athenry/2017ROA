@inproceedings{ ISI:000344116400031,
Author = {Krupin, O. and Wang, C. and Berini, P.},
Editor = {{Maclean, S and Plant, DV}},
Title = {{Strategies for leukemic biomarker detection using long-range surface
   plasmon-polaritons}},
Booktitle = {{PHOTONICS NORTH 2014}},
Series = {{Proceedings of SPIE}},
Year = {{2014}},
Volume = {{9288}},
Note = {{Conference on Photonics North, Montreal, CANADA, MAY 28-30, 2014}},
Organization = {{Univ Laval, Ctr doptique, Photonique Laser; Canadian Photon Ind
   Consortium; Inst Natl dOptique; Quebec Photon Network; Sytacom; Agilent
   Technologies}},
Abstract = {{The suitability and use of long-range surface plasmon-polaritons for
   leukemic biomarker detection is discussed. A novel optical biosensor
   comprised of gold straight waveguides embedded in CYTOP with an etched
   microfluidic channel was tested for detecting leukemia in patient serum.
   Gold surface functionalization involved the interaction of protein G
   (PG) with antibodies by first adsorbing PG on bare gold and then
   antibodies (Immunoglobulin G, IgG). Differentiation between healthy and
   leukemia patients was based on the difference in ratios of Ig kappa (Ig
   kappa) and Ig lambda (Ig lambda) light chains in both serums. The ratio
   for a normal patient is similar to 14 - 2, whereas for a leukemia
