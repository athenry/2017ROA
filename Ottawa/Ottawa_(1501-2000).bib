@inproceedings{ ISI:000342631000023,
Author = {Aref, Abdullah and Tran, Thomas},
Editor = {{Sokolova, M and VanBeek, P}},
Title = {{Using Ensemble of Bayesian Classifying Algorithms for Medical Systematic
   Reviews}},
