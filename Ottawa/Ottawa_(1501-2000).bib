@inproceedings{ ISI:000380512100138,
Author = {Wang, Wenyi and Zhao, Jiying},
Editor = {{Liu, X and ElBaz, D and Hsu, CH and Kang, K and Chen, W}},
Title = {{Chroma-keying based on global weighted sampling and Laplacian
   propagation}},
Booktitle = {{2014 IEEE 17th International Conference on Computational Science and
