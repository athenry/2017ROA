@inproceedings{ ISI:000350281300206,
Author = {Oh, W. T. and Vanapalli, S. K. and Sheikhtaheri, Mohamadjavad},
Editor = {{Khalili, N and Russell, AR and Khoshghalb, A}},
Title = {{Slope stability analysis for Champlain sea clay deposits using the
   mechanics of unsaturated soils}},
Booktitle = {{UNSATURATED SOILS: RESEARCH \& APPLICATIONS, VOLS 1 AND 2}},
Year = {{2014}},
Pages = {{1521-1527}},
Note = {{6th International Conference on Unsaturated Soils (UNSAT), Sydney,
   AUSTRALIA, JUL 02-04, 2014}},
Organization = {{Univ New S Wales; Ctr Infrastructure \& Safety}},
Abstract = {{The instability of the natural slopes in Champlain sea clay deposits of
   Eastern Canada can be attributed to the loss in the contribution of
   suction (i.e. wetting-induced collapse) towards the shear strength
   associated with subsequent increases in positive pore-water pressures in
   the weathered upper crust. The primary objective of this study is to
   propose an approach that can be used to estimate pore-water pressure
   distribution profiles and determine the safety factors of slopes in
   Champlain sea clay deposits extending the mechanics of unsaturated
   soils. To address this objective, seepage and slope stability were
   undertaken respectively using commercial finite element software, SEEP/W
   and SLOPE/W (Geo-Slope 2007) for a natural Champlain sea clay slope. The
   focus of the analysis was to show how the factor of safety of the slope
   decreases with time for a certain rainfall infiltration. The factor of
   safety versus time relationship derived from the study can be used to
   estimate critical pore-water pressure distribution profiles in a natural
   slope in Champlain sea clay deposits.}},
