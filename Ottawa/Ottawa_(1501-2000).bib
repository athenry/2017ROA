@inproceedings{ ISI:000361017900147,
Author = {Koohi, Iraj and Groza, Voicu Z.},
Book-Group-Author = {{IEEE}},
Title = {{Optimizing Particle Swarm Optimization Algorithm}},
Booktitle = {{2014 IEEE 27TH CANADIAN CONFERENCE ON ELECTRICAL AND COMPUTER
   ENGINEERING (CCECE)}},
Series = {{Canadian Conference on Electrical and Computer Engineering}},
Year = {{2014}},
