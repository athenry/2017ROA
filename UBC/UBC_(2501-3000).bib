@article{ ISI:000330417100072,
Author = {Quintero-Torres, Rafael and van Veggel, Frank C. J. M. and Young, Jeff
   F.},
Title = {{Temperature Dependence of Forster Thermalization and Population Decay in
   PbSe Nanocrystals}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2014}},
Volume = {{118}},
Number = {{2}},
Pages = {{1377-1385}},
Month = {{JAN 16}},
Abstract = {{Time-resolved photoluminescence spectroscopy is used to quantify the
   Forster-mediated exciton migration rate and the exciton population decay
   rate in drop-cast solid emulsions of PbSe colloidal nanocrystals
   emitting near 1.5 mu m from 77 to 300 K. For both unimodal and bimodal
   emulsions, at a fixed temperature in a given sample the measured time
   constants that characterize the fast transient decay (blue side of the
   spectrum) and buildup (red side of the spectrum) vary considerably with
   :emission wavelength, but the average fast time constant, similar to 25
   ns, varies little (i.e., within <25\%) With temperature: from 77 to 300
   K. Over the same temperature range, the exciton population lifetime,
   ascribed to nonradiative decay, decreases by similar to 5 times but is
   always longer than the Forster time constant. The increase of the
   Forster-mediated efficacy of exciton redistribution at low temperatures
   before they decay is therefore almost all due to the variation of the
   nonradiative decay rate. By analyzing the temporally resolved and
   steady-state emission spectra, it is noted that the time required for
   the exciton population to equilibrate is non exponential and
   considerably longer (similar to 150 ns) than the rapid decay and buildup
   time constants extracted from the decay curves at fixed wavelengths.}},
