@article{ ISI:000329770000012,
Author = {Rahmani, Amin and Taiebat, Mahdi and Finn, W. D. Liam},
Title = {{Nonlinear dynamic analysis of Melo land Road Overpass using
   three-dimensional continuum modeling approach}},
Journal = {{SOIL DYNAMICS AND EARTHQUAKE ENGINEERING}},
Year = {{2014}},
Volume = {{57}},
Pages = {{121-132}},
Month = {{FEB}},
Abstract = {{This paper presents a three-dimensional (3D) continuum nonlinear
   analysis of the Meloland Road Overpass (MRO) near El Centro, California.
   The modeling methodology and the computational tools are discussed in
   detail. The performance of the computational model is evaluated by
   comparing the computed responses with the responses recorded at the
   bridge site during the 1979 Imperial Valley and 2010 El Mayor-Cucapah
   earthquakes. Amongst the recorded earthquake events at the bridge site,
   these two events caused the strongest shaking. The comparison shows that
   the 3D model is potentially an effective tool for detailed analysis of a
   full bridge system including foundation soils, pile foundations,
   embankments, supporting columns, and the bridge structure itself in a
   unified system without relying on any ancillary models such as Winkler
   springs. Additional response parameters such as displacements, rockings,
   and bending moments are also evaluated although none of these was
   measured during the seismic events. (C) 2013 Elsevier Ltd. All rights
   reserved.}},
