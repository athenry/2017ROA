@article{ ISI:000343129000006,
Author = {Taiebat, Mahdi and Amirzehni, Elnaz and Finn, W. D. L.},
Title = {{Seismic design of basement walls: evaluation of current practice in
   British Columbia}},
Journal = {{CANADIAN GEOTECHNICAL JOURNAL}},
Year = {{2014}},
Volume = {{51}},
Number = {{9}},
Pages = {{1004-1020}},
Month = {{SEP}},
Abstract = {{The current state of practice for the seismic design of basement walls
   in Vancouver is based on the Mononobe-Okabe (M-O) method using a peak
   ground acceleration (PGA) of 0.46g mandated by the National Building
   Code of Canada (NBCC). A series of dynamic numerical analyses are
   conducted to study the seismic performance of basement walls designed
   for the M-O earth pressures derived from different fractions of the
   current building code mandated PGA. The walls are subjected to seven
   ground motions spectrally matched to the uniform hazard spectrum (UHS)
   prescribed by the NBCC for Vancouver. The analyses show that current
   engineering practice for designing basement walls based on the M-O
   method and using a PGA of 0.46g is too conservative. The analyses
   suggest that a wall designed using a 50\%-60\% PGA will give an
   acceptable performance in terms of drift ratio. The results of
   sensitivity analyses on input parameters are also presented and
   discussed.}},
