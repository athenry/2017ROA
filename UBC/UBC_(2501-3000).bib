@article{ ISI:000324022500039,
Author = {Oggier, German G. and Ordonez, Martin and Galvez, Juan M. and Luchino,
   Federico},
Title = {{Fast Transient Boundary Control and Steady-State Operation of the Dual
   Active Bridge Converter Using the Natural Switching Surface}},
Journal = {{IEEE TRANSACTIONS ON POWER ELECTRONICS}},
Year = {{2014}},
Volume = {{29}},
Number = {{2}},
Pages = {{946-957}},
Month = {{FEB}},
Abstract = {{This paper presents a boundary control scheme for dual active bridge
   (DAB) converters using the natural switching surface (NSS). The
   implementation of a curved switching surface for DAB converters is a new
   area of research undertaken in this paper. The proposed technique brings
   the benefit of unprecedented dynamic performance, already developed for
   nonisolated topologies (e. g., buck and boost), to this more complex
   isolated topology. The analysis provides insight into the natural
   trajectories of the DAB converters and creates an accurate framework in
   the normalized geometrical domain. As a result, the physical limits of
   the converter under study become evident. Those physical limits are
   exploited by employing the NSS to obtain fast transient response under
   start-up, sudden load transients, and reference change. In addition,
   fixed-frequency operation is one of the key features of the proposed
   control scheme, which allows optimizing the design of the high-frequency
   transformer. Experimental results are presented to validate the NSS for
   DAB converters and illustrate the benefits of the normalization
   technique.}},
