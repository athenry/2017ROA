@inproceedings{ ISI:000310776204052,
Author = {Shi, Xinmei and Maijer, Daan M. and Dumont, Guy},
Book-Group-Author = {{IEEE}},
Title = {{Nonlinear Identification for Control of Low Pressure Die Casting}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
