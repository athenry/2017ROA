@inproceedings{ ISI:000305070200081,
Author = {Najafi, Mohammad and Afsham, Narges and Abolmaesumi, Purang and Rohling,
   Robert},
Editor = {{Holmes, DR and Wong, KH}},
Title = {{Single Wall Closed-form Differential Ultrasound Calibration}},
Booktitle = {{MEDICAL IMAGING 2012: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
