@inproceedings{ ISI:000320611800136,
Author = {Alasaad, Amr and Shafiee, Kaveh and Gopalakrishnan, Sathish and Leung,
   Victor C. M.},
Book-Group-Author = {{IEEE}},
Title = {{Prediction-based Resource Allocation in Clouds for Media Streaming
   Applications}},
