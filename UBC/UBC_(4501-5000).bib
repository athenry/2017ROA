@inproceedings{ ISI:000312670200055,
Author = {Chen, Xun and Wang, Z. Jane and Mckeown, Martin J.},
Book-Group-Author = {{IEEE}},
Title = {{A Tridirectional Method for Corticomuscular Coupling Analysis in
   Parkinson's Disease}},
