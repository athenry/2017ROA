Editor = {{Free, M and Moats, M and Houlachi, G and Asselin, E and Allanore, A and Yurko, J and Wang, S}},
Title = {{INVESTIGATION OF CHARGE TRANSFER RESISTANCE AT PYRITE ELECTRODES
   MODIFIED BY GOLD AND SILVER NANOPARTICLES}},
Booktitle = {{ELECTROMETALLURGY 2012}},
Year = {{2012}},
