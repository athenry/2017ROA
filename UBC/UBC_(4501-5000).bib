@article{ ISI:000306666300006,
Author = {Desapriya, Ediriweera and Hewapathirane, D. Sesath and Romilly, Douglas
   P. and White, Marc I.},
Title = {{A Brief Educational Program Improves Awareness Regarding Whiplash
   Prevention Among a Cohort of Vehicle Fleet Managers in British Columbia,
   Canada}},
Journal = {{TRAFFIC INJURY PREVENTION}},
Year = {{2012}},
Volume = {{13}},
Number = {{4}},
Pages = {{373-377}},
Abstract = {{Objective: Previous research indicates that most vehicle occupants are
   unaware that a correctly adjusted, well-designed vehicular head
   restraint provides substantial protection against whiplash injuries.
   This study examined whether a brief educational intervention could
   improve awareness regarding whiplash injuries and prevention strategies
   among a cohort of vehicle fleet managers.
   Methods: A brief written survey was administered prior to, and
   approximately 1 h after a 30-min presentation on whiplash injury and
   prevention measures, which was delivered at a regional fleet manager
   meeting held in British Columbia, Canada (n = 27 respondents).
   Results: Respondents had low baseline knowledge levels regarding the
   causes, consequences, and prevention of whiplash. Following the
   presentation, however, respondents improved awareness in all of these
   domains and, most important, reported an increased motivation to
   implement changes based on this newly acquired knowledge.
   Conclusions: These results indicate that improved education practices
   and social marketing tools are potentially valuable to increase
   awareness among relevant stakeholders.}},
