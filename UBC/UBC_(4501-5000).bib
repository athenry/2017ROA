@inproceedings{ ISI:000312381401150,
Author = {Mai, Zicong and Nasiopoulos, Panos and Ward, Rabab K.},
Book-Group-Author = {{IEEE}},
Title = {{COMPUTATIONALLY EFFICIENT TONE-MAPPING OF HIGH-BIT-DEPTH VIDEO IN THE
   YCBCR DOMAIN}},
Booktitle = {{2012 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
Series = {{International Conference on Acoustics Speech and Signal Processing
   ICASSP}},
