@article{ ISI:000303353600001,
Author = {Faal, R. T. and Amiri, M. B. and Pirmohammadi, A. A. and Milani, A. S.},
Title = {{Vibration analysis of undamped, suspended multi-beam absorber systems}},
Journal = {{MECCANICA}},
Year = {{2012}},
Volume = {{47}},
Number = {{5}},
Pages = {{1059-1078}},
Month = {{JUN}},
Abstract = {{The vibration analysis of an Euler-Bernoulli beam with an attached
   rotary unit is first carried out assuming no unbalance. For comparison
   purposes, two different beam end boundary conditions are considered: a
   simply-supported and a clamped-clamped condition. The problem is then
   extended to the vibration behavior of the initial beam when subjected to
   a harmonic load due to an unbalance in the rotary unit. To absorb the
   ensuing vibrations, a secondary passive beam system is suspended from
   the primary beam which consists of two continuous leaf springs and three
   discrete masses. The absorption frequency is obtained by exploring the
   deflection norm of the primary beam versus dimensionless frequencies of
   the system. To ensure the appropriateness of the procedure for similar
   multi-beam absorber systems, an experimental set-up is established and
   analytical results are verified.}},
