@inproceedings{ ISI:000327200407064,
Author = {Gharehshiran, Omid Namvar and Krishnamurthy, Vikram and Yin, George},
Book-Group-Author = {{IEEE}},
Title = {{Tracking Equilibria with Markovian Evolution}},
Booktitle = {{2012 IEEE 51ST ANNUAL CONFERENCE ON DECISION AND CONTROL (CDC)}},
