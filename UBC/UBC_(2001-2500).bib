@article{ ISI:000340850800006,
Author = {Chen, Fan and Lu, Xiaodong and Altintas, Yusuf},
Title = {{A novel magnetic actuator design for active damping of machining tools}},
Journal = {{INTERNATIONAL JOURNAL OF MACHINE TOOLS \& MANUFACTURE}},
Year = {{2014}},
Volume = {{85}},
Pages = {{58-69}},
Month = {{OCT}},
Abstract = {{Parts and cutting tools with large structural flexibility experience
   both forced and chatter vibrations during machining, resulting in poor
   surface finish or damage to the machine. This paper presents the design
   principles of a novel 3 degrees of freedom linear magnetic actuator
   which increases the damping and static stiffness of flexible structures
   during machining. The proposed actuator can deliver 248 N force in two
   radial (x, y) directions and 34 N x m (torque) in torsional (theta)
   direction up to 850 Hz. The force and torque reduces to 107 N and 14.5 N
   x m at 2000 Hz, hence it can actively damp a wide range of structural
   modes. The magnetic force is linearized with respect to the input
   current using magnetic configuration design strategy. Loop shaping
   controllers are designed for active damping of boring bar vibrations.
   The static and dynamic stiffnesses of the boring bar were considerably
   increased with the designed actuator, leading to a significant increase
   in chatter-free material removal rates during cutting tests. (C) 2014
   Elsevier Ltd. All rights reserved.}},
