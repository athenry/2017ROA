@article{ ISI:000352142800007,
Author = {Wang, Sheng-Nan and Zhu, Xin-Qiao and Yan, Xiao-Hui and Deng, Jing-Fei
   and Wang, Rizhi and Wang, Xiao-Xiang},
Title = {{Nanostructured individual nacre tablet: a subtle designed
   organic-inorganic composite}},
Journal = {{CRYSTENGCOMM}},
Year = {{2015}},
Volume = {{17}},
Number = {{15}},
Pages = {{2964-2968}},
Abstract = {{From natural evolution, nacre, a biogenic organic-inorganic composite,
   fulfils an ingenious combination of remarkable strength and toughness.
   Its basic building block, the individual nacre tablet, assumes an
   important mechanical function. Unravelling its design strategies can
   hence provide essential information for the pursuit of high-performance
   artificial composites. Here, the nanostructure inside the individual
   nacre tablet (Pinctada maxima) was investigated using transmission
   electron microscopy techniques. Results show that organic matter is
   trapped inside the tablet in both an islet-like and sheet-like manner.
   More interestingly, the crystal structure of aragonite scaffold is
   continuous and homodromous throughout the whole tablet even in this
   case. A biomineralization mechanism of the tablet is discussed based on
   this finding.}},
