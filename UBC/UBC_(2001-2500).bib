@article{ ISI:000365403000024,
Author = {Zachkani, P. and Jackson, J. K. and Pirmoradi, F. N. and Chiao, M.},
Title = {{A cylindrical magnetically-actuated drug delivery device proposed for
   minimally invasive treatment of prostate cancer}},
Journal = {{RSC ADVANCES}},
Year = {{2015}},
Volume = {{5}},
Number = {{119}},
Pages = {{98087-98096}},
Abstract = {{A cylindrically shaped magnetically-actuated MEMS
   (microelectromechanical systems) drug delivery device for localized
   prostate cancer treatment is proposed in this work. The device is
   designed for implantation through a gauge 12 needle for minimally
   invasive procedures. The drug delivery device consists of a drug
   reservoir, a PDMS (polydimethylsiloxane) membrane, a magnetic block and
   a housing. Under external magnetic fields, the movement of the magnetic
   block deflects the membrane and discharges the drug through an aperture
   and into the housing. The housing has a large opening which allows the
   released drug to diffuse to the surrounding tissues while it prevents
   the tissues from touching the membrane. On-demand drug release with
   consistent release rates and device implantation using a needle into ex
   vivo porcine bladder tissue are demonstrated.}},
