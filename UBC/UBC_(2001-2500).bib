@inproceedings{ ISI:000366206800050,
Author = {Nouranian, Saman and Ramezani, Mahdi and Spadinger, Ingrid and Morris,
   William J. and Salcudean, Septimiu E. and Abolmaesumi, Purang},
Editor = {{Navab, N and Hornegger, J and Wells, WM and Frangi, AF}},
Title = {{Automatic Prostate Brachytherapy Preplanning Using Joint Sparse Analysis}},
Booktitle = {{MEDICAL IMAGE COMPUTING AND COMPUTER-ASSISTED INTERVENTION - MICCAI
   2015, PT II}},
