Author = {Noel, Adam and Cheung, Karen C. and Schober, Robert},
Book-Group-Author = {{IEEE}},
Title = {{Multi-Scale Stochastic Simulation for Diffusive Molecular Communication}},
Booktitle = {{2015 IEEE INTERNATIONAL CONFERENCE ON COMMUNICATIONS (ICC)}},
