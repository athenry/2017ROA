Author = {Zhu, Chunsheng and Sheng, Zhengguo and Leung, Victor C. M. and Shu, Lei
   and Yang, Laurence T.},
Title = {{Toward Offering More Useful Data Reliably to Mobile Cloud From Wireless
   Sensor Network}},
