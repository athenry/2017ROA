@article{ ISI:000359495400027,
Author = {Lee, ByungBog and Kim, Se-Jin},
Title = {{Energy-Efficient Sensor Device Personalization Scheme for the Internet
   of Things and Wireless Sensor Networks}},
Journal = {{IEICE TRANSACTIONS ON COMMUNICATIONS}},
Year = {{2015}},
Volume = {{E98B}},
Number = {{1}},
Pages = {{231-241}},
Month = {{JAN}},
Abstract = {{In this paper, we propose a novel energy-efficient sensor device
   management scheme called sensor device personalization (SDP) for the
   Internet of things (IoT) and wireless sensor networks (WSNs) based on
   the IEEE 802.15.4 unslotted carrier sense multiple access with collision
   avoidance (CSMA/CA). In the IoT and WSNs with the star topology, a
   coordinator device (CD), user devices (UDs), and sensor devices (SDs)
   compose a network, and the UDs such as smart phones and tablet PCs
   manage the SDs, which consist of various sensors and communication
   modules, e.g., smart fridge, robot cleaner, heating and cooling system,
   and so on, through the CD. Thus, the CD consumes a lot of energy to
   relay packets between the UDs and the SDs and also has a longer packet
   transmission delay. Therefore, in order to reduce the energy consumption
   and packet transmission delay, in the proposed SDP scheme, the UDs
   obtain a list of SD profiles (including SDs' address information) that
   the UDs want to manage from the CD, and then the UDs and the SDs
   directly exchange control messages using the addresses of the SDs.
   Through analytical models, we show that the proposed SDP scheme
   outperforms the conventional scheme in terms of normalized throughput,
   packet transmission delay, packet loss probability, and total energy
   consumption.}},
