Author = {Gallardo, Ricardo Ochoa and Hu, Alan J. and Ivanov, Andre and Mirian,
   Maryam S.},
Book-Group-Author = {{IEEE}},
Title = {{Reducing Post-Silicon Coverage Monitoring Overhead with Emulation and
   Bayesian Feature Selection}},
