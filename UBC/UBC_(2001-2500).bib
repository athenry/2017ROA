@inproceedings{ ISI:000375684100053,
Author = {Lozano, Hanni and Ito, Mabo},
Editor = {{Smari, WW}},
Title = {{A Reduced Complexity Instruction Set Architecture for Low Cost Embedded
   Processors}},
