@article{ ISI:000337879400029,
Author = {Aziznia, Amin and Oloman, Colin W. and Gyenge, Elod L.},
Title = {{Experimental advances and preliminary mathematical modeling of the
   Swiss-roll mixed-reactant direct borohydride fuel cell}},
