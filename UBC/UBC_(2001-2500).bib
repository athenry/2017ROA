Title = {{Deep Learning for Automatic Localization, Identification, and
   Segmentation of Vertebral Bodies in Volumetric MR Images}},
Booktitle = {{MEDICAL IMAGING 2015: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
Series = {{Proceedings of SPIE}},
