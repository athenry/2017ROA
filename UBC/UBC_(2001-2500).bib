@inproceedings{ ISI:000375091800029,
Author = {Wafa, A. and Nasiopoulos, P. and Leung, V. C. and Pourazad, Mahsa T.},
Book-Group-Author = {{IEEE}},
Title = {{Automatic Real-time 2D-to-3D Conversion for scenic views}},
