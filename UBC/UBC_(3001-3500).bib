@article{ ISI:000317159300014,
Author = {Jalaal, M. and Khorshidi, B. and Esmaeilzadeh, E.},
Title = {{Electrohydrodynamic (EHD) mixing of two miscible dielectric liquids}},
Journal = {{CHEMICAL ENGINEERING JOURNAL}},
Year = {{2013}},
Volume = {{219}},
Pages = {{118-123}},
Month = {{MAR 1}},
Abstract = {{A novel method is presented for mixing of two miscible dielectric
   liquids using D.C. electric field where Electrohydrodynamic (EHD) forces
   are used to increase the interface instability and fluids circulations
   in an enclosure, resulting to rapid dispersion of phases. Experiments
   were provided for a range of electrical potential (from 0 to 30 kV) and
   a Digital CCD camera is used to record the experimental observations.
   Results are analyzed, processing the images based on color intensities.
   The experimental results show that employing electric field is effective
   in providing a force able to cause instability on liquid-liquid
   interface and generate internal flows, significantly increasing the
   efficiency of mixing for the liquid-liquid system. The current results
   assist to step forward in design of EHD mixers, which are noteworthy,
