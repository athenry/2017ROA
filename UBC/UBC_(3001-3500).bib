@article{ ISI:000322682800088,
Author = {Islam, Md. Aminul and Farhat, Zoheir N. and Ahmed, Elsadig M. and
   Alfantazi, A. M.},
Title = {{Erosion enhanced corrosion and corrosion enhanced erosion of API X-70
   pipeline steel}},
Journal = {{WEAR}},
Year = {{2013}},
Volume = {{302}},
Number = {{1-2, SI}},
Pages = {{1592-1601}},
Month = {{APR-MAY}},
Abstract = {{Erosion-corrosion is common in oil and gas processing plants and
   pipelines where there is interaction between solid particles, corrosive
   fluid and target material. In this study, the synergism between erosion
   and corrosion of API pipeline steel has been assessed under liquid-solid
   impingement conditions in 2 g/l NaCl solution purged with CO2 as the
   corrosive media and aluminum oxide as the erodent. The total material
   loss rate and the components of erosion, corrosion and their synergistic
   interactions have been determined. A test setup was designed to mitigate
   the limitations of the in situ method (to avoid the fluctuation in
   electrochemical data due to bubble formation and turbulence). The flow
   velocity was varied from 36 to 81 m s(-1), while angle of incidence was
   kept constant at 90 degrees. The experimental results show that there is
   an immense correlation exists between erosion and corrosion. It was
   observed that erosion enhances corrosion and corrosion enhances erosion
   with each contributing to significant synergism. (C) 2013 Elsevier B.V.
   All rights reserved.}},
