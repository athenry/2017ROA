@article{ ISI:000319420800009,
Author = {Tomlinson, P. and Azizi-Alizamini, H. and Poole, W. J. and Sinclair, C.
   W. and Gharghouri, M. A.},
Title = {{Biaxial Deformation of the Magnesium Alloy AZ80}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS A-PHYSICAL METALLURGY AND
   MATERIALS SCIENCE}},
