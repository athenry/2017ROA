@article{ ISI:000317538100050,
Author = {Danon-Schaffer, Monica N. and Mahecha-Botero, Andres and Grace, John R.
   and Ikonomou, Michael},
Title = {{Transfer of PBDEs from e-waste to aqueous media}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
Year = {{2013}},
Volume = {{447}},
Pages = {{458-471}},
Month = {{MAR 1}},
Abstract = {{Experiments and analysis were carried out by contacting e-waste with
   distilled water and leachate from a major urban landfill. Contacting of
   crushed e-waste from different eras with leachate in a custom built
   end-over-end contactor led to appreciable mass transfer of
   Polybrominated diphenyl ethers (PBDEs) to the aqueous phase. Temperature
   had limited influence for the limited range (10-25 degrees C)
   investigated. Lower pH (in the range 4 to 9) generally resulted in
   higher transfer of PBDEs to the aqueous phase. Exposing e-waste to
   distilled water in the same contactor led to lower, but still
   appreciable PBDE analysed concentrations, than for leachate, probably
   due to dislodgement of fine dust from the surface of the e-waste
   particles. (C) 2012 Elsevier B.V. All rights reserved.}},
