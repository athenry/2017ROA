@article{ ISI:000315588100006,
Author = {Fletcher, M. and Siebert-Timmer, A. and Bichler, L. and Sediako, D.},
Title = {{Evolution of Strain and Microstructure During Creep of Wrought AE42 and
   ZE10 Magnesium Alloys}},
Journal = {{TRANSACTIONS OF THE INDIAN INSTITUTE OF METALS}},
Year = {{2013}},
Volume = {{66}},
Number = {{2}},
Pages = {{133-140}},
Month = {{APR}},
Abstract = {{Wrought magnesium alloys have been extensively used in the aerospace,
   electronics and automotive industries, where component weight is of
   concern and ambient temperatures remain below 100 A degrees C.
   Undesirable creep relaxation of the wrought alloys above this
   temperature has been generally attributed to grain boundary sliding and
   plastic deformation leading to intergranular failure. The objective of
   this study was to investigate the compressive creep performance and
   microstructure of two wrought magnesium alloys (AE42 and ZE10) developed
   for high temperature applications. The total deformation of the AE42 and
   ZE10 alloys was 2.4 and 0.2 \%, respectively, after 24 h creep test at
   175 A degrees C and 50 MPa. The poor creep performance of the AE42 alloy
   was explained via neutron diffraction studies which revealed that the
   elastic compressive response of planes was significantly more
   anisotropic in the AE42 than in the ZE10 alloy. Further, microstructural
   analysis revealed similar to 10 \% increase in grain size due to creep,
   with additional and twinning in the AE42 alloy. Precipitation of
   beta-Mg17Al12 phase in the AE42 alloy possibly contributed to grain
   boundary sliding and high plastic strain during creep testing.}},
