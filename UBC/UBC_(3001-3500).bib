@article{ ISI:000322332600027,
Author = {Arti, M. K. and Mallik, Ranjan K. and Schober, Robert},
Title = {{Beamforming and Combining in Two-Way AF MIMO Relay Networks}},
Journal = {{IEEE COMMUNICATIONS LETTERS}},
Year = {{2013}},
Volume = {{17}},
Number = {{7}},
Pages = {{1400-1403}},
Month = {{JUL}},
Abstract = {{In this letter, we propose a beamforming and combining scheme for a
   two-way amplify-and-forward (AF) multiple-input multiple-output (MIMO)
   relay network. An approximate expression for the symbol error rate of
   the scheme in Rayleigh fading for M-ary phase-shift keying is obtained
   based on the moment generating function of the instantaneous received
   signal-to-noise ratio. The diversity order is also analytically derived.
   It is shown by simulations that the proposed scheme outperforms existing
   schemes for beamforming and combining in two-way AF MIMO relay systems.
   We also discuss beamforming and combining in a two-way AF MIMO relay
   system with estimated channel gains.}},
