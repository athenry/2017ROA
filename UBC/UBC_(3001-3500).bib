@inproceedings{ ISI:000329611503099,
Author = {Palangi, Hamid and Ward, Rabab and Deng, Li},
Book-Group-Author = {{IEEE}},
Title = {{USING DEEP STACKING NETWORK TO IMPROVE STRUCTURED COMPRESSED SENSING
   WITH MULTIPLE MEASUREMENT VECTORS}},
Booktitle = {{2013 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
Series = {{International Conference on Acoustics Speech and Signal Processing
   ICASSP}},
Year = {{2013}},
