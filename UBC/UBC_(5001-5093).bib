@article{ ISI:000300789500003,
Author = {Chen, Min and Leung, Victor C. M. and Shu, Lei and Chao, Han-Chieh},
Title = {{On multipath balancing and expanding for wireless multimedia sensor
   networks}},
Journal = {{INTERNATIONAL JOURNAL OF AD HOC AND UBIQUITOUS COMPUTING}},
Year = {{2012}},
Volume = {{9}},
Number = {{2}},
Pages = {{95-103}},
Abstract = {{Multiple disjointed paths have been demonstrated to be effective in
   delivering multimedia traffic in Wireless Sensor Networks (WSNs), and
   improving the network performance in terms of bandwidth aggregation,
   reliability and network lifetime. In this paper, we investigate the use
   of Directional Geographical Routing (DGR) for multipath construction for
   multimedia data dissemination, and identify the challenging issue of
   achieving multipath balancing in proximity to the source/sink. The path
   construction is divided into expanding phase, parallel phase and
   converging phase in the proposed scheme, which includes two key
   algorithms, i.e., the detection algorithm for path construction phase
   and the deviation angle adjustment algorithm. Simulation results verify
   the effectiveness of the proposed scheme.}},
