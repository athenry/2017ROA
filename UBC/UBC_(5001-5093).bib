@inproceedings{ ISI:000371316700076,
Author = {Baghani, Ali and Eskandari, Hani and Wang, Weiqi and Da Costa, Daniel
   and Lathiff, Mohamed Nabil and Sahebjavaher, Ramin and Salcudean,
   Septimiu and Rohling, Robert},
