@inproceedings{ ISI:000392521000364,
Author = {Severin, J. and Eberhardt, E. and Leoni, L. and Fortin, S.},
Editor = {{Qian, Q and Zhou, Y}},
Title = {{Use of ground-based synthetic aperture radar to investigate the complex
   3-D kinematics of a large open pit slope}},
Booktitle = {{HARMONISING ROCK ENGINEERING AND THE ENVIRONMENT}},
