@article{ ISI:000376885800013,
Author = {Shirazi, Amir Hossein Masnadi and Nikpaik, Amir and Molavi, Reza and
   Lightbody, Sam and Djahanshahi, Hormoz and Taghivand, Mazhareddin and
   Mirabbasi, Shahriar and Shekhar, Sudip},
Title = {{On the Design of mm-Wave Self-Mixing-VCO Architecture for High
   Tuning-Range and Low Phase Noise}},
Journal = {{IEEE JOURNAL OF SOLID-STATE CIRCUITS}},
Year = {{2016}},
Volume = {{51}},
Number = {{5}},
Pages = {{1210-1222}},
Month = {{MAY}},
Note = {{IEEE Radio Frequency Integrated Circuits Symposium (RFIC), Phoenix, AZ,
   MAY 17-19, 2015}},
Organization = {{IEEE; MTT S; Electron Devices Soc; SSCS}},
Abstract = {{Frequency synthesis at mm-wave range suffers from a severe tradeoff
   between phase noise (PN) and frequency tuning range (FTR). This work
   presents the analysis and compares the performance of fundamental-mode
   voltage-controlled oscillators (F-VCOs) to harmonic-mode VCOs (H-VCOs).
   It is shown that unlike a mm-wave F-VCO, an H-VCO can simultaneously
   achieve higher FTR and lower PN. An H-VCO architecture, denoted as
   self-mixing VCO (SMV), is presented where the VCO core generates both
   the first (f(0)) and second harmonic (2f(0)) and then mixes them
   together to obtain the desired mm-wave thirdharmonic (3f(0)). Use of a
   Class-C push-push topology as the VCO core enhances the second-harmonic
   content to improve mixing efficiency, decreases parasitic capacitance,
   and improves PN. Compared to an F-VCO operating in a mm-wave band at a
   fundamental frequency that equals 3f(0), the proposed SMV architecture
   achieves about 2x higher FTR and a better PN performance. A 52.8-62.5
   GHz SMV prototype is designed and implemented in a 0.13 mu mCMOS
   process. Measurement results show that the VCO achieves an FTR of 16.8\%
   with a PN of -100.6 dBc/Hz at 1 MHz offset-resulting in an FTR-inclusive
   figure-of-merit (FoMT) of -190.85 dBc/Hz while consuming 7.6 mW from a
   1.2 V supply.}},
