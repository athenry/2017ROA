@article{ ISI:000385600100094,
Author = {Islam, Md. Jahidul and Meherier, Md. Salamah and Islam, A. K. M. Rakinul},
Title = {{Effects of waste PET as coarse aggregate on the fresh and harden
   properties of concrete}},
Journal = {{CONSTRUCTION AND BUILDING MATERIALS}},
Year = {{2016}},
Volume = {{125}},
Pages = {{946-951}},
Month = {{OCT 30}},
Abstract = {{This study investigates the effect of plastic as an alternative coarse
   aggregate on various fresh and harden properties of concrete.
   Polyethylene terephthalate (PET), a thermoplastic polymer, is considered
   as an alternative aggregate and replaced with natural coarse aggregate,
   such as brick chips. The PET aggregate is obtained by shredding, melting
   and crushing the collected waste PET bottles. The primary focus of the
   work is to observe compressive strength and unit weight of PET aggregate
   concrete (PAC) along with their workability in comparison with the
   natural aggregate concrete (NAC). With the increase in PET replacement
   ratio and w/c ratio lower unit weights and compressive strengths are
   observed for PAC compare to NAC. Compressive strength for 20\% PET
