@article{ ISI:000370879200006,
Author = {Nunoo, Samuel and Tannant, Dwayne D. and Newcomen, H. Warren},
Title = {{Slope monitoring practices at open pit porphyry mines in British
   Columbia, Canada}},
Journal = {{INTERNATIONAL JOURNAL OF MINING RECLAMATION AND ENVIRONMENT}},
Year = {{2016}},
Volume = {{30}},
Number = {{3}},
Pages = {{245-256}},
Month = {{MAY 3}},
Abstract = {{A survey of pit wall monitoring practices was conducted at all large
   open pit copper-molybdenum mines in British Columbia. The survey found
   that these mines tend to rely on visual inspection and prism surveys as
   the primary techniques for pit wall management. Open standpipe
   piezometers and wire-line extensometers are also used. Newer monitoring
   technologies are slow to be adopted by the mines. One exception is slope
   stability radar, which is used at two mines. All mines have established
   movement thresholds that trigger different operational responses. These
   thresholds do not appear to be based on the pit depth or rock mass
   conditions.}},
