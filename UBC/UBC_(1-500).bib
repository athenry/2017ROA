@article{ ISI:000379751000002,
Author = {Yang, Luanxia and Zhu, Bingcheng and Cheng, Julian and Holzman, Jonathan
   F.},
Title = {{Free-Space Optical Communications Using ON-OFF Keying and Source
   Information Transformation}},
Journal = {{JOURNAL OF LIGHTWAVE TECHNOLOGY}},
Year = {{2016}},
Volume = {{34}},
Number = {{11}},
Pages = {{2601-2609}},
Month = {{JUN 1}},
Abstract = {{Free-space optical communication using ON-OFF keying (OOK) and source
   information transformation is proposed. It is shown that the source
   information transformation allows the proposed system to detect the OOK
   signal without requiring the knowledge of instantaneous channel state
   information and the probability density function (pdf) of the turbulence
   model. Analytical expressions are derived for the pdf of the detection
   threshold, and an upper bound is obtained on the average bit error rate
   (BER). Numerical studies show that the proposed system can achieve
   comparable performance to the idealized adaptive detection system, with
   a greatly reduced level of implementation complexity and a
   signal-to-noise ratio performance loss of only 1.8 dB at a BER of 1 x
   10(-9) for a lognormal turbulence channel with sigma = 0.25.}},
