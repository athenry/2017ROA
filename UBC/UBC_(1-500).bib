@article{ ISI:000381593600006,
Author = {Kaya, Yavuz and Dowling, Jason},
Title = {{Application of Timoshenko beam theory to the estimation of structural
   response}},
Journal = {{ENGINEERING STRUCTURES}},
Year = {{2016}},
Volume = {{123}},
Pages = {{71-76}},
Month = {{SEP 15}},
Abstract = {{Multi-story buildings are usually instrumented at a limited number of
   floors. Consequently, the structural response (e.g., acceleration,
   displacement) at the non-instrumented floors remains unknown and needs
   to be estimated using the acceleration data recorded at the instrumented
   floors. The prevailing way to estimate the unknown structural response
   is to interpolate the recorded data over the height of the building.
   Other methods such as the Mode Shape Based Estimation (MSBE) and
   Timoshenko Beam Based Estimation (TBBE) methods, on the other hand, use
   the mode shapes of bending, shear and Timoshenko beam modeling to
   estimate the unknown structural response. The Factor Building at the
   UCLA campus in Los Angeles, California, USA is utilized to test the
