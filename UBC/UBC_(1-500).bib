@article{ ISI:000380067700031,
Author = {Dehghani, Soroush and Johnson, Thomas},
Title = {{A 2.4-GHz CMOS Class-E Synchronous Rectifier}},
Journal = {{IEEE TRANSACTIONS ON MICROWAVE THEORY AND TECHNIQUES}},
Year = {{2016}},
Volume = {{64}},
Number = {{5}},
Pages = {{1655-1666}},
Month = {{MAY}},
Abstract = {{A class-E synchronous rectifier has been designed and implemented using
   0.13-mu m CMOS technology. A design methodology based on the theory of
   time-reversal duality has been used where a class-E amplifier circuit is
   transformed into a class-E rectifier circuit. The methodology is
   distinctly different from other CMOS RF rectifier designs which use
   voltage multiplier techniques. Power losses in the rectifier are
   analyzed including saturation resistance in the switch, inductor losses,
   and current/voltage overlap losses. The rectifier circuit includes a
   50-Omega single-ended RF input port with on-chip matching. The circuit
   is self-biased and completely powered from the RF input signal.
   Experimental results for the rectifier show a peak RF-to-dc conversion
   efficiency of 30\% measured at a frequency of 2.4 GHz.}},
