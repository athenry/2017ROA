@article{ ISI:000390731700039,
Author = {Born, Brandon and Krupa, Jeffrey D. A. and Geoffroy-Gagnon, Simon and
   Hristovski, Ilija R. and Collier, Christopher M. and Holzman, Jonathan
   F.},
Title = {{Ultrafast Charge-Carrier Dynamics of Copper Oxide Nanocrystals}},
Journal = {{ACS PHOTONICS}},
Year = {{2016}},
Volume = {{3}},
Number = {{12}},
Pages = {{2475-2481}},
Month = {{DEC}},
Abstract = {{In this work, a detailed investigation is carried out on copper oxide,
   in the form of cupric oxide (CuO) nanocrystals. Particular attention is
   paid to the bandstructure and ultrafast charge-carrier dynamics.
   Transient absorption spectroscopy is carried out with an above-bandgap
   pump beam and below-bandgap probe beam to glean insight on the
   relaxation and recombination dynamics of the CuO nano crystals at
   various pump fluences. Three time constants are apparent. The first time
   constant varies with pump fluence from 330 to 630 fs, and it is
   attributed to momentum relaxation via carrier carrier scattering in the
   valence band as well as exciton exciton annihilation. The second time
   constant is constant at 2 ps, and it is attributed to energy relaxation
   via carrier-phonon scattering within the valence band. The third time
   constant is constant at SO ps, and it is attributed to trapping and
   recombination, due to the high density of trap states within the CuO
   nanocrystals. Such findings lay the foundation for future studies and
   applications of the emerging CuO material system.}},
