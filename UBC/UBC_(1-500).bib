@article{ ISI:000385600100009,
Author = {Sala, Elisa and Zanotti, Cristina and Passoni, Chiara and Marini,
   Alessandra},
Title = {{Lightweight natural lime composites for rehabilitation of Historical
   Heritage}},
Journal = {{CONSTRUCTION AND BUILDING MATERIALS}},
Year = {{2016}},
Volume = {{125}},
Pages = {{81-93}},
Month = {{OCT 30}},
Abstract = {{Failure and deterioration of structural interventions on masonry
   buildings demonstrated the need for compatible repairs. Mechanical,
   chemical, transport properties and density of Portland Cement concrete
   are poorly compatible with lime mortar masonry structures.
   Several natural lime composites are developed in this study for
   compatible Historical Heritage rehabilitation. Two relevant
   interventions with different strength/density requirements are
   considered: masonry vault filling and wooden floor non-structural
   overlay. Density minimization is attained with various lightweight
   aggregate (LWA) types/contents; different LWAs in one same mortar are
   also employed. Composites' density, strength, stiffness, cost, and
   carbon footprint are compared; the influence of different factors is
   discussed. (C) 2016 Elsevier Ltd. All rights reserved.}},
