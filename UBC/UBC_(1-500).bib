@article{ ISI:000384332000014,
Author = {Roustaei, A. and Chevalier, T. and Talon, L. and Frigaard, I. A.},
Title = {{Non-Darcy effects in fracture flows of a yield stress fluid}},
Journal = {{JOURNAL OF FLUID MECHANICS}},
Year = {{2016}},
Volume = {{805}},
Pages = {{222-261}},
Month = {{OCT}},
Abstract = {{We study non-inertial flows of single-phase yield stress fluids along
   uneven/rough-walled channels, e.g. approximating a fracture, with two
   main objectives. First, we re-examine the usual approaches to providing
   a (nonlinear) Darcy-type flow law and show that significant errors arise
   due to self-selection of the flowing region/fouling of the walls. This
   is a new type of non-Darcy effect not previously explored in depth.
   Second, we study the details of flow as the limiting pressure gradient
   is approached, deriving approximate expressions for the limiting
   pressure gradient valid over a range of different geometries. Our
   approach is computational, solving the two-dimensional Stokes problem
   along the fracture, then upscaling. The computations also reveal
   interesting features of the flow for more complex fracture geometries,
   providing hints about how to extend Darcy-type approaches effectively.}},
