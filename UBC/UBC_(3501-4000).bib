
@inproceedings{ ISI:000327357800289,
Author = {Zhang, Linjuan and Gao, Deyun and Gao, Shuai and Leung, Victor C. M.},
Book-Group-Author = {{IEEE}},
Title = {{SmartGeocast: Dynamic Abnormal Traffic Information Dissemination to
   Multiple Regions in VANET}},
