@article{ ISI:000315171100012,
Author = {Ghahremaninezhad, A. and Dixon, D. G. and Asselin, E.},
Title = {{Electrochemical and XPS analysis of chalcopyrite (CuFeS2) dissolution in
   sulfuric acid solution}},
Journal = {{ELECTROCHIMICA ACTA}},
Year = {{2013}},
Volume = {{87}},
Pages = {{97-112}},
Month = {{JAN 1}},
Abstract = {{The influence of potential on the species and surface films formed
   during electrodissolution of chalcopyrite were investigated in 0.5 M
   sulfuric acid solution at 25 degrees C. Cyclic voltammetry,
   chronoamperometry, potentiostatic and potentiodynamic polarization
   methods were applied. X-ray diffraction (XRD) and X-ray photoelectron
   spectroscopy (XPS) were used to analyze the minerals and product layers
   on the surface before and after electrodissolution. The chalcopyrite
   electrode was shown to be passive for potentials up to 0.90 V-SHE. Above
   this potential, transpassive dissolution occurs. Results of XPS studies
   have suggested that a metal-deficient sulfide film (Cu1-xFe1-yS2-z)
   consisting of cuprous sulfide (Cu-S) and iron sulfide (Fe-S) bonds, is
   the most plausible copper and iron containing sulfide phase which
   passivates the surface of chalcopyrite. It is demonstrated that the
   transpassive dissolution of chalcopyrite is significantly linked to
   oxidation of sulfur (from sulfide in the passive film to elemental
   sulfur and maybe sulfur species with higher oxidation states, e.g.
   thiosulfate). No elemental sulfur or polysulfide species were detected
   on the surface for potentials below 0.90 V-SHE. (C) 2012 Elsevier Ltd.
   All rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
