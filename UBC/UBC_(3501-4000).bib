Title = {{The Effect of Calcination Temperature on the Properties and
   Hydrodeoxygenation Activity of Ni2P Catalysts Prepared Using Citric Acid}},
Booktitle = {{NOVEL MATERIALS FOR CATALYSIS AND FUELS PROCESSING}},
Series = {{ACS Symposium Series}},
