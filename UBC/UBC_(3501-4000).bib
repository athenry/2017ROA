Author = {Shirzad, Navid and Van der Loos, H. F. Machiel},
Book-Group-Author = {{IEEE}},
Title = {{Adaptation of Task Difficulty in Rehabilitation Exercises Based on the
   User's Motor Performance and Physiological Responses}},
