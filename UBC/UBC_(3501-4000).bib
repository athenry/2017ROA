@article{ ISI:000308417000024,
Author = {Schaap, Allison and Chu, Winnie C. and Stoeber, Boris},
Title = {{Transport of airborne particles in straight and curved microchannels}},
Journal = {{PHYSICS OF FLUIDS}},
Year = {{2012}},
Volume = {{24}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{The measurement of airborne particles is important for environmental and
   exposure monitoring. Microfluidic technologies present potential
   advantages for aerosol monitoring but have been applied very little to
   the handling of airborne particles. In this paper, we examine the flow
   focusing and cross-streamline diffusion of aerosols in straight
   microchannels, and the size-based lateral displacement of aerosols
   caused by centrifugal forces in a curved channel. We present
   calculations, simulations, and experimental results verifying the
   models: measurements of the focusing and diffusion of 0.2 mu m and 0.75
   mu m particles in straight channels and of the size-dependent lateral
   displacement of particles between 0.2 mu m and 2 mu m in curved channels
   are demonstrated and shown to match well with the simulations. We
   observe lateral dispersion of the particles: particles closer to the top
   and bottom wall of the channel experience less lateral displacement than
   particles near the center due to the flow velocity distribution across
   the channel cross section. These results confirm that the microchannel
   techniques presented are a viable method for the size-based manipulation
   of airborne particles. (C) 2012 American Institute of Physics.
   {[}http://dx.doi.org/10.1063/1.4742900]}},
