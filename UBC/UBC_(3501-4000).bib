@inproceedings{ ISI:000326442500008,
Author = {Hung, Eddie and Eslami, Fatemeh and Wilton, Steven J. E.},
Book-Group-Author = {{IEEE}},
Title = {{Escaping the Academic Sandbox: Realizing VPR Circuits on Xilinx Devices}},
Booktitle = {{2013 IEEE 21ST ANNUAL INTERNATIONAL SYMPOSIUM ON FIELD-PROGRAMMABLE
   CUSTOM COMPUTING MACHINES (FCCM)}},
Year = {{2013}},
