@article{ ISI:000312836900023,
Author = {Seo, Jun-Bae and Leung, Victor C. M.},
Title = {{Performance Modeling and Stability of Semi-Persistent Scheduling with
   Initial Random Access in LTE}},
Journal = {{IEEE TRANSACTIONS ON WIRELESS COMMUNICATIONS}},
Year = {{2012}},
Volume = {{11}},
Number = {{12}},
Pages = {{4446-4456}},
Month = {{DEC}},
Abstract = {{In this paper we examine the feasibility of semi-persistent scheduling
   (SPS) for voice over IP (VoIP) by random access and evaluate its
   performance in terms of throughput of random access and traffic
   channels, and random access delay. We further investigate system
   stability issues and present methods to stabilize the system. To see the
   VoIP capacity gain, we show the maximum number of acceptable VoIP
   terminals without exceeding some front-end packet dropping (i.e., voice
   clipping) probability. In addition, we examine the effect of the
   parameter called implicit release after in the LTE standard on the
   system performance, which is used for silence period detection. Our
   performance evaluation model based on Equilibrium Point Analysis is
   compared to simulations.}},
