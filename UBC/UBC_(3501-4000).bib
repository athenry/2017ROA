@article{ ISI:000316562800045,
Author = {Eliyan, Faysal Fayez and Mahdi, El-Sadig and Alfantazi, Akram},
Title = {{Investigating the Corrosion of API-X100 Pipeline Steel in Aerated
   Carbonate Solutions by Electrochemical Methods}},
Journal = {{INTERNATIONAL JOURNAL OF ELECTROCHEMICAL SCIENCE}},
Year = {{2013}},
Volume = {{8}},
Number = {{1}},
Pages = {{578-590}},
Month = {{JAN}},
Abstract = {{This paper explores some key electrochemical aspects associated to the
   corrosion of API-X100 steel in aerated carbonate solutions. Investigated
   by polarization and electrochemical impedance spectroscopy tests, the
   corrosion rates and potentials, the cathodic reactions, and passivation
   are studied from the perspectives of carbonate content and temperature.
   The corrosion rates, more sensitive to temperature, were between 12 and
   18 mu A cm(-2), and the corrosion potentials decreased with carbonate
   content and increased with temperature. And reported for the first time,
   bicarbonate, which resulted from the CO32-/H2O equilibrium, contributed
