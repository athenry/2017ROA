Title = {{Gestures for Industry Intuitive Human-Robot Communication from Human
   Observation}},
Booktitle = {{PROCEEDINGS OF THE 8TH ACM/IEEE INTERNATIONAL CONFERENCE ON HUMAN-ROBOT
   INTERACTION (HRI 2013)}},
Series = {{ACMIEEE International Conference on Human-Robot Interaction}},
