@article{ ISI:000309303700005,
Author = {Eliyan, Faysal Fayez and Mohammadi, Farzad and Alfantazi, Akram},
Title = {{An electrochemical investigation on the effect of the chloride content
   on CO2 corrosion of API-X100 steel}},
Journal = {{CORROSION SCIENCE}},
Year = {{2012}},
Volume = {{64}},
Pages = {{37-43}},
Month = {{NOV}},
Abstract = {{This research argues that the corrosion rate for high strength steel in
   CO2-saturated solutions reaches a maximum at a threshold chloride
   content. At 20 degrees C, the corrosion rates increased from 60 mu
   A/cm(2) in 5 g/L solutions to a maximum of 185 mu A/cm(2) in 15 g/L
   solutions, and then became less in the more concentrated solutions. The
   anodic dissolution accelerated at 90 degrees C and the cathodic
   reactions decelerated with adding 10 vol.\% oil where the chloride
   content effect remained relatively the same. Some EIS experiments
   demonstrated an agreement with the polarization results without
   confirming that threshold effect of chloride on charge transfer. (C)
   2012 Elsevier Ltd. All rights reserved.}},
