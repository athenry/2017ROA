@article{ ISI:000351278800023,
Author = {Khan, Farid and Stoeber, Boris and Sassani, Farrokh},
Title = {{Modeling of linear micro electromagnetic energy harvesters with
   nonuniform magnetic field for sinusoidal vibrations}},
Journal = {{MICROSYSTEM TECHNOLOGIES-MICRO-AND NANOSYSTEMS-INFORMATION STORAGE AND
   PROCESSING SYSTEMS}},
