@article{ ISI:000352145400004,
Author = {Pourjafar, Mohammad and Chaparian, Emad and Sadeghy, Kayvan},
Title = {{Taylor-Couette instability of thixotropic fluids}},
Journal = {{MECCANICA}},
Year = {{2015}},
Volume = {{50}},
Number = {{6}},
Pages = {{1451-1465}},
Month = {{JUN}},
Abstract = {{Taylor-Couette instability of thixotropic fluids obeying Moore model is
   numerically investigated for the case in which the inner cylinder is
   rotating and the outer cylinder is fixed. A linear temporal stability
   analysis is invoked in which infinitesimally-small perturbations,
   represented by normal modes, are superimposed to the base flow and their
   time evolution is followed in order to detect the onset of instability.
   An eigenvalue problem is obtained which is solved numerically using
   finite-difference scheme coupled with the ``polyeig{''} function of the
   Matlab software. The neutral instability curve is plotted as a function
   of the thixotropic parameters of the Moore model for a broad range of
   parameters. Based on the results obtained in this work, it is concluded
   that the viscosity ratio in the Moore model has a destabilizing effect
   on circular Couette flow. On the other hand, an increase in the
   breakdown-to-buildup ratio in the Moore model is predicted to have a
   stabilizing or destabilizing effect on circular Couette flow depending
   on the gap size and also the magnitude of the thixotropy ratio.}},
