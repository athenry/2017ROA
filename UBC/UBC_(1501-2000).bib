@article{ ISI:000351279200021,
Author = {Ye, Zhitong and Su, Liushuai and Takahata, Kenichi and Su, Yufeng},
Title = {{FEA analysis and working performance of micro vibration energy harvester
   based on diamagnetic levitation}},
Journal = {{MICROSYSTEM TECHNOLOGIES-MICRO-AND NANOSYSTEMS-INFORMATION STORAGE AND
   PROCESSING SYSTEMS}},
Year = {{2015}},
Volume = {{21}},
Number = {{4}},
Pages = {{903-909}},
Month = {{APR}},
Abstract = {{A micro vibration energy harvester, based on diamagnetic levitation, is
   presented in this paper. It consists of a lifting permanent magnet, an
   upper pyrolytic graphite sheet, a floating permanent magnet, a lower
   pyrolytic graphite sheet and a copper coil on the two pyrolytic graphite
   sheets respectively. The gravity of the floating permanent magnet is
   balanced by the attractive force between the lifting magnet and the
   floating magnet. The diamagnetic force exerted on the floating permanent
   magnet by the two pyrolytic graphite sheets works as restoring force,
   which makes the floating permanent magnet return to its equilibrium
   position. By Ansoft Maxwell, comparative analysis between the attractive
   magnetic force and the diamagnetic force was carried out, so as to
   obtain appropriate key parameters of the structure. The maximum range of
   movement for the floating permanent magnet is 1.3 mm. The first resonant
   frequency is 11.3 Hz, at which the output voltage of the energy
   harvester is 98.18 mV. By eliminating mechanical contact during its
   movement, the energy harvester improves the efficiency of vibration
   energy harvesting, and it can provide sustainable power for
   micro-electro-mechanical system (MEMS).}},
