@inproceedings{ ISI:000380614500042,
Author = {Seidalinov, G. and Taiebat, M.},
Editor = {{Oka, F and Murakami, A and Uzuoka, R and Kimoto, S}},
Title = {{Propagation of seismic waves through saturated soft clay deposits:
   Constitutive and numerical modeling}},
Booktitle = {{Computer Methods and Recent Advances in Geomechanics}},
Year = {{2015}},
Pages = {{285-290}},
Note = {{Computer Methods and Recent Advances in Geomechanics, KYOTO, JAPAN, SEP
   22-25, 2014}},
Abstract = {{Soft clay deposits are known to potentially affect the character of
   seismic ground motion by amplifying it and making more destructive. A
   reliable seismic site response analysis is required to quantify these
   affects. An essential element of a reliable site response analysis is a
   constitutive model that can realistically describe the stress-strain
   response of the soil deposits and capture the effects of initial and
   evolving anisotropy, proper degradation of strength, and reduction of
   shear modulus during cyclic loading. When such representative models are
   robustly implemented in a nonlinear effective stress numerical platform
   they can be used to properly simulate the seismic wave propagation
   through clay deposits. A recent bounding surface extension of the
   SANICLAY family of models possesses most of the features required for
   cyclic response of clays, and provides a realistic accumulation of
   plastic shear strains, and a representative simulation of modulus
   reduction and damping ratio curves during successive loading cycles. The
   model, named SANICLAY-B, is implemented in the effective stress fully
   coupled nonlinear dynamic finite element program OpenSees. The superior
   features of the SANICLAY-B model, in comparison to the original SANICLAY
   model, have been illustrated in numerical simulation of propagation of
   seismic waves through saturated soft clay deposits.}},
