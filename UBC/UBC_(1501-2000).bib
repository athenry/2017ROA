@article{ ISI:000354890800005,
Author = {Bichler, L. and Ravindran, C.},
Title = {{Investigations on the Stress and Strain Evolution in AZ91D Magnesium
   Alloy Castings During Hot Tearing}},
Journal = {{JOURNAL OF MATERIALS ENGINEERING AND PERFORMANCE}},
Year = {{2015}},
Volume = {{24}},
Number = {{6, SI}},
Pages = {{2208-2218}},
Month = {{JUN}},
Note = {{Symosium on Innovation in Processing of Light Metals for Transportation
   Industries in honor of Prof. C. (Ravi) Ravindran held during Materials
   Science and Technology Meeting (MS\&T), Pittsburgh, PA, OCT 12-16, 2014}},
Abstract = {{Hot tearing in magnesium and aluminum alloys has been rigorously
   investigated in the past decades. To date, the interactions between the
   solidification parameters, microstructural development, and stress and
   strain at the onset of hot tearing in magnesium alloys remain unclear.
   In particular, the stress and strain conditions required to nucleate hot
   tears are unknown. In this research, ex situ neutron diffraction
   residual strain mapping was carried out on AZ91D magnesium alloy
   castings at the onset of hot tearing. The results indicate that tensile
   strain alone was not sufficient to nucleate a hot tear. A minimum
   threshold tensile stress of similar to 12 MPa was necessary to open
   interdendritic shrinkage pores into hot tears and enable their
   propagation. Further, deformation along (10 (1) over bar1)and (10 (1)
   over bar2) crystallographic planes played a dominant role on the
   high-temperature deformation and hot tearing in the AZ91D alloy.}},
