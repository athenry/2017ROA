@article{ ISI:000357438100054,
Author = {Islam, Md. Aminul and Alam, Tahrim and Farhat, Zoheir N. and Mohamed,
   Adel and Alfantazi, Akram},
Title = {{Effect of microstructure on the erosion behavior of carbon steel}},
