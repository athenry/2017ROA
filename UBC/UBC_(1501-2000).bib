@inproceedings{ ISI:000380411600111,
Author = {Guo, Jing and Zhao, Nan and Yu, F. Richard and Li, Ming and Leung,
   Victor C. M.},
Book-Group-Author = {{IEEE}},
Title = {{A Novel Anti-Jamming Scheme for Interference Alignment (IA)-Based
   Wireless Networks}},
Booktitle = {{2015 IEEE/CIC INTERNATIONAL CONFERENCE ON COMMUNICATIONS IN CHINA (ICCC)}},
Year = {{2015}},
Note = {{2015 IEEE/CIC International Conference on Communications in China
   (ICCC), Shenzhen, PEOPLES R CHINA, NOV 02-04, 2015}},
Organization = {{IEEE}},
Abstract = {{Interference alignment (IA) is a prospective technique of interference
   management in wireless networks, and jamming is a potential threat for
   security, which will degrade the performance of IA networks severely.
   However, the jamming issue is simply ignored in the existing IA studies.
   In this paper, we propose a novel anti-jamming scheme for the IA-based
   network, in which there exists an adversarial jammer outside the network
   intending to interrupt the transmission of IA users. In the proposed
   scheme, the jamming and interferences are constrained into the same
   subspace at each receiver, and thus the jamming signal can be eliminated
   perfectly along with interferences. To achieve this goal, more antennas
   should be equipped or fewer data streams can be transmitted in the
   network. Thus the feasibility conditions of the proposed scheme are
   derived in the cases of one and more streams for each user,
   respectively. Simulation results are presented to show the effectiveness
   of the proposed scheme.}},
