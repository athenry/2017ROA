@inproceedings{ ISI:000380561200147,
Author = {Zhou, Li and Hu, Xiping and Zhu, Chunsheng and Ngai, Edith C. -H. and
   Wang, Shan and Wei, Jibo and Leung, Victor C. M.},
Book-Group-Author = {{IEEE}},
Title = {{Green Small Cell Planning in Smart Cities under Dynamic Traffic Demand}},
