@inproceedings{ ISI:000348029400071,
Author = {Khallaghi, Siavash and Nouranian, Saman and Sojoudi, Samira and Ashab,
   Hussam A. and Machan, Lindsay and Chang, Silvia and Black, Peter and
   Gleave, Martin and Goldenberg, Larry and Abolmaesumi, Purang},
Editor = {{Yaniv, ZR and Holmes, DR}},
Title = {{Motion and Deformation Compensation for Freehand Prostate Biopsies}},
Booktitle = {{MEDICAL IMAGING 2014: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
Series = {{Proceedings of SPIE}},
