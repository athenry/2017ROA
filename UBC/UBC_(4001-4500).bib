@inproceedings{ ISI:000347686400058,
Author = {Brosch, Tom and Yoo, Youngjin and Li, David K. B. and Traboulsee,
   Anthony and Tam, Roger},
Editor = {{Golland, P and Hata, N and Barillot, C and Hornegger, J and Howe, R}},
Title = {{Modeling the Variability in Brain Morphology and Lesion Distribution in
   Multiple Sclerosis by Deep Learning}},
Booktitle = {{MEDICAL IMAGE COMPUTING AND COMPUTER-ASSISTED INTERVENTION - MICCAI
   2014, PT II}},
