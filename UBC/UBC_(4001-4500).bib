@inproceedings{ ISI:000341390300038,
Author = {Taherinejad, Nima and Lampe, Lutz and Mirabbasi, Shahriar},
Book-Group-Author = {{IEEE}},
Title = {{Adaptive Impedance Matching for Vehicular Power Line Communication
   Systems}},
