Author = {Miao, Shun and Wang, Z. Jane and Liao, Rui},
Book-Group-Author = {{IEEE}},
Title = {{NON-PARAMETRIC ORTHOGONAL SLICE TO VOLUME DEFORMABLE REGISTRATION:
   APPLICATION TO PET/MR RESPIRATORY MOTION COMPENSATION}},
Booktitle = {{2014 IEEE CHINA SUMMIT \& INTERNATIONAL CONFERENCE ON SIGNAL AND
   INFORMATION PROCESSING (CHINASIP)}},
