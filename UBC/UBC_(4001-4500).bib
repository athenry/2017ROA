Editor = {{Butts, J and Shenoi, S}},
Title = {{REINFORCEMENT LEARNING USING MONTE CARLO POLICY ESTIMATION FOR DISASTER
   MITIGATION}},
Booktitle = {{CRITICAL INFRASTRUCTURE PROTECTION VIII}},
Series = {{IFIP Advances in Information and Communication Technology}},
Year = {{2014}},
