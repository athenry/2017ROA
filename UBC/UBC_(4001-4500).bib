@inproceedings{ ISI:000348029400073,
Author = {Rasoulian, Abtin and Rohlin, Robert N. and Abolmaesumi, Purang},
Editor = {{Yaniv, ZR and Holmes, DR}},
Title = {{Automatic Labeling and Segmentation of Vertebrae in CT Images}},
Booktitle = {{MEDICAL IMAGING 2014: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
