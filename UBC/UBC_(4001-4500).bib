@inproceedings{ ISI:000345985300055,
Author = {Hamdi, P. and Stead, D. and Elmo, D.},
Editor = {{Alejano, R and Perucho, A and Olalla, C and Jimenez, R}},
Title = {{Characterizing the influence of micro-heterogeneity on the strength and
   fracture of rock using an FDEM-mu DFN approach}},
Booktitle = {{ROCK ENGINEERING AND ROCK MECHANICS: STRUCTURES IN AND ON ROCK MASSES}},
Year = {{2014}},
Pages = {{359-364}},
Note = {{ISRM European Symposium, Vigo, SPAIN, MAY 26-29, 2014}},
Organization = {{ISRM; Spanish Soc Rock Mech; Univ Vigo, Sch Min Engn}},
Abstract = {{Heterogeneity is often an integral part of composite materials such as
   rocks and concrete and may present in different forms including
   porosity, weak grain boundaries and microcracks. Microcracks are often
   present in crystalline rocks in two forms: natural and stress-induced;
   the amount of stress-induced microcracking increasing with depth and
   magnitude of tectonic stresses. In this study, the finite-discrete
   element method(FDEM) code ELFEN is used to model
   microcrack-heterogeneity by introducing sets of microcrack using the
   discrete fracture network (DFN) approach into the simulated intact
   medium. The applicability of this method in rock mechanics is then
   evaluated through a series of numerical simulations of uniaxial/biaxial
   compressive laboratory tests. The results of the FDEM-mu DFN models
   indicate that micro-heterogeneity has a profound influence on both the
   mechanical behaviour and resulting fracture pattern. An increase in the
   microcrack population leads to a reduction in the strength of the
   laboratory specimen and changes the character of the strength envelope.}},
