@article{ ISI:000327767400046,
Author = {Hejazi, Bijan and Grace, John R. and Bi, Xiaotao and Mahecha-Botero,
   Andres},
Title = {{Steam gasification of biomass coupled with lime-based CO2 capture in a
   dual fluidized bed reactor: A modeling study}},
