Title = {{Supporting Storage Configuration for I/O Intensive Workflows}},
Booktitle = {{PROCEEDINGS OF THE 28TH ACM INTERNATIONAL CONFERENCE ON SUPERCOMPUTING,
   (ICS'14)}},
