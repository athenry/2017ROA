@article{ ISI:000342682700003,
Author = {Yenjaichon, Wisarn and Grace, John R. and Lim, C. Jim and Kerekes,
   Richard J. and Bennington, Chad P. J.},
Title = {{Mixing quality in low consistency fibre suspensions downstream of an
   in-line mechanical mixer measured by electrical resistance tomography}},
Journal = {{NORDIC PULP \& PAPER RESEARCH JOURNAL}},
Year = {{2014}},
Volume = {{29}},
Number = {{3}},
Pages = {{392-400}},
Abstract = {{In-line mixers, combined with chemical injectors, are commonly used in
   pulp bleaching processes to pre-distribute chemicals in pulp suspensions
   ahead of tower reactors to achieve efficient lignin removal. This work
   investigated uniformity of liquid/gas in pulp suspensions downstream of
   an in-line mechanical mixer for 0.5 - 3.0\% wt. softwood and hardwood
   kraft pulp suspensions over a wide range of operating conditions.
   Electrical resistance tomography (ERT) was used to quantify mixing based
   on two novel mixing indices. For liquid mixing, jet penetration,
   mainstream velocity and impeller rotation speed were key parameters. Jet
   penetration to the axis of the pipe provided optimum mixing. Although
   mixing improved substantially with increasing mainstream velocity,
   increasing impeller speed at high mainstream velocity did not improve
   mixing significantly. Sufficient residence time in the high-shear zone
   around the impeller was required to improve mixing with increasing
   impeller speed. Gas mixing depended greatly on gas volume fraction,
   impeller speed and buoyancy. Optimum mixing was obtained at high
   superficial liquid/pulp velocity and at low gas volume fraction to
   prevent formation of a gas cavity around the impeller.}},
