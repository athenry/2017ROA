@inproceedings{ ISI:000380460100058,
Author = {Abdelhadi, Ameer M. S. and Lemieux, Guy G. F.},
Editor = {{Chen, JL and Yin, WB and Shibata, Y and Wang, LL and So, HKH and Ma, YC}},
Title = {{Deep and Narrow Binary Content-Addressable Memories using FPGA-based
   BRAMs}},
Booktitle = {{PROCEEDINGS OF THE 2014 INTERNATIONAL CONFERENCE ON FIELD-PROGRAMMABLE
   TECHNOLOGY (FPT)}},
