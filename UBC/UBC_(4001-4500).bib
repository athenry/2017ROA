Author = {Miao, Shun and Liao, Rui and Moran, Gerald and Butler, John and Pan, Li
   and Wang, Z. Jane},
Book-Group-Author = {{IEEE}},
Title = {{Dynamic MR-based Respiratory Motion Compensation for Hybrid PET/MR
   System}},
Booktitle = {{PROCEEDINGS OF THE 2014 9TH IEEE CONFERENCE ON INDUSTRIAL ELECTRONICS
   AND APPLICATIONS (ICIEA)}},
