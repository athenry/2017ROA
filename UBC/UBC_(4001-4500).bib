@article{ ISI:000324971000005,
Author = {Zebardast, H. R. and Rogak, S. and Asselin, E.},
Title = {{Kinetics of decomposition of hydrogen peroxide on the surface of
   magnetite at high temperature}},
Journal = {{JOURNAL OF ELECTROANALYTICAL CHEMISTRY}},
Year = {{2013}},
Volume = {{705}},
Pages = {{30-36}},
Month = {{SEP 15}},
Abstract = {{A novel electrochemical method was employed to accurately measure the
   kinetics of H2O2 decomposition on the surface of magnetite at elevated
   temperatures and pressures. Electrochemical impedance spectroscopy was
   used to probe the decay of H2O2 concentration by tracking the charge
   transfer resistance on the surface of a platinum electrode. It was found
   that the decomposition kinetics follow a first-order rate law with
   respect to H2O2 concentration and vary from 8.1 x 10(-4) to 7.9 x 10(-2)
   min(-1) when the temperature increases from 298 to 473 K (for a 0.2 g
   L-1 magnetite suspension). The mean calculated activation energy of 30
   +/- 1 kJ mol(-1) showed that H2O2 decomposition on the surface of
   magnetite particles is a chemically controlled process. (C) 2013
   Elsevier B.V. All rights reserved.}},
