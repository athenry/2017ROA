@inproceedings{ ISI:000351930400019,
Author = {Garcin, Thomas and Schmitt, Jean-Hubert and Militzer, Matthias},
Editor = {{Guedou, JY and Chone, J}},
Title = {{Application of laser ultrasonics to monitor microstructure evolution in
   Inconel 718 superalloy}},
