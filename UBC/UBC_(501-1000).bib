@article{ ISI:000389191200004,
Author = {Wicaksono, A. T. and Militzer, M. and Sinclair, C. W.},
Title = {{A molecular dynamics study of the effect of helium clusters on grain
   boundary migration in bcc iron}},
Journal = {{PHILOSOPHICAL MAGAZINE}},
Year = {{2016}},
Volume = {{96}},
Number = {{36, A}},
Pages = {{3746-3774}},
Abstract = {{The kinetic interplay between helium (He) segregation, He cluster
   formation and curvature-driven grain boundary migration in bcc iron
   (alpha-Fe) has been investigated using molecular dynamics simulations.
   He atoms that segregate to the migrating boundary are found to be
   trapped in vacant substitutional sites emitted by the migrating
   boundary. He atoms that form clusters in the bulk restrain the boundary
   migration via a pinning mechanism. The pinning pressure of He clusters
   is proportional to the density and the squared radius of each cluster. A
   cluster pinning model has been developed by taking into account the
   two-fold effect of clusters on the boundary migration: (1) reducing the
   boundary mobility and (2) acting as pinning objects that delay or even
   completely halt the boundary migration. The model is found to be in
   agreement with the simulation results.}},
