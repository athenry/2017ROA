@inproceedings{ ISI:000403950600049,
Author = {Liao, Lingxia and Leung, Victor C. M.},
Editor = {{Limam, N and Cheriet, M and Zhani, MF and Festor, O and KeithMarsoun, S and DosSantos, CRP}},
Title = {{LLDP Based Link Latency Monitoring in Software Defined Networks}},
Booktitle = {{2016 12TH INTERNATIONAL CONFERENCE ON NETWORK AND SERVICE MANAGEMENT AND
   WORKSHOPS(CNSM 2016)}},
Series = {{International Conference on Network and Service Management}},
Year = {{2016}},
Pages = {{330-335}},
Note = {{12th IEEE/IFIP/ACM International Conference on Network and Service
   Management (CNSM), Ecole Technologie Superiere, Montreal, CANADA, OCT
   31-NOV 04, 2016}},
Organization = {{ACM; ACM SIGCOMM; IEEE; IFIP; IEEE Commun Soc; Distributed Management
   Task Force Inc; IBM; IEEE Software Defined Networks; IEEE Green ICT;
   IEEE Big Data}},
Abstract = {{Current latency monitoring approaches for Software Defined Network often
   use the control plane as the infrastructure to inject time-stamp data
   packets as probe packets to measure the network latency at a particular
   time, but suffer from three major issues when the network latency needs
   to be continuously monitored: 1) the increased control plane's overhead,
   2) the feasibility of using data packets as probe packets, and 3) the
   increasing measurement error using OpenFlow messages to measure the time
   from the controller to a switch as the network scale grows. To overcome
   these issues, this paper proposes link latency monitoring using
   time-stamped Link Layer Discovery Protocol (LLDP) packets, aided by a
   linear calibration function to reduce errors of measuring
   switch-controller delays. Time stamping LLDP packets, which are used to
   discover the global network topology in SDNs, does not add extra
   workload to the control plane and the results always reach the
   controller thus while avoiding measurement failures that might occur in
   existing approaches. Our linear calibration function can reduce the
   measurement error to less than 5\% of the link latency measured by ping
   in a network with up to 30 switches and the link latency not less than
   1ms.}},
