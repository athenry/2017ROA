@article{ ISI:000372351900050,
Author = {Siddiqua, Poppy and O'Leary, Stephen K.},
Title = {{The steady-state and transient electron transport within bulk
   zinc-blende indium nitride: The impact of crystal temperature and doping
   concentration variations}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2016}},
Volume = {{119}},
Number = {{9}},
Month = {{MAR 7}},
Abstract = {{Within the framework of a semi-classical three-valley Monte Carlo
   electron transport simulation approach, we analyze the steady-state and
   transient aspects of the electron transport within bulk zinc-blende
   indium nitride, with a focus on the response to variations in the
   crystal temperature and the doping concentration. We find that while the
   electron transport associated with zinc-blende InN is highly sensitive
   to the crystal temperature, it is not very sensitive to the doping
   concentration selection. The device consequences of these results are
   then explored. (C) 2016 AIP Publishing LLC.}},
