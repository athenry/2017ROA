@article{ ISI:000370876500046,
Author = {Shao, Hua and Jing, Wenpeng and Wen, Xiangming and Lu, Zhaoming and
   Zhang, Haijun and Chen, Yawen and Ling, Dabing},
Title = {{Joint Optimization of Quality of Experience and Power Consumption in
   OFDMA Multicell Networks}},
Journal = {{IEEE COMMUNICATIONS LETTERS}},
Year = {{2016}},
Volume = {{20}},
Number = {{2}},
Pages = {{380-383}},
Month = {{FEB}},
Abstract = {{Quality of experience (QoE) and power consumption are two important
   considerations of OFDMA multicell networks. In this letter, we propose a
   fair QoE-based radio resource allocation (FQRA) for OFDMA multicell
   networks. Different from the previous work, the multiuser QoE per power
   consumption is introduced as the performance metric. A novel utility
   function, which can characterize the fairness of users' QoE and power
   consumption jointly, is proposed. Then a multiuser FQRA problem is
   formulated. Due to the nonconvex form of optimization, a two-step
   solution is devised in which the subchannel allocation and power
   allocation are performed iteratively. Simulation results show that our
   method can significantly improve the QoE per power consumption of cells
   and outperform the state-of-art schemes.}},
