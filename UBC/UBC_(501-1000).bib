@article{ ISI:000368207700022,
Author = {Sadeghifar, Hamidreza and Djilali, Ned and Bahrami, Majid},
Title = {{A compact closed-form Nusselt formula for laminar longitudinal flow
   between rectangular/square arrays of parallel cylinders with unequal row
   temperatures}},
Journal = {{INTERNATIONAL JOURNAL OF THERMAL SCIENCES}},
Year = {{2016}},
Volume = {{100}},
Pages = {{248-254}},
Month = {{FEB}},
Abstract = {{Axial flows over cylinders are frequently encountered in practice, e.g.
   in tubular heat exchangers and reactors. Using the Integral method,
   closed-form relationships are developed for heat transfer coefficients
   or Nusselt number inside a fluid flowing axially between a
   rectangular/square array of parallel cylinders with unequal
   temperatures. The model considers the temperature variations of
   cylinders from one row to another while assuming the same temperature
   for all the cylinders in each row. The model could well capture several
   sets of numerical data, which can be regarded as excellent in light of
   the simplicity and comprehensiveness of the model. The compact and
   accurate formulae developed in this work can be readily employed, and
   also implemented into any software or tools, for the estimation of Nu in
   tubular heat exchangers, fins systems, porous media and composite
   manufacturing. (C) 2015 Elsevier Masson SAS. All rights reserved.}},
