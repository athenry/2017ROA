@article{ ISI:000369168900009,
Author = {Starke, Christina and Berube, Pierre R.},
Title = {{Reuse of Air for Return Activated Sludge Pumping in Submerged Membrane
   Bioreactors}},
Journal = {{CHEMIE INGENIEUR TECHNIK}},
Year = {{2016}},
Volume = {{88}},
Number = {{1-2}},
Pages = {{70-76}},
Month = {{FEB}},
Abstract = {{The reuse of air added for gas sparging to airlift return activated
   sludge (RAS) in a membrane bioreactor (MBR) was investigated. Numerical
   modeling suggests that the RAS conveyance requirements in an MBR could
   be met for systems with a treatment capacity of 3.810(6)Ld(-1), and
   potentially for larger MBRs, depending on the system design (i.e.,
   U-shaped rather than linear systems). Pilot scale validation confirmed
   that airlift could be used to meet the RAS conveyance requirements.
   Optimal conditions were obtained for pilot scale systems with square air
   collectors, intermediate/large air collector angles, greater depth of
   submergence and individual tubing to redirect the airlifted flow.
   Implementation of airlift RAS conveyance in small to intermediate size
   MBRs can results in a reduction of approximately 20\% in operating
   costs.}},
