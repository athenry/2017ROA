@inproceedings{ ISI:000382313300023,
Author = {Yang, Zhen and Abulnaga, S. Mazdak and Carass, Aaron and Kansal, Kalyani
   and Jedynak, Bruno M. and Onyike, Chiadi and Ying, Sarah H. and Prince,
   Jerry L.},
