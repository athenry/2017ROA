@article{ ISI:000371417000002,
Author = {Han, Sang-wook and Kim, Hoon and Han, Youngnam and Cioffi, John M. and
   Leung, Victor C. M. and Zander, Jens},
Title = {{Cooperative multiple access in cognitive radios to enhance QoS for cell
   edge primary users: asynchronous algorithm and convergence}},
Journal = {{TELECOMMUNICATION SYSTEMS}},
Year = {{2016}},
Volume = {{61}},
Number = {{4}},
Pages = {{631-643}},
Month = {{APR}},
Note = {{6th Joint IFIP Wireless and Mobile Networking Conference (WMNC),
   Canadian Univ Dubai, Dubai, U ARAB EMIRATES, APR 23-25, 2013}},
Organization = {{IFIP; IEEE; IEEE Commun Soc; IFIP TC6}},
Abstract = {{Cognitive radio (CR) systems have been proposed for efficient usage of
   spare spectrum licensed to primary systems. This leads to the issue of
   providing as much spectrum to CR users as possible while not degrading
   the quality of service (QoS) of primary users of the spectrum. This
   paper proposes a novel cooperation scheme between primary and CR users
   to guarantee QoS of primary users up to the cell edge while making the
   licensed spectrum available for opportunistic access by the CR users. We
   suggest that the primary users at the cell edge, who have poor QoS,
   should allow secondary users to access their spectrum, while at the same
   time, the secondary users would help to enhance the primary users QoS
   using superposition coding on the primary users transmissions. Thus the
   proposed method can provide a so called ``win-win strategy{''} by
   benefiting both primary and CR users. The proposed cooperative access
   scheme in cognitive radios solves efficiently the sum-rate maximization
   problem on cognitive Gaussian Multiple Access Channels (GMACs) for power
   allocation of primary systems that cooperates with CR systems in a
   distributed fashion. We solved the problem using iterative Jacobian
   method in a distributed manner. A totally asynchronous distributed power
   allocation for sum-rate maximization on cognitive GMACs is suggested.
   Numerical results show that the QoS of primary users at the cell edge is
   improved by the proposed cooperative access scheme.}},
