@article{ ISI:000375943800011,
Author = {Osama, Ahmed and Sayed, Tarek and Easa, Said},
Title = {{Framework for evaluating risk of limited sight distance for permitted
   left-turn movements: case study}},
Journal = {{CANADIAN JOURNAL OF CIVIL ENGINEERING}},
Year = {{2016}},
Volume = {{43}},
Number = {{4}},
Pages = {{369-377}},
Month = {{APR}},
Abstract = {{A reliability analysis framework is used to evaluate the risk of limited
   sight distance for permitted left-turn movements due to the presence of
   opposing left-turn vehicles. Two signalized intersection approaches in
   the city of Surrey were used as case studies for the framework.
   Geometric and traffic video data was collected and analyzed using a
   computer vision tool to extract the input variables probability
   distributions. The data was used in the reliability analysis where
   first-order and Importance Sampling methods were performed. The analysis
   showed that the probability of non-compliance was considerable at one
   approach due to its large left-turn lane offset. The analysis also
   showed that the probability of non-compliance increased substantially
   when the obstacle vehicle was a bus rather than a passenger car.
   Moreover, the time gap had a higher impact on the probability of
   non-compliance compared to speed. Strategies were suggested to overcome
   the high probability of non-compliance.}},
