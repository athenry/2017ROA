@inproceedings{ ISI:000382775500009,
Author = {Xiao, Lu and Abbasian, Sadegh and Kumar, Rohit and Yogi, Shanu and
   Johnson, Thomas},
Book-Group-Author = {{IEEE}},
Title = {{Envelope Delta-Sigma Modulation with Adaptive Dither for
   Direct-Conversion Transmitter and Switch-Mode Power Amplifier}},
Booktitle = {{2016 IEEE 17TH ANNUAL WIRELESS AND MICROWAVE TECHNOLOGY CONFERENCE
   (WAMICON)}},
Year = {{2016}},
Note = {{17th Annual Wireless and Microwave Technology Conference (WAMICON),
   Clearwater Beach, FL, APR 11-13, 2016}},
Organization = {{IEEE}},
Abstract = {{A new encoder for radio frequency switch-mode power amplifiers is
   described. The encoder is designed for a transmitter architecture that
   uses envelope delta-sigma modulation (ESDM) and a direct conversion
   transmitter architecture. The ESDM is modified to include a dither
   signal that shapes the out-of-band spectrum while simultaneously
   maintaining good SNDR. The out-of-band dither can also be used to
   improve the power efficiency of the switch-mode power amplifier by
   rectifying the dither signal to recycle power. The encoder is designed
   to be implemented in a FPGA. Simulation and experimental results with an
   LTE modulated carrier are shown to demonstrate the concept.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
