@article{ ISI:000361035300095,
Author = {Murray, Kyle and Lu, Zeqin and Jayatilleka, Hasitha and Chrostowski,
   Lukas},
Title = {{Dense dissimilar waveguide routing for highly efficient thermo-optic
   switches on silicon}},
Journal = {{OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{23}},
Number = {{15}},
Pages = {{19575-19585}},
Month = {{JUL 27}},
Abstract = {{We analyze and demonstrate a method for increasing the efficiency of
   thermo-optic phase shifters on a silicon-on-insulator platform. The lack
   of cross-coupling between dissimilar waveguides allows highly dense
   waveguide routing under heating elements and a corresponding increase in
   efficiency. We demonstrate a device with highly dense routing of 9
   waveguides under a 10 m m wide heater and achieve a low switching power
   of 95 m W, extinction ratio greater than 20 dB, and less than 0.1 dB
   ripple in the through spectrum with a footprint of less than 800 mu m x
   180 mu m. The increase in waveguide density is found not to negatively
   impact the switch response time. (C) 2015 Optical Society of America}},
