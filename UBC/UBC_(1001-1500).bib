@article{ ISI:000365051400008,
Author = {Behzadfar, Ehsan and Ansari, Mahmoud and Konaganti, Vinod Kumar and
   Hatzikiriakos, Savvas G.},
Title = {{Extrudate swell of HDPE melts: I. Experimental}},
Journal = {{JOURNAL OF NON-NEWTONIAN FLUID MECHANICS}},
Year = {{2015}},
Volume = {{225}},
Pages = {{86-93}},
Month = {{NOV}},
Abstract = {{The extrudate swell of high-density polyethylenes (HDPEs) in capillary
   flow is studied using an optical micrometer attached to the capillary
   rheometer. The effects of molecular structure, shear rate, die
   geometrical characteristics (entrance angle, length-to-diameter ratio
   and diameter) and temperature on extrudate swell are studied
   systematically and found to have a significant effect. In particular,
   extrudate swell increases with decrease of LID ratio and changes
   nonlinearly with entrance angle exhibiting different trends for concave
   (swell increases from 60 to 180) and convex (swell decreases from 180 to
   300) angles. The swell also depends nonlinearly on die diameter,
   exhibiting a maximum at certain diameter. The molecular weight and its
   distribution (dominant) are also found to affect significantly the
   extrudate swell of HDPE. The ambient temperature is also an important
   parameter that is the higher its value, the higher the extrudate swell.
   Ultimate swell measurements have also been performed and the relative
   effects of stress relaxation and surface tension have been assessed
   independently through both experimentation and modeling. (C) 2015
   Elsevier B.V. All rights reserved.}},
