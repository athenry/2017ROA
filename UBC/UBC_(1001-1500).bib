Author = {Duan, Jianglan and Reilly, Carl and Maijer, Daan M. and Cockcroft, Steve
   L. and Phillion, Andre B.},
Title = {{Development of an Optimization Methodology for the Aluminum Alloy Wheel
   Casting Process}},
