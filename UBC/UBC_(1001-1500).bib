@article{ ISI:000364708800001,
Author = {Siebert-Timmer, A. and Bichler, L. and Mondal, K.},
Title = {{Degradation of Spark Plasma Sintered Yttria Stabilized Zirconia (YSZ)
   and CeO2-YSZ Ceramics in Supercritical Water}},
Journal = {{INTERNATIONAL JOURNAL OF APPLIED CERAMIC TECHNOLOGY}},
Year = {{2015}},
Volume = {{12}},
Number = {{6}},
Pages = {{1103-1111}},
Month = {{NOV-DEC}},
Abstract = {{Zirconia ceramics are often considered in the nuclear industry for high
   temperature applications due to their radiation resistance, chemical
   inertness, high thermal stability and low thermal conductivity. However,
   wide application of zirconia ceramics is hindered by their degradation
   behavior in aqueous media. In this research, moderately dense 8mol\%
   Yttria Stabilized Zirconia (YSZ) as well as YSZ based ceramics
   containing 5, 10 and 15mol\% CeO2 were fabricated via Spark Plasma
   Sintering (SPS). Sintered ceramics were subjected to static degradation
   testing in supercritical water at 400 degrees C and 31MPa. The weight
   loss and microstructural changes of the ceramics associated with
   supercritical water exposure were studied, and the results suggest that
   the addition of CeO2 significantly reduced the degradation rate and
   improved the bulk structural stability of the ceramic composite. This
   behavior was attributed to the enhanced stabilization of the zirconia
   lattice, preventing the evolution of the undesirable monoclinic-ZrO2
   phase.}},
