@inproceedings{ ISI:000368490000027,
Author = {Winiarski, B. and Benedetti, M. and Fontanari, V. and Allahkarami, M.
   and Hanan, J. C. and Schajer, G. S. and Withers, P. J.},
Editor = {{Bossuyt, S and Schajer, G and Carpinteri, A}},
Title = {{Comparative Analysis of Shot-Peened Residual Stresses Using Micro-Hole
   Drilling, Micro-Slot Cutting, X-ray Diffraction Methods and
   Finite-Element Modelling}},
Booktitle = {{RESIDUAL STRESS, THERMOMECHANICS \& INFRARED IMAGING, HYBRID TECHNIQUES
   AND INVERSE PROBLEMS, VOL 9}},
