@article{ ISI:000346230300014,
Author = {Laval, Jorge A. and Toth, Christopher S. and Zhou, Yi},
Title = {{A parsimonious model for the formation of oscillations in car-following
   models}},
Journal = {{TRANSPORTATION RESEARCH PART B-METHODOLOGICAL}},
Year = {{2014}},
Volume = {{70}},
Pages = {{228-238}},
Month = {{DEC}},
Abstract = {{This paper shows that the formation and propagation of traffic
   oscillations in the absence of lane changes can be explained by the
   stochastic nature of drivers' acceleration processes. By adding a white
   noise to drivers' desired acceleration in free-flow, oscillations are
   produced that accord well with observation. This theory suggests that
   driver error is a function of roadway geometry, that it determines the
   average speed at the bottleneck, as well as oscillation period and
   amplitude. The model has been implemented with a single additional
   parameter compared to the kinematic wave model with bounded
   accelerations. (C) 2014 Elsevier Ltd. All rights reserved.}},
