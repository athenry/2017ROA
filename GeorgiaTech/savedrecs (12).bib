
@article{ ISI:000357689100008,
Author = {White, Douglas E. and Sylvester, Jonathan B. and Levario, Thomas J. and
   Lu, Hang and Streelman, J. Todd and McDevitt, Todd C. and Kemp, Melissa
   L.},
Title = {{Quantitative multivariate analysis of dynamic multicellular morphogenic
   trajectories}},
