@article{ ISI:000346972800030,
Author = {Lee, Hyung-Min and Kwon, Ki Yong and Li, Wen and Ghovanloo, Maysam},
Title = {{A Power-Efficient Switched-Capacitor Stimulating System for
   Electrical/Optical Deep Brain Stimulation}},
