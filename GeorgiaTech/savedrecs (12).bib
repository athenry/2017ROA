@inproceedings{ ISI:000354938100193,
Author = {Romer, Anne and Kim, Jin-Yeon and Jacobs, Laurence J.},
Editor = {{Chimenti, DE and Bond, LJ}},
Title = {{An Analytical and Numerical Study of the Nonlinear Reflection at a
   Stress-Free Surface}},
Booktitle = {{41ST ANNUAL REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE
