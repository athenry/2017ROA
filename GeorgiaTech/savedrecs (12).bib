Author = {Matlack, Kathryn H. and Kim, Jin-Yeon and Wall, James J. and Qu, Jianmin
   and Jacobs, Laurence J.},
Editor = {{Chimenti, DE and Bond, LJ}},
Title = {{Monitoring Microstructural Evolution in Irradiated Steel with Second
   Harmonic Generation}},
Booktitle = {{41ST ANNUAL REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE
