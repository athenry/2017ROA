@inproceedings{ ISI:000399133903076,
Author = {Zhou, Enlu and Xie, Wei},
Book-Group-Author = {{IEEE}},
Title = {{SIMULATION OPTIMIZATION WHEN FACING INPUT UNCERTAINTY}},
Booktitle = {{2015 WINTER SIMULATION CONFERENCE (WSC)}},
Series = {{Winter Simulation Conference Proceedings}},
Year = {{2015}},
Pages = {{3714-3724}},
Note = {{Winter Simulation Conference (WSC), Huntington Beach, CA, DEC 06-09,
   2015}},
Abstract = {{Stochastic simulation is driven by the input model, which is a
   collection of distributions that model the randomness in the system. The
   input model is often constructed from data, and hence input uncertainty
   arises due to the finiteness of data. Simulation optimization has been
   mostly studied under the assumption of a known input model, without
   accounting for input uncertainty. We propose a new framework to study
   simulation optimization under input uncertainty, with the goal to
   balance the trade-off between optimizing under the estimated input model
   and hedging against the risk brought by input uncertainty. A simple
   numerical example illustrates different formulations under the new
   framework, compared with the usual formulation for simulation
   optimization.}},
