@inproceedings{ ISI:000354938100146,
Author = {Williams, Westin B. and Michaels, Thomas E. and Michaels, Jennifer E.},
Editor = {{Chimenti, DE and Bond, LJ}},
Title = {{Detection and Characterization of Impact Damage in Composite Panels
   using Multiple Ultrasonic Methods}},
Booktitle = {{41ST ANNUAL REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE
   EVALUATION, VOL 34}},
Series = {{AIP Conference Proceedings}},
Year = {{2015}},
Volume = {{1650}},
