Title = {{Unobtrusive Monitoring of Cardiovascular Health at Home Using a Modified
   Weighing Scale}},
Booktitle = {{6TH EUROPEAN CONFERENCE OF THE INTERNATIONAL FEDERATION FOR MEDICAL AND
   BIOLOGICAL ENGINEERING}},
Series = {{IFMBE Proceedings}},
Year = {{2015}},
Volume = {{45}},
