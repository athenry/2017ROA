@inproceedings{ ISI:000354938100094,
Author = {Chen, Xin and Michaels, Jennifer E. and Michaels, Thomas E.},
Editor = {{Chimenti, DE and Bond, LJ}},
Title = {{Incremental Scattering of Guided Waves from a Notch Originating at a
   Through-hole}},
Booktitle = {{41ST ANNUAL REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE
   EVALUATION, VOL 34}},
Series = {{AIP Conference Proceedings}},
Year = {{2015}},
Volume = {{1650}},
Pages = {{799-806}},
Note = {{41st Annual Review of Progress in Quantitative Nondestructive Evaluation
   (QNDE), Boise, ID, JUL 20-25, 2014}},
Organization = {{Ctr Nondestruct Evaluat; Quantitat Nondestruct Evaluat Programs; Amer
   Soc Nondestruct Testing; World Federat NDE Ctr; Natl Sci Fdn, Ind Univ
   Co Operat Res Ctr Program}},
Abstract = {{Cracks, which frequently initiate from fastener holes as a result of
   stress concentration, are one of the most common defects in metallic
   plate-like structures. Among a variety of methods for crack detection,
   ultrasonic guided waves have been shown to be effective. To examine the
   performance of guided wave methods in the laboratory, notches are often
   used to simulate cracks. While extensive research has focused on the
   scattering of guided waves from a notch as well as a hole, limited work
   has been done on the incremental scattering resulting from the addition
   of a notch to an existing hole. This scenario is of particular interest
   for in situ monitoring of fastener holes, where the goal is to detect
   changes in scattering caused by crack initiation and growth. An
   experimental approach is taken here where a broadband chirp excitation
   is applied to surface-mounted PZT transducers to generate guided waves
   in an aluminum plate, and the out-of-plane particle motion is measured
   by a laser vibrometer. Notches are introduced at a through-hole, and the
   corresponding far field scattering information is obtained from
   full-circle laser scans taken before and after introduction of the
   notch. The laser received signals are then filtered to generate
   equivalent responses to narrowband tone burst excitations at different
   center frequencies. It is shown that as the excitation frequency
   increases, i.e., the wavelength approaches the defect size, the angular
   scattering patterns have a more complicated lobe structure. As expected,
   it can be seen for all frequencies considered that the incremental
   scattering patterns are no longer symmetric with respect to the incident
   direction about the line perpendicularly bisecting the notch, which is
