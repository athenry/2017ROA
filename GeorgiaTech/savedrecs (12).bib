@inproceedings{ ISI:000354938100214,
Author = {Liu, Hongye and Michaels, Jennifer E. and Chen, Xin and Michaels, Thomas
   E. and He, Cunfu},
Editor = {{Chimenti, DE and Bond, LJ}},
Title = {{3D Finite Element Modeling of Lamb Wave Scattering from a Sessile Water
   Droplet}},
Booktitle = {{41ST ANNUAL REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE
   EVALUATION, VOL 34}},
Series = {{AIP Conference Proceedings}},
Year = {{2015}},
