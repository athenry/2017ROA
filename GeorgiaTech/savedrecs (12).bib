@article{ ISI:000347017400028,
Author = {Meirom, Roi A. and Clark, Trevor E. and Muhlstein, Christopher L.},
Title = {{The effects of texture and grain morphology on the fracture toughness
   and fatigue crack growth resistance of nanocrystalline platinum films}},
Journal = {{INTERNATIONAL JOURNAL OF FATIGUE}},
Year = {{2015}},
Volume = {{70}},
Pages = {{258-269}},
Month = {{JAN}},
Abstract = {{The fracture toughness and fatigue crack growth resistance of
   nanocrystalline materials are significantly affected by the thickness of
   the specimen. In this work we relate the mechanical properties of
   nanocrystalline platinum films to their texture and grain morphology.
   Tensile, creep and fatigue testing of annealed, similar to 1 mu m films
   resulted in mechanical properties similar to the as-received films
   (yield strength of similar to 1.2 GPa, fracture toughness similar to
   17.8 MPa root m, and a fatigue crack growth power law exponent of
   similar to 4.2). However, the breakdown of the initially columnar grain
   morphology had a marked effect on the transition point from an
   intergranular to transgranular fatigue cracking mode. Finite element
   modeling suggests that cyclic (fatigue) grain coarsening and the
   transition from inter- to transgranular cracking modes are a result of
   the relative importance of dislocation slip accommodation on in-plane
   and through-thickness oriented slip directions. (C) 2014 Elsevier Ltd.
   All rights reserved.}},
