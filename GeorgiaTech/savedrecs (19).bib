@article{ ISI:000322854700029,
Author = {Yasseri, Saam and Rahnema, Farzad},
Title = {{Subgroup decomposition method in diffusion theory}},
Journal = {{ANNALS OF NUCLEAR ENERGY}},
Year = {{2013}},
Volume = {{60}},
Pages = {{235-241}},
Month = {{OCT}},
Abstract = {{A new method for solving the fine-group diffusion equation is developed
   by estimating the in-scattering and fission source terms with consistent
   coarse-group diffusion solutions iteratively. This is achieved by
   extending the subgroup decomposition (SGD) method to diffusion theory
   initially developed in neutron transport theory. Additionally, a new
