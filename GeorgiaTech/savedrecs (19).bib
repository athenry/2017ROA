@article{ ISI:000325488200017,
Author = {Du, Liang and Restrepo, Jose A. and Yang, Yi and Harley, Ronald G. and
   Habetler, Thomas G.},
Title = {{Nonintrusive, Self-Organizing, and Probabilistic Classification and
   Identification of Plugged-In Electric Loads}},
Journal = {{IEEE TRANSACTIONS ON SMART GRID}},
Year = {{2013}},
Volume = {{4}},
Number = {{3}},
Pages = {{1371-1380}},
Month = {{SEP}},
Abstract = {{Electricity consumption for plugged-in electric loads (PELs) accounts
   for more use than any other single end-use service in residential and
   commercial buildings. PELs possess potentials to be efficiently managed
   for many purposes. However, few existing load identification methods are
   designed for PELs to handle challenges such as the diversity within each
   type of PELs and similarity between different types of PELs with similar
   front-end power supply units. Existing methods provide only absolute
   decisions which are not reliable when handling these challenges. This
   paper presents a simple yet efficient and practical hybrid supervised
   self-organizing map (SSOM)/Bayesian identifier for PELs. The proposed
   identifier can classify PELs into clusters by inherent similarities due
   to similar front-end power supply topologies, extract and utilize
   statistical information, and provide the probability of the unknown load
   belonging to a specific type of load. Tests based on real-world data
   validate that the proposed methods are accurate, robust, and applicable.}},
