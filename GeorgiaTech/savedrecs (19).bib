Language = {{English}},
Affiliation = {{Satir, S (Reprint Author), Georgia Inst Technol, Sch Elect \& Comp Engn, Atlanta, GA 30332 USA.
   Satir, Sarp; Zahorian, Jaime, Georgia Inst Technol, Sch Elect \& Comp Engn, Atlanta, GA 30332 USA.
   Degertekin, F. Levent, Georgia Inst Technol, Woodruff Sch Mech Engn, Atlanta, GA 30332 USA.}},
DOI = {{10.1109/TUFFC.2013.2838}},
