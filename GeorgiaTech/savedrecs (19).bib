@article{ ISI:000323845100011,
Author = {Feng, Steven Si and Lin, Risa and Gauck, Volker and Jaeger, Dieter},
Title = {{Gain Control of Synaptic Response Function in Cerebellar Nuclear Neurons
   by a Calcium-Activated Potassium Conductance}},
Journal = {{CEREBELLUM}},
Year = {{2013}},
Volume = {{12}},
Number = {{5}},
Pages = {{692-706}},
Month = {{OCT}},
Abstract = {{Small conductance Ca2+-activated potassium (SK) current provides an
   important modulator of excitatory synaptic transmission, which undergoes
   plastic regulation via multiple mechanisms. We examined whether
   inhibitory input processing is also dependent on SK current in the
   cerebellar nuclei (CN) where inhibition provides the only route of
   information transfer from the cerebellar cortical Purkinje cells. We
   employed dynamic clamping in conjunction with computer simulations to
   address this question. We found that SK current plays a critical role in
   the inhibitory synaptic control of spiking output. Specifically,
   regulation of SK current density resulted in a gain control of spiking
   output, such that low SK current promoted large output signaling for
   large inhibitory cell input fluctuations due to Purkinje cell
   synchronization. In contrast, smaller nonsynchronized Purkinje cell
   input fluctuations were not amplified. Regulation of SK density in the
   CN therefore would likely lead to important consequences for the
   transmission of synchronized Purkinje cell activity to the motor system.}},
