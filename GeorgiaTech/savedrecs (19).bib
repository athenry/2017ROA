@article{ ISI:000325544900004,
Author = {Colak, Ozgen U. and Ahzi, Said and Remond, Yves},
Title = {{Cooperative viscoplasticity theory based on the overstress approach for
   modeling large deformation behavior of amorphous polymers}},
Journal = {{POLYMER INTERNATIONAL}},
Year = {{2013}},
Volume = {{62}},
Number = {{11}},
Pages = {{1560-1565}},
Month = {{NOV}},
Abstract = {{A micromechanically based formulation of the cooperative model is
   incorporated into the viscoplasticity theory based on overstress (VBO)
   model. The plastic shear strain rate given by the cooperative model is
   used as a flow function which is responsible for rate and temperature
   dependence in the VBO model. In this way, thermomechanical analysis can
   be performed under different loading rates and temperatures of amorphous
   polymers. Introducing strain softening, the temperature- and
   strain-rate-dependent elasticity moduli are two other modifications of
   the VBO formulation. The validity of the newly proposed cooperative VBO
   model is demonstrated by modeling the uniaxial compression behavior of
   poly(methyl methacrylate) under different temperatures and strain rates.
