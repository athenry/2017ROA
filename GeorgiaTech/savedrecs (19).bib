@article{ ISI:000327443700001,
Author = {Edmonds, Christopher M. and Hesketh, Peter J. and Nair, Sankar},
Title = {{Polymer translocation in solid-state nanopores: Dependence on
   hydrodynamic interactions and polymer configuration}},
Journal = {{CHEMICAL PHYSICS}},
Year = {{2013}},
Volume = {{425}},
Pages = {{1-13}},
Month = {{NOV 8}},
Abstract = {{We present a Brownian dynamics investigation of 3-D Rouse and Zimm
   polymer translocation through solid-state nanopores. We obtain different
   scaling exponents alpha for both polymers using two initial
   configurations: minimum energy, and ` steady-state'. For forced
   translocation, Rouse polymers (no hydrodynamic interactions), shows a
   large dependence of a on initial configuration and voltage. Higher
   voltages result in crowding at the nanopore exit and reduced alpha. When
   the radius of gyration is in equilibrium at the beginning and end of
   translocation, alpha = 1 + upsilon where upsilon is the Flory exponent.
   For Zimm polymers (including hydrodynamic interactions), crowding is
   reduced and alpha = 2 upsilon. Increased pore diameter does not affect
   alpha at moderate voltages that reduce diffusion effects. For unforced
   translocation using narrow pores, both polymers give alpha = 1 + 2
   upsilon. Due to increased polymer-pore interactions in the narrow pore,
   hydrodynamic drag effects are reduced, resulting in identical scaling.
   (C) 2013 Elsevier B. V. All rights reserved.}},
