@article{ ISI:000325946500007,
Author = {Kim, Seongjun and Kim, Hee Jin and Choi, Suk and Lochner, Zachary and
   Ryou, Jae-Hyun and Dupuis, Russell D. and Ahn, Kwang-Soon and Kim,
   Hyunsoo},
Title = {{Electrical Characteristics of Ti/Al Contacts on AlInN:Mg/GaN
   Heterostructures}},
Journal = {{JAPANESE JOURNAL OF APPLIED PHYSICS}},
Year = {{2013}},
Volume = {{52}},
Number = {{10, 2, SI}},
Month = {{OCT}},
Abstract = {{The electrical characteristics of a Ti/Al metal contact on Mg-doped
   AlInN/GaN heterostructures were investigated using a transmission line
   model. The as-deposited Ti/Al contact exhibited near Ohmic contact with
   a specific contact resistance of 1.78 x 10(-2) Omega cm(2), while the
   thermal annealing performed at 600 degrees C led to better Ohmic contact
   with a contact resistance of 9.38 x 10(-4) Omega cm(2). This could be
   attributed to the formation of a direct Ohmic path connecting the
   contact and the two-dimensional electron gas beneath an AlInN:Mg barrier
   upon thermal annealing, namely, a spike contact through the indiffusion
   of Al as verified from secondary ion mass spectroscopy.
   Current-voltage-temperature measurements showed an insignificant
   temperature dependence in both the sheet resistance and the contact
   resistance, which seemed to be associated with the distinctive feature
   of the spike contact. (C) 2013 The Japan Society of Applied Physics}},
