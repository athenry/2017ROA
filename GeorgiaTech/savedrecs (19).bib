@article{ ISI:000324737900016,
Author = {Liu, Qingtao and Liu, Long and Zhou, Jingwen and Shin, Hyun-dong and
   Chen, Rachel R. and Madzak, Catherine and Li, Jianghua and Du, Guocheng
   and Chen, Jian},
Title = {{Biosynthesis of homoeriodictyol from eriodictyol by flavone 3
   `-O-methyltransferase from recombinant Yarrowia lioplytica: Heterologous
   expression, biochemical characterization, and optimal transformation}},
Journal = {{JOURNAL OF BIOTECHNOLOGY}},
Year = {{2013}},
Volume = {{167}},
Number = {{4}},
Pages = {{472-478}},
Month = {{SEP 20}},
Abstract = {{In this work, we attempted to synthesize homoeriodictyol by transferring
   one methyl group of S-adenosyl-L-methionine (SAM) to eriodictyol using
   flavone 3'-O-methyltransferase ROMT-9, which was produced by recombinant
   Yarrowia lipolytica. Specifically, the ROMT-9 gene from rice was
   synthesized and cloned into the multi-copy integrative vector pINA1297,
   and was further expressed in Y. lipolytica with a growth phase-dependent
   constitutive promoter hp4d. The highest ROMT-9 activity reached 5.53 U/L
   after 4 days of culture in shake flask. The optimal pH and temperature
   of the purified ROMT-9 were 8.0 and 37 degrees C, respectively. The
   purified enzyme was stable up to 40 degrees C, and retained more than
   80\% of its maximal activity between pH 6.5 and 9.0. The recombinant
   ROMT-9 did not require Mg2+ for catalysis, while was completely
   inhibited in the presence of 5 mM Zn2+, Cu2+, Ba2+, Al3+, or Ni2+. The
   purified ROMT-9 was used to synthesize homoeriodictyol, and the maximal
   transformation ratio reached 52.4\% at 16 h under the following
   conditions: eriodictyol 0.2 g/L, ROMT-9 0.16 g/L, SAM 0.2 g/L, CH3OH 6\%
   (v/v), temperature 37 degrees C, and pH 8.0. This work provides an
   alternative strategy for efficient synthesis of homoeriodictyol and
   compared to the traditional plant extraction or chemical synthesis, the
   biotransformation approach generates less environmental pollution and
   has a great potential for the sustainable production of homoeriodictyol.
   (c) 2013 Elsevier B.V. All rights reserved.}},
