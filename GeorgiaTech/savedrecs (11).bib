@inproceedings{ ISI:000363246400002,
Author = {Yao, Donggang},
Editor = {{Zatloukal, M}},
Title = {{Maxwell Models with Relaxation in Logarithmic Strains}},
Booktitle = {{NOVEL TRENDS IN RHEOLOGY VI}},
