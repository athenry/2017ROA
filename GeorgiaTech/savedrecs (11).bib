@inproceedings{ ISI:000370722900375,
Author = {Park, Jong Seok and Wang, Hua},
Book-Group-Author = {{IEEE}},
Title = {{A Transformer-Based Poly-Phase Network for Ultra-Broadband Quadrature
   Signal Generation}},
