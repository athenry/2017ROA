@inproceedings{ ISI:000370259206019,
Author = {L'Afflitto, Andrea and Haddad, Wassim M.},
Book-Group-Author = {{IEEE}},
Title = {{Optimal Singular Control for Nonlinear Semistabilization}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
