@inproceedings{ ISI:000370259205075,
Author = {Haddad, Wassim M. and L'Afflitto, Andrea},
Book-Group-Author = {{IEEE}},
Title = {{Finite-Time Partial Stability Theory and Fractional Lyapunov
   Differential Inequalities}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2015}},
Pages = {{5347-5352}},
Note = {{American Control Conference, Chicago, IL, JUL 01-03, 2015}},
