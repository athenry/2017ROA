Title = {{An Assessment of Computational Fluid Dynamics and Semi-empirical
   approaches for Vertical Axis Wind Turbine Analysis}},
Booktitle = {{PROCEEDINGS OF THE AASRI INTERNATIONAL CONFERENCE ON INDUSTRIAL
   ELECTRONICS AND APPLICATIONS (IEA 2015)}},
Series = {{AER-Advances in Engineering Research}},
Year = {{2015}},
