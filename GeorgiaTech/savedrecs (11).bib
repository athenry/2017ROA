@inproceedings{ ISI:000380453000132,
Author = {Fei, Zhennan and Akesson, Knut and Reveliotis, Spyros},
Book-Group-Author = {{IEEE}},
Title = {{Symbolic Computation of Boundary Unsafe States in Complex Resource
