@inproceedings{ ISI:000380430700044,
Author = {Zheng, Keke and Cho, Young K.},
Editor = {{Chong, WO and Chang, J and Parrish, K and Berardi, U}},
Title = {{A Convenient Thermal Performance Evaluating Method for Unitary AC (DX)
   Units}},
