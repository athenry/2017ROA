Author = {Chen, Qimei and Yu, Guanding and Yin, Rui and Li, Geoffrey Ye},
Book-Group-Author = {{IEEE}},
Title = {{Joint User Association and Resource Allocation for Energy-Efficient
   Multi-Stream Aggregation}},
