@inproceedings{ ISI:000371717206180,
Author = {Sha, Ying and Phan, John H. and Wang, May D.},
Book-Group-Author = {{IEEE}},
Title = {{Effect of Low-Expression Gene Filtering on Detection of Differentially
   Expressed Genes in RNA-Seq Data}},
Booktitle = {{2015 37TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
   MEDICINE AND BIOLOGY SOCIETY (EMBC)}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
