@inproceedings{ ISI:000380538900008,
Author = {Tran, Anh V. and Wang, Yan},
Editor = {{Poole, W and Christensen, S and Kalidindi, S and Luo, A and Madison, J and Raabe, D and Sun, X}},
Title = {{A MOLECULAR DYNAMICS SIMULATION MECHANISM WITH IMPRECISE INTERATOMIC
   POTENTIALS}},
