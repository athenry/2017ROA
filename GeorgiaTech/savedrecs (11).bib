@inproceedings{ ISI:000371471001137,
Author = {Attarzadeh, Hourieh and Lim, Sung Kyu and Ytterdal, Trond},
Book-Group-Author = {{IEEE}},
Title = {{Stacking Integration Methodologies in 3D IC for 3D Ultrasound Image
   Processing Application: A Stochastic Flash ADC Design Case Study}},
