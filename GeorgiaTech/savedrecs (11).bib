@inproceedings{ ISI:000370259203005,
Author = {Ali, Usman and Yan, Yuan and Mostofi, Yasamin and Wardi, Yorai},
Book-Group-Author = {{IEEE}},
Title = {{An Optimal Control Approach for Communication and Motion Co-optimization
   in Realistic Fading Environments}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2015}},
Pages = {{2930-2935}},
Note = {{American Control Conference, Chicago, IL, JUL 01-03, 2015}},
