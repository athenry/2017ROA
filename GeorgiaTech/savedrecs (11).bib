Author = {Yu, Jian and Li, Geoffrey Ye and Yin, Changchuan and Zhu, Xiaolong and
   Fu, Yusun},
Book-Group-Author = {{IEEE}},
Title = {{Transmission Mode Selection for Downlink Transmission in LTE-A Networks}},
Booktitle = {{2015 IEEE 26TH ANNUAL INTERNATIONAL SYMPOSIUM ON PERSONAL, INDOOR, AND
