@inproceedings{ ISI:000371705200178,
Author = {Tomko, Megan and Lucero, Briana and Turner, Cameron and Linsey, Julie},
Book-Group-Author = {{IEEE}},
Title = {{Establishing Functional Concepts Vital for Design by Analogy}},
