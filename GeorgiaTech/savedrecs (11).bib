@inproceedings{ ISI:000380425300035,
Author = {Whitaker, Bradley M. and Anderson, David V.},
Book-Group-Author = {{IEEE}},
Title = {{LEARNING ANOMALOUS FEATURES VIA SPARSE CODING USING MATRIX NORMS}},
Booktitle = {{2015 IEEE SIGNAL PROCESSING AND SIGNAL PROCESSING EDUCATION WORKSHOP
   (SP/SPE)}},
Year = {{2015}},
Pages = {{196-201}},
Note = {{2015 IEEE Signal Processing and Signal Processing Education Workshop
   (SP/SPE), Salt Lake City, UT, AUG 09-12, 2015}},
Abstract = {{Our goal is to find anomalous features in a dataset using the sparse
   coding concept of dictionary learning. Rather than using the averaged
   column l(2)-norm for the dictionary update as is typically done in
   sparse coding, we explore using three matrix norms: vertical bar
   vertical bar.vertical bar vertical bar(1), vertical bar vertical
   bar.vertical bar vertical bar(2), and vertical bar vertical bar.vertical
   bar vertical bar(infinity). Minimizing the matrix norms represents
   minimizing a maximum deviation in the reconstruction error rather than
   an average deviation, hopefully allowing us to find features that
   contribute significantly but infrequently to sample training points. We
   find that while solving for the dictionaries using matrix norm
   minimization takes longer to compute, all three methods are able to
   recover a known basis from a simple set of training data. In addition,
   the vertical bar vertical bar(1) matrix norm is able to recover a known
   anomalous feature in the training data that the other norms ( including
   the standard averaged l(2)-norm) are unable to find.}},
