@inproceedings{ ISI:000369107500065,
Author = {Li, Tso-Wei and Park, Jong Seok and Wang, Hua},
Book-Group-Author = {{IEEE}},
Title = {{A 2-24GHz 360 degrees Full-Span Differential Vector Modulator Phase
   Rotator with Transformer-Based Poly-Phase Quadrature Network}},
Booktitle = {{2015 IEEE CUSTOM INTEGRATED CIRCUITS CONFERENCE (CICC)}},
Series = {{IEEE Custom Integrated Circuits Conference}},
Year = {{2015}},
Note = {{IEEE Custom Integrated Circuits Conference (CICC), San Jose, CA, SEP
   28-30, 2015}},
Organization = {{IEEE}},
Abstract = {{This paper presents a differential vector modulator phase rotator
   performing 360 degrees full span phase interpolation over a first-ever
   decade-wide instantaneous bandwidth (2GHz-24GRz). A wideband poly-phase
   network based on folded transformer 90 degrees couplers generates
   accurate differential quadrature signals with low passive loss (2dB)
   over a decade bandwidth. Two 5-bit linear digital variable gain
   amplifiers (VGAs) scale the quadrature signals, which are then combined
   for the target phase interpolations. Our phase rotator occupies a very
   compact chip size (1.2mm-by-1.8mm) in a standard 65nm CMOS process.
   Moreover, our design does not require any tuning element, band selection
   switch, or frequency-dependent code compensation/look-up table, ensuring
   that each phase interpolation code can be used for all the in-band
   operation frequencies. Additionally, the maximum RATS quantization phase
   error is only 1.22 degrees within 1.5dB output magnitude variation for
   full 360 degrees interpolation from 2GHz to 24GHz.}},
