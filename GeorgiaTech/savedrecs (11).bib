@article{ ISI:000358817600010,
Author = {Olynick, Deirdre L. and Perera, Pradeep and Schwartzberg, Adam and
   Kulshreshta, Prashant and De Oteyza, Dimas G. and Jarnagin, Nathan and
   Henderson, Cliff and Sun, Zhiwei and Gunkel, Ilja and Russell, Thomas
   and Budden, Matthias and Rangelow, Ivo W.},
Title = {{Selective Laser Ablation in Resists and Block Copolymers for High
   Resolution Lithographic Patterning}},
Journal = {{JOURNAL OF PHOTOPOLYMER SCIENCE AND TECHNOLOGY}},
Year = {{2015}},
Volume = {{28}},
Number = {{5}},
Pages = {{663-668}},
Abstract = {{Previously, we demonstrated an all dry, selective laser ablation
   development in methyl acetoxy calixarene (MAC6) which produced high
   resolution (15-25 nm half-pitch), high aspect ratio features not
   achievable with wet development. In this paper, we investigate the
   selective laser ablation process as a means to create a block copolymer
   derived lithographic pattern through the selective removal of one block.
   Two block copolymer systems were investigated PS-b-PHOST, and
