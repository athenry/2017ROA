@inproceedings{ ISI:000371717204090,
Author = {Chahibi, Youssef and Balasingham, Ilangko},
Book-Group-Author = {{IEEE}},
Title = {{An Intra-body Molecular Communication Networks Framework for Continuous
   Health Monitoring and Diagnosis}},
Booktitle = {{2015 37TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
