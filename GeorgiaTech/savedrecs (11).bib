@article{ ISI:000375842300051,
Author = {King, Jared C. and Li, Huayu and Grover, Martha A. and Kawajiri,
   Yoshiaki and Rousseau, Ronald W.},
Title = {{Optimization of two-stage cooling profile in unseeded batch
   crystallization}},
Journal = {{IFAC PAPERSONLINE}},
Year = {{2015}},
Volume = {{48}},
Number = {{8}},
Pages = {{297-302}},
Note = {{9th IFAC Symposium on Advanced Control of Chemical Processes ADCHEM
   2015, Whistler, CA, JUN 07-10, 2015}},
Abstract = {{A two-stage cooling protocol for batch cooling crystallization was
   optimized to maximize mean crystal size while minimizing the variance in
   size. A population balance model represents the crystallization process,
   incorporating crystal growth and nucleation, for use in a
   multi-objective optimization. At all points along the Pareto front, the
   optimal strategy has the nucleation event occurring on the temperature
   plateau between the two cooling stages. This shortens the nucleation
   period supersaturation is rapidly depleted, ultimately generating fewer
   crystals of larger size. In comparison to a simple linear cooling
   profile, the two step strategy produces crystals of larger size with
   smaller variance. (C) 2015, IFAC (International Federation of Automatic
   Control) Hosting by Elsevier Ltd. All rights reserved.}},
