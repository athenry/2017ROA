Affiliation = {{Park, SJ (Reprint Author), Georgia Inst Technol, Sch Elect \& Comp Engn, Interconnect \& Packaging Ctr, Atlanta, GA 30332 USA.
   Park, Sung Joo; Choi, Jae Young; Swaminathan, Madhavan, Georgia Inst Technol, Sch Elect \& Comp Engn, Interconnect \& Packaging Ctr, Atlanta, GA 30332 USA.}},
ISSN = {{2165-4107}},
ISBN = {{978-1-4673-2538-7}},
Keywords = {{SSN; simultaneous switching noise; ground bounce; reference voltage;
   referencing plane; pseudo-differential interface}},
