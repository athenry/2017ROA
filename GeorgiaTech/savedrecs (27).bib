@inproceedings{ ISI:000324346800115,
Author = {Qin, Tongran and Grigoriev, Roman O.},
Book-Group-Author = {{ASME}},
Title = {{CONVECTION, EVAPORATION, AND CONDENSATION OF SIMPLE AND BINARY FLUIDS IN
   CONFINED GEOMETRIES}},
Booktitle = {{PROCEEDINGS OF THE ASME MICRO/NANOSCALE HEAT AND MASS TRANSFER
   INTERNATIONAL CONFERENCE, 2012}},
Year = {{2012}},
