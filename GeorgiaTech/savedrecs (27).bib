Author = {Zhao, Yong and Juang, Biing-Hwang (Fred)},
Editor = {{Matthews, MB}},
Title = {{EXPLOITING SPARSITY IN STRANDED HIDDEN MARKOV MODELS FOR AUTOMATIC
   SPEECH RECOGNITION}},
