@inproceedings{ ISI:000324346800097,
Author = {Chen, Liang and Kumar, Satish},
Book-Group-Author = {{ASME}},
Title = {{THERMAL TRANSPORT IN GRAPHENE SUPPORTED ON COPPER}},
