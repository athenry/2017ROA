@inproceedings{ ISI:000316563200075,
Author = {McMenamin, Peter and Jow, Uei-Ming and Kiani, Mehdi and Ghovanloo,
   Maysam},
Book-Group-Author = {{IEEE}},
Title = {{Real Time Control of a Wireless Powering and Tracking System for
   Long-Term and Large-Area Electrophysiology Experiments}},
Booktitle = {{2012 IEEE BIOMEDICAL CIRCUITS AND SYSTEMS CONFERENCE (BIOCAS):
   INTELLIGENT BIOMEDICAL ELECTRONICS AND SYSTEM FOR BETTER LIFE AND BETTER
   ENVIRONMENT}},
Series = {{Biomedical Circuits and Systems Conference}},
