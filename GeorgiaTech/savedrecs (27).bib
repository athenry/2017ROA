@inproceedings{ ISI:000327200403040,
Author = {Yucelen, Tansel and Haddad, Wassim M.},
Book-Group-Author = {{IEEE}},
Title = {{A Robust Adaptive Control Architecture with L-infinity Transient and
   Steady-State Performance Guarantees}},
Booktitle = {{2012 IEEE 51ST ANNUAL CONFERENCE ON DECISION AND CONTROL (CDC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2012}},
