Title = {{Simultaneous Switching Noise Analysis of Reference Voltage Rails for
   Pseudo Differential Interfaces}},
Booktitle = {{2012 IEEE 21ST CONFERENCE ON ELECTRICAL PERFORMANCE OF ELECTRONIC
   PACKAGING AND SYSTEMS}},
Series = {{IEEE Conference on Electrical Performance of Electronic Packaging and
   Systems-EPEPS}},
