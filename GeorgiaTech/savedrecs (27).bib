@inproceedings{ ISI:000327200402107,
Author = {Haddad, Wassim M. and Sadikhov, Teymur},
Book-Group-Author = {{IEEE}},
Title = {{Extended Kalman-Yakubovich-Popov Conditions and Stability of Feedback
   Interconnections for Dissipative Discontinuous Dynamical Systems}},
Booktitle = {{2012 IEEE 51ST ANNUAL CONFERENCE ON DECISION AND CONTROL (CDC)}},
