Author = {Chen, I-Fan and Lee, Chin-Hui},
Book-Group-Author = {{International Speech Communications Association}},
Title = {{A Study on Using Word-Level HMMs to Improve ASR Performance over
   State-of-the-Art Phone-Level Acoustic Modeling for LVCSR}},
