@inproceedings{ ISI:000319225501069,
Author = {Lendermann, Peter and Thirunavukkarasu, Annamalai and Low, Malcolm Yoke
   Hean and McGinnis, Leon F.},
Editor = {{Laroque, C and Himmelspach, J and Pasupathy, R}},
Title = {{INITIAL PROVISIONING AND SPARE PARTS INVENTORY NETWORK OPTIMISATION IN A
   MULTI MAINTENANCE BASE ENVIRONMENT}},
Booktitle = {{2012 WINTER SIMULATION CONFERENCE (WSC)}},
Series = {{Winter Simulation Conference Proceedings}},
Year = {{2012}},
Note = {{Winter Simulation Conference (WSC), Berlin, GERMANY, DEC 09-12, 2012}},
Abstract = {{Aviation spare parts provisioning is a highly complex problem.
   Traditionally, provisioning has been carried out using a conventional
   Poisson-based approach where inventory quantities are calculated
   separately for each part number and demands from different operations
   bases are consolidated into one single location. In an environment with
   multiple operations bases, however, such simplifications can lead to
   situations in which spares - although available at another airport -
   first have to be shipped to the location where the demand actually
   arose, leading to flight delays and cancellations. In this paper we
   demonstrate how simulation-based optimisation can help with the
   multi-location inventory problem by quantifying synergy potential
