Title = {{A 100 MHz MEMS SiBAR Phase Modulator for Quadrature Phase Shift Keying}},
Booktitle = {{2012 IEEE INTERNATIONAL FREQUENCY CONTROL SYMPOSIUM (FCS)}},
Series = {{IEEE International Frequency Control Symposium}},
Year = {{2012}},
Note = {{IEEE International Frequency Control Symposium (IFCS), Baltimore, MD,
   MAY 21-24, 2012}},
Organization = {{IEEE; UFFC}},
Abstract = {{A technique is proposed to employ the phase shifting capability of a
   MEMS resonator to form the basic component of a switchless quadrature
   phase shift keying (QPSK) scheme. The advantages of such an approach
   include direct baseband to RF modulation, no generation of
   intermodulation products since the resonator is operated linearly, and
   small size, cost, and power dissipation. An example phase modulator
   system built using a 100 MHz SiBAR demonstrates at least 54 degrees of
   relative phase shift.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
