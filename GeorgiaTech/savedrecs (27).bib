@inproceedings{ ISI:000316563200079,
Author = {Kiani, Mehdi and Ghovanloo, Maysam},
Book-Group-Author = {{IEEE}},
Title = {{Pulse Delay Modulation (PDM) A New Wideband Data Transmission Method to
   Implantable Medical Devices in Presence of a Power Link}},
Booktitle = {{2012 IEEE BIOMEDICAL CIRCUITS AND SYSTEMS CONFERENCE (BIOCAS):
   INTELLIGENT BIOMEDICAL ELECTRONICS AND SYSTEM FOR BETTER LIFE AND BETTER
   ENVIRONMENT}},
Series = {{Biomedical Circuits and Systems Conference}},
Year = {{2012}},
Pages = {{256-259}},
Note = {{IEEE Biomedical Circuits and Systems Conference (BioCAS), Hsinchu,
   TAIWAN, NOV 28-30, 2012}},
Organization = {{IEEE; CAS; EMB; Natl Chiao Tung Univ; Taiwan Engn Med Biol Assoc
   (TWEMBA)}},
Abstract = {{This paper introduces a new technique, called pulse delay modulation
   (PDM), for wideband data transmission across inductive links along with
   the power carrier. It consumes small power, provides robustness against
   strong power carrier interference, and does not affect the power
   transfer efficiency (PTE). In PDM, a pattern of narrow pulses are
   transmitted at precise time delays for each data bit through an
   inductive link to initiate a short oscillatory pattern across the
   receiver (Rx) data coil. This oscillation shifts the zero-crossings of
   the undesired sinusoidal power carrier interference on the Rx data coil,
   resulting in a phase shift between the Rx power and data signals, from
   which the data bit stream can be recovered. We describe the PDM
   theoretical foundation and demonstrate its operation via post-layout
   simulation of a PDM transceiver, designed in a 0.35-mu m standard CMOS
   process, using a realistic inductive link model. The PDM transceiver is
   expected to achieve a data rate of 13.56 Mbps at 1 cm data coils
   separation with up to 2 mm misalignment while delivering >250 mW through
   an orthogonal power transmission link to an implantable medical device.}},
