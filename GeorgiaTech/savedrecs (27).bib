@inproceedings{ ISI:000317001300090,
Author = {Chatterjee, A. and Deyati, S. and Muldrey, B. and Devarakond, S. and
   Banerjee, A.},
Book-Group-Author = {{IEEE}},
Title = {{Validation Signature Testing: A Methodology for Post-Silicon Validation
   of Analog/Mixed-Signal Circuits}},
Booktitle = {{2012 IEEE/ACM INTERNATIONAL CONFERENCE ON COMPUTER-AIDED DESIGN (ICCAD)}},
Series = {{ICCAD-IEEE ACM International Conference on Computer-Aided Design}},
