@article{ ISI:000299890500008,
Author = {Zhu, Yong and Qin, Qingquan and Xu, Feng and Fan, Fengru and Ding, Yong
   and Zhang, Tim and Wiley, Benjamin J. and Wang, Zhong Lin},
Title = {{Size effects on elasticity, yielding, and fracture of silver nanowires:
   In situ experiments}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2012}},
Volume = {{85}},
Number = {{4}},
Month = {{JAN 30}},
Abstract = {{This paper reports the quantitative measurement of a full spectrum
   ofmechanical properties of fivefold twinned silver (Ag) nanowires (NWs),
   including Young's modulus, yield strength, and ultimate tensile
   strength. In-situ tensile testing of Ag NWs with diameters between 34
   and 130 nm was carried out inside a scanning electron microscope (SEM).
   Young's modulus, yield strength, and ultimate tensile strength all
   increased as the NW diameter decreased. The maximum yield strength in
   our tests was found to be 2.64 GPa, which is about 50 times the bulk
   value and close to the theoretical value of Ag in the < 110 >
   orientation. The size effect in the yield strength is mainly due to the
   stiffening size effect in the Young's modulus. Yield strain scales
   reasonably well with the NW surface area, which reveals that yielding of
   Ag NWs is due to dislocation nucleation from surface sources. Pronounced
   strain hardening was observed for most NWs in our study. The strain
   hardening, which has not previously been reported for NWs, is mainly
   attributed to the presence of internal twin boundaries.}},
