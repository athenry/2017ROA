@article{ ISI:000326095000003,
Author = {Hong, Jung-Il and Ding, Yong and Moon, Kyoung-Sik and Wong, C. P.},
Title = {{Enhanced Diffusion of Silver Atoms on the Surface of Nanoparticles at
   Low Temperatures}},
Journal = {{JOURNAL OF ELECTRONIC PACKAGING}},
Year = {{2013}},
Volume = {{135}},
Number = {{1}},
Month = {{MAR}},
Abstract = {{Microstructure development of 35 nm silver nanoparticles during the low
   temperature sintering was examined in situ as the ambient temperature
   increased from room temperature up to 450 degrees C using X-ray
   diffraction and electron microscopy techniques. Measured particle size
   increased rapidly up to similar to 90 nm in the temperature range
   between 130 and 250 degrees C, which is thought to be from the atomic
   diffusion on the surfaces of nanoparticles. On the other hand, further
   increase of the annealing temperature results in little or almost no
   change in the grain size. Therefore, the sintering effect due to the
   surface diffusion of silver atoms is active only on the surface of
   nanoparticles whose size is less than similar to 90 nm, indicating
   enhanced atomic mobility of silver atoms on the surface of
   nanoparticles.}},
