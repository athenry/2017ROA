@article{ ISI:000319173600005,
Author = {Raghav, Vrishank and Komerath, Narayanan},
Title = {{An Exploration of Radial Flow on a Rotating Blade in Retreating Blade
   Stall}},
Journal = {{JOURNAL OF THE AMERICAN HELICOPTER SOCIETY}},
Year = {{2013}},
Volume = {{58}},
Number = {{2}},
Month = {{APR}},
Abstract = {{The nature of radial flow during retreating blade stall on a two-bladed
   teetering rotor with cyclic pitch variation is investigated using laser
   sheet visualization and particle image velocimetry in a low-speed wind
   tunnel. The velocity field above the retreating blade at 2700 azimuth
   shows the expected development of a radially directed jet layer close to
   the blade surface in the otherwise separated flow region. This jet is
   observed to break up into discrete structures, limiting the spanwise
   growth of the radial velocity in the jet layer. The discrete structures
   are shown to derive their vorticity from the ``radial jet{''} layer near
   the surface, rather than from the freestream at the edge of the
   separated region. The separation line determined using velocity data
   shows the expected spanwise variation. The results of this study are
   also correlated in a limited range of extrapolation to the phenomena
   encountered on a full-scale horizontal axis wind turbine in yaw.}},
