@article{ ISI:000318268800080,
Author = {Fang, Yingcui and Blinn, Kevin and Li, Xiaxi and Weng, Guojun and Liu,
   Meilin},
Title = {{Strong coupling between Rhodamine 6G and localized surface plasmon
   resonance of immobile Ag nanoclusters fabricated by direct current
   sputtering}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{102}},
Number = {{14}},
Month = {{APR 8}},
Abstract = {{We made clean silver nano-clusters (AgNCs) on glass substrates by DC
   magnetron sputtering of a high purity Ag target in a high vacuum
   chamber. The AgNCs film shows strong localized surface plasmon resonance
   (LSPR) due to the coupling among Ag nanoparticles in the AgNCs and the
   coupling between AgNCs. The LSPR indicates strong coupling with
   Rhodamine 6G (R6G) adsorbed on the AgNC surface, which enhances the R6G
   absorption intensity and broadens the absorption wavelength range. This
   result promotes plasmonic nanoparticles to be better used in solar
   cells. (C) 2013 AIP Publishing LLC.}},
