@article{ ISI:000315961300036,
Author = {Thai, Trang T. and Aubert, Herve and Pons, Patrick and DeJean, Gerald
   and Tentzeris, Manos M. and Plana, Robert},
Title = {{Novel Design of a Highly Sensitive RF Strain Transducer for Passive and
   Remote Sensing in Two Dimensions}},
