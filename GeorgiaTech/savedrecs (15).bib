@article{ ISI:000333277100015,
Author = {Xiong, Cong and Lu, Lu and Li, Geoffrey Ye},
Title = {{Energy-Efficient Spectrum Access in Cognitive Radios}},
Journal = {{IEEE JOURNAL ON SELECTED AREAS IN COMMUNICATIONS}},
