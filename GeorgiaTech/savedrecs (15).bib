@article{ ISI:000334139500001,
Author = {Siebert, Sebastian and Teizer, Jochen},
Title = {{Mobile 3D mapping for surveying earthwork projects using an Unmanned
   Aerial Vehicle (UAV) system}},
Journal = {{AUTOMATION IN CONSTRUCTION}},
Year = {{2014}},
Volume = {{41}},
Pages = {{1-14}},
Month = {{MAY}},
Abstract = {{Unmanned Aerial Vehicle (UAV) systems as a data acquisition platform and
   as a measurement instrument are becoming attractive for many surveying
   applications in civil engineering. Their performance, however, is not
   well understood for these particular tasks. The scope of the presented
   work is the performance evaluation of a UAV system that was built to
   rapidly and autonomously acquire mobile three-dimensional (3D) mapping
   data. Details to the components of the UAV system (hardware and control
   software) are explained. A novel program for photogrammetric flight
   planning and its execution for the generation of 3D point clouds from
   digital mobile images is explained. A performance model for estimating
