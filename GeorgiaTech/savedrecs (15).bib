@article{ ISI:000333488100025,
Author = {Liu, Yaodong and Chae, Han Gi and Choi, Young Ho and Kumar, Satish},
Title = {{Effect of carbon nanotubes on sintering behavior of alumina prepared by
   sol-gel method}},
Journal = {{CERAMICS INTERNATIONAL}},
Year = {{2014}},
Volume = {{40}},
Number = {{5}},
Pages = {{6579-6587}},
Month = {{JUN}},
Abstract = {{Alumina (Al2O3)/carbon nanotube (CNT) (99/1 by weight) composite was
   prepared by mixing CNT dispersion with AlCl3-based gel, followed by high
   temperature sintering at a temperature up to 1150 degrees C in argon.
   Composite alumina precursor showed phase transition order from amorphous
   to gamma-Al2O3 after sintered at 900 degrees C for 2 h, partially to
   theta-Al2O3 after sintered at 1000 degrees C for 2 h, and then partially
   to alpha-Al2O3 after sintered at 1150 degrees C for 2 h. By comparison,
   control alumina precursor directly transformed from amorphous to
   alpha-Al2O3 after sintered at a relatively low temperature of 600
   degrees C for 2 h. Composite alumina showed porous structure with pore
   diameter ranging from 100 nm to 2 mu m, whereas control alumina was
   relatively pore-free. The elevated alumina-crystal phase transition
   temperatures and the formation of porous structure were ascribed to the
   presence of CNTs in alumina precursor. The composite alumina sintered at
   900 degrees C for 2 h containing only gamma-Al2O3 had a BET surface area
   of 138 m(2)/g, which was significantly higher than that of control
   alumina sintered at 1150 degrees C for 2 h containing only alpha-Al2O3,
   15 m(2)/g. (C) 2013 Elsevier Ltd and Techna Group S.r.l. All rights
   reserved.}},
