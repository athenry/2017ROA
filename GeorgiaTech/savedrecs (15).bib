@article{ ISI:000333941400034,
Author = {DePrince, III, A. Eugene and Kennedy, Matthew R. and Sumpter, Bobby G.
   and Sherrill, C. David},
Title = {{Density-fitted singles and doubles coupled cluster on graphics
   processing units}},
Journal = {{MOLECULAR PHYSICS}},
