@article{ ISI:000331078800011,
Author = {Zhou, Yinhua and Shim, Jae Won and Fuentes-Hernandez, Canek and Khan,
   Talha M. and Kippelen, Bernard},
Title = {{Inverted organic solar cells with polymer-modified fluorine-doped tin
   oxide as the electron-collecting electrode}},
Journal = {{THIN SOLID FILMS}},
