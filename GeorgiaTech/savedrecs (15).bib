@article{ ISI:000334182700060,
Author = {Huang, Shenming and Liu, Yajun and Chen, Yang and Kang, Zhitao},
Title = {{Thermodynamic Descriptions and Phase Diagrams for Pb-S and Bi-S Binary
   Systems}},
Journal = {{JOURNAL OF ELECTRONIC MATERIALS}},
Year = {{2014}},
Volume = {{43}},
Number = {{4}},
Pages = {{1237-1243}},
Month = {{APR}},
Abstract = {{In this work, thermodynamic optimization of the Pb-S and Bi-S binary
   systems has been performed using the calculation of phase diagrams
   approach. The Gibbs free energies of the liquid phases in the two binary
   systems are treated with the associated solution model. The two
