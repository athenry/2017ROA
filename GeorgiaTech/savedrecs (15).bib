@article{ ISI:000331406900008,
Author = {Huang, Jin and Chen, Y. H. and Guo, Konghui},
Title = {{Novel Approach to Multibody System Modeling: Cascading and Clustering}},
Journal = {{JOURNAL OF AEROSPACE ENGINEERING}},
Year = {{2014}},
Volume = {{27}},
Number = {{2}},
Pages = {{279-290}},
Month = {{MAR 1}},
Abstract = {{A novel approach for discrete multibody system modeling is presented.
   The approach segments the multibody system into a series of subsystems
   with kinematic constraints. The equations of motion of the unconstrained
   subsystems are either easier to obtain or already exist. The
   Udwadia-Kalaba equation is then introduced to calculate the constraint
   forces due to the constraints. The approach does not require any
   auxiliary variables, such as Lagrange multipliers, and does not need to
   use any projection. It can systematically reach the model via clustering
   of the segments.}},
