@article{ ISI:000333253800008,
Author = {Xu, Hao and Arson, Chloe},
Title = {{ANISOTROPIC DAMAGE MODELS FOR GEOMATERIALS: THEORETICAL AND NUMERICAL
   CHALLENGES}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL METHODS}},
Year = {{2014}},
Volume = {{11}},
Number = {{2, SI}},
Month = {{MAR}},
Abstract = {{A new anisotropic damage model for rock is formulated and discussed.
   Flow rules are derived with the energy release rate conjugate to damage,
   which is thermodynamically consistent. Drucker-Prager yield function is
   adapted to make the damage threshold depend on damage energy release
   rate and to distinguish between tension and compression strength.
   Positivity of dissipation is ensured by using a nonassociate flow rule
   for damage, while nonelastic deformation due to damage is computed by an
   associate flow rule. Simulations show that the model meets thermodynamic
   requirements, follows a rigorous formulation, and predicts expected
   trends for damage, deformation and stiffness.}},
