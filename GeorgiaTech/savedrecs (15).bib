@article{ ISI:000333536600020,
Author = {Li, Jian and Zhang, Liwei and Choi, Jeong Yeol and Yang, Vigor and Lin,
   Kuo-Cheng},
Title = {{Ignition Transients in a Scramjet Engine with Air Throttling Part 1:
   Nonreacting Flow}},
