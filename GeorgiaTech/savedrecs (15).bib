Author = {Tsuji, Paul and Poulson, Jack and Engquist, Bjoern and Ying, Lexing},
Title = {{SWEEPING PRECONDITIONERS FOR ELASTIC WAVE PROPAGATION WITH SPECTRAL
   ELEMENT METHODS}},
