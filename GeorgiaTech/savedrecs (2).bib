@article{ ISI:000377587800015,
Author = {Dudani, Jaideep S. and Buss, Colin G. and Akana, Reid T. K. and Kwong,
   Gabriel A. and Bhatia, Sangeeta N.},
Title = {{Sustained-Release Synthetic Biomarkers for Monitoring Thrombosis and
   Inflammation Using Point-of-Care Compatible Readouts}},
Journal = {{ADVANCED FUNCTIONAL MATERIALS}},
