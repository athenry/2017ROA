@article{ ISI:000377152900005,
Author = {Zhu, Zhiyuan and Yu, Min and Jin, Yufeng},
Title = {{Surface-enhanced Raman Scattering Sensor Based on the Ag-Zn(OH)F Network}},
Journal = {{CHEMISTRY LETTERS}},
Year = {{2016}},
Volume = {{45}},
Number = {{6}},
Pages = {{595-597}},
Month = {{JUN 5}},
Abstract = {{In this letter, we propose a novel and practicable route for fabricating
   a highly sensitive, recyclable surface-enhanced Raman scattering (SERS)
   sensor based on the Ag-Zn(OH)F network. The fabricated Ag-Zn(OH)F
   network showed rapid photocatalytic activities under ultraviolet (UV)
   irradiation, which formed the premise for the recyclable SERS substrate.
   The fabricated SERS sensor was able to detect Rhodamine-6G (R6G) in low
   concentrations ranging from 10(-13) to 10(-8)M. The ability to detect
   multicomponent analytes was also demonstrated by detecting mixtures of
   R6G and 4-methylbenzoic acid (4-MBA). In addition, the detection
   sensitivity was enhanced by bending the microcapillary.}},
