@article{ ISI:000375007400001,
Author = {Olsen, Jesse E. and Parsons, Charles K. and Martins, Luis L. and Ivanaj,
   Vera},
Title = {{Gender Diversity Programs, Perceived Potential for Advancement, and
   Organizational Attractiveness: An Empirical Examination of Women in the
   United States and France}},
Journal = {{GROUP \& ORGANIZATION MANAGEMENT}},
Year = {{2016}},
Volume = {{41}},
Number = {{3}},
Pages = {{271-309}},
Month = {{JUN}},
Abstract = {{We conducted an experimental study to compare the effects of
   organization-sponsored gender diversity management programs on women's
   evaluations of organizational attractiveness in two countries: the
   United States and France. Importantly, we examined perceived potential
   for advancement as a mediator of the relationship, thus elaborating on
   an underlying mechanism implied by signaling theory. Results from a
   sample of 230 women in the United States and France provided overall
   support for the model. We found that country and individual
   characteristics affect how diversity management signals are translated
   into perceptions of the organization. We discuss the theoretical and
   practical contributions of the study, as well as limitations and
   opportunities for future research.}},
