@article{ ISI:000379956300002,
Author = {Abbas, Waseem and Egerstedt, Magnus and Liu, Chun-Hung and Thomas, Robin
   and Whalen, Peter},
Title = {{Deploying Robots With Two Sensors in K-1,K-6-Free Graphs}},
Journal = {{JOURNAL OF GRAPH THEORY}},
Year = {{2016}},
Volume = {{82}},
Number = {{3}},
Pages = {{236-252}},
Month = {{JUL}},
Abstract = {{Let G be a graph of minimum degree at least 2 with no induced subgraph
   isomorphic to K-1,K-6. We prove that if G is not isomorphic to one of
   eight exceptional graphs, then it is possible to assign two-element
   subsets of \{1, 2, 3, 4, 5\} to the vertices of G in such a way that for
   every i is an element of \{1, 2, 3, 4, 5\} and every vertex v is an
   element of V (G) the label i is assigned to v or one of its neighbors.
   It follows that G has fractional domatic number at least 5/2. This is
   motivated by a problem in robotics and generalizes a result of Fujita,
   Yamashita, and Kameda who proved that the same conclusion holds for all
   3-regular graphs. (C) 2015 Wiley Periodicals, Inc.}},
