@article{ ISI:000376097300029,
Author = {Wang, Li and Li, Ruoguang and Cao, Chunyan and Stuber, Gordon L.},
Title = {{SNR Analysis of Time Reversal Signaling on Target and Unintended
   Receivers in Distributed Transmission}},
Journal = {{IEEE TRANSACTIONS ON COMMUNICATIONS}},
Year = {{2016}},
Volume = {{64}},
Number = {{5}},
Pages = {{2176-2191}},
Month = {{MAY}},
Abstract = {{This paper analyzes the effect of distributed time-reversal (DTR)
   transmission scheme on the signal-to-noise ratio (SNR) at its intended
   and unintended receivers. By focusing the temporal and spatial signal
   energy on the intended receiver, DTR can effectively maintain a
   satisfactory SNR level while lowering received signal level at passive
   eavesdroppers or unintended co-channel users. The DTR performance is
   analyzed in terms of a SNR gap between the desired and unintended
   receivers. The SNR gain of DTR is also analyzed over traditional
   distributed direct transmission and several cooperative beamforming
   transmission schemes without time-reversal. Numerical results
   demonstrate the performance improvement of the time-reversal
   transmission and the validity of our analytical results.}},
