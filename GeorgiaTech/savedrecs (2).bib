@article{ ISI:000382488800014,
Author = {Langley, Jason and Huddleston, Daniel E. and Merritt, Michael and Chen,
   Xiangchuan and McMurray, Rebecca and Silver, Michael and Factor, Stewart
   A. and Hu, Xiaoping},
Title = {{Diffusion Tensor Imaging of the Substantia Nigra in Parkinson's Disease
   Revisited}},
Journal = {{HUMAN BRAIN MAPPING}},
Year = {{2016}},
Volume = {{37}},
Number = {{7}},
Pages = {{2547-2556}},
Month = {{JUL}},
Abstract = {{Objective: To analyze diffusion tensor imaging (DTI) data in the
   substantia nigra (SN) using a more consistent region of interest (ROI)
   defined by neuromelanin-sensitive MRI in order to assess Parkinson's
   disease (PD) related changes in diffusion characteristics in the SN.
   Methods: T-1-weighted and DTI data were obtained in a cohort of 37
   subjects (17 control subjects and 20 subjects with PD). The subjects in
   the PD group were clinically diagnosed PD patients with an average
   Unified Parkinsonian Disease Rating Scale (UPDRS)-III score of 23.2 +/-
   9.3. DTI data were analyzed using SN ROIs defined by
   neuromelanin-sensitive MRI and, for comparison, with ROIs defined on
   T-2-weighted images (b = 0 images). Results: Compared with control
   subjects, significantly lower fractional anisotropy was observed in PD
   in the neuromelanin SN ROI but not in the ROI derived from the
   T-2-weighted image. This decrease was largest in the rostral and lateral
   portions of the neuromelanin volume, which were found to have more
   hypointensity in the T-2-weighted image and, presumably, higher iron
   content in the PD group. In addition, a larger decrease in fractional
   anisotropy was seen in the SN region of interest on the side
   contralateral to the side exhibiting more severe symptoms. These results
   indicate that the use of neuromelanin sensitive MRI to define the ROI in
   the SN for analyzing DTI data leads to more significant results,
   enhancing the robustness of DTI study and DTI based biomarkers of PD.
   (C) 2016 Wiley Periodicals, Inc.}},
