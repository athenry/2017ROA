@article{ ISI:000376805500002,
Author = {Li, Yang and Niu, Junfeng and Shang, Enxiang and Crittenden, John
   Charles},
Title = {{Influence of dissolved organic matter on photogenerated reactive oxygen
   species and metal-oxide nanoparticle toxicity}},
Journal = {{WATER RESEARCH}},
Year = {{2016}},
Volume = {{98}},
Pages = {{9-18}},
Month = {{JUL 1}},
Abstract = {{The effect of humic acid (HA) or fulvic acid (FA) on reactive oxygen
   species (ROS) generation by six metal-oxide nanoparticles (NPs) and
   their toxicities toward Escherichia coli was investigated under UV
   irradiation. Dissolved organic matter (DOM) decreased (OH)-O-center dot
   generation by TiO2, ZnO, and Fe2O3, with FA inhibiting (OH)-O-center dot
   generation more than HA. The generated (OH)-O-center dot in NPs/DOM
   mixtures was higher than the measured concentrations because DOM
   consumes (OH)-O-center dot faster than its molecular probe. None of
