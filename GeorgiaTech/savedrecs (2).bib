@article{ ISI:000376417600080,
Author = {Muroyama, Alexander P. and Loutzenhiser, Peter G.},
Title = {{Kinetic Analyses of Gasification and Combustion Reactions of
   Carbonaceous Feedstocks for a Hybrid Solar/Autothermal Gasification
   Process To Continuously Produce Synthesis Gas}},
Journal = {{ENERGY \& FUELS}},
Year = {{2016}},
Volume = {{30}},
Number = {{5}},
Pages = {{4292-4299}},
Month = {{MAY}},
Abstract = {{A hybrid solar/autothermal steam gasification process to continuously
   produce synthesis gas from carbonaceous feedstocks is considered.
   Langmuir-Hinshelwood-type reaction mechanisms for gasification with CO2
   and H2O(v) and combustion were used with non-isothermal thermogravimetry
   to determine kinetic parameters for three feedstocks: (1) activated
   charcoal, (2) bituminous coal char, and (3) miscanthus char. Temperature
   ranges of 550-960 and 1120-1270 K and concentrations of 10-40\% O-2-Ar
   and 20-100\% H2O-CO2 were evaluated for combustion and gasification,
   respectively. Carbonaceous feedstock microstructures were examined using
   scanning electron microscopy. Surface area and porosity were
   characterized using Brunauer-Emmett-Teller analysis.}},
