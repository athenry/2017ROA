@article{ ISI:000314378900017,
Author = {Russell, Ryan P. and Arora, Nitin},
Title = {{Global Point Mascon Models for Simple, Accurate, and Parallel
   Geopotential Computation}},
Journal = {{JOURNAL OF GUIDANCE CONTROL AND DYNAMICS}},
Year = {{2012}},
Volume = {{35}},
Number = {{5}},
Pages = {{1568-1581}},
Month = {{SEP-OCT}},
Note = {{21st AAS/AIAA Space Flight Mechanics Meeting, New Orleans, LA, FEB
   13-17, 2011}},
Organization = {{AAS; AIAA}},
Abstract = {{A global point mascon model for the geopotential is proposed that is
   memory light to execute, simple to implement (at any derivative level),
   and is naturally amenable to parallelism. In contrast, conventional
   high-fidelity geopotential calculation using spherical harmonics is
   expensive, cumbersome to implement, and relies on recursive nonparallel
   relations. To solve for the global point mascon model, the gravity
   inversion problem is posed classically as a large and dense
   least-squares estimation problem. The well-known ill-conditioned nature
   of the inversion is overcome in part using orthogonal solution methods,
   a judicious choice for the mascon distribution, and numerically
   preferred summation techniques. A variety of resolutions are examined
   including global point mascon models with up to 30,720 mascons.
   Measurements are simulated using truncated spherical harmonics
   evaluations from the GGM02C gravity field derived from the GRACE
   spacecraft. Resolutions are chosen to target residual levels at least an
   order of magnitude smaller than the published expected errors of the
   GGM02C. A single central processing unit Fortran implementation is found
   to be approximately equal in speed when compared with spherical
   harmonics for all resolutions, whereas a parallel implementation on an
   inexpensive graphics processing unit leads to an order of magnitude
   speedup in the case of a 156 x 156 gravity field. In addition, the
   global point mascon model is found to be particularly attractive for
   MATLAB users due to MATLAB's efficient use of large matrix operations.}},
