Author = {Llatser, Ignacio and Kremers, Christian and Cabellos-Aparicio, Albert
   and Jornet, Josep Miguel and Alarcon, Eduard and Chigrin, Dmitry N.},
Title = {{Graphene-based nano-patch antenna for terahertz radiation}},
