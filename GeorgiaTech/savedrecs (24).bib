@article{ ISI:000310261200011,
Author = {Olthof, Selina and Mehraeen, Shafigh and Mohapatra, Swagat K. and
   Barlow, Stephen and Coropceanu, Veaceslav and Bredas, Jean-Luc and
   Marder, Seth R. and Kahn, Antoine},
Title = {{Ultralow Doping in Organic Semiconductors: Evidence of Trap Filling}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2012}},
Volume = {{109}},
Number = {{17}},
Month = {{OCT 26}},
Abstract = {{Tail states in organic semiconductors have a significant influence on
   device performances by acting as traps in charge transport. We present a
   study of the controlled passivation of acceptor tail states in fullerene
   C-60 by the addition of electrons introduced by molecular n doping.
   Using ultralow doping, we are able to successively fill the traps with
   charges and examine the changes in conductivity, activation energy,
   mobility, and Fermi-level position. Passivation of the traps leads to an
   increase of the electron mobility in C-60 by more than 3 orders of
   magnitude, to reach 0.21 cm(2)/(Vs).}},
