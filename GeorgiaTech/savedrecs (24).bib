@article{ ISI:000209015100005,
Author = {Popovic, Ray and Goldsman, David},
Title = {{Easy Gram-Charlier Valuations of Options}},
Journal = {{JOURNAL OF DERIVATIVES}},
Year = {{2012}},
Volume = {{20}},
Number = {{2}},
Pages = {{79-97}},
Month = {{WIN}},
Abstract = {{Valuations and sensitivities of contingent claims on arithmetic averages
   are difficult to come by. Employing several common underlying financial
   processes, we examine a closed-form approximation of the distributions
   that determine the pricing and hedging of Asian contingent claims. For a
   chosen process, an estimate of the initial six moments of the
   distribution of the arithmetic average is constructed via simulation.
   The sample moments are then used as inputs to a Gram-Charlier expansion
   to approximate the average's probability density function. We
   demonstrate through a number of examples when the Gram-Charlier
   methodology can be expected to perform well.}},
