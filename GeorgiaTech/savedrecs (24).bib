@article{ ISI:000312780000047,
Author = {Das, Arnab and Rohatgi, Ajeet},
Title = {{Simultaneous iron gettering and passivation of p-type monocrystalline
   silicon using a negatively charged aluminum-doped dielectric}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2012}},
Volume = {{101}},
Number = {{25}},
Month = {{DEC 17}},
Abstract = {{Rapid gettering of iron from p-type c-Si has been achieved using a
   negatively charged spin-on Al-doped glass. After a 10 min oxidation to
   cure the Al-doped glass, > 99\% of Fe can be gettered from silicon
   wafers. This is comparable to, and under some processing conditions
   better than, the efficiency of conventional POCl3 gettering. In the same
   short oxidation step, the Al-doped glass also passivates p-Si surfaces
   with surface recombination velocities of 100 cm/s and 10 600 cm/s
   achieved for surface doping of 6 x 10(15) cm(-3) and 4 x 10(19) cm(-3),
   respectively. These passivation results are comparable to those achieved
   with thermal SiO2 layers. (C) 2012 American Institute of Physics.
   {[}http://dx.doi.org/10.1063/1.4771979]}},
