@article{ ISI:000314372900001,
Author = {Nguyen, Justin J. and Turano, Stephan and Ready, W. Jud},
Title = {{The Synthesis of Carbon Nanotubes Grown on Metal Substrates: A Review}},
Journal = {{NANOSCIENCE AND NANOTECHNOLOGY LETTERS}},
Year = {{2012}},
Volume = {{4}},
Number = {{12}},
Pages = {{1123-1131}},
Month = {{DEC}},
Abstract = {{Carbon nanotubes are highly conductive quasi-one dimensional structures
   that have been vigorously explored for many applications due to their
   unique properties. In many thermal and electrical applications where
   high conductivity is desired, both the carbon nanotubes and their growth
   substrate need to be as conductive as possible. However, carbon nanotube
   growth conditions limit many substrates from being applied due to the
   sensitivity of the transition metal catalyst from being poisoned or lost
   from intermetallic diffusion. These difficulties become further
   complicated with the incorporation of bulk metal substrates compared to
   more commonly used Si- or SiO2-based substrates. Despite this, there
   have been promising investigations of carbon nanotube growth on metals
   and a study summarizing recent advances has yet to be published. This
   review aims to fulfill this need and discuss current progress and
   challenges. Successful growth techniques and catalyst support layers are
   emphasized and it is observed that a better understanding of the
   microstructural evolution of catalyst and substrate is needed.}},
