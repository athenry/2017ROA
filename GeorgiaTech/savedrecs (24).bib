@article{ ISI:000311111900014,
Author = {Xie, Shuifen and Jin, Mingshang and Tao, Jing and Wang, Yucai and Xie,
   Zhaoxiong and Zhu, Yimei and Xia, Younan},
Title = {{Synthesis and Characterization of Pd@MxCu1-x (M=Au, Pd, and Pt)
   Nanocages with Porous Walls and a Yolk-Shell Structure through Galvanic
   Replacement Reactions}},
