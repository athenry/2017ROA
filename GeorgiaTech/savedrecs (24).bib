@article{ ISI:000309329300052,
Author = {Bernal, A. and Tselev, A. and Kalinin, Sergei and Bassiri-Gharb, Nazanin},
Title = {{Effects of lateral and substrate constraint on the piezoresponse of
   ferroelectric nanostructures}},
