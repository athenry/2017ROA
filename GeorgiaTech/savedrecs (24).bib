@article{ ISI:000310261200007,
Author = {Aguilar, Jeffrey and Lesov, Alex and Wiesenfeld, Kurt and Goldman,
   Daniel I.},
Title = {{Lift-Off Dynamics in a Simple Jumping Robot}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2012}},
Volume = {{109}},
Number = {{17}},
Month = {{OCT 26}},
Abstract = {{We study vertical jumping in a simple robot comprising an actuated
   mass-spring arrangement. The actuator frequency and phase are
   systematically varied to find optimal performance. Optimal jumps occur
   above and below (but not at) the robot's resonant frequency f(0). Two
   distinct jumping modes emerge: a simple jump, which is optimal above
   f(0), is achievable with a squat maneuver, and a peculiar stutter jump,
   which is optimal below f(0), is generated with a countermovement. A
   simple dynamical model reveals how optimal lift-off results from
   nonresonant transient dynamics.}},
