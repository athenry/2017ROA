@article{ ISI:000310592000002,
Author = {Yoda, Minami and Kim, Myeongsub},
Title = {{Studying Interfacial Transport With Evanescent Wave-Based Particle
   Velocimetry and Thermometry}},
Journal = {{HEAT TRANSFER ENGINEERING}},
Year = {{2013}},
Volume = {{34}},
Number = {{2-3}},
Pages = {{101-112}},
Note = {{9th International Conference on Nanochannels, Microchannels and
   Minichannels, Univ Alberta Edmonton, Edmonton, CANADA, JUN 19-22, 2011}},
Organization = {{Amer Soc Mech Engineers}},
Abstract = {{Interfacial phenomena due to surface forces are important in
   microfluidic devices with their relatively large surface areas and small
   volumes. This article reviews our recent studies measuring fluid
   velocities in Poiseuille and electrokinetically driven flows over the
   first similar to 0.4 mu m next to the wall from the motion of an
   ensemble of O(10(5)) fluorescent particles illuminated by evanescent
   waves. Because the evanescent-wave intensity decays exponentially with
   wall-normal distance, the particle-wall separation can be determined
   from the brightness of each particle image, and used to estimate the
   steady-state distribution of the tracers near the wall. More recently,
   evanescent-wave illumination has been combined with fluorescence
   thermometry, to estimate water temperature fields from changes in the
   fluorescence intensity of aqueous fluorophore solutions. Poiseuille flow
   of a fluorescein solution at Reynolds numbers of 3.3 and 8.3 through a
   heated minichannel was illuminated with evanescent waves to measure
   solution temperatures at an average distance of 74 nm from the wall. The
   temperature results obtained over three different regions were in good
   agreement with numerical predictions of the wall surface temperature,
   even in the presence of temperature variations exceeding 9 degrees C
   over the 1-mm width of the channel.}},
