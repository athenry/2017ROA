@article{ ISI:000310834700006,
Author = {Engineer, Faramroze G. and Nemhauser, George L. and Savelsbergh, Martin
   W. P. and Song, Jin-Hwa},
Title = {{The Fixed-Charge Shortest-Path Problem}},
Journal = {{INFORMS JOURNAL ON COMPUTING}},
