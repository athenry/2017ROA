@inproceedings{ ISI:000377723700218,
Author = {Putnam, Zachary R. and Braun, Robert D.},
Editor = {{Tanygin, S and Park, RS and Starchville, TF and Newman, LK}},
Title = {{PRECISION LANDING AT MARS USING DISCRETE-EVENT DRAG MODULATION}},
