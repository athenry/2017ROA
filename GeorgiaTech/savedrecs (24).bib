@article{ ISI:000314748600027,
Author = {Li, Xinying and Yu, Jianjun and Dong, Ze and Chien, Hung-Chang and Jia,
   Zhensheng and Chi, Nan},
Title = {{WDM transmission of 108.4-Gbaud PDM-QPSK signals (40 x 433.6-Gb/s) over
   2800-km SMF-28 with EDFA-only}},
Journal = {{OPTICS EXPRESS}},
Year = {{2012}},
Volume = {{20}},
Number = {{26}},
Pages = {{B217-B222}},
Month = {{DEC 10}},
Abstract = {{We experimentally demonstrated the transmission of 40 x 433.6Gb/s
   Nyquist wavelength-division-multiplexing (N-WDM) optical time-division-
   multiplexing (OTDM) over 2800-km single-mode fiber (SMF)-28 with
   Erbium-doped fiber amplifier (EDFA)-only amplification, adopting
   polarization-division-multiplexing carrier-suppressed return-to-zero
   quadrature-phase-shift-keying (PDM-CSRZ-QPSK) modulation as well as post
   filter and 1-bit maximum likelihood sequence estimation (MLSE). Each
   channel occupies 100GHz, yielding a spectral efficiency of 4.05b/s/Hz.
   The bit-error ratio (BER) of all channels is less than the pre-forward-
   error-correction (pre-FEC) limit of 3.8 x 10(-3) after 2800-km SMF-28
   transmission. (C)2012 Optical Society of America}},
