Author = {Sun, Jie and Yu, Jiancheng and Zhang, Aiqun and Zhang, Fumin},
Book-Group-Author = {{IEEE}},
Title = {{Navigation Positioning Algorithm for Underwater Gliders in
   Three-Dimensional Space}},
