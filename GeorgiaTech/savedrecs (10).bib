Author = {Zhang, Xiaoming and Zhang, Dong and Lee, Kok-Meng and Ding, Han},
Book-Group-Author = {{IEEE}},
Title = {{Flexible Workpiece Vibration Suppression in Milling Process Based on a
   New Response Metric}},
Booktitle = {{2015 IEEE/ASME INTERNATIONAL CONFERENCE ON ADVANCED INTELLIGENT
   MECHATRONICS (AIM)}},
Series = {{IEEE ASME International Conference on Advanced Intelligent Mechatronics}},
Year = {{2015}},
