@inproceedings{ ISI:000380581600424,
Author = {Wu, Ji and Li, Miao and Lee, Chin-Hui},
Book-Group-Author = {{ISCA-INT SPEECH COMMUN ASSOC}},
Title = {{An Entropy Minimization Framework for Goal-Driven Dialogue Management}},
Booktitle = {{16TH ANNUAL CONFERENCE OF THE INTERNATIONAL SPEECH COMMUNICATION
   ASSOCIATION (INTERSPEECH 2015), VOLS 1-5}},
Year = {{2015}},
