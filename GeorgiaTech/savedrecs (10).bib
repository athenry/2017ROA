@inproceedings{ ISI:000381622900074,
Author = {Ahmed, Aftab and Verriest, Erik I.},
Book-Group-Author = {{IEEE}},
Title = {{Design of Linear \& Nonlinear Observers for a Turning Process With
   Implicit State-Dependent Delay}},
Booktitle = {{2015 53RD ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2015}},
