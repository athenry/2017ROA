@inproceedings{ ISI:000381554501125,
Author = {Zhao, Huihua and Horn, Jonathan and Reher, Jacob and Paredes, Victor and
   Ames, Aaron D.},
Book-Group-Author = {{IEEE}},
Title = {{A Hybrid Systems and Optimization-Based Control Approach to Realizing
   Multi-Contact Locomotion on Transfemoral Prostheses}},
Booktitle = {{2015 54TH IEEE CONFERENCE ON DECISION AND CONTROL (CDC)}},
Year = {{2015}},
