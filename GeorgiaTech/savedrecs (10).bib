@article{ ISI:000348958000003,
Author = {Xiong, Liming and Xu, Shuozhi and McDowell, David L. and Chen, Youping},
Title = {{Concurrent atomistic-continuum simulations of dislocation-void
   interactions in fcc crystals}},
Journal = {{INTERNATIONAL JOURNAL OF PLASTICITY}},
Year = {{2015}},
Volume = {{65}},
Pages = {{33-42}},
Month = {{FEB}},
Abstract = {{Dislocation interactions with distributed condensed vacancy clusters in
   fcc metals were simulated via a concurrent atomistic-continuum method.
   Due to void strengthening, the dislocation lines are found to bow as a
   result of pinning on the original glide plane and undergo depinning
   through drawing out screw dipoles and forming prismatic loops on the
   secondary slip plane. We discovered an inertia-induced transition
   between Hirsch looping and void shearing mechanisms as the void spacing
   ranges from the scale of nm to hundreds of nm. Contrary to prior
   understanding, simulations suggest that large voids nm in diameter) can
   behave as weak barriers to dislocation motions under high strain-rate
   dynamic conditions. (C) 2014 Elsevier Ltd. All rights reserved.}},
