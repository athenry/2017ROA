@inproceedings{ ISI:000382389301032,
Author = {Zhang, Chuanji and Han, Chong and Akyildiz, Ian F.},
Book-Group-Author = {{IEEE}},
Title = {{Three Dimensional End-to-End Modeling and Directivity Analysis for
   Graphene-based Antennas in the Terahertz Band}},
