@article{ ISI:000350578800006,
Author = {Russell, Marie and Mokhtarian, Patricia},
Title = {{How real is a reported desire to travel for its own sake? Exploring the
   `teleportation' concept in travel behaviour research}},
Journal = {{TRANSPORTATION}},
Year = {{2015}},
Volume = {{42}},
Number = {{2}},
Pages = {{333-345}},
Month = {{MAR}},
Abstract = {{The `teleportation test' has been used in travel behaviour research for
   more than a decade, as a means of assessing whether an individual views
   travel purely as a disutility. The teleportation concept has been used
   successfully in qualitative research to elicit responses and clarify
   attitudes to travel time. However, survey-based studies have revealed an
   apparent inconsistency, in that many people who report an ideal travel
   time greater than zero, and/or do not consider their travel time as
   wasted, also report wanting to teleport. This note reviews a range of
   studies involving the teleportation concept, highlights the
   inconsistency, suggests reasons for it, and proposes a research approach
   for testing the validity of those reasons.}},
