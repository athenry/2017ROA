Title = {{An Adaptive Scheme of Sharing Compressed Flow Information Among
   Networked Underwater Gliders}},
Booktitle = {{2015 IEEE International Conference on Cyber Technology in Automation,
   Control, and Intelligent Systems (CYBER)}},
Series = {{IEEE Annual International Conference on Cyber Technology in Automation
   Control and Intelligent Systems}},
Year = {{2015}},
