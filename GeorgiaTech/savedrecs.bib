@article{ ISI:000400693900001,
Author = {Singh, Gautam and Fu, Jinxin and Agra-Kooijman, Dena M. and Song,
   Jang-Kun and Vengatesan, M. R. and Srinivasarao, Mohan and Fisch,
   Michael R. and Kumar, Satyendra},
Title = {{X-ray and Raman scattering study of orientational order in nematic and
   heliconical nematic liquid crystals}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2016}},
Volume = {{94}},
Number = {{6}},
Month = {{DEC 13}},
Abstract = {{The temperature dependence of the orientational order parameters <
   P-2(cos beta)> and < P-4(cos beta)> in the nematic (N) and twist-bend
   nematic (N-tb) phases of the liquid crystal dimer CB7CB have been
   measured using x-ray and polarized Raman scattering. The < P-2(cos
   beta)> obtained from both techniques are the same, while < P-4(cos
   beta)>, determined by Raman scattering is, as expected, systematically
   larger than its x-ray value. Both order parameters increase in theN
   phase with decreasing temperature, drop across the N-N-tb transition,
   and continue to decrease. In the N-tb phase, the x-ray value of <
   P-4(cos beta)> eventually becomes negative, providing a direct and
   independent confirmation of a conical molecular orientational
   distribution. The heliconical tilt angle a, determined from
   orientational distribution functions in the N-tb phase, increases to
   similar to 24 degrees at similar to 15 K below the transition. In the
   N-tb phase, alpha(T)alpha(T {*}-T)(lambda), with lambda = 0.19 +/- 0.03.
   The transition supercools by 1.7 K, consistent with its weakly
   first-order nature. The value of lambda is close to 0.25 indicating
   close proximity to a tricritical point.}},
