@article{ ISI:000395259300012,
Author = {Sadeghi-Tohidi, F. and Pierron, O. N.},
Title = {{Towards prediction of the fatigue life of Ni microbeams under extreme
   stress gradients}},
Journal = {{EXTREME MECHANICS LETTERS}},
Year = {{2016}},
Volume = {{9}},
Number = {{1}},
Pages = {{97-107}},
Month = {{DEC}},
Abstract = {{This work investigated the effect of two extreme normalized stress
   gradients (eta = 17\% vs. 36\% mu m(-1)) on the fully-reversed bending
   fatigue behavior (fatigue life and fatigue crack propagation curves) of
   20-mu m-thick, electroplated Ni microbeams, in humid air environments.
   The results highlight the significant challenge in predicting the
   bending fatigue life of microbeams subjected to extreme stress
   gradients, which was measured to be three order of magnitudes larger for
   eta = 36\% mu m(-1) at a stress amplitude of similar to 450 MPa. The
   fatigue life is dominated by the ultraslow growth of microstructurally
   small cracks, which is a strong function of the normalized stress
   gradient. For eta= 17\% mu m(-1), the crack growth rates are initially
   about one order of magnitude larger than for. = 36\% mu m(-1) and, in
   contrast to the larger stress gradient microbeams, do not decrease with
   increasing crack size. This singular behavior results in low Basquin and
   Coffin-Manson exponents (in absolute value) compared to eta= 0. As a
   result, the fatigue endurance limit increases from 35\% to 50\% of the
   tensile strength for eta increasing from 17\% to 36\% mu m(-1), compared
   to 30\% in the absence of stress gradients. The environmental effects
   are also discussed. (C) 2016 Elsevier Ltd. All rights reserved.}},
