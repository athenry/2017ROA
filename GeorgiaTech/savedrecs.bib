@article{ ISI:000388553400010,
Author = {Li, Zhenhai and Lee, Hyunjung and Zhu, Cheng},
Title = {{Molecular mechanisms of mechanotransduction in integrin-mediated
   cell-matrix adhesion}},
Journal = {{EXPERIMENTAL CELL RESEARCH}},
Year = {{2016}},
Volume = {{349}},
Number = {{1}},
Pages = {{85-94}},
Month = {{NOV 15}},
Abstract = {{Cell-matrix adhesion complexes are multi-protein structures linking the
   extracellular matrix (ECM) to the cytoskeleton. They are essential to
   both cell motility and function by bidirectionally sensing and
   transmitting mechanical and biochemical stimulations. Several types of
   cell-matrix adhesions have been identified and they share many key
   molecular components, such as integrins and actin-integrin linkers.
   Mechanochemical coupling between ECM molecules and the actin
   cytoskeleton has been observed from the single cell to the single
   molecule level and from immune cells to neuronal cells. However, the
   mechanisms underlying force regulation of integrin-mediated
   mechanotransduction still need to be elucidated. In this review article,
   we focus on integrin-mediated adhesions and discuss force regulation of
   cell-matrix adhesions and key adaptor molecules, three different
   force-dependent behaviors, and molecular mechanisms for mechanochemical
   coupling in force regulation.}},
