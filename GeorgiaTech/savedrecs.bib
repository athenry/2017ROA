@article{ ISI:000391079700039,
Author = {Xiong, Xunhui and Wang, Guanhua and Lin, Yuwei and Wang, Ying and Ou,
   Xing and Zheng, Fenghua and Yang, Chenghao and Wang, Jeng-Han and Liu,
   Meilin},
Title = {{Enhancing Sodium Ion Battery Performance by Strongly Binding
   Nanostructured Sb2S3 on Sulfur-Doped Graphene Sheets}},
Journal = {{ACS NANO}},
Year = {{2016}},
Volume = {{10}},
Number = {{12}},
Pages = {{10953-10959}},
Month = {{DEC}},
Abstract = {{Sodium ion batteries (SIBs) have been considered a promising alternative
   to lithium ion batteries for large-scale energy storage. However, their
   inferior electrochemical performances, especially cyclability, become
   the major challenge for further development of SIBs. Large volume change
   and sluggish diffusion kinetics are generally considered to be
   responsible for the fast capacity degradation. Here we report the strong
   chemical bonding of nanostructured Sb2S3 on sulfur-doped graphene sheets
   (Sb2S3/SGS) that enables a stable capacity retention of 83\% for 900
   cycles with high capacities and excellent rate performances. To the best
   of our knowledge, the cycling performance of the Sb2S3/SGS composite is
   superior to those reported for any other Sb-based materials for SIBs.
   Computational calculations demonstrate that sulfur-doped graphene (SGS)
   has a stronger affinity for Sb2S3 and the discharge products than pure
   graphene, resulting in a robust composite architecture for outstanding
   cycling stability. Our study shows a feasible and effective way to solve
   the long-term cycling stability issue for SIBs.}},
