@article{ ISI:000391723600018,
Author = {Huang, Renke and Cokkinides, George and Hedrington, Jr., Clinton and
   Meliopoulos, Sakis A. P.},
Title = {{Distribution System Distributed Quasi-Dynamic State Estimator}},
Journal = {{IEEE TRANSACTIONS ON SMART GRID}},
Year = {{2016}},
Volume = {{7}},
Number = {{6}},
Pages = {{2761-2770}},
Month = {{NOV}},
Abstract = {{With increasing deployment of smart meters and other smart grid sensors
   in distribution systems, the amount of available measurements has been
   growing. This change enables implementation of comprehensive
   distribution system state estimators, a basic tool that provides the
   real-time model and operating conditions of the distribution network.
   The performance of many smart grid applications is dependent upon an
   accurate and reliable real-time model. This paper presents the
   development of a distributed quasi-dynamic state estimator (DS-DQSE) for
   distribution systems utilizing smart meter and other sensor data, as
   well as traditional SCADA data. The DS-DQSE implementation described in
   this paper works for both radial or meshed distribution networks and
   variable configuration systems, as distribution systems may be
   reconfigured following the clearing of a fault. This paper provides the
   problem formulation, solution methodology, modeling issues unique to
   distribution systems, and test results on multiple distribution
   circuits.}},
