@article{ ISI:000382410100116,
Author = {Ding, Longjiang and Zhao, Minggang and Ma, Ye and Fan, Sisi and Wen,
   Zhen and Huang, Jingyun and Liang, Jingjing and Chen, Shougang},
Title = {{Triggering interface potential barrier: A controllable tuning mechanism
   for electrochemical detection}},
Journal = {{BIOSENSORS \& BIOELECTRONICS}},
Year = {{2016}},
Volume = {{85}},
Pages = {{869-875}},
Month = {{NOV 15}},
Abstract = {{A novel theory of employing interface potential barriers as a
   controllabe tuning factor for electrochemical detection is proposed. The
   3D NiO/PANI/ZnO hierarchical heterostructure is fabricated by thermal
   oxidation, electropolymerization and electrodeposition. The 3D
   NiO/PANI/ZnO heterostructure is then chose as a model for
   electrochemical detection of dopamine, uric acid and ascorbic acid. The
   p-n and p-p junction interface potential barriers are employed as tuning
   factors to achieve high selectivity and sensitivity. Our results
   demonstrate the electrochemical response to different targets can be
   controllable enhanced or weakened by rational design of interface
   potential barriers. The potential barrier height phi(p-n) is an enhanced
   tuning factor, and phi(p-p) is a selective tuning factor. We afford a
   controllable adjustive approach to achieve desired selectivity and
   sensitivity. (C) 2016 Elsevier B.V. All rights reserved.}},
