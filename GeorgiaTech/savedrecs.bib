@article{ ISI:000386959700002,
Author = {Yang, Qichen and Qin, Jiangchao and Saeedifard, Maryam},
Title = {{A Postfault Strategy to Control the Modular Multilevel Converter Under
   Submodule Failure}},
Journal = {{IEEE TRANSACTIONS ON POWER DELIVERY}},
Year = {{2016}},
Volume = {{31}},
Number = {{6}},
Pages = {{2453-2463}},
Month = {{DEC}},
Abstract = {{The modular multilevel converter ( MMC) has become one of the most
   promising topologies for high-voltage applications. Although the MMC has
   a modular and redundant structure, which can significantly enhance its
   reliability and fault tolerance, its fault handling capability can be
   further improved by adopting a postfault strategy. In this paper, a
   postfault strategy based on the neutral-shift method is proposed, which,
   subsequent to bypassing the faulty submodule( s), aims at improving the
   maximum attainable line-to-line voltages of the MMC. Based on a newly
   developed internal variable analysis method, the impact of the
   neutral-shift strategy on the operation of the MMC is analyzed. In
   conjunction with the proposed postfault strategy, a modified circulating
   current suppression ( CCS) method is used to mitigate the
   double-frequency component of the circulating currents. Performance and
   effectiveness of the proposed neutral-shift strategy with and without
   the CCS method is evaluated based on time-domain simulation studies in
   the PSCAD/EMTDC software environment.}},
