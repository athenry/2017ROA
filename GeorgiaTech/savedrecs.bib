@article{ ISI:000385484100034,
Author = {Mao, Yimin and Kriegel, Robert M. and Bucknall, David G.},
Title = {{The crystal structure of poly(ethylene furanoate)}},
Journal = {{POLYMER}},
Year = {{2016}},
Volume = {{102}},
Pages = {{308-314}},
Month = {{OCT 12}},
Abstract = {{The crystal structure of poly(ethylene furanoate) (PEF) has been
   determined using X-ray fiber diffraction (XRFD) combined with molecular
   modeling. Analysis of the XRFD data suggested the space group was P2(1),
   with a monoclinic unit cell where a = 5.784 angstrom, b = 6.780
   angstrom, c = 20.296 angstrom, and gamma = 103.3 degrees, giving a
   calculated unit cell density of the crystal phase was 1.562 g/cm(3). The
   repeating unit consisted of two monomers related to each other by a 2(1)
   screw axis. It was also shown that two PEF chains were included in each
   unit cell, with the center-chain being staggered with respect to the
   corner-chain. Polymer chains were found to be near planar, although
   careful evaluation showed that the best fit with the data required the
   cell to contain a mixture of 3/12 and 5/12 center-chain-staggering. (C)
   2016 Published by Elsevier Ltd.}},
