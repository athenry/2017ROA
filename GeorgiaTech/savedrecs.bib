@article{ ISI:000384853900002,
Author = {Gu, Manzhan and Lu, Xiwen and Gu, Jinwei and Zhang, Ying},
Title = {{Single-machine scheduling problems with machine aging effect and an
   optional maintenance activity}},
Journal = {{APPLIED MATHEMATICAL MODELLING}},
Year = {{2016}},
Volume = {{40}},
Number = {{21-22}},
Pages = {{8862-8871}},
Month = {{NOV}},
Abstract = {{This paper considers two single-machine scheduling problems with a new
   type of aging effect, which is dominated by the processing speed of the
   machine. During the whole scheduling horizon, the machine is subject to
   an optional maintenance, and the duration of the maintenance depends on
   the length of the uptime before it. The objective is to schedule all
   jobs and find the location of the maintenance so as to minimize the
   makespan or the total completion times. The two problems are proved to
   be NP-complete, and two dynamic programming algorithms are proposed to
   solve the problems. We analyze the computation complexity of the
   algorithms, and show that the problems under study are solvable in
   polynomial time if the processing loads of all jobs are uniformly
   bounded. (C) 2016 Elsevier Inc. All rights reserved.}},
