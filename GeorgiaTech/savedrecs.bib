@article{ ISI:000388716500008,
Author = {Angoshtari, Arzhang and Yavari, Arash},
Title = {{Hilbert complexes of nonlinear elasticity}},
Journal = {{ZEITSCHRIFT FUR ANGEWANDTE MATHEMATIK UND PHYSIK}},
Year = {{2016}},
Volume = {{67}},
Number = {{6}},
Month = {{DEC}},
Abstract = {{We introduce some Hilbert complexes involving second-order tensors on
   flat compact manifolds with boundary that describe the kinematics and
   the kinetics of motion in nonlinear elasticity. We then use the general
   framework of Hilbert complexes to write Hodge-type and Helmholtz-type
   orthogonal decompositions for second-order tensors. As some applications
   of these decompositions in nonlinear elasticity, we study the strain
   compatibility equations of linear and nonlinear elasticity in the
   presence of Dirichlet boundary conditions and the existence of stress
   functions on non-contractible bodies. As an application of these Hilbert
   complexes in computational mechanics, we briefly discuss the derivation
   of a new class of mixed finite element methods for nonlinear elasticity.}},
