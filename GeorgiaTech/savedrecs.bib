@article{ ISI:000387484200006,
Author = {Pritchard, Benjamin P. and Chow, Edmond},
Title = {{Horizontal Vectorization of Electron Repulsion Integrals}},
Journal = {{JOURNAL OF COMPUTATIONAL CHEMISTRY}},
Year = {{2016}},
Volume = {{37}},
Number = {{28}},
Pages = {{2537-2546}},
Month = {{OCT 30}},
Abstract = {{We present an efficient implementation of the Obara-Saika algorithm for
   the computation of electron repulsion integrals that utilizes vector
   intrinsics to calculate several primitive integrals concurrently in a
   SIMD vector. Initial benchmarks display a 2-4 times speedup with AVX
   instructions over comparable scalar code, depending on the basis set.
   Speedup over scalar code is found to be sensitive to the level of
   contraction of the basis set, and is best for (l(A)l(B)vertical bar
   l(C)l(D)) quartets when l(D)=0 or l(B)=l(D)=0, which makes such a
   vectorization scheme particularly suitable for density fitting. The
   basic Obara-Saika algorithm, how it is vectorized, and the performance
   bottlenecks are analyzed and discussed. (C) 2016 Wiley Periodicals, Inc.}},
