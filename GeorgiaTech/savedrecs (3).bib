@article{ ISI:000377264900001,
Author = {Valenta, Christopher R. and Durgin, Gregory D.},
Title = {{Ultra-low-power energy harvesting using power-optimized waveforms}},
Journal = {{WIRELESS POWER TRANSFER}},
Year = {{2016}},
Volume = {{3}},
Number = {{1}},
Pages = {{1-8}},
Month = {{MAR}},
Abstract = {{Power-optimized waveforms (POWs) are the enabling technology for
   realizing an internet-of-things (IoTs). An IoT will require billions or
   trillions of sensors, which must rely on passive, backscatter
   communication to facilitate the wireless transfer of information.
   Passive, backscatter sensors are uniquely suited for an IoT because of
   their ease of installation, low-cost, and lack of potentially toxic
   batteries. POW's primary benefit is that they can greatly improve the
   energy-harvesting efficiency of passive sensors, which increases their
   range and reliability. An overview of POWs is presented followed by
   measured results validated by a theoretical model and computer
   simulations. These measured results conducted at 5.8 GHz demonstrate the
   highest reported efficiency of a low-power, microwave energy-harvesting
   circuit of 26.3\% at an input power of -10.2 dBm when using an
   excitation signal with a peak-to-average-power ratio of 12.}},
