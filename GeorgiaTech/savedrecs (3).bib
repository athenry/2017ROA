@article{ ISI:000373655800056,
Author = {Thomas, Evan A. and Tellez-Sanchez, Sarita and Wick, Carson and Kirby,
   Miles and Zambrano, Laura and Rosa, Ghislaine Abadie and Clasen, Thomas
   F. and Nagel, Corey},
Title = {{Behavioral Reactivity Associated With Electronic Monitoring of
   Environmental Health Interventions-A Cluster Randomized Trial with Water
   Filters and Cookstoves}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2016}},
Volume = {{50}},
Number = {{7}},
Pages = {{3773-3780}},
Month = {{APR 5}},
Abstract = {{Subject reactivity-when research participants change their behavior in
   response to being observed-has been documented showing the effect of
   human observers. Electronics sensors are increasingly used to monitor
   environmental health interventions, but the effect of sensors on
   behavior has not been assessed. We conducted a cluster randomized
   controlled trial in Rwanda among 170 households (70 blinded to the
   presence of the sensor, 100 open) testing whether awareness of an
   electronic monitor would result in a difference in weekly use of
   household water filters and improved cookstoves over a four-week
   surveillance period. A 63\% increase in number of uses of the water
   filter per week between the groups was observed in week 1, an average of
   4.4 times in the open group and 2.83 times in the blind group, declining
   in week 4 to an insignificant 55\% difference of 2.82 uses in the open,
   and 1.93 in the blind. There were no significant differences in the
   number of stove uses per week between the two groups. For both filters
   and stoves, use decreased in both groups over four-week installation
   periods. This study suggests behavioral monitoring should attempt to
   account for reactivity to awareness of electronic monitors that persists
   for weeks or more.}},
