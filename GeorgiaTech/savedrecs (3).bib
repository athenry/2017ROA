@article{ ISI:000370894900013,
Author = {Phillips, Oluwadamilola and Schwartz, Jared M. and Kohl, Paul A.},
Title = {{Thermal decomposition of poly(propylene carbonate): End-capping,
   additives, and solvent effects}},
Journal = {{POLYMER DEGRADATION AND STABILITY}},
Year = {{2016}},
Volume = {{125}},
Pages = {{129-139}},
Month = {{MAR}},
Abstract = {{Two routes to the thermal decomposition of poly(propylene carbonate)
   (PPC) have been considered polymer chain-end unzipping and random chain
   scission. The inhibition and catalysis of each mechanism has been
   studied. End-capping of low molecular weight (2 kDa) PPC has been
   achieved and confirmed with NMR analysis resulting in stabilization from
   unzipping. Citric acid was used as an additive and shown to stabilize
   the PPC backbone from random chain scission. The combination of both
   stabilizing agents (i.e. unzipping and ransom chain scission) results in
   the highest thermal stability. Photo-acid and base generators were
   incorporated into films and used to catalyze decomposition pathways of
   PPC. Residual solvent effects in PPC, such as methanol used in its
   purification, have been shown to affect the thermal stability through
   interactions with the backbone of PPC. This study demonstrates a widened
   thermal stability window for PPC, such as when used as a sacrificial
   material. (C) 2016 Elsevier Ltd. All rights reserved.}},
