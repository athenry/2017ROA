@article{ ISI:000373947500015,
Author = {Qiao, Deli and Qian, Haifeng and Li, Geoffrey Ye},
Title = {{Broadbeam for Massive MIMO Systems}},
Journal = {{IEEE TRANSACTIONS ON SIGNAL PROCESSING}},
Year = {{2016}},
Volume = {{64}},
Number = {{9}},
Pages = {{2365-2374}},
Month = {{MAY 1}},
Abstract = {{Massive MIMO has been identified as one of the promising disruptive air
   interface techniques to address the huge capacity requirement of 5G
   wireless communications. For practical deployment of such systems, the
   control message needs to be broadcast to all users reliably in the cell
   using broadbeam. A perfect broadbeam is expected to have the same
   radiated power in all directions to cover users in any place in a cell.
   In this paper, we will show that there is no non-trivial solution for
   perfect broadbeam. Therefore, we develop a method for generating
   broadbeam that can allow tiny fluctuation in radiated power. Overall,
   this can serve as an ingredient for practical deployment of the massive
   MIMO systems.}},
