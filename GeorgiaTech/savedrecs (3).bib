@article{ ISI:000371887800002,
Author = {Shin, So-Yoon and Saravanakumar, Balasubramaniam and Ramadoss,
   Ananthakumar and Kim, Sang Jae},
Title = {{Fabrication of PDMS-based triboelectric nanogenerator for self-sustained
   power source application}},
Journal = {{INTERNATIONAL JOURNAL OF ENERGY RESEARCH}},
Year = {{2016}},
Volume = {{40}},
Number = {{3, SI}},
Pages = {{288-297}},
Month = {{MAR 10}},
Abstract = {{In this report, we present a simple approach to fabricate the
   polydimethylsiloxane (PDMS)-based triboelectric nanogenerator (TENG)
   that can be effectively harvest irregular mechanical energy and the
   energy used to drive a small commercial electronic component. The device
   exhibited the maximum open-circuit voltage and short-circuit current of
   6.3V and 0.635 mu A respectively, with maximum output power of 178 nW.
   In addition, we measured the performance of the fabricated device with
   different load resistance. Finally, the fabricated PDMS-based
   triboelectric nanogenerator can directly drive 10 commercial LEDs
   connected in series, when the force is applied to the device through
   finger. These results demonstrate the potential application of
   PDMS-based triboelectric nanogenerator for self-powered systems.
   Copyright (C) 2015 John Wiley \& Sons, Ltd.}},
