@article{ ISI:000371831000009,
Author = {Yong, Lengxob and Peichel, Catherine L. and McKinnon, Jeffrey S.},
Title = {{Genetic Architecture of Conspicuous Red Ornaments in Female Threespine
   Stickleback}},
Journal = {{G3-GENES GENOMES GENETICS}},
Year = {{2016}},
Volume = {{6}},
Number = {{3}},
Pages = {{579-588}},
Month = {{MAR 1}},
Abstract = {{Explaining the presence of conspicuous female ornaments that take the
   form of male-typical traits has been a longstanding challenge in
   evolutionary biology. Such female ornaments have been proposed to evolve
   via both adaptive and nonadaptive evolutionary processes. Determining
   the genetic underpinnings of female ornaments is important for
   elucidating the mechanisms by which such female traits arise and persist
   in natural populations, but detailed information about their genetic
   basis is still scarce. In this study, we investigated the genetic
   architecture of two ornaments, the orange-red throat and pelvic spine,
   in the threespine stickleback (Gasterosteus aculeatus). Throat
   coloration is male-specific in ancestral marine populations but has
   evolved in females in some derived stream populations, whereas sexual
   dimorphism in pelvic spine coloration is variable among populations. We
   find that ornaments share a common genetic architecture between the
