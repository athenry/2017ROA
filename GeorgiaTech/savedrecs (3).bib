@article{ ISI:000378141300034,
Author = {Peters, Andrew J. and Lawson, Richard A. and Nation, Benjamin D. and
   Ludovice, Peter J. and Henderson, Clifford L.},
Title = {{Calculations of the free energy of dislocation defects in lamellae
   forming diblock copolymers using thermodynamic integration}},
Journal = {{JOURNAL OF MICRO-NANOLITHOGRAPHY MEMS AND MOEMS}},
Year = {{2016}},
Volume = {{15}},
Number = {{2}},
Month = {{APR}},
Abstract = {{State-of-the-art directed self-assembly (DSA) of block copolymer (BCP)
   methods still yield defect densities orders of magnitude higher than is
   necessary in semiconductor fabrication. The defect free energy of a
   dislocation pair or jog defect, one of the most common defects found in
   BCP-DSA, is calculated via thermodynamic integration using a
   coarse-grained molecular dynamics model as a function of chi and the
   degree of polymerization (N). It is found that chi N is not the best
   predictor of defect free energy and that a single chi N value can yield
   vastly different free energies when chi and N are different. Defect free
   energy was highly dependent on defect location relative to the
   underlayer, and free energy differences similar to 100 kT were found
   among the three possible defect locations on a 1: 3 guiding pattern. It
   was found that increasing molar mass dispersity (D) significantly
   reduced defect free energy. Extrapolating from D up to 1.5 suggests that
   the defect will occur in equal proportions to the defect free state at a
   D of around 1.6 for this system. It was found that long chains tended to
   concentrate near the defect and stabilize the defect. (C) 2016 Society
   of Photo-Optical Instrumentation Engineers (SPIE)}},
