@article{ ISI:000352922700026,
Author = {Zhao, Songfang and Gao, Yongju and Zhang, Guoping and Deng, Libo and Li,
   Jinhui and Sun, Rong and Wong, Ching-Ping},
Title = {{Covalently bonded nitrogen-doped carbon-nanotube-supported Ag hybrid
   sponges: Synthesis, structure manipulation, and its application for
   flexible conductors and strain-gauge sensors}},
Journal = {{CARBON}},
Year = {{2015}},
Volume = {{86}},
Pages = {{225-234}},
Month = {{MAY}},
Abstract = {{Flexible strain-gauge sensors are an essential component in electronic
   devices, and pressure sensing is also an important function. Here, we
   report an ultrastable and highly sensitive sensor based on covalently
   bonded nitrogen-doped carbon-nanotube-supported Ag (N-CNT/Ag) hybrid
   sponges with hierarchical binary-network architectures via a
   unidirectional freezing technique. The covalent bonding utilizing
   hyperbranched polyglycerol (HPG) as bridges endows the sponges with
