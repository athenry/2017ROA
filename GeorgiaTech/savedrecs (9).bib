@article{ ISI:000351800500006,
Author = {Peng, Yue and Li, Junhua and Si, Wenzhe and Li, Xiang and Shi, Wenbo and
   Luo, Jinming and Fu, Jie and Crittenden, John and Hao, Jiming},
Title = {{Ceria promotion on the potassium resistance of MnOx/TiO2 SCR catalysts:
   An experimental and DFT study}},
Journal = {{CHEMICAL ENGINEERING JOURNAL}},
Year = {{2015}},
Volume = {{269}},
Pages = {{44-50}},
Month = {{JUN 1}},
Abstract = {{The deactivation of catalysts by potassium and the promotion of cerium
   on MnOx/TiO2 for selective catalytic reduction (SCR) of NO with NH3 are
   investigated by both experiments and OH calculations. Catalyst poisoning
   by K is due to the decrease of surface acidity, the loss of reducibility
   and the improvement of stable nitrite/nitrate species. When the catalyst
   is modified by CeO2, SCR activities of both fresh and poisoned catalysts
   are improved. Cerium provides new Lewis acid sites for NH3 adsorption,
   which does not seem to be influenced by K. The Ce 4f orbitals of
   poisoned surfaces are nearly unaffected, which preserve reducibility of
   catalyst for NH3 activation. Stable, inactive nitrite species do not
   build up and the reactive nitrate species are still present on the
   surface. These advantages can be attributed to the unique active sites,
   the {[}-0-Ce-O-Mn-0-](units). These active sites induce the movement of
   alkali metals to assemble together, leaving more surface active sites
   for SCR reaction. (C) 2015 Elsevier B.V. All rights reserved.}},
