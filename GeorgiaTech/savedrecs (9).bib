@article{ ISI:000355009400011,
Author = {Sadeghi-Tohidi, F. and Pierron, O. N.},
Title = {{Extreme stress gradient effects on microstructural fatigue crack
   propagation rates in Ni microbeams}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{106}},
Number = {{20}},
Month = {{MAY 18}},
Abstract = {{The fatigue crack propagation behavior of microstructurally small cracks
   growing under extreme stress gradients was investigated in Ni microbeams
   under fully reversed cyclic loading. A technique to calculate the crack
   growth rates in microbeams with two different normalized stress
   gradients (17\% and 50\% mu m(-1)) is developed and validated.
   Decreasing crack propagation rates are observed over the first 2 mu m,
   and the rates are more than 1 order of magnitude slower for the devices
   with 50\% mu m(-1) stress gradients. This fundamental knowledge is
   critical to predict the fatigue reliability of advanced metallic
   microcomponents under bending such as in microelectromechanical systems
   or flexible/stretchable electronics. (C) 2015 AIP Publishing LLC.}},
