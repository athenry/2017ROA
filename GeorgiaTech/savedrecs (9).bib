@article{ ISI:000352901100003,
Author = {Rugango, R. and Goeders, J. E. and Dixon, T. H. and Gray, J. M. and
   Khanyile, N. B. and Shu, G. and Clark, R. J. and Brown, K. R.},
Title = {{Sympathetic cooling of molecular ion motion to the ground state}},
Journal = {{NEW JOURNAL OF PHYSICS}},
Year = {{2015}},
Volume = {{17}},
Month = {{MAR 12}},
Abstract = {{We demonstrate sympathetic sideband cooling of a (CaH+)-Ca-40 molecular
   ion co-trapped with a Ca-40(+) atomic ion in a linear Paul trap. Both
   axial modes of the two-ion chain are simultaneously cooled to near the
   ground state of motion. The center of mass mode is cooled to an average
   quanta of harmonic motion (n) over bar (COM) = 0.13 +/- 0.03,
   corresponding to a temperature of 12.47 +/- 0.03 mu K. The breathing
   mode is cooled to (n) over bar (BM) = 0.05 +/- 0.02, corresponding to a
   temperature of 15.36 +/- 0.01 mu K.}},
