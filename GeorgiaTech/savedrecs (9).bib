@article{ ISI:000354906500074,
Author = {Tummala, Naga Rajesh and Bruner, Christopher and Risko, Chad and Bredas,
   Jean-Luc and Dauskardt, Reinhold H.},
Title = {{Molecular-Scale Understanding of Cohesion and Fracture in P3HT:Fullerene
   Blends}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2015}},
Volume = {{7}},
Number = {{18}},
Pages = {{9957-9964}},
Month = {{MAY 13}},
Abstract = {{Quantifying cohesion and understanding fracture phenomena in thin-film
   electronic devices are necessary for improved materials design and
   processing criteria. For organic photovoltaics (OPVs), the cohesion of
   the photoactive layer portends its mechanical flexibility, reliability,
   and lifetime. Here, the molecular mechanism for the initiation of
   cohesive failure in bulk heterojunction (BHJ) OPV active layers derived
   from the semiconducting polymer poly(3-hexylthiophene) {[}P3HT] and two
   nannosubstituted fullerenes is examined experimentally and through
   molecular-dynamics simulations. The results detail how, under identical
   conditions) cohesion significantly changes due to minor variations in
   the fullerene adduct functionality, an important materials consideration
   that needs to be taken into account across fields where soluble
   fullerene derivatives are used.}},
