@article{ ISI:000339890400006,
Author = {Bai, Peng and Zhu, Guang and Zhou, Yu Sheng and Wang, Sihong and Ma,
   Jusheng and Zhang, Gong and Wang, Zhong Lin},
Title = {{Dipole-moment-induced effect on contact electrification for
   triboelectric nanogenerators}},
Journal = {{NANO RESEARCH}},
Year = {{2014}},
Volume = {{7}},
Number = {{7}},
Pages = {{990-997}},
Month = {{JUL}},
Abstract = {{Triboelectric nanogenerators (TENGs) have been demonstrated as an
   effective way to harvest mechanical energy to drive small electronics.
   The density of triboelectric charges generated on contact surfaces
   between two distinct materials is a critical factor for dictating the
   output power. We demonstrate an approach to effectively tune the
   triboelectric properties of materials by taking advantage of the dipole
   moment in polarized polyvinylidene fluoride (PVDF), leading to
   substantial enhancement of the output power density of the TENG. The
   output voltage ranged from 72 V to 215 V under a constant contact force
   of 50 N. This work not only provides a new method of enhancing output
   power of TENGs, but also offers an insight into charge transfer in
   contact electrification by investigating dipole-moment-induced effects
   on the electrical output of TENGs.}},
