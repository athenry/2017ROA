@article{ ISI:000340550500017,
Author = {Sun, Tianmeng and Wang, Yi and Wang, Yucai and Xu, Jinbin and Zhao, Xin
   and Vangveravong, Suwanna and Mach, Robert H. and Xia, Younan},
Title = {{Using SV119-Gold Nanocage Conjugates to Eradicate Cancer Stem Cells
   Through a Combination of Photothermal and Chemo Therapies}},
Journal = {{ADVANCED HEALTHCARE MATERIALS}},
Year = {{2014}},
Volume = {{3}},
Number = {{8, SI}},
Pages = {{1283-1291}},
Month = {{AUG}},
Abstract = {{Cancer stem cells (CSCs) are believed to be responsible for the
   long-term growth of a tumor, as well as its metastasis and recurrence
   after conventional therapies. Here, it is demonstrated that the sigma-2
   receptor is overexpressed on the surface of breast CSCs, and thus could
   serve as a biomarker for the purpose of targeting. Breast CSCs are
   targeted with Au nanocages (AuNCs) by functionalizing their surfaces
   with SV119, a synthetic small molecule capable of binding to the sigma-2
   receptor with high specificity. The interiors of the AuNCs could also be
   loaded with an anticancer drug to be selectively delivered to breast
   CSCs and released in a controllable fashion. The results demonstrate
   that the SV119-AuNC conjugate can serve as a new platform to carry out
   photothermal and chemo therapies simultaneously, eradicating breast CSCs
   more effectively through a synergetic effect.}},
