@article{ ISI:000387578200003,
Author = {Preininger, Marcela K. and Jha, Rajneesh and Maxwell, Joshua T. and Wu,
   Qingling and Singh, Monalisa and Wang, Bo and Dalal, Aarti and Mceachin,
   Zachary T. and Rossoll, Wilfried and Hales, Chadwick M. and Fischbach,
   Peter S. and Wagner, Mary B. and Xu, Chunhui},
Title = {{A human pluripotent stem cell model of catecholaminergic polymorphic
   ventricular tachycardia recapitulates patient-specific drug responses}},
Journal = {{DISEASE MODELS \& MECHANISMS}},
Year = {{2016}},
Volume = {{9}},
Number = {{9}},
Pages = {{927-939}},
Month = {{SEP 1}},
Abstract = {{Although beta-blockers can be used to eliminate stress-induced
   ventricular arrhythmias in patients with catecholaminergic polymorphic
   ventricular tachycardia (CPVT), this treatment is unsuccessful in
   similar to 25\% of cases. Induced pluripotent stem cell-derived
   cardiomyocytes (iPSC-CMs) generated from these patients have potential
   for use in investigating the phenomenon, but it remains unknown whether
   they can recapitulate patient-specific drug responses to beta-blockers.
   This study assessed whether the inadequacy of beta-blocker therapy in an
   individual can be observed in vitro using patient-derived CPVT iPSC-CMs.
   An individual with CPVT harboring a novel mutation in the type 2 cardiac
   ryanodine receptor (RyR2) was identified whose persistent ventricular
   arrhythmias during beta-blockade with nadolol were abolished during
   flecainide treatment. iPSC-CMs generated from this patient and two
   control individuals expressed comparable levels of
   excitation-contraction genes, but assessment of the sarcoplasmic
   reticulum Ca2+ leak and load relationship revealed intracellular Ca2+
   homeostasis was altered in the CPVT iPSC-CMs. beta-adrenergic
   stimulation potentiated spontaneous Ca2+ waves and unduly frequent,
   large and prolonged Ca2+ sparks in CPVT compared with control iPSC-CMs,
   validating the disease phenotype. Pursuant to the patient's in vivo
   responses, nadolol treatment during beta-adrenergic stimulation achieved
   negligible reduction of Ca2+ wave frequency and failed to rescue Ca2+
   spark defects in CPVT iPSC-CMs. In contrast, flecainide reduced both
   frequency and amplitude of Ca2+ waves and restored the frequency, width
   and duration of Ca2+ sparks to baseline levels. By recapitulating the
   improved response of an individual with CPVT to flecainide compared with
   beta-blocker therapy in vitro, these data provide newevidence that
   iPSC-CMs can capture basic components of patient-specific drug
   responses.}},
