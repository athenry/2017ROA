@article{ ISI:000385318700036,
Author = {Barry, Matthew M. and Agbim, Kenechi A. and Rao, Parthib and Clifford,
   Corey E. and Reddy, B. V. K. and Chyu, Minking K.},
Title = {{Geometric optimization of thermoelectric elements for maximum efficiency
   and power output}},
Journal = {{ENERGY}},
Year = {{2016}},
Volume = {{112}},
Pages = {{388-407}},
Month = {{OCT 1}},
Abstract = {{The geometry of n- and p-type thermoelectric elements (TE) in terms of
   the cross-sectional area and length were optimized to yield either
   maximum thermal conversion efficiency eta(th,max) or maximum power
   output P-o,P-max. The optimization process incorporated
   temperature-dependent material properties and independent thermal and
   electrical contact resistances that are a function of the contact area
   per TE leg. Additionally, eta(th,max) and P-o,P-max were quantified by
   simultaneously optimizing the TE geometry and varying the hot-side fluid
   temperature, cold- and hot-side heat exchanger effective area and heat
   transfer coefficients using a complete one-dimensional thermal
   resistance network model. Optimized compared to non-optimized
   geometries, excluding contact resistances, achieve a maximum 10.4\%
   increase in P-o,P-max and 3.2\% increase in eta(th,max) for conditions
   studied. Optimized compared to non-optimized geometries, taking into
   account independent thermal and electrical contact resistances, exhibit
   a 29\% increase in volumetric power density and 12\% increase in
   volumetric efficiency in comparison to non-optimized cases. (C) 2016
   Elsevier Ltd. All rights reserved.}},
