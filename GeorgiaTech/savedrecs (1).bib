@article{ ISI:000393041100001,
Author = {Fallek, S. D. and Herold, C. D. and McMahon, B. J. and Maller, K. M. and
   Brown, K. R. and Amini, J. M.},
Title = {{Transport implementation of the Bernstein-Vazirani algorithm with ion
   qubits}},
Journal = {{NEW JOURNAL OF PHYSICS}},
Year = {{2016}},
Volume = {{18}},
Month = {{AUG 11}},
Abstract = {{Using trapped ion quantum bits in a scalable microfabricated surface
   trap, we perform the Bernstein-Vazirani algorithm. Our architecture
   takes advantage of the ion transport capabilities of such a trap. The
   algorithm is demonstrated using two-and three-ion chains. For three
   ions, an improvement is achieved compared to a classical system using
   the same number of oracle queries. For two ions and one query, we
   correctly determine an unknown bit string with probability 97.6(8)\%.
   For three ions, we succeed with probability 80.9(3)\%.}},
