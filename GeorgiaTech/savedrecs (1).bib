@article{ ISI:000382354100007,
Author = {Kao, Tsung-Ting and Kim, Jeomoh and Detchprohm, Theeradetch and Dupuis,
   Russell D. and Shen, Shyh-Chiang},
Title = {{High-Responsivity GaN/InGaN Heterojunction Phototransistors}},
Journal = {{IEEE PHOTONICS TECHNOLOGY LETTERS}},
Year = {{2016}},
Volume = {{28}},
Number = {{19}},
Pages = {{2035-2038}},
Month = {{OCT 1}},
Abstract = {{We report high-responsivity GaN/InGaN heterojunction phototransistors
   (HPTs) grown on sapphire substrates. Under the ultraviolet (UV) photon
   illumination from the front side of the wafer, an HPT shows broad
   photoresponse spectrum with the short-wavelength cutoff wavelength well
   beyond lambda = 280 nm, and the UV-to-visible-band rejection ratio is >8
   x 10(3). The responsivity (R-lambda) of HPT is greater than 8 A/W at
   lambda = 373 nm, and is greater than 3 A/W at lambda = 280 nm as the
   device is biased at V-CE = 10 V. As the HPT is biased at the near
   breakdown voltage (V-CE > 35 V), the responsivity performance was
   enhanced due to the carrier multiplication, resulting in R-lambda > 100
   A/W at V-CE = 40 V for P-opt = 1.73 mu W/cm(2) at lambda = 373 nm. These
   results demonstrate that GaN/InGaN HPTs can achieve low light detection
   with a broadband photon response in the near-UV-to-deep-UV spectral
   ranges.}},
