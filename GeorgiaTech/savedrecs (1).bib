@article{ ISI:000386070400009,
Author = {Guo, Wei and Fan, Jiancun and Li, Geoffrey Ye and Yin, Qinye and Zhu,
   Xiaolong and Fu, Yusun},
Title = {{MIMO Transmission With Vertical Sectorization for LTE-A Downlink}},
Journal = {{IEEE WIRELESS COMMUNICATIONS LETTERS}},
Year = {{2016}},
Volume = {{5}},
Number = {{4}},
Pages = {{372-375}},
Month = {{AUG}},
Abstract = {{Recently, vertical sectorization (VS) has been proposed for
   multiple-input multiple-output (MIMO) transmission in long term
   evolution advanced (LTE-A) downlink systems. In this paper, we will
   investigate MIMO transmission with VS for LTE-A systems. We first divide
   each cell into two sectors in vertical direction so that more users can
   be co-scheduled and more refined beams can be formed in each sector. We
   then address how to jointly optimize two tilt angles, user scheduling,
   and resource block allocation for a practical LTE-A system, and provide
   an efficient VS-based MIMO scheduling algorithm with low-complexity. The
   performance improvement of the proposed algorithm is demonstrated by
   system level simulation.}},
