@article{ ISI:000380702200003,
Author = {Zhao, Ruiying and Chen, Ye-Hwa and Jiao, Shengjie},
Title = {{Optimal Robust Control for Constrained Fuzzy Dynamic Systems:
   Semi-infinite Case}},
Journal = {{INTERNATIONAL JOURNAL OF FUZZY SYSTEMS}},
Year = {{2016}},
Volume = {{18}},
Number = {{4}},
Pages = {{557-569}},
Month = {{AUG}},
Abstract = {{We consider a constrained state and constrained control design problem
   for fuzzy dynamic systems. The uncertainty in the system is time-varying
   and bounded, with the bound information prescribed by a fuzzy set. The
   control input of systems is also constrained to be one-sided (either
   positive or negative). An optimal design of a robust control is
   proposed. It is proven that the global solution to an optimal design
   problem always exists and is unique. Neither the system nor the control
   is fuzzy heuristic if-then rule-based.}},
