@article{ ISI:000382409800027,
Author = {Pradel, Ken C. and Jevasuwan, Wipakorn and Suwardy, Joko and Bando,
   Yoshio and Fukata, Naoki and Wang, Zhong Lin},
Title = {{Solution derived p-ZnO/n-Si nanowire heterojunctions for photodetection}},
Journal = {{CHEMICAL PHYSICS LETTERS}},
Year = {{2016}},
Volume = {{658}},
Pages = {{158-161}},
Month = {{AUG 1}},
Abstract = {{While there is considerable interest in zinc oxide nanomaterials for
   optoelectronics research, one weakness of the material is the difficulty
   in producing p-type zinc oxide. This can be attributed to a number of
   factors such as acceptor instability, donor compensation during growth,
   and the formation of deep acceptors. Recently, it was discovered that
   antimony is a stable p-type dopant in hydrothermally grown ZnO
   nanowires, and this method has been modified to produce ultra-long
   nanowires and homojunction thin films. In order to broaden the
   applicability of this new material, it is important to investigate how
   it interacts with other semiconducting nanomaterials. In this work we
   demonstrate a solution grown p-ZnO/n-Si nanowire heterojunction. After
