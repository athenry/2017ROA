@article{ ISI:000394786300001,
Author = {Chen, Jun and Huang, Yi and Zhang, Nannan and Zou, Haiyang and Liu,
   Ruiyuan and Tao, Changyuan and Fan, Xing and Wang, Zhong Lin},
Title = {{Micro-cable structured textile for simultaneously harvesting solar and
   mechanical energy}},
Journal = {{NATURE ENERGY}},
Year = {{2016}},
Volume = {{1}},
Month = {{SEP 12}},
Abstract = {{Developing lightweight, flexible, foldable and sustainable power sources
   with simple transport and storage remains a challenge and an urgent need
   for the advancement of next-generation wearable electronics. Here, we
   report a micro-cable power textile for simultaneously harvesting energy
   from ambient sunshine and mechanical movement. Solar cells fabricated
   from lightweight polymer fibres into micro cables are then woven via a
   shuttle-flying process with fibre-based triboelectric nanogenerators to
   create a smart fabric. A single layer of such fabric is 320 mu m thick
   and can be integrated into various cloths, curtains, tents and so on.
   This hybrid power textile, fabricated with a size of 4 cm by 5 cm, was
   demonstrated to charge a 2 mF commercial capacitor up to 2V in 1 min
   under ambient sunlight in the presence of mechanical excitation, such as
   human motion and wind blowing. The textile could continuously power an
   electronic watch, directly charge a cell phone and drive water splitting
   reactions.}},
