@article{ ISI:000342798900014,
Author = {Dykes, John and Costello, Mark and Fresconi, Frank and Cooper, Gene},
Title = {{Periodic projectile linear theory for aerodynamically asymmetric
   projectiles}},
Journal = {{PROCEEDINGS OF THE INSTITUTION OF MECHANICAL ENGINEERS PART G-JOURNAL OF
   AEROSPACE ENGINEERING}},
Year = {{2014}},
Volume = {{228}},
Number = {{11}},
Pages = {{2094-2107}},
Month = {{SEP}},
Abstract = {{A new analytical tool is proposed to aid in the design and performance
   evaluation of advanced guided projectile concepts. A projectile linear
   theory applicable to aerodynamically asymmetric configurations is
   created, leading to a linear, periodic dynamic system. Utilizing
   concepts in Floquet theory, stability of asymmetric projectile
   configurations is explored. While stability of many asymmetric
   projectile configurations can be accurately predicted using averaged
   linear, constant coefficient system dynamics, there are some
   configurations where the use of linear periodic systems theory is
   required for accurate stability prediction. This fact is shown by
   comparing stability boundaries of an example projectile configuration
   using the conventional projectile linear theory model and the new
   periodic projectile linear theory model.}},
