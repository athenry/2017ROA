@article{ ISI:000340792100005,
Author = {Liu, Shuo and Yan, Fengping and Peng, Wanjing and Feng, Ting and Dong,
   Ze and Chang, Geekung},
Title = {{Tunable Dual-Wavelength Thulium-Doped Fiber Laser by Employing a HB-FBG}},
Journal = {{IEEE PHOTONICS TECHNOLOGY LETTERS}},
Year = {{2014}},
Volume = {{26}},
Number = {{18}},
Pages = {{1809-1812}},
Month = {{SEP 15}},
Abstract = {{A novel tunable dual-wavelength thulium-doped fiber laser is
   demonstrated experimentally. The wavelength-tuning by employing a high
   birefringence fiber Bragg grating is proposed in 2-mu m band lasing for
   the first time. By adjusting the polarization controller, stable
   dual-wavelength operation at the wavelengths of 1941.40 and 1942.21 nm
   is obtained. The optical signal-to-noise ratio is better than 48 dB. A
   15.5\% slope efficiency is achieved using a 90\% output coupling ratio
   for laser extracting. The wavelength-tuning range in dual-wavelength
   operation is as wide as 6.93 nm.}},
