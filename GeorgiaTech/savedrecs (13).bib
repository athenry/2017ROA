@article{ ISI:000343318500006,
Author = {Guvanasen, Gareth S. and Mancini, Michelle L. and Calhoun, William A.
   and Rajaraman, Swaminathan and DeWeerth, Stephen P.},
Title = {{Polydimethylsiloxane Microstencils Molded on 3-D-Printed Templates}},
Journal = {{JOURNAL OF MICROELECTROMECHANICAL SYSTEMS}},
