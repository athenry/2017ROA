@article{ ISI:000342995800044,
Author = {Liu, Xi and Thadesar, Paragkumar A. and Taylor, Christine L. and Oh,
   Hanju and Kunz, Martin and Tamura, Nobumichi and Bakir, Muhannad S. and
   Sitaraman, Suresh K.},
Title = {{In-situ microscale through-silicon via strain measurements by
   synchrotron x-ray microdiffraction exploring the physics behind data
   interpretation}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2014}},
Volume = {{105}},
Number = {{11}},
Month = {{SEP 15}},
Abstract = {{In-situ microscale thermomechanical strain measurements have been
   performed in combination with synchrotron x-ray microdiffraction to
   understand the fundamental cause of failures in microelectronics devices
   with through-silicon vias. The physics behind the raster scan and data
   analysis of the measured strain distribution maps is explored utilizing
   the energies of indexed reflections from the measured data and applying
   them for beam intensity analysis and effective penetration depth
   determination. Moreover, a statistical analysis is performed for the
   beam intensity and strain distributions along the beam penetration path
