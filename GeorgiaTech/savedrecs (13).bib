@article{ ISI:000343955700070,
Author = {Dong, Dandan and Zeng, Jinfeng and Huang, Fang and Ma, Yanqing},
Title = {{Effects of Metal Ions (Mg2+, Ca2+, Cd2+ and Ze(2+)) on Adenosine
   Triphosphate Hydrolysis}},
Journal = {{ASIAN JOURNAL OF CHEMISTRY}},
Year = {{2014}},
Volume = {{26}},
Number = {{18, B}},
Pages = {{6243-6248}},
Month = {{SEP}},
Abstract = {{The interaction between metal ions (M: Mg2+, Ca2+, Cd2+ and Zn2+),
   N-alpha-4-tosyl-L-arginine methyl ester hydrochloride (TAME) and
   adenosine triphosphate has been studied by NMR spectra. The catalytic
   influence of the combined effects on hydrolysis was examined, using H-1
   and P-31 NMR spectra. In M-ATP-TAME tertiary systems, adenosine
   triphosphate interacts with the Metal ions, and TAME. The act sites are
   beta, gamma-phosphate groups and adenine ring, The interaction force are
   electrostatic-, cation-pi and pi-pi stacking interactions. The rate
   constant Of adenosine triphosphate hydrolysis in M-TAME-ATP ternary
   systems increased in the following order, Zn2+ > Ca2+ > Cd2+ > mg(2+).
   Different catalytic behaviours were discussed in terms of the
   corresponding metal ion properties. At the same time, a reasonable
   mechanism has been proposed that adenosine triphosphate hydrolysis
   catalyzed by M-TAME occurs through an addition-elimination reaction
   sequence.}},
Publisher = {{ASIAN JOURNAL OF CHEMISTRY}},
