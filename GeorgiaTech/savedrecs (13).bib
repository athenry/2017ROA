@article{ ISI:000343336000038,
Author = {Muroyama, Alexander and Shinn, Tyler and Fales, Roger and Loutzenhiser,
   Peter G.},
Title = {{Modeling of a Dynamically-Controlled Hybrid Solar/Autothermal Steam
   Gasification Reactor}},
Journal = {{ENERGY \& FUELS}},
Year = {{2014}},
Volume = {{28}},
Number = {{10}},
Pages = {{6520-6530}},
Month = {{OCT}},
Abstract = {{In this modeling study, steam gasification of carbonaceous materials is
   examined in a hybrid solar/autothermal process designed to continuously
   produce synthesis gas. The process operates in three modes: (1) steam
   gasification where the necessary process heat to drive the chemical
   reaction is derived from concentrated solar irradiation, (2) autothermal
   steam gasification where pure oxygen is used to combust a portion of the
   carbonaceous feedstock for process heat, i.e., oxy-combustion, and (3) a
   combined mode during periods of low direct-normal solar irradiation
   where process heat is derived from a combination of solar irradiation
   and oxy-combustion. The resulting syngas can be continuously converted
   to liquid fuels via FischerTropsch or other catalytic processes.
   Equilibrium compositions for different reaction stoichiometric ratios of
   steam, O-2, and a carbonaceous material are examined over a 3001900 K
   temperature range using Gibbs free energy minimization. The process is
   then analyzed in a simplified finite-volume model using hourly
   direct-normal solar irradiation data, coupling chemical flows;
   conductive, convective, and radiative heat transfer; and a dynamic
   feed-forward control system. Seasonal comparisons of the chemical solar
   energy storage from the solar gasification due to a calorific upgrade of
   the carbonaceous feedstock, process efficiency, and reduced
   anthropogenic CO2 emissions are discussed. Model cold gas (upgrade)
   ratios of 1.2 and solar-to-fuel efficiencies of 39\% were achieved
   during complete solar operation, while a cold gas efficiency of 87\% was
   maintained during autothermal operation. This indicates the successful
   storage of solar energy while producing a continuous stream of synthesis
   gas.}},
