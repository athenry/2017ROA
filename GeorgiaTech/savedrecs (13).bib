@article{ ISI:000341469400021,
Author = {Agrawal, Gaurav and Oh, Jungmin and Sreedhar, Balamurali and Tie, Shan
   and Donaldson, Megan E. and Frank, Timothy C. and Schultz, Alfred K. and
   Bommarius, Andreas S. and Kawajiri, Yoshiaki},
Title = {{Optimization of reactive simulated moving bed systems with modulation of
   feed concentration for production of glycol ether ester}},
Journal = {{JOURNAL OF CHROMATOGRAPHY A}},
Year = {{2014}},
Volume = {{1360}},
Pages = {{196-208}},
Month = {{SEP 19}},
Abstract = {{In this article, we extend the simulated moving bed reactor (SMBR) mode
   of operation to the production of propylene glycol methyl ether acetate
   (DOWANOL (TM) PMA glycol ether) through the esterification of
   1-methoxy-2-propanol (DOWANOL (TM) PM glycol ether) and acetic acid
   using AMBERLYST (TM) 15 as a catalyst and adsorbent. In addition, for
   the first time, we integrate the concept of modulation of the feed
   concentration (ModiCon) to SMBR operation. The performance of the
   conventional (constant feed) and ModiCon operation modes of SMBR are
   analyzed and compared. The SMBR processes are designed using a model
   based on a multi-objective optimization approach, where a transport
   dispersive model with a linear driving force for the adsorption rate has
   been used for modeling the SMBR system. The adsorption equilibrium and
   kinetics parameters are estimated from the batch and single column
   injection experiments by the inverse method. The multiple objectives are
   to maximize the production rate of DOWANOL (TM) PMA glycol ether,
   maximize the conversion of the esterification reaction and minimize the
   consumption of DOWANOL (TM) PM glycol ether which also acts as the
   desorbent in the chromatographic separation. It is shown that ModiCon
   achieves a higher productivity by 12-36\% over the conventional
   operation with higher product purity and recovery. (C) 2014 Elsevier
   B.V. All rights reserved.}},
