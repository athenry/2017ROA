@article{ ISI:000344382500018,
Author = {Averett, Rodney D.},
Title = {{Experimental mechanics of magnetic microparticle-induced strain on
   fibrin clots}},
Journal = {{JOURNAL OF BIOMEDICAL MATERIALS RESEARCH PART A}},
Year = {{2014}},
Volume = {{102}},
Number = {{12}},
Pages = {{4365-4370}},
Month = {{DEC}},
Abstract = {{In this study, a novel technique was developed in which magnetic
   microparticles (MMPs) and quantum dots (QDs) were successfully
   incorporated into fibrin clots. The MMPs were added at concentrations of
   0.1 and 1 wt \% of the fibrin content in an effort to determine if a
   magnetic field could be used to mechanically stretch the fibrin network,
   simulating how cells may invade a network. The QDs were added at a
   dilute concentration of 0.1 wt \% to determine if the mechanical
   properties of the fibrin network would be significantly altered and to
   ascertain if the overall stretch on the network could be observed. Based
   on strain sweep and frequency sweep rheological analysis, it was
   determined that both QDs and MMPs incorporated into fibrin networks at
   0.1 wt \% caused irreversible plastic deformation in the fibrin clot
   sample, as evidenced by a precipitous decline in the storage modulus
   value. (c) 2014 Wiley Periodicals, Inc. J Biomed Mater Res Part A: 102A:
   4365-4370, 2014.}},
