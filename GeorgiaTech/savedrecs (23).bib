@article{ ISI:000312914600006,
Author = {Liu, Jin and Guo, Hanzheng and Pang, Xinchang and Tan, Xiaoli and Akinc,
   Mufit and Lin, Zhiqun and Bowler, Nicola},
Title = {{Dynamics of polystyrene-block-poly(methylmethacrylate) (PS-b-PMMA)
   diblock copolymers and PS/PMMA blends: A dielectric study}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{2013}},
Volume = {{359}},
Pages = {{27-32}},
Month = {{JAN 1}},
Abstract = {{The dielectric relaxation properties of poly( methylmethacrylate)
   (PMMA), polystyrene-block-poly (methylmethacrylate) (PS-b-PMMA) diblock
   copolymers and PS/PMMA blends were investigated by broadband dielectric
   spectroscopy in the frequency range from 0.01 Hz to 1 MHz and
   temperature range from -40 to 150 degrees C. By parametric fitting
   techniques, an alpha relaxation process associated with the glass
   transition, the secondary beta relaxation process due to localized
   motions of side groups, and the conductivity at low frequencies were
   identified and modeled for all samples. Based on the modeling results,
   the characteristic relaxation time, fragility index, activation
   energies, and relaxation strength of PMMA and PS-b-PMMA diblock
   copolymers were compared. It was found that the structural alpha
   relaxation of the copolymers was suppressed compared with that of PMMA.
   The covalent chemical bonds that link PS and PMMA blocks in the diblock
   copolymers are therefore assumed to dominate over the PMMA-PMMA, PMMA-PS
   and PS-PS bonds that exist in the PMMA and PS/PMMA samples, accounting
   for the suppression of the cooperative structural alpha relaxation and
   for larger activation energies of the localized beta relaxation of the
   diblock copolymers. Further, the activation energy barriers of localized
   beta relaxations in the copolymers are observed to increase, above the
   glass transition temperature. (C) 2012 Elsevier B.V. All rights
   reserved.}},
