@inproceedings{ ISI:000320452600079,
Author = {Dawson, Alexander J. and Michaels, Jennifer E.},
Editor = {{Thompson, DO and Chimenti, DE}},
Title = {{LAMB WAVE MODE DECOMPOSITION USING ANNULAR TRANSDUCERS AND QUADRATURE
   SIGNALS}},
Booktitle = {{REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE EVALUATION, VOLS 32A
   AND 32B}},
Series = {{AIP Conference Proceedings}},
