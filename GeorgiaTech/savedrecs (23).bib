Title = {{PS-b-PHEMA: Synthesis, Characterization, and Processing of a High chi
   Polymer for Directed Self-Assembly Lithography}},
Booktitle = {{ALTERNATIVE LITHOGRAPHIC TECHNOLOGIES V}},
Series = {{Proceedings of SPIE}},
Year = {{2013}},
