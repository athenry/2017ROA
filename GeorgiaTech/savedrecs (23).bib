@article{ ISI:000312654600016,
Author = {Tang, Shiyuyun and Antonov, Ivan and Borodovsky, Mark},
Title = {{MetaGeneTack: ab initio detection of frameshifts in metagenomic
   sequences}},
Journal = {{BIOINFORMATICS}},
Year = {{2013}},
Volume = {{29}},
Number = {{1}},
Pages = {{114-116}},
Month = {{JAN}},
Abstract = {{Frameshift (FS) prediction is important for analysis and biological
   interpretation of metagenomic sequences. Since a genomic context of a
   short metagenomic sequence is rarely known, there is not enough data
   available to estimate parameters of species-specific statistical models
   of protein-coding and non-coding regions. The challenge of ab initio FS
   detection is, therefore, two fold: (i) to find a way to infer necessary
   model parameters and (ii) to identify positions of frameshifts (if any).
   Here we describe a new tool, MetaGeneTack, which uses a heuristic method
   to estimate parameters of sequence models used in the FS detection
   algorithm. It is shown on multiple test sets that the MetaGeneTack FS
   detection performance is comparable or better than the one of earlier
   developed program FragGeneScan.}},
