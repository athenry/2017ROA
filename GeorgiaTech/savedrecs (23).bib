@article{ ISI:000321043700012,
Author = {Dieci, Luca and Papini, Alessandra and Pugliese, Alessandro},
Title = {{APPROXIMATING COALESCING POINTS FOR EIGENVALUES OF HERMITIAN MATRICES OF
   THREE PARAMETERS}},
Journal = {{SIAM JOURNAL ON MATRIX ANALYSIS AND APPLICATIONS}},
Year = {{2013}},
Volume = {{34}},
Number = {{2}},
Pages = {{519-541}},
Abstract = {{We consider a Hermitian matrix valued function A(x) is an element of
   C-nxn, smoothly depending on parameters x is an element of Omega subset
   of R-3, where Omega is an open bounded region of R-3. We develop an
   algorithm to locate parameter values where the eigenvalues of A
   coalesce: conical intersections of eigenvalues. The crux of the method
   requires one to monitor the geometric phase matrix of a Schur
   decomposition of A, as A varies on the surface S bounding Omega. We
   develop (adaptive) techniques to find the minimum variation
   decomposition of A along loops covering S and show how this can be used
   to detect conical intersections. Further, we give implementation details
   of a parallelization of the technique, as well as details relative to
   the case of locating conical intersections for a few of A's dominant
   eigenvalues. Several examples illustrate the effectiveness of our
   technique.}},
