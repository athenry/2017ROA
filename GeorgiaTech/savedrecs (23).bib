@inproceedings{ ISI:000375256305013,
Author = {Komerath, Narayanan M.},
Book-Group-Author = {{ASEE}},
Title = {{A Case Study on Advancing Learning in An Upper-Level Engineering Course}},
Booktitle = {{2013 ASEE ANNUAL CONFERENCE}},
Series = {{ASEE Annual Conference \& Exposition}},
Year = {{2013}},
Note = {{ASEE Annual Conference, Atlanta, GA, JUN 23-26, 2013}},
Organization = {{ASEE}},
Abstract = {{A case study is condensed from the evolving experience of advancing
   learning in complex senior-level engineering courses. An advanced high
   speed aerodynamics course is set in curricular context and described.
   The need and approach to advance the depth of comprehension and the
   capabilities of our students are presented, followed by the approach
   taken, and data on student performance over the past two years. The
   portfolio of student confirms the excellence demonstrated in tests.
   However, the end-of semester course evaluation and the present
   administrative processes project a negative attitude and severe
   impediments to implementing advancing learning. Pointed comments from
   the better-performing students reveal intense frustration with the
   perceived dilution of work ethic and degree requirements, and cites the
   damage being done to institutions, the industry and the morale of the
