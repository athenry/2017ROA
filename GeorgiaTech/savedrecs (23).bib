@article{ ISI:000315165100015,
Author = {Sun, Wenping and Liu, Mingfei and Feng, Shi and Liu, Wei and Park, Hyeon
   Cheol and Liu, Meilin},
Title = {{Hydrogen oxidation at the Pt-BaZr0.1Ce0.7Y0.1Yb0.1O3-delta (BZCYYb)
   interface}},
Journal = {{PHYSICAL CHEMISTRY CHEMICAL PHYSICS}},
Year = {{2013}},
Volume = {{15}},
Number = {{11}},
Pages = {{3820-3826}},
Abstract = {{An asymmetric cell based on a proton conductor,
   BaZr0.1Ce0.7Y0.1Yb0.1O3-delta (BZCYYb), with a well-defined patterned Pt
   electrode was prepared to study the kinetics and mechanism of the
   hydrogen oxidation reaction under typical conditions for fuel cell
   operation and hydrogen separation, including operating temperature and
   hydrogen partial pressure. Steady-state polarization curves were
   carefully analyzed to determine the apparent exchange current density,
   limiting current density, and charge transfer coefficients. The
   empirical reaction order, as estimated from the dependence of electrode
   polarization (R-p) and exchange current density on the partial pressure
   of hydrogen (P-H2), varied from 0.55 to 0.71. The results indicate that
   hydrogen dissociation contributes the most to the rate-limiting step of
   the hydrogen oxidation reaction taking place at the Pt-BZCYYb interface.
   At high current densities, surface diffusion of electroactive species
   appears to contribute to the rate-limiting step as well.}},
