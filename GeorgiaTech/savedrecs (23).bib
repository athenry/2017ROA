Author = {Chavarria-Reyes, Elias and Akyildiz, Ian E.},
Book-Group-Author = {{IEEE}},
Title = {{Radio Access Network Energy Minimization in Multi-Layer Heterogeneous
   Wireless Systems}},
Booktitle = {{2013 IEEE 24TH INTERNATIONAL SYMPOSIUM ON PERSONAL, INDOOR, AND MOBILE
