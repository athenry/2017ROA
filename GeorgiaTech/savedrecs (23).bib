Author = {Goetschalckx, Marc and Huang, Edward and Mital, Pratik},
Editor = {{Paredis, CJJ and Bishop, C and Bodner, D}},
Title = {{Trading off Supply Chain Risk and Efficiency through Supply Chain
   DesignProcedia Computer Science 16 (2013) 658-667 1877-0509 c 2013 The
