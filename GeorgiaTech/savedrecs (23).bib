@article{ ISI:000324491100016,
Author = {Xia, Xiaohu and Rycenga, Matthew and Qin, Dong and Xia, Younan},
Title = {{A silver nanocube on a gold microplate as a well-defined and highly
   active substrate for SERS detection}},
Journal = {{JOURNAL OF MATERIALS CHEMISTRY C}},
