@article{ ISI:000313605800012,
Author = {Gray, Warren D. and Wu, Ruo Jia and Yin, Xiang and Zhou, Jihan and
   Davis, Michael E. and Luo, Ying},
Title = {{Dendrimeric Bowties Featuring Hemispheric-Selective Decoration of
   Ligands for microRNA-Based Therapy}},
Journal = {{BIOMACROMOLECULES}},
Year = {{2013}},
Volume = {{14}},
Number = {{1}},
Pages = {{101-109}},
Month = {{JAN}},
Abstract = {{Dendrimers feature a defined number of terminal groups that may bind RNA
   or be functionalized with bioactive molecules. These competing uses of
   terminal groups may create an impasse if the requisite density of
   ligands depletes the number of terminal groups for binding sufficient
   RNA, or vice versa. A novel dendrimeric platform is needed that
   maintains high ligand density while retaining sufficient
   microRNA-binding terminal groups. Here we present a dendrimeric
   ``bowtie{''} consisting of one-half devoted to microRNA binding and the
   other half to ligand presentation. We demonstrate its suitability as a
   transfection agent by delivering miR-126 to human vascular endothial
   cells (HUVECs) via polyarginine- and RGD-modified bowties and evaluating
   the downstream effects on proliferation and tube formation. Our findings
   indicate that the bowtie elicits desired responses and may possess
   superior delivery properties compared to nondecorated dendrimeric
   materials. The bowtie system thereby provides a new design model for
   developing dendrimeric delivery vehicles for RNAi therapeutics.}},
