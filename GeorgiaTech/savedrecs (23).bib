@inproceedings{ ISI:000323077100049,
Author = {Peters, Andrew J. and Lawson, Richard A. and Ludovice, Peter J. and
   Henderson, Clifford L.},
Editor = {{Tong, WM and Resnick, DJ}},
Title = {{Effects of block copolymer polydispersity and chi N on pattern line edge
   roughness and line width roughness from directed self-assembly of
   diblock copolymers}},
Booktitle = {{ALTERNATIVE LITHOGRAPHIC TECHNOLOGIES V}},
Series = {{Proceedings of SPIE}},
Year = {{2013}},
Volume = {{8680}},
Note = {{Conference on Alternative Lithographic Technologies V, San Jose, CA, FEB
   25-28, 2013}},
Organization = {{SPIE; AZ Elect Mat}},
Abstract = {{This paper addresses two fundamental issues: (1) the connection between
   block copolymer polydispersity (as measured by a polydisperisty index
   (PDI)) and pattern LER/LWR limits and (2) the connection between block
   copolymer chi N value and pattern LER/LWR limits. In this work, we have
   used coarse grained molecular dynamics (MD) simulations of BCP DSA to
   study the effect of block copolymer PDI on DSA properties including
   LER/LWR and patterning capability. It is observed that as PDI increases
   from 1 to values of similar to 1.3, there is little effect on pattern
   LER/LWR, and as PDI increases above similar to 1.3 the LER/LWR increases
   slowly with increasing PDI. This suggests that LER/LWR concerns are not
   a major determinant in terms of specifying block copolymer PDI
   requirements for DSA processes. Concerning chi N and LER/LWR, there is a
   sharp increase in roughness for chi N<30. Because of the sharp increase
   at such low chi N values, it is unlikely that BCP DSA processes for
   semiconductor manufacturing will be able to operate at low chi N values
   even though microphase separation still occurs at these low chi N
   values.}},
