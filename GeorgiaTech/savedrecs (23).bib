@article{ ISI:000321044000003,
Author = {Gupte, Akshay and Ahmed, Shabbir and Cheon, Myun Seok and Dey, Santanu},
Title = {{SOLVING MIXED INTEGER BILINEAR PROBLEMS USING MILP FORMULATIONS}},
Journal = {{SIAM JOURNAL ON OPTIMIZATION}},
Year = {{2013}},
Volume = {{23}},
Number = {{2}},
Pages = {{721-744}},
Abstract = {{In this paper, we examine a mixed integer linear programming
   reformulation for mixed integer bilinear problems where each
   bilinearterm involves the product of a nonnegative integer variable and
   a nonnegative continuous variable. This reformulation is obtained by
   first replacing a general integer variable with its binary expansion and
   then using McCormick envelopes to linearize the resulting product of
   continuous and binary variables. We present the convex hull of the
   underlying mixed integer linear set. The effectiveness of this
   reformulation and associated facet-defining inequalities are
   computationally evaluated on five classes of instances.}},
