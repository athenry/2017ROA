@inproceedings{ ISI:000323074200023,
Author = {Carrara, Matteo and Ruzzene, Massimo},
Editor = {{Kundu, T}},
Title = {{Numerical and Experimental Analysis of Guided Waves Propagation in
   Composite Plates}},
