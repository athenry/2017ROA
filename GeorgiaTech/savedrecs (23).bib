Author = {Chen, Xin and Michaels, Jennifer E. and Michaels, Thomas E.},
Editor = {{Thompson, DO and Chimenti, DE}},
Title = {{ESTIMATION OF GUIDED WAVE SCATTERING MATRICES FROM SPATIALLY DISTRIBUTED
   TRANSDUCER ARRAYS}},
Booktitle = {{REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE EVALUATION, VOLS 32A
   AND 32B}},
Series = {{AIP Conference Proceedings}},
Year = {{2013}},
Volume = {{1511}},
