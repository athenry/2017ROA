@inproceedings{ ISI:000375256305039,
Author = {Komerath, Narayanan M.},
Book-Group-Author = {{ASEE}},
Title = {{Renovating an Ancient Low Speed Wind Tunnel: A Student Team Project Case
   Study}},
Booktitle = {{2013 ASEE ANNUAL CONFERENCE}},
Series = {{ASEE Annual Conference \& Exposition}},
Year = {{2013}},
Note = {{ASEE Annual Conference, Atlanta, GA, JUN 23-26, 2013}},
Organization = {{ASEE}},
Abstract = {{This study describes how a team of aerospace engineering students
   planned, organized, implemented and recorded a fast-paced project in
   support of the repair and renovation of a very old, major university low
   speed wind tunnel. This is a case study in learning within and across
   disciplines, involving students at all levels from freshman to PhD. The
   problem involved having to learn a wide variety of topics on the job
   with very tight deadlines. Solution methods had to be developed and
   implemented. Experiments had to be conceived, designed and refined.
   Analysis methods were used, with innovations required to adapt classical
   methods to the particular needs of the problem. The pedagogical aspects
   of interest include how modern students learn under such circumstances,
   the methods of teamwork, the role of technology, and the methods needed
   to ensure safety and competent completion of projects.}},
