@inproceedings{ ISI:000357299604098,
Author = {Balakrishnan, Ravikumar and Akyildiz, Ian F.},
Book-Group-Author = {{IEEE}},
Title = {{Local Mobility Anchoring for Seamless Handover in Coordinated Small
   Cells}},
Booktitle = {{2013 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
Series = {{IEEE Global Communications Conference}},
Year = {{2013}},
Pages = {{4489-4494}},
Note = {{IEEE Global Communications Conference (GLOBECOM), Atlanta, GA, DEC
   09-13, 2013}},
Organization = {{IEEE}},
Abstract = {{Small cells are redefining the traditional cellular system concepts.
   Coordination among small cells offers several benefits in terms of
   utilizing existing network infrastructure to support advanced
   interference management, mobility management as well as self-organizing
   (SON) functions. In this work, the problem of handover management is
   studied. The coordination between small cells is generally not
   considered in many existing handover management solutions resulting in
   increased handover cost, session interruption time and core network
   overload. To this end, a novel local anchor-based architecture for
   coordinated small cells is proposed in this paper based on which three
   handover schemes are presented. A mathematical framework is developed to
   analyze the performance of the proposed schemes and Markov models are
   utilized to obtain closed-form expressions for key handover parameters
