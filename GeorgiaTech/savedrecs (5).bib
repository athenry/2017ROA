@inproceedings{ ISI:000382313300077,
Author = {Ma, Ling and Guo, Rongrong and Tian, Zhiqiang and Venkataraman, Rajesh
   and Sarkar, Saradwata and Liu, Xiabi and Tade, Funmilayo and Schuster,
   David M. and Fei, Baowei},
