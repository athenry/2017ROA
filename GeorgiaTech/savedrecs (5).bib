@inproceedings{ ISI:000382315800006,
Author = {Ma, Ling and Guo, Rongrong and Tian, Zhiqiang and Venkataraman, Rajesh
   and Sarkar, Saradwata and Liu, Xiabi and Nieh, Peter T. and Master,
   Viraj V. and Schuster, David M. and Fei, Baowei},
