@inproceedings{ ISI:000389835800042,
Author = {Hua, Yougen and Zhao, Tao and Zhu, Pengli and Lian, Xianwen and Zhu, Yu
   and Su, Haibo and Sun, Rong and Wong, Ching-Ping},
