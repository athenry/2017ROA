@article{ ISI:000388628400001,
Author = {Du, Xiaotang Tony and Lee, Daniel T. and Hsieh, Jeffery S.},
Title = {{Inkjet ink behaviors and its implication in adsorption deinking}},
Journal = {{SEPARATION SCIENCE AND TECHNOLOGY}},
Year = {{2016}},
Volume = {{51}},
Number = {{18}},
Pages = {{2857-2867}},
Abstract = {{The deinking process of inkjet deinking was evaluated by following
   INGEDE Method 11 and the influence of the pulping chemistry was studied.
   The inkjet ink behavior during pulping and papermaking was also
   investigated to distinguish between redeposited ink and bound ink, which
   has shown that redeposited ink is the major concern during paper
   recycling. In order to prevent the ink redeposition, adsorption has been
   used to improve inkjet removal during pulping. Chitosan was selected as
   an effective adsorbent, and the ISO brightness was improved by 5 points
   compared with flotation deinking.}},
