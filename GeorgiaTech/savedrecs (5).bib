Title = {{Water-dispersible graphene paste for flexible conductive patterns and
   films}},
Booktitle = {{2016 17TH INTERNATIONAL CONFERENCE ON ELECTRONIC PACKAGING TECHNOLOGY
   (ICEPT)}},
Year = {{2016}},
Pages = {{200-205}},
Note = {{17th International Conference on Electronic Packaging Technology
