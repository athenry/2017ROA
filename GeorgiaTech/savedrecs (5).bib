@inproceedings{ ISI:000388377100770,
Author = {Li, Sensen and Chi, Taiyun and Park, Jong Seok and Wang, Hua},
Book-Group-Author = {{IEEE}},
Title = {{A Multi-Feed Antenna for Antenna-Level Power Combining}},
Booktitle = {{2016 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2016}},
Pages = {{1589-1590}},
Note = {{IEEE-Antennas-and-Propagation-Society International Symposium, Fajardo,
   PR, JUN 26-JUL 01, 2016}},
Organization = {{Inst Elect \& Elect Engineers Antennas \& Propagat Soc; Inst Elect \&
   Elect Engineers}},
Abstract = {{This paper proposes a multi-feed antenna structure that synthesizes the
   desired radiation characteristics with antenna-level power combining.
   Compared with an antenna array that spatially combines the radiation
   E-field and H-field at the far-field, the proposed multi-feed antenna
   combines the radiation power from different feeds directly on the
   antenna. Such a unique structure occupies a much smaller footprint and
   offers the flexibility to optimize the antenna driving impedances based
   on the locations of the antenna feeds. As two proof-of-concept designs,
   a two-feed 0.5 lambda slot antenna and a three-feed 0.7 lambda slot
   antenna with the input feeding networks are designed at 10.3GHz. A
   conventional single-feed 0.5 lambda slot antenna and a single-feed 0.7
   lambda slot antenna are also implemented as the reference designs. Well
   matched radiation patterns between the proposed multi-feed antennas and
   the conventional single-feed antennas are observed, demonstrating the
   antenna-level power combining capability of the proposed multi-feed
   antennas.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
