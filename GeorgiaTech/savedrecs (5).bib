@inproceedings{ ISI:000379883000068,
Author = {Wang, Yan},
Book-Group-Author = {{ASME}},
Title = {{QUANTIFICATION OF MODEL-FORM UNCERTAINTY IN DRIFT-DIFFUSION SIMULATION
   USING FRACTIONAL DERIVATIVES}},
Booktitle = {{INTERNATIONAL DESIGN ENGINEERING TECHNICAL CONFERENCES AND COMPUTERS AND
   INFORMATION IN ENGINEERING CONFERENCE, 2015, VOL 1A}},
Year = {{2016}},
