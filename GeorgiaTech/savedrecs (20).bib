@article{ ISI:000321890800006,
Author = {Tandon, Rajan and Paliwal, Bhasker and Gibson, Cory},
Title = {{Practical aspects of using Hertzian ring crack initiation to measure
   surface flaw densities in glasses: influence of humidity, friction and
   searched areas}},
Journal = {{PHILOSOPHICAL MAGAZINE}},
Year = {{2013}},
Volume = {{93}},
Number = {{21}},
Pages = {{2847-2863}},
Month = {{JUL 1}},
Abstract = {{Ring crack initiation loads on glass, using spherical Tungsten carbide
   (WC) and glass (G) indenters, are measured and analysed. Our
   measurements demonstrate that environmental humidity plays a key role in
   determining the load to fracture; experiments conducted without
   controlling this variable cannot be used to obtain material properties.
   The role of friction is explicitly considered for dissimilar (WC-G)
   elastic contacts. For this material pair, the stresses at fracture are
   well described by a boundary lubrication value of friction coefficient.
   The fracture loads are used in a fracture-mechanics formulation to
   calculate crack sizes on glass surfaces. The searched-area' concept for
   dissimilar contacts is described, and used to provide crack density
   values for these surfaces.}},
