@article{ ISI:000322030600003,
Author = {Kiani, Mehdi and Ghovanloo, Maysam},
Title = {{A 20-Mb/s Pulse Harmonic Modulation Transceiver for Wideband Near-Field
   Data Transmission}},
Journal = {{IEEE TRANSACTIONS ON CIRCUITS AND SYSTEMS II-EXPRESS BRIEFS}},
Year = {{2013}},
Volume = {{60}},
Number = {{7}},
Pages = {{382-386}},
Month = {{JUL}},
Abstract = {{This brief presents a fully integrated low-power transceiver for
   wideband near-field data transmission across inductive telemetry links
   based on pulse harmonic modulation (PHM). This PHM transceiver uses two
   narrow pulses with specific amplitudes and timing at the transmitter
   (Tx) to suppress intersymbol interference (ISI) at the receiver (Rx)
   high-Q LC tank to achieve high data rates (DRs) without reducing the
   inductive link quality factor to extend its bandwidth. This method
   significantly improves the range and selectivity of the link. The PHM Rx
   architecture is based on noncoherent energy detection utilizing a novel
   pulse-based automatic gain control circuit, which significantly reduces
   the power consumption (46\%) and ISI at short coupling distances. The
   PHM transceiver was fabricated in a 0.35-mu m standard CMOS process and
   achieved a measured DR of 20 Mb/s with a bit error rate of 8.7 x 10(-8)
   at a 10-mm distance using planar figure-8 coils. The PHM Tx and Rx power
   consumptions were 180 and 12.15 pJ/bit, respectively, at a 1.8-V supply
   voltage.}},
