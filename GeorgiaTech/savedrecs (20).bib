@article{ ISI:000322521100017,
Author = {Mao, Wenbin and Oron, Alexander and Alexeev, Alexander},
Title = {{Fluid transport in thin liquid films using traveling thermal waves}},
Journal = {{PHYSICS OF FLUIDS}},
Year = {{2013}},
Volume = {{25}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{Using long wave theory and direct numerical solutions of the
   Navier-Stokes equations, we investigate thermocapillary flows arising in
   a thin liquid film covering a heated solid substrate with non-uniform
   temperature in the form of traveling thermal waves. Our results indicate
   that unidirectionally propagating interfacial waves are formed in the
   liquid film. The interfacial waves transport liquid, thereby creating a
   net pumping effect. We show that the frequency of thermal waves leading
   to the most efficient pumping is defined by their wave length and weakly
   depends on other system parameters. The results are useful for designing
   new methods for transporting liquids in open microfluidic devices. (C)
   2013 AIP Publishing LLC.}},
