@article{ ISI:000320214600005,
Author = {He, Ming and Qiu, Feng and Lin, Zhiqun},
Title = {{Toward High-Performance Organic-Inorganic Hybrid Solar Cells: Bringing
   Conjugated Polymers and Inorganic Nanocrystals in Close Contact}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY LETTERS}},
Year = {{2013}},
Volume = {{4}},
Number = {{11}},
Pages = {{1788-1796}},
Month = {{JUN 6}},
Abstract = {{Organic-inorganic hybrid solar cells composed of conjugated polymers
   (CPs) and inorganic nanocrystal (NC) semiconductors have garnered
   considerable attention as a potential alternative to traditional silicon
   solar cells due to the capacity of producing high-efficiency solar
   energy in a cost-effective manner. The combination of advantageous
   characteristics of CPs and NCs enables the construction of
   nanostructured high-performance, lightweight, flexible, large-area, and
   low-cost hybrid solar cells. However, it remains a grand challenge to
   control the film morphology and interfacial structure of such
   organic/inorganic semiconductor blends on the nanoscale. In this
   Perspective, we highlight the strategies of implementing close contact
   between CPs and NCs by tailoring the colloidal synthesis, the
   coordination reaction, and the chemical modification of CPs. As such,
   they offer promising opportunities for rationally controlling the phase
   separation between electron-donating CPs and electron-accepting NCs,
   increasing the interfacial areas between them, enhancing their
   electronic interaction, and thus substantially promoting the
   photovoltaic performance of the resulting organic-inorganic hybrid solar
   cells.}},
