@article{ ISI:000319172200034,
Author = {McPhie, M. G. and Capolungo, L. and Dunn, A. Y. and Cherkaui, M.},
Title = {{Interfacial trapping mechanism of He in Cu-Nb multilayer materials}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2013}},
Volume = {{437}},
Number = {{1-3}},
Pages = {{222-228}},
Month = {{JUN}},
Abstract = {{He atom trapping in hetero-interphase materials is studied by atomistic
   simulations, focusing on the KSI and KSmin interfaces in Cu-Nb. If the
   bulk crystalline materials are defect free, single He atoms eventually
   become absorbed into the interfacial region via one of two different
   processes. In the first process, all He atoms arriving at the interface
   from the Cu side of the interface and some He atoms arriving from the Nb
   side, are trapped via the formation of a helium-vacancy (Hey) cluster in
   the second or third interfacial planes of the copper crystal. The
   immobile HeV cluster is found to be stable against dissociation and
   recombination. In the second case the He atoms are absorbed as
   interstitial atoms in one of the terminal planes. This process is
   dependent on the interstitial content of the interface and is found to
   be weak in the case of the KSI interface. (C) 2013 Elsevier B.V. All
   rights reserved.}},
