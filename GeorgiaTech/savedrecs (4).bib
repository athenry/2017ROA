@inproceedings{ ISI:000400601400073,
Author = {Chou, Remi A. and Bloch, Matthieu R. and Yener, Aylin},
Book-Group-Author = {{IEEE}},
Title = {{Universal Covertness for Discrete Memoryless Sources}},
Booktitle = {{2016 54TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
