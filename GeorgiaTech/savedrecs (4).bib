@inproceedings{ ISI:000393004000016,
Author = {Fronk, Matthew and Leamy, Michael J.},
Book-Group-Author = {{ASME}},
Title = {{WAVEFORM INVARIANCE IN NONLINEAR PERIODIC SYSTEMS USING HIGHER ORDER
   MULTIPLE SCALES}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
   CONFERENCES AND COMPUTERS AND INFORMATION IN ENGINEERING CONFERENCE,
   2016, VOL 8}},
Year = {{2016}},
