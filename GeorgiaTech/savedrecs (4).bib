@inproceedings{ ISI:000393000300036,
Author = {Park, Sang-in and Rosen, David W.},
Book-Group-Author = {{ASME}},
Title = {{HOMOGENIZATION OF MECHANICAL PROPERTIES FOR ADDITIVELY MANUFACTURED
   PERIODIC LATTICE STRUCTURES CONSIDERING JOINT STIFFENING EFFECTS}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
