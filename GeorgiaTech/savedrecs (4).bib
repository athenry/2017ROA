@inproceedings{ ISI:000393365100053,
Author = {Wang, Yan},
Book-Group-Author = {{ASME}},
Title = {{ACCELERATING STOCHASTIC DYNAMICS SIMULATION WITH CONTINUOUS-TIME QUANTUM
   WALKS}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
