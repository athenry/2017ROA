@inproceedings{ ISI:000400601400018,
Author = {Xie, Yao and Seversky, Lee},
Book-Group-Author = {{IEEE}},
Title = {{Sequential Low-Rank Change Detection}},
Booktitle = {{2016 54TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
