@inproceedings{ ISI:000399156200072,
Author = {Chen, Huangke and Zhu, Xiaomin and Qiu, Dishan and Liu, Ling},
Editor = {{Foster, I and Radia, N}},
Title = {{Uncertainty-Aware Real-Time Workflow Scheduling in the Cloud}},
Booktitle = {{PROCEEDINGS OF 2016 IEEE 9TH INTERNATIONAL CONFERENCE ON CLOUD COMPUTING
   (CLOUD)}},
Series = {{IEEE International Conference on Cloud Computing}},
Year = {{2016}},
Pages = {{577-584}},
Note = {{9th IEEE International Conference on Cloud Computing (CLOUD), San
   Francisco, CA, JUN 27-JUL 02, 2016}},
Organization = {{IEEE; IEEE Comp Soc; Serv Comp; Serv Soc; Serv Comp; Cloud Comp;
   BigDATA; hp; IBM; ERICSSON; SAP; IBM Res; Huawei; Object Management Grp;
   IEEE Cloud Comp; Business Proc Integrat \& Management; ITProfessional;
   Computing Now; IEEE Transact Serv Comp; Int Journal Web Serv Res; IEEE
   Comp Soc Tech Comm Serv Comp; Serv Comp Profess Interest Commun}},
Abstract = {{Scheduling real-time workflows running in the Cloud often need to deal
   with uncertain task execution time and minimize uncertainty propagation
   during the workflow runtime. Efficient scheduling approaches can
   minimize the operational cost of Cloud providers and provide higher
   guarantee of the quality of services (QoS) for Cloud consumers. However,
   most of the existing workflow scheduling approaches are designed for the
   individual workflow runtime environments that are deterministic. Such
   static workflow schedulers are inadequate for multiple and dynamic
   workflows, each with possibly uncertain task execution time. In this
   paper, we address the problem of minimizing uncertainty propagation in
   real-time workflow scheduling. We first introduce an uncertainty-aware
   scheduling architecture to mitigate the impact of uncertainty factors on
   the quality of workflow schedules. Then we present a dynamic workflow
   scheduling algorithm (PRS) that can dynamically exploit proactive and
   reactive scheduling methods. Finally, we conduct extensive experiments
   using real-world workflow traces and our experimental results show that
   PRS outperforms two representative scheduling algorithms in terms of
   costs (up to 60\%), resource utilization (up to 40\%) and deviation (up
   to 70\%).}},
