@inproceedings{ ISI:000402634701131,
Author = {Okamoto, Kazuhide and Tsiotras, Panagiotis},
Book-Group-Author = {{IEEE}},
Title = {{A New Hybrid Sensorimotor Driver Model with Model Predictive Control}},
Booktitle = {{2016 IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN, AND CYBERNETICS
   (SMC)}},
