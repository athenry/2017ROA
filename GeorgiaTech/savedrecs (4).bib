@article{ ISI:000393852200113,
Author = {Grew, Kyle N. and McClure, Joshua P. and Chu, Deryn and Kohl, Paul A.
   and Ahlfield, John M.},
Title = {{Understanding Transport at the Acid-Alkaline Interface of Bipolar
   Membranes}},
