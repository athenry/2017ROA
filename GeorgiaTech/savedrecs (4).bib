@article{ ISI:000368563000004,
Author = {Bag, Santanu and Patel, Romesh J. and Bunha, Ajaykumar and Grand,
   Caroline and Berrigan, J. Daniel and Dalton, Matthew J. and Leever,
   Benjamin J. and Reynolds, John R. and Durstock, Michael F.},
Title = {{Tandem Solar Cells from Accessible Low Band-Gap Polymers Using an
   Efficient Interconnecting Layer}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2016}},
Volume = {{8}},
Number = {{1}},
Pages = {{16-19}},
Month = {{JAN 13}},
Abstract = {{Tandem solar cell architectures are designed to improve device
   photoresponse by enabling the capture of wider range of solar spectrum
   as compared to single-junction device. However, the practical
   realization of this concept in bulk-heterojunction polymer systems
   requires the judicious design of a transparent interconnecting layer
   compatible with both polymers. Moreover, the polymers selected should be
   readily synthesized at large scale (>1 kg) and high performance. In this
   work, we demonstrate a novel tandem polymer solar cell that combines low
   band gap poly isoindigo {[}P(T3-iI)-2], which is easily synthesized in
   kilogram quantities, with a novel Cr/MoO3 interconnecting layer. Cr/MoO3
   is shown to be greater than 80\% transparent above 375 nm and an
   efficient interconnecting layer for P(T3-iI)-2 and PCDTBT, leading to
   6\% power conversion efficiencies under AM 1.5G illumination. These
   results serve to extend the range of interconnecting layer materials for
   tandem cell fabrication by establishing, for the first time, that a
   thin, evaporated layer of Cr/MoO3 can work as an effective
   interconnecting layer in a tandem polymer solar cells made with scalable
   photoactive materials.}},
