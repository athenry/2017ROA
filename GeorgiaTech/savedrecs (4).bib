@article{ ISI:000367902600046,
Author = {Ding, Yong and Pradel, Ken C. and Wang, Zhong Lin},
Title = {{In situ transmission electron microscopy observation of ZnO polar and
   non-polar surfaces structure evolution under electron beam irradiation}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2016}},
Volume = {{119}},
Number = {{1}},
Month = {{JAN 7}},
Abstract = {{Using in situ transmission electron microscopy, we investigated the
   dynamic reconstruction and evolution of ZnO polar and non-polar surfaces
   under high-energy electron beam irradiation. Electron beam radiolysis
   creates oxygen vacancies and a Zn rich (0001) surface. Positive polar
   charges at the (0001) surface expel loosely bonded Zn ions to diffuse
   away from the (0001) polar surface. As a result, mass loss was observed
   around the (0001) surface. Dehydration by the electron beam breaks the
   charge balance on the (000 (\$) over bar1) polar surface. The negative
   charges on the (000 (\$) over bar1) surface suppress the radiolysis
   effect and further absorb Zn ions to the surface to neutral the polar
   charges. The ideal stacking sequences of Zn ions in hexagonal ZnO
   structure can be considered as ABAB. along its c axis, while the
   absorbed individual Zn ion on the (000 (\$) over bar1) surface occupies
   the C site to form three bonds with surface O ions beneath, instead of
   one bond in the ideal structure. With more Zn ion absorption and surface
