@inproceedings{ ISI:000392361800064,
Author = {Tol, Serife and Degertekin, F. Levent and Erturk, Alper},
Book-Group-Author = {{ASME}},
Title = {{DRAMATIC ENHANCEMENT OF ELASTIC WAVE ENERGY HARVESTING USING A
   GRADIENT-INDEX PHONONIC CRYSTAL LENS}},
Booktitle = {{PROCEEDINGS OF THE ASME CONFERENCE ON SMART MATERIALS, ADAPTIVE
   STRUCTURES AND INTELLIGENT SYSTEMS, 2016, VOL 2}},
