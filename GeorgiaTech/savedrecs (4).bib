@inproceedings{ ISI:000400048107101,
Author = {Vasile, Cristian-Ioan and Leahy, Kevin and Cristofalo, Eric and Jones,
   Austin and Schwager, Mac and Belta, Calin},
Book-Group-Author = {{IEEE}},
Title = {{Control in Belief Space with Temporal Logic Specifications}},
