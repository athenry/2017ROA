Author = {Vachtsevanos, George and Georgoulas, George and Nikolakopoulos, George},
Book-Group-Author = {{IEEE}},
Title = {{Fault Diagnosis, Failure Prognosis and Fault Tolerant Control of
   Aerospace/Unmanned Aerial Systems}},
