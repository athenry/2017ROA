@inproceedings{ ISI:000388376106035,
Author = {Young, Carol and Zhang, Fumin},
Book-Group-Author = {{IEEE}},
Title = {{A Learning Algorithm to Select Consistent Reactions to Human Movements}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
