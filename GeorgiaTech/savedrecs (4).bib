Title = {{Mechanics-Based Design of Underactuated Robotic Walking Gaits: Initial
   Experimental Realization}},
Booktitle = {{2016 IEEE-RAS 16TH INTERNATIONAL CONFERENCE ON HUMANOID ROBOTS
   (HUMANOIDS)}},
Series = {{IEEE-RAS International Conference on Humanoid Robots}},
Year = {{2016}},
