@article{ ISI:000368958200008,
Author = {Shi, Guo-Liang and Xu, Jiao and Peng, Xing and Tian, Ying-Ze and Wang,
   Wei and Han, Bo and Zhang, Yu-Fen and Feng, Yin-Chang and Russell,
   Armistead G.},
Title = {{Using a new WALSPMF model to quantify the source contributions to PM2.5
   at a harbour site in China}},
Journal = {{ATMOSPHERIC ENVIRONMENT}},
Year = {{2016}},
Volume = {{126}},
Pages = {{66-75}},
Month = {{FEB}},
Abstract = {{PM2.5 variances have adverse impacts on human beings and the
   environment: therefore, source apportionment is very important and is a
   hot global topic. In this work, a new model called WALSPMF is proposed
   and evaluated for its accuracy. First, a synthetic test was carried out
   to compare the estimated source profile and contributions with the
   synthetic ones. Average absolute error (AAE) values were also calculated
   between the estimated and synthetic source contributions; most of the
   values were low (<15\%), which indicated that the results of the WALSPMF
   model might be acceptable. Next, samples of PM2.5 were collected from a
   large harbour sampling site in China (Tanggu). The PM2.5 mean level was
   110.63 mu g m(-3), with a range of 28.67 mu g m(-3)-302.17 mu g m(-3).
   The ambient PM2.5 dataset was separately introduced into both the
   WALSPMF and EPAPMF 5.0 models to identify the possible sources and their
   contributions. Five source categories were extracted by the two models
   and can be identified in the following consistent order: coal combustion
   (33\% for WALSPMF, 30\% for EPAPMF 5.0), secondary nitrate (19\% for
   WALSPMF, 21\% for EPAPMF 5.0), crustal dust (18\% for WALSPMF, 22\% for
   EPAPMF 5.0), secondary sulphate (16\% for WALSPMF, 15\% for EPAPMF 5.0),
   and vehicle exhaust (14\% for WALSPMF, 12\% for EPAPMF 5.0). The
   positive results of multiple verifications suggested good performance of
   the WALSPMF model; thus, it is essential to put this new model forward
   as a way to potentially enrich the modern source apportionment
   technique. (C) 2015 Elsevier Ltd. All rights reserved.}},
