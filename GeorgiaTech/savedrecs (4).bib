@inproceedings{ ISI:000401963304159,
Author = {Wu, Qingqing and Li, Geoffrey Ye and Chen, Wen and Ng, Derrick Wing Kwan},
Book-Group-Author = {{IEEE}},
Title = {{Spectrum-Power Trading for Energy-Efficient Small Cell}},
Booktitle = {{2016 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
