@inproceedings{ ISI:000388376100066,
Author = {Rana, Muhammad Asif and Yao, Ningshi and Mukhopadhyay, Shayok and Zhang,
   Fumin and Warren, Emilie and Payne, Christine},
Book-Group-Author = {{IEEE}},
Title = {{Modeling the effect of nanoparticles \& the bistability of transmembrane
   potential in non-excitable cells}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2016}},
Pages = {{400-405}},
Note = {{American Control Conference (ACC), Boston, MA, JUL 06-08, 2016}},
Organization = {{Amer Automat Control Council}},
Abstract = {{We present a simple dynamical model for the transmembrane potential in
   non-excitable mammalian cells. This model allows us to mimic the effect
   of nanoparticles on the transmembrane potential, by decreasing the
   permeability of potassium ion channels. We show that our model agrees
   with the trends observed in our experiments. However, it also exhibits
   opposing trends under certain conditions, which were not seen in the
   experiments. This indicates that the membrane potential can be bistable.
   We analyze the cellular conditions which may cause this apparent
   bistability. The effects of adding nanoparticles on sodium and chloride
   ion channels are also studied using the proposed model. We hypothesize
   that nanoparticles may also block the sodium and chloride ion channels
   but the extent of blockage of these ion channels may differ from that of
   potassium ion channels.}},
