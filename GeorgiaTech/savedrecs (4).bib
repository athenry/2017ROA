@inproceedings{ ISI:000405376800072,
Author = {Chockalingam, Sriram P. and Thankachan, Sharma V. and Aluru, Srinivas},
Book-Group-Author = {{IEEE}},
Title = {{A Parallel Algorithm for Finding All Pairs k-Mismatch Maximal Common
   Substrings}},
Booktitle = {{SC `16: PROCEEDINGS OF THE INTERNATIONAL CONFERENCE FOR HIGH PERFORMANCE
   COMPUTING, NETWORKING, STORAGE AND ANALYSIS}},
Year = {{2016}},
Pages = {{784-794}},
Note = {{International Conference on High Performance Computing, Networking,
   Storage and Analysis (SC), Salt Lake City, UT, NOV 13-18, 2016}},
Organization = {{IEEE Comp Soc; ACM; SIGPHC}},
Abstract = {{We present an efficient parallel algorithm for the following problem:
   Given an input collection D of n sequences of total length N, a length
   threshold phi and a mismatch threshold k, report all k-mismatch maximal
   common substrings of length at least f over all pairs of strings in D.
   This problem is motivated by clustering and assembly applications in
   computational biology, where D is a collection of millions of short DNA
   sequences. Sequencing errors and massive size of these datasets
   necessitate efficient parallel approximate sequence matching algorithms.
   We present a novel distributed memory parallel algorithm that solves
   this approximate sequence matching problem in O ((N/p log N + occ)
   log(k) N) expected time and takes only O (log(k+1) N) expected rounds of
   global communications, under some realistic assumptions, where p is the
   number of processors and occ is the output size. To our knowledge, this
   is the first provably sub-quadratic time algorithm for solving this
   problem. We demonstrate the performance and scalability of our algorithm
   using large high throughput sequencing data sets.}},
