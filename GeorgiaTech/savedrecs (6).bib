@inproceedings{ ISI:000379703300025,
Author = {Shukla, Dhwanil and Hiremath, Nandeesh and Motahari, Nicholas and
   Komerath, Narayanan},
Book-Group-Author = {{ASME}},
Title = {{GENESIS OF THE AIRLOAD VARIATIONS ON CYLINDERS OF SMALL ASPECT RATIOS}},
