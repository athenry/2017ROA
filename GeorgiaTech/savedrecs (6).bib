@article{ ISI:000374268000040,
Author = {Li, Ying and Zhang, Shan-Lin and Li, Cheng-Xin and Wei, Tao and Yang,
   Guan-Jun and Li, Chang-Jiu and Liu, Meilin},
Title = {{La2NiO4+delta Infiltration of Plasma-Sprayed LSCF Coating for Cathode
   Performance Improvement}},
