@article{ ISI:000367739000015,
Author = {He, Yejun and Pan, Zhengzheng and Cheng, Xudong and He, Yuan and Qiao,
   Jian and Tentzeris, Manos M.},
Title = {{A Novel Dual-Band, Dual-Polarized, Miniaturized and Low-Profile Base
   Station Antenna}},
