@article{ ISI:000367003400019,
Author = {Luo, Jianjun and Fan, Feng Ru and Jiang, Tao and Wang, Zhiwei and Tang,
   Wei and Zhang, Cuiping and Liu, Mengmeng and Cao, Guozhong and Wang,
   Zhong Lin},
Title = {{Integration of micro-supercapacitors with triboelectric nanogenerators
   for a flexible self-charging power unit}},
Journal = {{NANO RESEARCH}},
Year = {{2015}},
Volume = {{8}},
Number = {{12}},
Pages = {{3934-3943}},
Month = {{DEC}},
Abstract = {{The rapid development of portable and wearable electronic devices has
   increased demand for flexible and efficient energy harvesting and
   storage units. Conventionally, these are built and used separately as
   discrete components. Herein, we propose a simple and cost-effective
   laser engraving technique for fabricating a flexible self-charging
   micro-supercapacitor power unit (SCMPU), by integrating a triboelectric
   nanogenerator (TENG) and a micro-supercapacitor (MSC) array into a
   single device. The SCMPU can be charged directly by ambient mechanical
