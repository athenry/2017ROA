@article{ ISI:000373911500057,
Author = {Amirkhanian, A. N. and Spring, D. W. and Roesler, J. R. and Paulino, G.
   H.},
Title = {{Forward and Inverse Analysis of Concrete Fracture Using the Disk-Shaped
   Compact Tension Test}},
Journal = {{JOURNAL OF TESTING AND EVALUATION}},
Year = {{2016}},
Volume = {{44}},
Number = {{1}},
Pages = {{625-634}},
Month = {{JAN}},
Abstract = {{A new concrete fracture geometry is presented, which can quantify
   multiple fracture properties from a single specimen test. The
   disk-shaped compact tension (DCT) geometry allows specimens to be
   fabricated from laboratory cylinders or field cores. The DCT fracture
   test characterizes the concrete's critical stress intensity factor,
   K-IC, critical crack-tip opening displacement, CTODc, and initial
   fracture energy, G(f), as well as the specimen-dependent total fracture
   energy, G(F). The DCT-based fracture properties have the same
   experimental variation as the single-edge notched beam test. The
   experimentally derived fracture parameters were implemented into a
   cohesive zone model, which enabled estimation of concrete tensile
   strength from field-extracted cores.}},
