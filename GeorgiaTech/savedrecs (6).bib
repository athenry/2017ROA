@article{ ISI:000369509800010,
Author = {Kooreman, Gabriel and Rahnema, Farzad},
Title = {{ENHANCED HYBRID DIFFUSION-TRANSPORT SPATIAL HOMOGENIZATION METHOD}},
Journal = {{NUCLEAR TECHNOLOGY}},
Year = {{2015}},
Volume = {{192}},
Number = {{3, SI}},
Pages = {{264-277}},
