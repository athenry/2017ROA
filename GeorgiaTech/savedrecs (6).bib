Title = {{An Investigation of the SET Response of Devices and Differential Pairs
   in a 32-nm SOI CMOS Technology}},
Journal = {{IEEE TRANSACTIONS ON NUCLEAR SCIENCE}},
Year = {{2015}},
Volume = {{62}},
Number = {{6, 1}},
