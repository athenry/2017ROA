@inproceedings{ ISI:000377420000026,
Author = {Motahari, Nicholas and Hiremath, Nandeesh and Shukla, Dhwanil and
   Liberi, Brandon and Thorell, Nikolaus and Komerath, Narayanan},
Book-Group-Author = {{ASME}},
Title = {{GENERALIZED AIRLOADS PREDICTION FOR BLUFF BODIES TRANSPORTED AS SLUNG
