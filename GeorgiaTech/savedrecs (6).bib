@article{ ISI:000378891500005,
Author = {Naithani, Ved and Lucia, Lucian A. and Banerjee, Sujit},
Title = {{Soy flour and soy lecithin improve paper strength and formation}},
Journal = {{NORDIC PULP \& PAPER RESEARCH JOURNAL}},
Year = {{2016}},
Volume = {{31}},
Number = {{3}},
Pages = {{407-410}},
Abstract = {{Addition of soy flour to a batch repulper in a recycle linerboard mill
   led to a 6\% increase in ring crush at a cost of about \$2/tonne. In
   laboratory work, soy lecithin provided similar increases in burst and
   other parameters for both virgin and recycle pulps. The underlying
   mechanisms through which soy flour and lecithin act are different.
   Protein solubilized from soy flour in the repulper promotes fiber
   bonding through charge effects. Lecithin coats fiber surfaces and
   reduces surface roughness as shown by nanoscale imaging. It is proposed
   that inter-fiber friction is reduced as a consequence, which improves
   formation.}},
