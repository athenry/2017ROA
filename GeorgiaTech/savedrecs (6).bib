@inproceedings{ ISI:000371907800061,
Author = {Zhao, Youxuan and Qiu, Yanjun and Jacobs, Laurence J. and Qu, Jianmin},
Editor = {{Chimenti, DE and Bond, LJ}},
Title = {{A Micromechanics Model for the Acoustic Nonlinearity Parameter in Solids
   with Distributed Microcracks}},
Booktitle = {{42ND ANNUAL REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE
   EVALUATION: INCORPORATING THE 6TH EUROPEAN-AMERICAN WORKSHOP ON
   RELIABILITY OF NDE}},
Series = {{AIP Conference Proceedings}},
Year = {{2016}},
Volume = {{1706}},
Note = {{42nd Annual Review of Progress in Quantitative Nondestructive Evaluation
   (QNDE), Minneapolis, MN, JUL 26-31, 2015}},
Organization = {{Ctr Nondestruct Evaluat; QNDE Programs}},
Abstract = {{As a longitudinal wave propagates through a linearly elastic solid with
   distributed cracks, the solid is subjected to cyclic tension and
   compression. During the tensile cycles, a crack might be open and its
   faces are traction-free. During the compressive cycles, a crack might be
   closed and its faces are in contact. Such contact may also be frictional
   because of crack face roughness. Such tension and compression asymmetry
   causes acoustic nonlinearity. This paper develops a micromechanics model
   that relates the crack density to the acoustic nonlinearity parameter.
   The model is based on a micromechanics homogenization of the cracked
   solid under dynamic loading. It is shown that the acoustic nonlinearity
   parameter is proportional to the crack density. Furthermore, the
   acoustic nonlinearity parameter also depends on the frequency of the
   wave motion, and the coefficient of friction of the crack faces. Unlike
   the second harmonic generated by dislocations, the amplitude of the
   second harmonic due to crack face contact is proportional to the
   amplitude of the fundamental frequency. To validate the micromechanics
   model, the finite element method is used to simulate wave propagation in
   solid with randomly distributed microcracks. The micromechanics model
   predictions agree well with the finite element simulation results.}},
