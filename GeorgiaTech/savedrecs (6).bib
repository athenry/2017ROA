@article{ ISI:000375800500016,
Author = {Phonekeo, Sulisay and Dave, Tanvi and Kern, Matthew and Franklin, Scott
   V. and Hu, David L.},
Title = {{Ant aggregations self-heal to compensate for the Ringelmann effect}},
Journal = {{SOFT MATTER}},
Year = {{2016}},
Volume = {{12}},
Number = {{18}},
Pages = {{4214-4220}},
Abstract = {{Fire ants, Solenopsis invicta, link their bodies together to form
   structures such as rafts, bivouacs and bridges. Such structures are in
   danger of being damaged by natural disturbances such as passing water
   currents. In this combined experimental and theoretical study, we
   investigate the self-healing of ant assemblages. We press two ant
   aggregations together and measure the forces to pull them apart. As the
   group size increases, the contribution of each ant decreases. This
   phenomenon, known as the Ringelmann effect, or social loafing, has
   previously been shown for cattle and humans. In this study, we show that
   it is a challenge for ants as well. We rationalize this effect with an
   agent-based simulation which exhibits the Ringelmann effect of ants that
   periodically make and break links with each other, but grip with higher
   probability if the ants are stretched. Over time, ants compensate for
   the Ringelmann effect by building more links. We use a mathematical
   model to show that the rate of new links is proportional to the number
   of free ants in the cluster. The principles found here may inspire new
