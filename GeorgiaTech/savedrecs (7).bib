@article{ ISI:000365464800036,
Author = {Ye, Chunhong and Malak, Sidney T. and Hu, Kesong and Wu, Weibin and
   Tsukruk, Vladimir V.},
Title = {{Cellulose Nanocrystal Microcapsules as Tunable Cages for Nano- and Micro
   particles}},
Journal = {{ACS NANO}},
Year = {{2015}},
Volume = {{9}},
Number = {{11}},
Pages = {{10887-10895}},
Month = {{NOV}},
Abstract = {{We demonstrate the fabrication of highly open spherical cages with large
   through pores using high aspect ratio cellulose nanocrystals with
   ``haystack' shell morphology. In contrast to traditional ultrathin shell
   polymer microcapsules with random porous morphology and pore sizes below
   10 nm with limited molecular permeability of individual macromolecules,
   the resilient cage-like microcapsules show a remarkable open network
   morphology that facilitates across-shell transport of large solid
   particles with a diameter from 30 to 100 nm. Moreover, the transport
   properties of solid nanoparticles through these shells can be
   pH-triggered without disassembly of these shells. Such behavior allows
   for the controlled loading and unloading of solid nanoparticles with
   much larger dimensions than molecular objects reported for conventional
   polymeric microcapsules.}},
