@article{ ISI:000368668100010,
Author = {Su, Amy M. and Styczynski, Mark P.},
Title = {{Manipulation of metabolism in complex eukaryotic systems to control
   cellular state}},
Journal = {{CURRENT OPINION IN CHEMICAL ENGINEERING}},
Year = {{2015}},
Volume = {{10}},
Pages = {{63-69}},
Month = {{NOV}},
Abstract = {{The regulation of metabolism is critical to many important cellular
   processes in higher eukaryotes, and metabolites themselves can have
   significant regulatory potential in complex phenotypes. Thus, external
   control of metabolism for both biotechnological and biomedical ends is
   of great importance. There has been increasing emphasis on using
   non-genetic approaches for direct, transient control of metabolism and
   cellular state, with particularly promising applications in stem cell
   biomanufacturing and cancer. Metabolite-based methods have been used to
   facilitate expansion of stem cells, to control and prevent their
   differentiation, and to reprogram cells to a pluripotent state. Similar
   approaches are also being explored to inhibit the growth of cancer
   cells.}},
