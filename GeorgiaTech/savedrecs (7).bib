
@article{ ISI:000363437900001,
Author = {Tuo, Rui and Wu, C. F. Jeff},
Title = {{EFFICIENT CALIBRATION FOR IMPERFECT COMPUTER MODELS}},
Journal = {{ANNALS OF STATISTICS}},
Year = {{2015}},
Volume = {{43}},
Number = {{6}},
Pages = {{2331-2352}},
Month = {{DEC}},
Abstract = {{Many computer models contain unknown parameters which need to be
   estimated using physical observations. Tuo and Wu (2014) show that the
   calibration method based on Gaussian process models proposed by Kennedy
   and O'Hagan {[}J. R. Stat. Soc. Ser. B. Stat. Methodol. 63 (2001)
   425-464] may lead to an unreasonable estimate for imperfect computer
   models. In this work, we extend their study to calibration problems with
   stochastic physical data. We propose a novel method, called the L-2
   calibration, and show its semiparametric efficiency. The conventional
   method of the ordinary least squares is also studied. Theoretical
   analysis shows that it is consistent but not efficient. Numerical
   examples show that the proposed method outperforms the existing ones.}},
