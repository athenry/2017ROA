@article{ ISI:000362419900094,
Author = {Xiao, Jiangnan and Li, Xinying and Xu, Yuming and Zhang, Ziran and Chen,
   Long and Yu, Jianjun},
Title = {{W-band OFDM photonic vector signal generation employing a single
   Mach-Zehnder modulator and precoding}},
Journal = {{OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{23}},
Number = {{18}},
Pages = {{24029-24034}},
Month = {{SEP 7}},
Abstract = {{We present a simple radio-over-fiber (RoF) link architecture for
   millimeter-wave orthogonal frequency division multiplexing (OFDM)
   transmission using only one Mach-Zehnder modulator (MZM) and precoding
   technique. In the transmission system, the amplitudes and the phase of
   the driving radio-frequency (RF) OFDM signal on each sub-carrier are
   precoded, to ensure that the OFDM signal after photodetector (PD) can be
   restored to original OFDM signal. The experimental results show that the
   bit-error ratios (BERs) of the transmission system are less than the
   forward-error-correction (FEC) threshold of 3.8 x 10(-3), which
   demonstrates that the generation of OFDM vector signal based on our
   proposed scheme can be employed in our system architecture. (C) 2015
   Optical Society of America}},
