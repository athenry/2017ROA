@article{ ISI:000366534900020,
Author = {Xu, Jinquan and Chen, Ye-Hwa and Guo, Hong},
Title = {{Robust levitation control for maglev systems with guaranteed bounded
   airgap}},
Journal = {{ISA TRANSACTIONS}},
Year = {{2015}},
Volume = {{59}},
Pages = {{205-214}},
Month = {{NOV}},
Abstract = {{The robust control design problem for the levitation control of a
   nonlinear uncertain maglev system is considered. The uncertainty is
   (possibly) fast time-varying. The system has magnitude limitation on the
   airgap between the suspended chassis and the guideway in order to
   prevent undesirable contact. Furthermore, the (global) matching
   condition is not satisfied. After a three-step state transformation, a
   robust control scheme for the maglev vehicle is proposed, which is able
   to guarantee the uniform boundedness and uniform ultimate boundedness of
   the system, regardless of the uncertainty. The magnitude limitation of
   the airgap is guaranteed, regardless of the uncertainty. (C) 2015 ISA.
   Published by Elsevier Ltd. All rights reserved.}},
