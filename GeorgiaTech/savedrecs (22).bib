@inproceedings{ ISI:000329611507114,
Author = {Zhao, Yong and Juang, Biing-Hwang (Fred)},
Book-Group-Author = {{IEEE}},
Title = {{MODELING HETEROGENEOUS DATA SOURCES FOR SPEECH RECOGNITION USING
   SYNCHRONOUS HIDDEN MARKOV MODELS}},
Booktitle = {{2013 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
