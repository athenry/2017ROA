@inproceedings{ ISI:000333734900207,
Author = {Hu, Youmin and Wu, Bo and Vaughan, Joshua and Singhose, William},
Book-Group-Author = {{IEEE}},
Title = {{Oscillation Suppressing for an Energy Efficient Bridge Crane Using Input
   Shaping}},
Booktitle = {{2013 9TH ASIAN CONTROL CONFERENCE (ASCC)}},
Year = {{2013}},
Note = {{9th Asian Control Conference (ASCC), Istanbul, TURKEY, JUN 23-26, 2013}},
Abstract = {{Traditional bridge cranes waste a large amount of energy because payload
   oscillation causes the operator to move the massive bridge back and
   forth in order to damp out the swing. The energy efficient bridge crane
   (EEBC) is a new crane concept that adds a Micro Bridge fine-positioning
   stage to the typical dual-bridge crane. With this Micro/Macro
   dual-bridge design, there is the potential to save energy and improve
   positioning precision. A planar dynamic model of the EEBC is developed
   in this paper. Then, the dynamic performance of the EEBC is analyzed. An
   input shaper is used to suppress the oscillation of payload and improve
   payload-positioning accuracy. Simulation results show that the EEBC has
   the potential advantages of saving energy and improving positioning
   precision.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
