@article{ ISI:000315314400001,
Author = {Donnell, K. M. and Zoughi, R. and Kurtis, K. E.},
Title = {{Demonstration of microwave method for detection of alkali-silica
   reaction (ASR) gel in cement-based materials}},
Journal = {{CEMENT AND CONCRETE RESEARCH}},
Year = {{2013}},
Volume = {{44}},
Pages = {{1-7}},
Month = {{FEB}},
Abstract = {{Microwave methods have previously shown success in detecting changes in
   moisture content and binding (e.g., bound water vs. free water) in
   cement-based materials through extensive analysis of their reflection
   coefficient. Here, the novel use of microwave measurements is
   demonstrated to distinguish between mortars containing alkali-silica
   reactive (ASR) aggregate and non-reactive aggregate. The variations in
   measurements are linked to the production of ASR gel and its tendency to
   attract free water from its environment. This paper presents the
   analysis and results of the interaction of microwave signals with ASR
   gel-producing mortar samples and those without the ASR gel. (c) 2012
   Elsevier Ltd. All rights reserved.}},
