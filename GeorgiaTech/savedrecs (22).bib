@inproceedings{ ISI:000327210205052,
Author = {Yucelen, Tansel and Haddad, Wassim M.},
Book-Group-Author = {{IEEE}},
Title = {{Low-Frequency Learning and Fast Adaptation in Model Reference Adaptive
   Control for Safety-Critical Systems}},
Booktitle = {{2013 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
