Author = {Chalk, P. S. and Toereyin, H. and Bhatti, P. T.},
Book-Group-Author = {{IEEE}},
Title = {{Differential Measure of Matched Linear Accelerometers as a Sensing
   Strategy for the Development of a Vestibular Prosthesis}},
