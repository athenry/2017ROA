@inproceedings{ ISI:000332828000056,
Author = {Du, Zhihui and Sun, Hongyang and He, Yuxiong and He, Yu and Bader, David
   A. and Zhang, Huazhe},
Book-Group-Author = {{IEEE Comp Soc}},
Title = {{Energy-Efficient Scheduling for Best-Effort Interactive Services to
   Achieve High Response Quality}},
Booktitle = {{IEEE 27TH INTERNATIONAL PARALLEL AND DISTRIBUTED PROCESSING SYMPOSIUM
   (IPDPS 2013)}},
