@inproceedings{ ISI:000331367402123,
Author = {Kim, Michael D. and Ueda, Jun},
Editor = {{Amato, N}},
Title = {{Dynamics-based motion deblurring for a biologically-inspired camera
   positioning mechanism}},
Booktitle = {{2013 IEEE/RSJ INTERNATIONAL CONFERENCE ON INTELLIGENT ROBOTS AND SYSTEMS
   (IROS)}},
Series = {{IEEE International Conference on Intelligent Robots and Systems}},
Year = {{2013}},
Pages = {{2689-2694}},
