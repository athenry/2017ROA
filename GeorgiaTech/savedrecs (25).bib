@article{ ISI:000311387900003,
Author = {Mezencevova, Andrea and Garas, Victor and Nanko, Hiroki and Kurtis,
   Kimberly E.},
Title = {{Influence of Thermomechanical Pulp Fiber Compositions on Internal Curing
   of Cementitious Materials}},
Journal = {{JOURNAL OF MATERIALS IN CIVIL ENGINEERING}},
Year = {{2012}},
Volume = {{24}},
Number = {{8}},
Pages = {{970-975}},
Month = {{AUG}},
Abstract = {{This research examines the efficiency of thermomechanical pulp (TMP)
   fibers, both untreated and after chemical treatment to produce
   holocellulose and alpha-cellulose, for internal curing. In addition, the
   effect of TMP fibers on early hydration behavior was investigated. The
   results show that TMP and alpha-cellulose fibers had a negligible effect
   on cement hydration. Holocellulose, conversely, significantly reduced
   the rate of hydration, noticeably delaying setting time. The addition of
   the fibers in dosages to provide an additional amount of entrained water
   of 0.05 g per gram of cement resulted in a decrease of early autogenous
   shrinkage of cement paste. Holocellulose, reducing autogenous shrinkage
   of cement paste by 93\%, was shown to be the most effective for internal
   curing; however, its adverse effect on cement hydration may require
   acceleratory admixtures when used in concrete. Thermomechanical pulp and
   alpha-cellulose showed a similar efficiency in mitigating autogenous
   shrinkage; these fibers reduced shrinkage of cement paste by
   approximately 51 and 45\%, respectively. DOI:
   10.1061/(ASCE)MT.1943-5533.0000446. (C) 2012 American Society of Civil
   Engineers.}},
