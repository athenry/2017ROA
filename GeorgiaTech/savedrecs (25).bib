@article{ ISI:000309495200011,
Author = {Ma, Xiaojuan and Cao, Shilin and Luo, Xiaolin and Zhang, Yushuang and
   Huang, Fang and Huang, Liulian and Chen, Lihui},
Title = {{Pulp and Fiber Characterization of Bamboo Dissolving Pulp}},
Journal = {{JOURNAL OF BIOBASED MATERIALS AND BIOENERGY}},
Year = {{2012}},
Volume = {{6}},
Number = {{3}},
Pages = {{323-328}},
Month = {{JUN}},
Abstract = {{A sequence of treatments consisting of prehydrolysis-kraft-pulping and
   chlorine dioxide (D,), peroxide enriched alkaline extraction (Ep),
   chlorine dioxide (D-2), chelating (Q) and peroxide (P) bleaching
   sequences (D-1-Ep-D-2-QP) were investigated as a process for preparing
   dissolving-grade pulp for cellulose acetate production. Mao bamboo
   (Phyllostachys heterocycle cv pubescens) was used for characterizing
   chemical composition and crystallinity of the fiber in each treating
   stage. The results showed that prehydrolysis could remove more than 60\%
   pentosan. The subsequent extraction of hemicellulose mainly occurred in
   D-1-Ep bleaching stage, especially in Ep stage, while the viscosity loss
   existed in all the procedures. Whiteness gains of the pulp were
   substantial in D-2 stage on the condition of removal of most of lignin.
   As bleaching proceeded, the alpha-cellulose content increased because of
   the purification of fiber. Dissolving pulp with pentosan content 2.8\%,
   alpha-cellulose 95.4\%, viscosity 16.4 mPa.s and whiteness 91.0\% ISO
   was obtained through the above process. The fiber crystallinity
   increased from 38.4\% to 68.9\% when the bamboo material was treated by
   prehydrolysis-kraft pulping and D-1 bleaching stages; then decreased in
   Ep-D-2-QP stages to 58\%.}},
