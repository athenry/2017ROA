@article{ ISI:000308290400016,
Author = {Lipke, David W. and Zhang, Yunshu and Cai, Ye and Sandhage, Kenneth H.},
Title = {{Intragranular Tungsten/Zirconium Carbide Nanocomposites via a Selective
   Liquid/Solid Displacement Reaction}},
Journal = {{JOURNAL OF THE AMERICAN CERAMIC SOCIETY}},
Year = {{2012}},
Volume = {{95}},
Number = {{9, SI}},
Pages = {{2769-2772}},
Month = {{SEP}},
Abstract = {{A selective liquid/solid displacement reaction has been used, for the
   first time, to convert non-oxide ceramic solid solutions into composites
   comprised of intragranular metal nanoparticles embedded within non-oxide
   ceramic microcrystals. The reaction of (ZrxW1-x)C microparticles with a
   ZrCu liquid at 1300 degrees C resulted in the formation of W/ZrC
   microparticles with a core-shell morphology. The internal microparticle
   core was comprised of W nanoparticles embedded within a single crystal
   ZrC matrix, whereas the W-free shell was comprised of an epitaxial layer
   of ZrC. Such a selective liquid/solid reaction process may be used to
   generate a variety of intragranular metal/ceramic-matrix composites.}},
