@article{ ISI:000302199300022,
Author = {Nernirovski, Arkadi},
Title = {{On safe tractable approximations of chance constraints}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
Year = {{2012}},
Volume = {{219}},
Number = {{3}},
Pages = {{707-718}},
Month = {{JUN 16}},
Abstract = {{A natural way to handle optimization problem with data affected by
   stochastic uncertainty is to pass to a chance constrained version of the
   problem, where candidate solutions should satisfy the randomly perturbed
   constraints with probability at least 1 - epsilon. While being
   attractive from modeling viewpoint, chance constrained problems ``as
   they are{''} are, in general, computationally intractable. In this
