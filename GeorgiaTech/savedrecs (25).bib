@article{ ISI:000305356300004,
Author = {Hezave, Ali Zeinolabedini and Esmaeilzadeh, Feridun},
Title = {{Solubility Measurement of Diclofenac Acid in the Supercritical CO2}},
Journal = {{JOURNAL OF CHEMICAL AND ENGINEERING DATA}},
Year = {{2012}},
Volume = {{57}},
Number = {{6}},
Pages = {{1659-1664}},
Month = {{JUN}},
Abstract = {{The equilibrium solubility of the diclofenac acid in supercritical
   carbon dioxide was determined with a static method. The measurements
   were performed at pressures ranging from (120 to 400) bar and
   temperatures from (308.15 to 338.15) K. The results show that diclofenac
   acid solubility was increased by increasing pressure. The experimental
   solubility data were well correlated with enhanced density-based models
   including the Chrastil model, Mendez-Santiago and Teja model, Kumar and
   Johnston model, and the Bartle and co-workers model with an average
   absolute relative deviation (AARD) of 8.7 \%, 6.6 \%, 7.2 \%, and 9.7
   \%, respectively. In addition, the solubility data satisfied the
   self-consistency test, proposed by Mendez-Santiago and Teja.}},
