@article{ ISI:000306061400039,
Author = {Dickerson, Andrew K. and Shankles, Peter G. and Madhavan, Nihar M. and
   Hu, David L.},
Title = {{Mosquitoes survive raindrop collisions by virtue of their low mass}},
Journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA}},
Year = {{2012}},
Volume = {{109}},
Number = {{25}},
Pages = {{9822-9827}},
Month = {{JUN 19}},
Abstract = {{In the study of insect flight, adaptations to complex flight conditions
   such as wind and rain are poorly understood. Mosquitoes thrive in areas
   of high humidity and rainfall, in which raindrops can weigh more than 50
   times a mosquito. In this combined experimental and theoretical study,
   we here show that free-flying mosquitoes can survive the high-speed
   impact of falling raindrops. High-speed videography of those impacts
   reveals a mechanism for survival: A mosquito's strong exoskeleton and
   low mass renders it impervious to falling drops. The mosquito's low mass
   causes raindrops to lose little momentum upon impact and so impart
   correspondingly low forces to the mosquitoes. Our findings demonstrate
   that small fliers are robust to in-flight perturbations.}},
