   Cressler, John D.},
Title = {{Establishing Best-Practice Modeling Approaches for Understanding
   Single-Event Transients in Gb/s SiGe Digital Logic}},
Journal = {{IEEE TRANSACTIONS ON NUCLEAR SCIENCE}},
Year = {{2012}},
Volume = {{59}},
Number = {{4, 1}},
Pages = {{958-964}},
Month = {{AUG}},
