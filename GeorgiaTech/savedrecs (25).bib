@article{ ISI:000308043400010,
Author = {Brunelli, Nicholas A. and Didas, Stephanie A. and Venkatasubbaiah,
   Krishnan and Jones, Christopher W.},
Title = {{Tuning Cooperativity by Controlling the Linker Length of
   Silica-Supported Amines in Catalysis and CO2 Capture}},
Journal = {{JOURNAL OF THE AMERICAN CHEMICAL SOCIETY}},
Year = {{2012}},
Volume = {{134}},
Number = {{34}},
Pages = {{13950-13953}},
Month = {{AUG 29}},
Abstract = {{Cooperative interactions between amino-alkylsilanes and silanols on a
   silica surface can be controlled by varying the length of the alkyl
   linker attaching the amine to the silica surface from C1 (methyl) to C5
   (pentyl). The linker length strongly affects the catalytic cooperativity
   of amines and silanols in aldol condensations as well as the adsorptive
   cooperativity for CO2 capture. The catalytic cooperativity increases
   with the linker length up to propyl (C3), with longer, more flexible
   linkers (up to CS) providing no additional benefit or hindrance. Short
   linkers (C1 and C2) limit the beneficial amine-silanol cooperativity in
   aldol condensations, resulting in lower catalytic rates than with the
   C3+ linkers. For the same materials, the adsorptive cooperativity
   exhibits similar trends for CO2 capture efficiency.}},
