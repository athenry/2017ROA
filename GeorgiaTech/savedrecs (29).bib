@inproceedings{ ISI:000335710200097,
Author = {Volovoi, Vitali and Valenzuela, Rene and Bhatnagar, Himanshu and
   Nordlund, Scott and Davis, Fred and Villeneuve, Frederic and Guo, Yan
   and Patel, Chirag},
Book-Group-Author = {{ASME}},
Title = {{COMPONENT-BASED APPROACH TO MAINTENANCE MODELING OF GAS TURBINES}},
