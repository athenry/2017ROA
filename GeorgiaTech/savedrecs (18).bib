@inproceedings{ ISI:000366666800130,
Author = {Lee, Seonwoo and Baxley, Robert J.},
Editor = {{Jamalipour, A and Deng, DJ}},
Title = {{Achieving Positive Rate With Undetectable Communication Over AWGN and
   Rayleigh Channels}},
