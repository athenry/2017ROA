@inproceedings{ ISI:000369900404109,
Author = {Lin, Jian and Weitnauer, Mary Ann},
Book-Group-Author = {{IEEE}},
Title = {{Pulse-level Beam-switching MAC with Energy Control in Picocell Terahertz
   Networks}},
