@article{ ISI:000327123000047,
Author = {Li, Lester and Roethel, Sarah and Breedveld, Victor and Hess, Dennis W.},
Title = {{Creation of low hysteresis superhydrophobic paper by deposition of
   hydrophilic diamond-like carbon films}},
Journal = {{CELLULOSE}},
Year = {{2013}},
Volume = {{20}},
Number = {{6}},
Pages = {{3219-3226}},
Month = {{DEC}},
Abstract = {{The creation of low hysteresis superhydrophobic paper is reported using
   a combination of oxygen plasma etching and plasma deposition of an 80 nm
   non-fluorinated, hydrophilic diamond-like carbon (DLC) coating. The DLC
   has an equilibrium (flat surface) contact angle (theta (e) ) of 68.2A
   degrees A A +/- A 1.5A degrees, which is well below the 90A degrees
   contact angle that is typically believed to be a prerequisite for
   superhydrophobicity. Coating of paper substrates with the DLC film
   yields an advancing contact angle of 124.3A degrees A A +/- A 4.1A
   degrees, but the surface remains highly adhesive, with a receding
   contact angle < 10A degrees. After 60 min of plasma etching and DLC
   coating, a low hysteresis, superhydrophobic surface is formed with an
   advancing contact angle of 162.0A degrees A A +/- A 6.3A degrees and
   hysteresis of 8.7A degrees A A +/- A 1.9A degrees. To understand the
   increase in contact angle and decrease in hysteresis, atomic force
   microscopy and optical profilometry studies were performed. The data
   demonstrates that while little additional nanoscale roughness is
   imparted beyond the first 5 min of etching, the roughness at the
   microscale continually increases. The hierarchical structure provides
   the appropriate roughness to create low hysteresis superhydrophobic
   paper from a hydrophilic coating.}},
