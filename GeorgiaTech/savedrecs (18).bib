@inproceedings{ ISI:000379987100020,
Author = {Broadwell, Douglas and Paredis, Christiaan J. J.},
Book-Group-Author = {{ASME}},
Title = {{USING SYSML TO ELICIT A VALUE MODEL IN MULTI-STAKEHOLDER VALUE-DRIVEN
   SYSTEM DESIGN}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
