@inproceedings{ ISI:000380462600020,
Author = {Shareef, Zeeshan and Usman, Zubair and Rana, Muhammad Asif},
Book-Group-Author = {{IEEE}},
Title = {{Optimization of Static and Dynamic Anti-Windup Compensator using New
   Improved Particle Swarm Optimization Algorithm}},
