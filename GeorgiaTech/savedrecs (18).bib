@inproceedings{ ISI:000369851400092,
Author = {Kong, Yiming and Zhou, Qi and Ma, Xiaoli},
Book-Group-Author = {{IEEE Comp Soc}},
Title = {{Lattice Reduction Aided Transceiver Design for Multiuser MIMO Downlink
   Transmissions}},
Booktitle = {{2014 IEEE MILITARY COMMUNICATIONS CONFERENCE: AFFORDABLE MISSION
