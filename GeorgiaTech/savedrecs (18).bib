@article{ ISI:000328967900072,
Author = {Greenlee, Jordan D. and Shank, Joshua C. and Tellekamp, M. Brooks and
   Zhang, En Xia and Bi, Jinshun and Fleetwood, Daniel M. and Alles,
   Michael L. and Schrimpf, Ronald D. and Doolittle, W. Alan},
Title = {{Radiation Effects on LiNbO2 Memristors for Neuromorphic Computing
   Applications}},
Journal = {{IEEE TRANSACTIONS ON NUCLEAR SCIENCE}},
Year = {{2013}},
Volume = {{60}},
Number = {{6, 1}},
Pages = {{4555-4562}},
Month = {{DEC}},
Note = {{50th annual IEEE Nuclear and Space Radiation Effects Conference (NSREC),
   San Francisco, CA, JUL 08-12, 2013}},
Abstract = {{The effects of X-ray and proton radiation on a LiNbO2 analog memristor
   are investigated by I-V hysteresis, Electrochemical Impedance
   Spectroscopy, low-frequency AC voltage, and X-ray diffraction analysis.
   Both electrical and structural characterization of an irradiated
   memristor show that irradiation leads to an increased level of defects
   in the LiNbO2 crystalline lattice. These radiation-induced defects
   facilitate faster lithium movement as shown by electrochemical impedance
   spectroscopy measurements on the as-grown and irradiated memristor.
   X-ray radiation improves ionic motion in the bulk of the device and
   increases the ionic resistance at the LiNbO2-metal interface. In the
   case of proton radiation, the memristance response improves due to an
   increase in ionic motion in the bulk and at the interfaces. It is also
   shown by Monte Carlo simulations that proton irradiation of LiNbO2
   results in structural damage, which was verified experimentally by an
   X-ray diffraction study.}},
