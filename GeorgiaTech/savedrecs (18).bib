Author = {Lee, Kam Y. and Brown, David and Kumar, Satish},
Book-Group-Author = {{IEEE}},
Title = {{Performance Evaluation of Silicon Nanowire Arrays Based Thermoelectric
   Generators}},
Booktitle = {{2014 IEEE INTERSOCIETY CONFERENCE ON THERMAL AND THERMOMECHANICAL
   PHENOMENA IN ELECTRONIC SYSTEMS (ITHERM)}},
