@inproceedings{ ISI:000383780001053,
Author = {Ferguson, Daniel Michael and Newstetter, Wendy C. and Fisher, Eden and
   Gangopadhyay, Paula and Cawthorne, Jr., James Edwin and Condoor, Sridhar
   S. and Coyle, Edward J. and Wroblewski, Donald and Huellstrunk, Cornelia},
