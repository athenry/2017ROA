@inproceedings{ ISI:000370073806132,
Author = {L'Afflitto, Andrea and Haddad, Wassim M. and Bakolas, Efstathios},
Book-Group-Author = {{IEEE}},
Title = {{Partial-State Stabilization and Optimal Feedback Control}},
Booktitle = {{2014 IEEE 53RD ANNUAL CONFERENCE ON DECISION AND CONTROL (CDC)}},
