@inproceedings{ ISI:000366983004044,
Author = {Bhattacharyya, Raunak P. and Pritchett, Amy R.},
Book-Group-Author = {{IEEE}},
Title = {{A COMPUTATIONAL STUDY OF AUTONOMY AND AUTHORITY IN AIR TRAFFIC CONTROL}},
