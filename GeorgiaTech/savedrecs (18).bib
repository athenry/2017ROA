@article{ ISI:000327428000003,
Author = {Cheng, Lin and Liu, Cheng and Dong, Ze and Wang, Jing and Zhu, Ming and
   Chang, Gee-Kung},
Title = {{Real-time dual-band wireless videos in millimeter-wave radio-over-fiber
   system}},
Journal = {{OPTICAL FIBER TECHNOLOGY}},
Year = {{2013}},
Volume = {{19}},
Number = {{6}},
Pages = {{529-532}},
Month = {{DEC}},
Abstract = {{A dual-band converged radio-over-fiber (RoF) access system at 60-GHz and
   100-GHz millimeter-wave (mm-wave) is proposed. Real-time end-to-end
   delivery of two channels of independent high-definition (HD) video
   services simultaneously carried on 60-GHz and 100-GHz radios is
   demonstrated for the first time. PRBS data transmission with equivalent
   data rate and format is also tested to characterize the system
   performance. The analysis of the spectrum from the beating signal
   indicates the entire 60-GHz band and the W-band can be retrieved without
   interference. The real-time HD video display and error-free (BER < 10-9)
   data transmission demonstrate the feasibility of the proposed wireless
   access system using converged fiber-optic and mm-wave RoF techniques.
