@inproceedings{ ISI:000366603200149,
Author = {Garcia-Vergara, Sergio and Serrano, Miguel M. and Chen, Yu-Ping and
   Howard, Ayanna M.},
Book-Group-Author = {{IEEE}},
Title = {{Developing a Baseline for Upper-Body Motor Skill Assessment using a
   Robotic Kinematic Model}},
