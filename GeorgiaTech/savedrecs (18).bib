@inproceedings{ ISI:000369851400095,
Author = {Kong, Yiming and Ma, Xiaoli},
Book-Group-Author = {{IEEE Comp Soc}},
Title = {{Lattice Reduction Techniques in MIMO Systems with Correlated Channels}},
Booktitle = {{2014 IEEE MILITARY COMMUNICATIONS CONFERENCE: AFFORDABLE MISSION
