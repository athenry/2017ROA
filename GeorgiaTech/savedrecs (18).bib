@inproceedings{ ISI:000369851400169,
Author = {Laitrakun, Seksan and Coyle, Edward J.},
Book-Group-Author = {{IEEE Comp Soc}},
Title = {{Adaptive Reliability-Based Splitting Algorithms with Collision Inference
   for Sequential Detection}},
