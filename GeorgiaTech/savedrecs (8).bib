@article{ ISI:000358618500012,
Author = {Chi, Taiyun and Luo, Jun and Hu, Song and Wang, Hua},
Title = {{A Multi-Phase Sub-Harmonic Injection Locking Technique for Bandwidth
   Extension in Silicon-Based THz Signal Generation}},
Journal = {{IEEE JOURNAL OF SOLID-STATE CIRCUITS}},
Year = {{2015}},
Volume = {{50}},
Number = {{8, SI}},
Pages = {{1861-1873}},
