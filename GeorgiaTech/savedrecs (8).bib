@article{ ISI:000355770800001,
Author = {Brindley, K. A. and Kirka, M. M. and Fernandez-Zelaia, P. and Neu, R. W.},
Title = {{Thermomechanical Fatigue of Mar-M247: Extension of a Unified
   Constitutive and Life Model to Higher Temperatures}},
Journal = {{JOURNAL OF ENGINEERING MATERIALS AND TECHNOLOGY-TRANSACTIONS OF THE ASME}},
Year = {{2015}},
Volume = {{137}},
Number = {{3}},
Month = {{JUL}},
Abstract = {{The predictive capability of the Sehitoglu-Boismier unified constitutive
   and life model for Mar-M247 Ni-base superalloy is extended from a
   maximum temperature of 871 degrees C to 1038 degrees C. The unified
   constitutive model suitable for thermomechanical loading is adapted and
   calibrated using the response from isothermal cyclic experiments
   conducted at temperatures from 500 degrees C to 1038 degrees C at
   different strain rates with and without dwells. The flow rule function
   and parameters as well as the temperature dependence of the evolution
   equation for kinematic hardening are established. Creep and stress
   relaxation are critical to capture in this elevated temperature regime.
   The coarse-grained polycrystalline microstructure exhibits a high
   variability in the predicted cyclic response due to the variation in the
   elastic response associated with the orientation of the crystallographic
   grains. The life model accounts for fatigue, creep, and environmental
   damage under both isothermal and thermomechanical fatigue (TMF).}},
