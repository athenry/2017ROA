@article{ ISI:000352171800009,
Author = {Lei, Baiying and Zhou, Feng and Tan, Ee-Leng and Ni, Dong and Lei,
   Haijun and Chen, Siping and Wang, Tianfu},
Title = {{Optimal and secure audio watermarking scheme based on self-adaptive
   particle swarm optimization and quaternion wavelet transform}},
Journal = {{SIGNAL PROCESSING}},
Year = {{2015}},
Volume = {{113}},
Pages = {{80-94}},
Month = {{AUG}},
Abstract = {{In this paper, a new audio watermarking scheme based on self-adaptive
   particle swarm optimization (SAPSO) and quatemion wavelet transform
   (QWT) is proposed. By obtaining optimal watermark strength using a
   uniquely designed objective function, SAPSO addresses the conflicting
   problem of robustness, imperceptibility, and capacity of audio
   watermarking scheme using self-adjusted parameters. To withstand
   de-synchronization attack, a synchronization sequence generated by
   chaotic signals is also adopted in our scheme. Furthermore, the
   utilization of chaotic signals significantly enhances the security of
   the proposed scheme. The experimental results validate that our scheme
   is not only robust against de-synchronization attack, but also typical
   signal manipulations and StirMark attack. Our comparative analysis also
   revealed that the proposed scheme outperforms the state-of-the-arts
   audio watermarking schemes. (C) 2015 Published by Elsevier B.V.}},
