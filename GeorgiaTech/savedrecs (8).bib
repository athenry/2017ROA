@article{ ISI:000358590700005,
Author = {Navaei, Milad and Mahdavifar, Alireza and Dimandja, Jean-Marie D. and
   McMurray, Gary and Hesketh, Peter J.},
Title = {{All Silicon Micro-GC Column Temperature Programming Using Axial Heating}},
Journal = {{MICROMACHINES}},
Year = {{2015}},
Volume = {{6}},
Number = {{7}},
Pages = {{865-878}},
Month = {{JUL}},
Abstract = {{In this work we present a high performance micro gas chromatograph
   column with a novel two dimensional axial heating technique for faster
   and more precise temperature programming, resulting in an improved
   separation performance. Three different axial resistive heater designs
   were simulated theoretically on a 3.0 m x 300 mu m x 50 mu m column for
   the highest temperature gradient on a 22 by 22 mu m column. The best
   design was then micro-fabricated and evaluated experimentally. The
   simulation results showed that simultaneous temperature gradients in
   time and distance along the column are possible by geometric
   optimization of the heater when using forced convection. The gradients
   along the column continuously refocused eluting bands, offsetting part
   of the chromatographic band spreading. The utility of this method was
   further investigated for a test mixture of three hydrocarbons (hexane,
   octane, and decane).}},
