@article{ ISI:000359316100050,
Author = {Yatavelli, Reddy L. N. and Mohr, Claudia and Stark, Harald and Day,
   Douglas A. and Thompson, Samantha L. and Lopez-Hilfiker, Felipe D. and
   Campuzano-Jost, Pedro and Palm, Brett B. and Vogel, Alexander L. and
   Hoffmann, Thorsten and Heikkinen, Liine and Aijala, Mikko and Ng, Nga L.
   and Kimmel, Joel R. and Canagaratna, Manjula R. and Ehn, Mikael and
   Junninen, Heikki and Cubison, Michael J. and Petaja, Tuukka and Kulmala,
   Markku and Jayne, John T. and Worsnop, Douglas R. and Jimenez, Jose L.},
Title = {{Estimating the contribution of organic acids to northern hemispheric
   continental organic aerosol}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2015}},
Volume = {{42}},
Number = {{14}},
Pages = {{6084-6090}},
Month = {{JUL 28}},
Abstract = {{Using chemical ionization mass spectrometry to detect particle-phase
   acids and aerosol mass spectrometry (AMS) measurements from Colorado,
   USA, and two studies in Hyytiala, Finland, we quantify the fraction of
   organic aerosol (OA) mass that is composed of molecules with acid
   functional groups (f(acid)). Molecules containing one or more carboxylic
   acid functionality contributed approximately 29\% (45-51\%) of the OA
   mass in Colorado (Finland). Organic acid mass concentration correlates
   well with AMS m/z 44 (primarily CO2+), a commonly used marker for highly
   oxidized aerosol. Using the average empirical relationship between AMS
   m/z 44 and organic acids in these three studies, together with m/z 44
   data from 29 continental northern hemispheric (NH) AMS data sets, we
   estimate that molecules containing carboxylic acid functionality
   constitute on average 28\% (range 10-50\%) of NH continental OA mass
   with typically higher values at rural/remote sites and during summer and
   lower values at urban sites and during winter.}},
