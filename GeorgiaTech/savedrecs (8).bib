@article{ ISI:000360421000016,
Author = {Tsai, Yichang (James) and Wu, Yiching and Lai, James},
Title = {{CRITICAL ASSESSMENT OF JOINTED PLAIN CONCRETE PAVEMENT PERFORMANCE USING
   40-YEARS OF CONDITION EVALUATION DATA}},
Journal = {{JOURNAL OF MARINE SCIENCE AND TECHNOLOGY-TAIWAN}},
Year = {{2015}},
Volume = {{23}},
Number = {{3}},
Pages = {{380-387}},
Month = {{JUN}},
