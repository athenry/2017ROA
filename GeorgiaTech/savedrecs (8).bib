@article{ ISI:000357014300036,
Author = {Wang, Cong and Jeon, Hamhee and Kim, Nam-Young},
Title = {{A compact spiral-coupled directional coupler for a mobile RFID reader
   via integrated passive technology}},
Journal = {{MICROWAVE AND OPTICAL TECHNOLOGY LETTERS}},
Year = {{2015}},
Volume = {{57}},
Number = {{9}},
Pages = {{2142-2146}},
Month = {{SEP}},
Abstract = {{This article presents the design and implementation of a
   high-performance, compact spiral-coupled directional coupler using an
   advanced integrated passive device (IPD) fabrication process on a GaAs
   substrate for use in mobile radio frequency identification (RFID) reader
   applications. The IPD fabrication step is introduced specifically to
   enable the directional coupler, which is constructed by the
   transformation of the intertwined mutual inductors and air-bridges, to
   have a compact size and high Q-factor. The fabricated coupler achieves
   only 0.43 dB of insertion loss S-21 and it exhibits the coupling S-31 of
   15.7 dB and the isolation S-41 of 27.2 dB. This presented coupler has an
   extra small size of only 0.72 x 0.82 mm(2) and an operational frequency
   of 910 MHz, which is appropriate for use in an RFID reader system. (c)
   2015 Wiley Periodicals, Inc. Microwave Opt Technol Lett 57:2142-2146,
