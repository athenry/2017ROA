@inproceedings{ ISI:000346492601119,
Author = {Sadikhov, Teymur and Haddad, Wassim M. and Malisoff, Michael},
Book-Group-Author = {{IEEE}},
Title = {{A Universal Feedback Controller for Discontinuous Dynamical Systems
   using Nonsmooth Control Lyapunov Functions}},
Booktitle = {{2014 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2014}},
Pages = {{1174-1179}},
Note = {{American Control Conference, Portland, OR, DEC 04-06, 2014}},
