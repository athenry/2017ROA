Title = {{Spall properties of Al 5083 plate fabricated using equi-channel angular
   pressing (ECAP) and rolling}},
Booktitle = {{18TH APS-SCCM AND 24TH AIRAPT, PTS 1-19}},
Series = {{Journal of Physics Conference Series}},
Year = {{2014}},
Volume = {{500}},
Note = {{18th Joint Int Conf of the APS Topical-Grp on Shock Compress of
