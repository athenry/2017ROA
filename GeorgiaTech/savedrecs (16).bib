@inproceedings{ ISI:000346499600265,
Author = {Lee, Kok-Meng and Lin, Chun-Yeon},
Book-Group-Author = {{IEEE}},
Title = {{Design Concept of a Novel EM-Array Magnetic Scanning System for
   Continuous Motion Control of Maximum MFD}},
Booktitle = {{2014 IEEE/ASME INTERNATIONAL CONFERENCE ON ADVANCED INTELLIGENT
   MECHATRONICS (AIM)}},
Series = {{IEEE ASME International Conference on Advanced Intelligent Mechatronics}},
Year = {{2014}},
