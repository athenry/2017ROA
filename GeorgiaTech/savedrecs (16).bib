@inproceedings{ ISI:000346499600199,
Author = {Ji, Jingjing and Lee, Kok-Meng and Wang, Donghai},
Book-Group-Author = {{IEEE}},
Title = {{Real-time Computational Model for Visualizing Compliant Beam Motion of a
   Flexible Mobile-sensing Node}},
Booktitle = {{2014 IEEE/ASME INTERNATIONAL CONFERENCE ON ADVANCED INTELLIGENT
   MECHATRONICS (AIM)}},
Series = {{IEEE ASME International Conference on Advanced Intelligent Mechatronics}},
Year = {{2014}},
