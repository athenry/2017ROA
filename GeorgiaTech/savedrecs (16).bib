@inproceedings{ ISI:000345005900062,
Author = {Kim, Byungchil and Li, Nianqiang and Choi, Daeyoung and Locquet, A. and
   Citrin, D. S.},
Editor = {{Panajotov, K and Sciamanna, M and Valle, A and Michalzik, R}},
Title = {{Bifurcation diagram of an external-cavity semiconductor laser:
   experiment and theory}},
Booktitle = {{SEMICONDUCTOR LASERS AND LASER DYNAMICS VI}},
Series = {{Proceedings of SPIE}},
Year = {{2014}},
Volume = {{9134}},
Note = {{Conference on Semiconductor Lasers and Laser Dynamics VI, Brussels,
   BELGIUM, APR 14-17, 2014}},
Organization = {{SPIE; Brussels Photon Team; Fonds Wetenschappelijk Onderzoek}},
Abstract = {{We report experimental bifurcation diagrams (BDs) of an external-cavity
   semiconductor laser (ECSL). We have focused on the case of the ECSL
   biased just above threshold to moderate and subjected to feedback from a
   distant reflector and observed a sequence of bifurcations involving
   bifurcation cascade as well as intermittency between multiple coexisting
   attractors. More Importantly, we reiterate: the results map out, for the
   first time to our knowledge, detailed BDs of the ECSL as a function of
   feedback strength for various external cavity lengths and currents, thus
   covering a significant portion of parameter space. We have grounded our
   discussion in extensive theoretical studies based on the Lang-Kobayashi
