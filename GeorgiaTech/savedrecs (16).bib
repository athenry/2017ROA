@article{ ISI:000333579600008,
Author = {Lei, J. and McLane, L. T. and Curtis, J. E. and Temenoff, J. S.},
Title = {{Characterization of a multilayer heparin coating for biomolecule
   presentation to human mesenchymal stem cell spheroids}},
Journal = {{BIOMATERIALS SCIENCE}},
Year = {{2014}},
Volume = {{2}},
Number = {{5}},
Pages = {{666-673}},
Abstract = {{Mesenchymal stem cell (MSC) therapies have the potential to treat many
   pathologies, however, controlling cell fate after implantation remains
   challenging. We have used a multilayer technology to graft a range of 5
   mu g mL(-1)-5 mg mL(-1) heparin onto the surface of MSC aggregates.
   Heparin coating does not affect cell viability (seen through LIVE/DEAD
   staining) and cell anti-inflammatory properties (seen through co-culture
   with activated monocytes) and facilitates sequestration by coated cells
   of a growth factor (TGF-beta 1) that remains bioactive. This system can
   maximize the therapeutic potential of MSC-based treatments because the
   cell surface-loaded protein could both signal the cells to influence
   transplanted cell fate and be released into the surrounding environment
   to help repair injured tissue.}},
