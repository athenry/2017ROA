Author = {Natarajan, Jayaram and Kapoor, Sahil and Bhatta, Debesh and Chatterjee,
   Abhijit and Singh, Adit},
Book-Group-Author = {{IEEE}},
Title = {{Timing Variation Adaptive Pipeline Design: Using Probabilistic Activity
   Completion Sensing With Backup Error Resilience}},
Booktitle = {{2014 27TH INTERNATIONAL CONFERENCE ON VLSI DESIGN AND 2014 13TH
   INTERNATIONAL CONFERENCE ON EMBEDDED SYSTEMS (VLSID 2014)}},
Series = {{International Conference on VLSI Design}},
Year = {{2014}},
