@inproceedings{ ISI:000342312400091,
Author = {Thiele, Sebastian and Matlack, Kathryn H. and Kim, Jin-Yeon and Qu,
   Jianmin and Wall, James J. and Jacobs, Laurence J.},
Editor = {{Chimenti, DE and Bond, LJ and Thompson, DO}},
Title = {{Assessment of Precipitation in Alloy Steel Using Nonlinear Rayleigh
   Surface Waves}},
Booktitle = {{40TH ANNUAL REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE
   EVALUATION: INCORPORATING THE 10TH INTERNATIONAL CONFERENCE ON
   BARKHAUSEN NOISE AND MICROMAGNETIC TESTING, VOLS 33A \& 33B}},
Series = {{AIP Conference Proceedings}},
Year = {{2014}},
Volume = {{1581}},
Pages = {{682-689}},
Note = {{10th International Conference on Barkhausen and Micro-Magnetics (ICBM),
   Baltimore, MD, JUL 21-26, 2013}},
Organization = {{Quantitat Nondestruct Evaluat Programs; Amer Soc Nondestruct Testing;
   NDE Centers, World Federat; Stresstech Grp \& Int Conf Barkhausen Noise
   \& Micromagnet Testing Org Comm; Int Comm NDT; Natl Sci Fdn Ind Univ
   Cooperat Res Ctr; Iowa State Univ, Ctr Nondestruct Evaluat}},
Abstract = {{Nonlinear ultrasonic waves have shown to be sensitive to various
   microstructural changes in metals including coherent precipitates; these
   precipitates introduce a strain field in the lattice structure. The
   thermal aging of certain alloy steels leads to the formation of coherent
   precipitates, which pin dislocations and contribute to the generation of
   a second harmonic component. A precipitate hardenable material namely
   17-4 PH stainless steel is thermally treated in this research to obtain
   different precipitation stages, and then the influence of precipitates
   on the acoustic nonlinearity parameter is assessed. Conclusions about
   the microstrucutural changes in the material are drawn based on the
   results from a nonlinear Rayleigh surface wave measurement and
