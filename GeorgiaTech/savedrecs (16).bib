@inproceedings{ ISI:000346492602006,
Author = {Polymeneas, Evangelos and Benosman, Mouhacine},
Book-Group-Author = {{IEEE}},
Title = {{Finite Time Protocols for Multi-Agent Control of Distributed Generation
   and Responsive Loads}},
Booktitle = {{2014 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2014}},
Note = {{American Control Conference, Portland, OR, DEC 04-06, 2014}},
