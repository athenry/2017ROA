@inproceedings{ ISI:000346492604081,
Author = {Zafeiropoulos, Spyridon and Tsiotras, Panagiotis},
Book-Group-Author = {{IEEE}},
Title = {{Design of a Lane-Tracking Driver Steering Assist System and Its
