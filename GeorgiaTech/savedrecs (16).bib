@article{ ISI:000331486700007,
Author = {Grothe, Pamela R. and Cardozo, Nestor and Mueller, Karl and Ishiyama,
   Tatsuya},
Title = {{Propagation history of the Osaka-wan blind thrust, Japan, from trishear
   modeling}},
Journal = {{JOURNAL OF STRUCTURAL GEOLOGY}},
Year = {{2014}},
Volume = {{58}},
Pages = {{79-94}},
Month = {{JAN}},
Abstract = {{Mapping the nucleation and 3D fault tip growth of the active Osaka-wan
   blind thrust provides an opportunity to asses how reactivated thrusts
   build slip from preexisting faults and the threat they pose as sources
   of large earthquakes. Analysis of folded growth strata, based on 2D
   trishear inverse modeling allows a range of best-fit models of the
   evolution of slip and propagation of the fault to be defined. The depth
   of the fault tip at 1200 ka varies between similar to 1.5-4.5 km,
   suggesting the fault grew upward from high in the crust, and that it is
   reactivated. From its onset at similar to 1500 ka, the fault grew
   rapidly along strike in similar to 300 ky, and upwards with a P/S ratio
   of 2.5-3.0, but variable fault slip in space and time. Shallower depths
   of the fault tip at initiation and thinner basin fill correlates with
   slower propagation with time, contradicting models that argue for
   sediments as inhibitors of fault growth. Results also suggest the
   displacement profile of the currently active thrust is offset from its
   predecessor, assuming shallower depths to the original fault correlate
