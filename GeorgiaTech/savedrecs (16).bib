@inproceedings{ ISI:000339374200051,
Author = {Yang, C. S. Walter and DesRoches, Reginald},
Editor = {{Liao, WH}},
Title = {{Smart hybrid rotary damper}},
Booktitle = {{ACTIVE AND PASSIVE SMART STRUCTURES AND INTEGRATED SYSTEMS 2014}},
