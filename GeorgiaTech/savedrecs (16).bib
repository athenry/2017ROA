@inproceedings{ ISI:000349928200089,
Author = {Verma, Nalin and Skow, Ellen and Cunefare, Kenneth A. and Erturk, Alper},
Book-Group-Author = {{ASME}},
Title = {{POWER DENSITY PERFORMANCE IMPROVEMENTS FOR HIGH PRESSURE RIPPLE ENERGY
   HARVESTING}},
Booktitle = {{PROCEEDINGS OF THE ASME CONFERENCE ON SMART MATERIALS ADAPTIVE
   STRUCTURES AND INTELLIGENT SYSTEMS - 2013, VOL 2}},
Year = {{2014}},
