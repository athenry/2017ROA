@inproceedings{ ISI:000348126000023,
Author = {Hashmi, Ali J. and Eftekhar, Ali A. and Adibi, Ali and Amoozegar, Farid},
Editor = {{Carapezza, EM and Datskos, PG and Tsamis, C}},
Title = {{Lower Bound on Number and Sizes of Telescopes in an Optical Array
   Receiver for Deep Space Optical Communication}},
Booktitle = {{UNMANNED/UNATTENDED SENSORS AND SENSOR NETWORKS X}},
Series = {{Proceedings of SPIE}},
