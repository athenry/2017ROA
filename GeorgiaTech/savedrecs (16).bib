@article{ ISI:000344184300066,
Author = {Cao, Shilin and Ma, Xiaojuan and Lin, Ling and Huang, Fang and Huang,
   Liulian and Chen, Lihui},
Title = {{Morphological and Chemical Characterization of Green Bamboo
   (Dendrocalamopsis oldhami (Munro) Keng f.) for Dissolving Pulp
   Production}},
Journal = {{BIORESOURCES}},
Year = {{2014}},
Volume = {{9}},
Number = {{3}},
Pages = {{4528-4539}},
Abstract = {{With the sustained growth of dissolving pulp demand all over the world,
   the search for alternative bamboo materials has come into focus in China
   due to the shortage of wood and the abundance of bamboo resources. In
   this study, to obtain updated information concerning green bamboo
   growing in southeastern China and to develop its processing technologies
   for dissolving pulp, the fiber morphology, chemical composition,
   elemental composition, degree of polymerization (DP) of cellulose, and
   crystallinity index (Crl) of cellulose were investigated. The
   experimental results show that green bamboo has potential for use as
   dissolving pulp because it has a lower Runkel ratio and fines content
   than moso bamboo, and a much lower lignin content and similar
   alpha-cellulose and hemicellulose contents compared to softwoods and
   hardwoods. Compared to the cortex and culm, the node had the shortest
   fibers and more than 30\% of fines, the highest content of extractives
   and lignin, and the lowest alpha-cellulose content. As a result, a
   de-knotting operation prior to cooking can contribute to the production
   of high-grade dissolving pulp. The DP and Crl of cellulose from the node
   were much lower than that of cellulose from the culm and cortex.
   Moreover, green bamboo had the high content of ash, primarily
   distributed in the cortex. The concentration of Si was 4487 ppm in the
   cortex, nearly five times higher than that in the culm and node.}},
