Author = {Shakil, Sadia and Magnuson, Matthew E. and Keilholz, Shella D. and Lee,
   Chin-Hui},
Book-Group-Author = {{IEEE}},
Title = {{Cluster-based analysis for characterizing dynamic functional
   connectivity}},
Booktitle = {{2014 36TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
   MEDICINE AND BIOLOGY SOCIETY (EMBC)}},
