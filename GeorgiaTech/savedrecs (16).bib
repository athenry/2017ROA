@inproceedings{ ISI:000337900800087,
Author = {Zhou, Wenchao and Loney, Drew and Fedorov, Andrei G. and Degertekin, F.
   Levent and Rosen, David W.},
Editor = {{Bartolo, PJD and DeLemos, ACS and Pereira, AMH and Mateus, AJD and Ramos, C and DosSantos, C and Oliveira, D and Pinto, E and Craveiro, F and Bartolo, HMCDTG and Almeia, HD and Sousa, I and Matias, JM and Durao, L and Gaspar, M and Alves, NMF and Carreira, P and Ferreira, T and Marques, T}},
Title = {{On a three-dimensional Lattice Boltzmann Model of droplet impingement
   for ink-jet deposition}},
Booktitle = {{HIGH VALUE MANUFACTURING: ADVANCED RESEARCH IN VIRTUAL AND RAPID
   PROTOTYPING}},
Year = {{2014}},
Pages = {{511-518}},
Note = {{6th International Conference on Advanced Research in Virtual and
   Physical Prototyping (VRatP), Leiria, PORTUGAL, OCT 01-05, 2013}},
Organization = {{Polytechn Inst Leiria, Ctr Rapid \& Sustainable Prod Dev}},
Abstract = {{Ink-jet deposition enables more economic and scalable manufacturing than
   other additive techniques. Studying the interface dynamics of multiple
   droplet interaction is critical to understanding droplet deposition
   process and control of manufacturing results. However, little research
   progress has been reported because computational cost with commercial
   software packages prohibits 3D simulation of droplet impingement under
   relevant conditions. This paper presents an efficient numerical model
   based on the Lattice Boltzmann Method that enables simulation of
   multiple-droplet impingement in 3D. A lattice Boltzmann formulation is
   proposed to solve the governing equations of the phase-field model in
   commercial software COMSOL. A new set of wall boundary conditions for
   the lattice Boltzmann formulation has also been proposed based on
   conservation principles to ensure correct contact line dynamics. The
   simulation results of our model are compared with that of COMSOL and
   experimental data from literature. Results show our model has not only a
   significant speed improvement but also better accuracy than COMSOL. We
   have also demonstrated the capability of our numerical model for
   studying the interface evolution of multiple-droplet impingement.}},
