@inproceedings{ ISI:000348027200043,
Author = {Leamy, Michael J. and Autrusson, Thibaut B. and Staszewski, Wieslaw J.
   and Uhl, Tadeusz and Packo, Pawel},
Editor = {{Kundu, T}},
Title = {{Local computational strategies for predicting wave propagation in
   nonlinear media}},
