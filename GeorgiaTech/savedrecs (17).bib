@inproceedings{ ISI:000361554400421,
Author = {Su, Wenjing and Cook, Benjamin and Tentzeris, Manos and Mariotti, Chiara
   and Roselli, Luca},
Book-Group-Author = {{IEEE}},
Title = {{A Novel Inkjet-Printed Microfluidic Tunable Coplanar Patch Antenna}},
Booktitle = {{2014 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM
   (APSURSI)}},
