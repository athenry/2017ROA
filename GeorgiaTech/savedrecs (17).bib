@inproceedings{ ISI:000362796100097,
Author = {Aridogan, Ugur and Basdogan, Ipek and Erturk, Alper},
Book-Group-Author = {{ASME}},
Title = {{ELECTROELASTIC FINITE ELEMENT MODELING AND EXPERIMENTAL VALIDATION OF
   STRUCTURALLY-INTEGRATED PIEZOELECTRIC ENERGY HARVESTER}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
   CONFERENCES AND COMPUTERS AND INFORMATION IN ENGINEERING CONFERENCE,
   2013, VOL 8}},
Year = {{2014}},
