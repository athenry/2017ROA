Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
Language = {{English}},
Affiliation = {{Rashid, MW (Reprint Author), Georgia Inst Technol, Sch Elect \& Comp Engn, Atlanta, GA 30332 USA.
   Rashid, M. Wasequr; Ghovanloo, Maysam; Degertekin, F. Levent, Georgia Inst Technol, Sch Elect \& Comp Engn, Atlanta, GA 30332 USA.
   Tekes, Coskun; Degertekin, F. Levent, Georgia Inst Technol, George W Woodruff Sch Mech Engn, Atlanta, GA 30332 USA.}},
DOI = {{10.1109/ULTSYM.2014.0381}},
ISSN = {{1948-5719}},
