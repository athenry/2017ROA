@article{ ISI:000331782000029,
Author = {Ma, Pibo and Jiang, Gaoming and Gao, Zhe and Zhang, Qi and Xia, Dong},
Title = {{Tension and Tear Behaviors of Co-woven-knitted Fabric with Photograph
   Investigation}},
