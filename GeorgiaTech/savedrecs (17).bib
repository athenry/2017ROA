@article{ ISI:000359193600006,
Author = {Meyer, Joseph F. and Singh, Preet M.},
Title = {{RECOVERY BOILER SUPERHEATER CORROSION: THE SOLUBILITY OF PROTECTIVE
   METAL OXIDES IN MOLTEN SALTS}},
Journal = {{J-FOR-JOURNAL OF SCIENCE \& TECHNOLOGY FOR FOREST PRODUCTS AND PROCESSES}},
Year = {{2014}},
Volume = {{4}},
Number = {{4}},
Pages = {{56-64}},
Abstract = {{Molten salt corrosion is a major concern in the pulp and paper industry
   because it limits recovery boiler efficiency by preventing increases in
   steam temperature. Alloys used in superheater tubes depend on the
   stability of the oxide film formed on the surface. Typically, the
   superheater temperature is kept below the first melting temperature of
   the deposits formed on the tube surface. If deposits melt at the
   surface, oxides with low solubility in deposit salts will tend to be
   more protective when the salt reaches equilibrium. The solubility of
   metal oxides (Al2O3, Fe2O3, Cr2O3, NiO, and SiO2) in molten superheater
   deposits was evaluated. The salt composition used was 73.9 wt\% Na2SO4,
   10.2 wt\% KCl, 11.5 wt\% Na2CO3, and 4.4 wt\% K2SO4. The solubility of
   NiO was found to be the least in this salt, followed by Fe2O3, Cr2O3,
   Al2O3, and SiO2. Cr2O3 and Al2O3 were found to have similar solubility.
   Pure metals as well as candidate alloys were also tested to quantify
   their relative corrosion behaviour in molten simulated superheater
   deposits. Nickel-based alloys performed much better than most iron-based
   stainless steels. However, Esshete 1250, an austenitic stainless steel
   with higher Mn and lower Ni content, showed very good resistance to
   molten salts even at 750 degrees C.}},
