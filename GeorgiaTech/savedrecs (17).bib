@article{ ISI:000326371000002,
Author = {Sun, Yuming and Heo, Yeonsook and Tan, Matthias H. Y. and Xie, Huizhi
   and Wu, C. F. Jeff and Augenbroe, Godfried},
Title = {{Uncertainty quantification of microclimate variables in building energy
   models}},
