Title = {{A Comparative Study of Matching Algorithms for Natural Markers}},
Booktitle = {{2014 XVI SYMPOSIUM ON VIRTUAL AND AUGMENTED REALITY (SVR)}},
Series = {{Symposium on Virtual and Augmented Reality}},
Year = {{2014}},
