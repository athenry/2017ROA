Language = {{English}},
Affiliation = {{Glezer, A (Reprint Author), Georgia Inst Technol, George W Woodruff Sch Mech Engn, Atlanta, GA 30332 USA.
   Hidalgo, Pablo; Glezer, Ari, Georgia Inst Technol, George W Woodruff Sch Mech Engn, Atlanta, GA 30332 USA.}},
ISSN = {{1065-2221}},
