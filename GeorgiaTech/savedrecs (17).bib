@inproceedings{ ISI:000361554401207,
Author = {Vena, Arnaud and Tedjini, Marwan and Bjorninen, Toni and Sydanheimo,
   Lauri and Ukkonen, Leena and Tentzeris, Manos M.},
Book-Group-Author = {{IEEE}},
Title = {{A Novel Inkjet-Printed Wireless Chipless Strain and Crack Sensor on
   Flexible Laminates}},
Booktitle = {{2014 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM
   (APSURSI)}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2014}},
Pages = {{1294-1295}},
Note = {{IEEE Antennas-and-Propagation-Society International Symposium (APSURSI),
   Memphis, TN, JUL 06-11, 2014}},
Organization = {{Inst Elect Elect Engineers; Inst Elect Elect Engineers Antennas \&
   Propagat Soc}},
Abstract = {{We studied a low-cost chipless radio-frequency identification strain
   sensor tag operating at 2.45 GHz. The sensor is based on a 18.5 x 18.5
   mm(2) single split ring resonator which is fully inkjet printed on a
   flexible substrate. For an applied strain of 20 \%, a silver-based ink
   strip printed on a polyimide substrate showed an irreversible
   conductivity variation up to 700 \%. Correspondingly, when illuminated
   by an incident plane wave, the printed sensor features an
   electromagnetic response with a magnitude strongly correlated with the
   strain applied on it. We recorded up to 15 dB variation in the radar
   cross section. Both, the simulations and wireless measurements in indoor
   environment validated the sensor operation.}},
Publisher = {{IEEE}},
