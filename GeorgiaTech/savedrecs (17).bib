@inproceedings{ ISI:000361554401011,
Author = {Naishadham, Krishna},
Book-Group-Author = {{IEEE}},
Title = {{Design of a Graphene Loaded Slot Antenna with 100:1 Bandwidth for
   Wireless Sensor Applications}},
Booktitle = {{2014 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM
   (APSURSI)}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2014}},
