@article{ ISI:000331156200045,
Author = {Ding, Xifeng and Gao, Xiaojia and Shen, Jingyu and Wang, Jun and Zhu,
   Wenliang and Wang, Xiong and Jiang, Jinguo},
Title = {{Cobalt-free Sr0.7Y0.3CuO2+delta as a cathode for
   intermediate-temperature solid oxide fuel cell}},
