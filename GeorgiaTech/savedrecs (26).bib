@article{ ISI:000302780700003,
Author = {Goodarzi, Ehsan and Mirzaei, Majid and Ziaei, Mina},
Title = {{Evaluation of dam overtopping risk based on univariate and bivariate
   flood frequency analyses}},
Journal = {{CANADIAN JOURNAL OF CIVIL ENGINEERING}},
Year = {{2012}},
Volume = {{39}},
Number = {{4}},
Pages = {{374-387}},
Month = {{APR}},
Abstract = {{There is a growing tendency to assess safety of dams by mathematical and
   statistical methods in hydrosystem engineering. This research presents
   the application of risk and uncertainty analysis to dam overtopping
   based on univariate and bivariate flood frequency analyses by applying
   Gumbel logistic distribution. The bivariate frequency analyses produced
   six inflow hydrographs with a joint return period of 100 years.
   Afterward, the overtopping risk of the Doroudzan Dam was evaluated for
   all six inflow hydrographs by considering quantile of flood peak
   discharge, initial depth of water in the reservoir, and discharge
   coefficient of spillway as uncertain variables and using two uncertainty
   analysis methods; Monte Carlo simulation and Latin hypercube sampling.
   Finally, the results of both univariate and bivariate frequency analyses
   were compared to show the significance of bivariate analysis on dam
   overtopping.}},
