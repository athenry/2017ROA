@article{ ISI:000303492000024,
Author = {Yeom, Hojun and Park, Hodong and Chang, Young-Hui and Park, Youngchol
   and Lee, Kyoung-Joung},
Title = {{Stimulus Artifact Suppression Using the Stimulation Synchronous Adaptive
   Impulse Correlated Filter for Surface EMG Application}},
Journal = {{JOURNAL OF ELECTRICAL ENGINEERING \& TECHNOLOGY}},
Year = {{2012}},
Volume = {{7}},
Number = {{3}},
Pages = {{451-458}},
Month = {{MAY}},
Abstract = {{The voluntary EMG (vEMG) signal from electrically stimulated muscle is
   very useful for feedback control in functional electrical stimulation.
   However, the recorded EMG signal from surface electrodes has unwanted
   stimulation artifact and M-wave as well as vEMG. Here, we propose an
   event-synchronous adaptive digital filter for the suppression of
   stimulation artifact and M-wave in this application. The proposed method
   requires a simple experimental setup that does not require extra
   hardware connections to obtain the reference signals of adaptive digital
   filter. For evaluating the efficiency of this proposed method, the
   filter was tested and compared with a least square (LS) algorithm using
   previously measured data. We conclude that the cancellation of both
   primary and residual stimulation artifacts is enhanced with an
   event-synchronous adaptive digital filter and shows promise for clinical
   application to rehabilitate paretic limbs. Moreover because this
   algorithm is far simpler than the LS algorithm, it is portable and ready
   for real-time application.}},
