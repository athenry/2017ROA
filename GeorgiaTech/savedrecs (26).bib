Abstract = {{Solution-based n-doping of the polymer
   poly\{{[}N,N'-bis(2-octyldodecyl)-naphthalene-1,
   4,5,8-bis(dicarboximide)-2,6-diyl]-alt-5,5'-(2,2'-bithiophene)\}
