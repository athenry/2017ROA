@article{ ISI:000313925600005,
Author = {Gupta, Kishor and Cook, Fred L.},
Title = {{Recovery and Reuse of Polyvinyl Alcohol from Desize Effluent Streams via
   Vacuum Flash Evaporation}},
Journal = {{AATCC REVIEW}},
Year = {{2012}},
Volume = {{13}},
Number = {{3}},
Pages = {{47-56}},
Month = {{MAY-JUN}},
Abstract = {{Recovery and recycling of polyvinyl alcohol (PVA) size from desize
   effluent streams by ultrafiltration (UF) is more than 35-years old, but
   is still not widely used in the global textile industry due to
   disadvantages, including high pumping and membrane cleaning/replacement
   costs. An environmentally-friendly, and economical technology for PVA
   size recovery and recycling is needed that can reduce energy and water
   consumption. Vacuum flash evaporation (VFE) overcomes most of the
   disadvantages of the current UF process. In this study, VFE technology
   was investigated in the recovery and recycling of single-component PVA
   size. Cotton towel yarns (both pile and ground) slashed with PVA size
   recovered by VFE maintained projected weaving properties. No added wax
   was needed in the recycled size slashing formulations.}},
