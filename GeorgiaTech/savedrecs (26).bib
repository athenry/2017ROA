@article{ ISI:000300436800089,
Author = {Chen, Yongping and Zhang, Chengbin and Shi, Mingheng and Peterson,
   George P.},
Title = {{Slip boundary for fluid flow at rough solid surfaces}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2012}},
Volume = {{100}},
Number = {{7}},
Month = {{FEB 13}},
Abstract = {{A molecular dynamics simulation of slip boundary for fluid flow past a
   solid surface incorporating roughness effect as characterized by fractal
   geometry has been conducted with a focus on the origin of slip, fluid
   structure, and slip boundary flow. The results indicate that interfacial
   slip develops provided that the wall is effectively uncorrugated.
   Compared with the atomically smooth surface, extra viscous dissipation
   is induced for shear flow past a rough surface and leading to a
   reduction in boundary slip. In particular, we find that a more irregular
   topography decreases the boundary slip even for the same statistical
   roughness height. (C) 2012 American Institute of Physics.
   {[}doi:10.1063/1.3685490]}},
