@article{ ISI:000302772600020,
Author = {Yin, Yunjie and Wang, Chaoxia and Wang, Youjiang},
Title = {{Preparation and colloidal dispersion behaviors of silica sol doped with
   organic pigment}},
Journal = {{JOURNAL OF SOL-GEL SCIENCE AND TECHNOLOGY}},
Year = {{2012}},
Volume = {{62}},
Number = {{2}},
Pages = {{266-272}},
Month = {{MAY}},
Abstract = {{Silica sol doped with organic pigment was prepared by hydrolyzing
   tetraethoxysilane with a basic catalyst via dispersing pigment in silica
   sol. The colloidal properties of SiO2/pigment hybrid sol and its
   deposited film were investigated. The presence of pigment in
   SiO2/pigment hybrid sol affects the Zeta potential, particle size and
   surface tension compared to the silica sol without pigment. The
   SiO2/pigment hybrid sol exhibited good dispersion stability in the
   centrifuge process. The maximum absorption wavelength was consistent
   with that of the pigment disperse solution, indicating that the pigment
   in SiO2/pigment hybrid sol remained unchanged. Thermogravimetric
   analysis of the contents of organic component in silica sol and
   SiO2/pigment hybrid sol were conducted, and the differential value was
   ascribed to the weight of the pigment and the condensate of
   polyoxyethylene octylphenol ether (OP-10) and
   gamma-Glycidoxypropyltrimethoxysilane (KH-560). The surface topography
   of SiO2/pigment hybrid silica film was characterized by AFM. The
   analysis of silica sol doped with organic pigment provides useful
   information for an effective pathway to disperse pigment on fiber and
   other substrates.}},
