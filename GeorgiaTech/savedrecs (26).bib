@article{ ISI:000304065100003,
Author = {Sieling, Fred H. and Archila, Santiago and Hooper, Ryan and Canavier,
   Carmen C. and Prinz, Astrid A.},
Title = {{Phase response theory extended to nonoscillatory network components}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2012}},
Volume = {{85}},
Number = {{5, 2}},
Month = {{MAY 14}},
Abstract = {{New tools for analysis of oscillatory networks using phase response
   theory (PRT) under the assumption of pulsatile coupling have been
   developed steadily since the 1980s, but none have yet allowed for
   analysis of mixed systems containing nonoscillatory elements. This
   caveat has excluded the application of PRT to most real systems, which
   are often mixed. We show that a recently developed tool, the functional
   phase resetting curve (fPRC), provides a serendipitous benefit: it
   allows incorporation of nonoscillatory elements into systems of
   oscillators where PRT can be applied. We validate this method in a model
   system of neural oscillators and a biological system, the pyloric
   network of crustacean decapods.}},
