Title = {{EXPERIMENTAL CHARACTERIZATION OF CREEP DAMAGE IN A WELDED STEEL PIPE
   SECTION USING A NONLINEAR ULTRASONIC TECHNIQUE}},
Booktitle = {{REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE EVALUATION, VOLS 31A
   AND 31B}},
Series = {{AIP Conference Proceedings}},
Year = {{2012}},
