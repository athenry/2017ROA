Editor = {{Molthen, RC and Weaver, JB}},
Title = {{Detection of Cancer Metastasis Using a Novel Macroscopic Hyperspectral
   Method}},
Booktitle = {{MEDICAL IMAGING 2012: BIOMEDICAL APPLICATIONS IN MOLECULAR, STRUCTURAL,
   AND FUNCTIONAL IMAGING}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8317}},
