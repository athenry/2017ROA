@article{ ISI:000298325500034,
Author = {Pekkucuksen, Ibrahim and Altunbasak, Yucel},
Title = {{Edge Strength Filter Based Color Filter Array Interpolation}},
Journal = {{IEEE TRANSACTIONS ON IMAGE PROCESSING}},
Year = {{2012}},
Volume = {{21}},
Number = {{1}},
Pages = {{393-397}},
Month = {{JAN}},
Abstract = {{For economic reasons, most digital cameras use color filter arrays
   instead of beam splitters to capture image data. As a result of this,
   only one of the required three color samples becomes available at each
   pixel location and the other two need to be interpolated. This process
   is called Color Filter Array (CFA) interpolation or demosaicing. Many
   demosaicing algorithms have been introduced over the years to improve
   subjective and objective interpolation quality. We propose an
   orientation-free edge strength filter and apply it to the demosaicing
   problem. Edge strength filter output is utilized both to improve the
   initial green channel interpolation and to apply the constant color
   difference rule adaptively. This simple edge directed method yields
   visually pleasing results with high CPSNR.}},
