@inproceedings{ ISI:000309240000055,
Author = {Kim, Dae Hyun and Topaloglu, Rasit Onur and Lim, Sung Kyu},
Book-Group-Author = {{IEEE}},
Title = {{Block-level 3D IC Design with Through-Silicon-Via Planning}},
Booktitle = {{2012 17TH ASIA AND SOUTH PACIFIC DESIGN AUTOMATION CONFERENCE (ASP-DAC)}},
Series = {{Asia and South Pacific Design Automation Conference Proceedings}},
Year = {{2012}},
Pages = {{335-340}},
