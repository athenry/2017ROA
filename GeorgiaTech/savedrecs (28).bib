@article{ ISI:000304479200002,
Author = {Li, Hailong and Chai, Xin-Sheng and DeMartini, Nikolai},
Title = {{Oxalate Release and Formation during Alkaline Pulping}},
Journal = {{JOURNAL OF WOOD CHEMISTRY AND TECHNOLOGY}},
Year = {{2012}},
Volume = {{32}},
Number = {{3}},
Pages = {{187-197}},
Abstract = {{In this work, we investigated the release and formation of oxalate
   during alkaline pulping of southern pine and acacia. The results showed
   that a significant amount of oxalate is formed during alkaline pulping
   in addition to the oxalate released from the wood. Acacia was found to
   have more oxalate in the wood and it also resulted in more oxalate
   formation than southern pine. A higher hydroxide concentration leads to
   higher oxalate content in the black liquor during the early stages of
   the cook. Oxalate formation can be described as a function of pulping
   time and temperature (H-factor), and the initial hydroxide concentration
   of the cooking liquor. More work is needed to understand the fundamental
   mechanism to produce a more fundamental predictive model for other wood
   species.}},
