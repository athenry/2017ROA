@inproceedings{ ISI:000309227500039,
Author = {Banerjee, Aritra and Devarakond, Shyam and Sen, Shreyas and Banerjee,
   Debashis and Chatterjee, Abhijit},
Book-Group-Author = {{IEEE}},
Title = {{Testing of Digitally Assisted Adaptive Analog/RF Systems Using Tuning
   Knob - Performance Space Estimation}},
Booktitle = {{2012 17TH IEEE EUROPEAN TEST SYMPOSIUM (ETS)}},
Series = {{Proceedings of the European Test Symposium}},
Year = {{2012}},
Note = {{17th IEEE European Test Symposium (ETS), Annecy, FRANCE, MAY 28-JUN 01,
   2012}},
Organization = {{IEEE; IEEE Comp Soc; Gopel Elect; ARM; Qualcomm; IEEE Comp Soc, Test
   Technol Tech Council (TTTC)}},
Abstract = {{Testing of adaptive analog/RF systems is challenging as any test
   procedure must ensure that the system adapts correctly to external
   perturbations (process, workload) without incurring the excessive test
   time associated with iterative tuning procedures. This problem is made
   worse by the increased number of adaptation settings ({''}tuning knob
   values{''}) and the requirement of measuring specifications at all of
   these settings. In this paper, a new test technique is proposed that
   allows the closed loop performance of the adaptation procedure to be
   predicted from a set of open-loop tests. The optimal knob settings where
   the system should be tested in open-loop are found using a gradient
   based search algorithm and optimized test signals are generated such
   that the error in performance prediction across different tuning knob
   settings is minimized. The results of these tests are then mapped to the
   performance of the adaptive system which is validated implicitly without
   incurring large testing and tuning costs. Simulation results and
   hardware measurement results prove the validity of the proposed
   technique.}},
