@inproceedings{ ISI:000305624700016,
Author = {Sood, Ashok K. and Welser, Roger E. and Richwine, Robert A. and Puri,
   Yash R. and Dupuis, Russell D. and Ryou, Jae-Hyun and Dhar, Nibir K. and
   Suvarna, P. and Shahedipour-Sandvik, F.},
