@article{ ISI:000307852500004,
Author = {Katz, Donald and Garrow, Laurie A.},
Title = {{The Impact of Bus Door Crowding on Operations and Safety}},
Journal = {{JOURNAL OF PUBLIC TRANSPORTATION}},
Year = {{2012}},
Volume = {{15}},
Number = {{2}},
Pages = {{71-93}},
Abstract = {{This study examines how bus design factors influence door crowding and
   quantifies how door crowding relates to operational performance and
   passenger safety. Results are based on data collected for 2,807 stops in
   Dhaka, Bangladesh. Door crowding is affected by multiple bus design
   factors, including door placement, aisle length, presence of a front
   seating area, and service type. Increases in door crowding are
   associated with longer marginal boarding times and an increased number
   of unsafe boarding and alighting movements that occur when the bus has
   not come to a complete stop. Results underscore the importance of
   educating conductors on the dangers associated with door crowding.}},
