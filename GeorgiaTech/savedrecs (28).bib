@inproceedings{ ISI:000304192100011,
Author = {Yi, Xiaohua and Vyas, Rushi and Cho, Chunhee and Fang, Chia-Hung and
   Cooper, James and Wang, Yang and Leon, Roberto T. and Tentzeris, Manos
   M.},
Editor = {{Tomizuka, M and Yun, CB and Lynch, JP}},
Title = {{Thermal effects on a passive wireless antenna sensor for strain and
   crack sensing}},
Booktitle = {{SENSORS AND SMART STRUCTURES TECHNOLOGIES FOR CIVIL, MECHANICAL, AND
   AEROSPACE SYSTEMS 2012, PTS 1 AND 2}},
Series = {{Proceedings of SPIE}},
