Language = {{English}},
Affiliation = {{Lu, H (Reprint Author), Georgia Inst Technol, Sch Chem \& Biomol Engn, Atlanta, GA 30332 USA.
   Nahabedian, John F.; Stirman, Jeffrey N.; Lu, Hang, Georgia Inst Technol, Sch Chem \& Biomol Engn, Atlanta, GA 30332 USA.
   Nahabedian, John F.; Qadota, Hiroshi; Benian, Guy M., Emory Univ, Dept Pathol, Atlanta, GA 30322 USA.}},
DOI = {{10.1016/j.ymeth.2011.11.005}},
ISSN = {{1046-2023}},
