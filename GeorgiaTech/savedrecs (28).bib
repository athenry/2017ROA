Title = {{NONLINEAR RAYLEIGH WAVES TO DETECT INITIAL DAMAGE LEADING TO STRESS
   CORROSION CRACKING IN CARBON STEEL}},
Booktitle = {{REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE EVALUATION, VOLS 31A
   AND 31B}},
Series = {{AIP Conference Proceedings}},
Year = {{2012}},
