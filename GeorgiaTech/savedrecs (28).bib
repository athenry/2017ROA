Booktitle = {{2012 IEEE 25TH INTERNATIONAL CONFERENCE ON MICRO ELECTRO MECHANICAL
   SYSTEMS (MEMS)}},
Series = {{Proceedings IEEE Micro Electro Mechanical Systems}},
