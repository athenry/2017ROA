@inproceedings{ ISI:000309933900070,
Author = {Xu, Ju and Razeeb, Kafil M. and Sitaraman, Suresh K. and Mathewson, Alan},
Book-Group-Author = {{IEEE}},
Title = {{The Fabrication Of Ultra Long Metal Nanowire Bumps and Their Application
   as Interconnects}},
