@article{ ISI:000310829800013,
Author = {Mills, Zachary Grant and Aziz, Basat and Alexeev, Alexander},
Title = {{Beating synthetic cilia enhance heat transport in microfluidic channels}},
Journal = {{SOFT MATTER}},
Year = {{2012}},
Volume = {{8}},
Number = {{45}},
Pages = {{11508-11513}},
Abstract = {{Using computational modeling, we probed the utility of actuated
   synthetic cilia for enhancing heat transport in microfluidic channels.
   Cilia are elastic filaments that are attached to the bottom channel wall
   with a constant tilt and actuated by a periodical vertical force applied
   to their free ends. We show that periodical oscillations of elastic
   cilia mix the heated fluid and create secondary flows in the
   microchannel that facilitate heat transport between channel walls. The
   magnitude of the secondary flows and the cilium deformation pattern are
   controlled by the frequency and amplitude of the periodic driving force.
   Thus, by varying the force parameters one can effectively regulate the
   local heat transport in ciliated microchannels.}},
