@inproceedings{ ISI:000306913600191,
Author = {Shi, Fan and Michaels, Jennifer E. and Lee, Sang Jun},
Editor = {{Thompson, DO and Chimenti, DE}},
Title = {{AN ULTRASONIC GUIDED WAVE METHOD TO ESTIMATE APPLIED BIAXIAL LOADS}},
Booktitle = {{REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE EVALUATION, VOLS 31A
   AND 31B}},
