@inproceedings{ ISI:000311614500091,
Author = {Ma, Chao and Chen, Lianyi and Xu, Jiaquan and Fehrenbacher, Axel and Li,
   Yan and Pfefferkorn, Frank E. and Duffie, Neil A. and Zheng, Jing and
   Li, Xiaochun},
