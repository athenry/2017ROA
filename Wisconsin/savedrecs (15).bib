Title = {{Development of Diffusion Barrier Coatings for Mitigation of
   Fuel-Cladding Chemical Interactions}},
Booktitle = {{ELECTROPHORETIC DEPOSITION: FUNDAMENTALS AND APPLICATIONS IV}},
Series = {{Key Engineering Materials}},
