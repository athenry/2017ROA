@inproceedings{ ISI:000335097100052,
Author = {Viswanathan, S. and Rakovec, N. and Foster, D. E.},
Book-Group-Author = {{ASME}},
Title = {{MICROSCALE STUDY OF ASH ACCUMULATION PROCESS IN DPF WALLS USING THE
   DIESEL EXHAUST FILTRATION ANALYSIS (DEFA) SYSTEM}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNAL COMBUSTION ENGINE DIVISION FALL
   TECHNICAL CONFERENCE - 2012}},
