@article{ ISI:000298868300003,
Author = {Nemet, Gregory F. and Brandt, Adam R.},
Title = {{Willingness to Pay for a Climate Backstop: Liquid Fuel Producers and
   Direct CO2 Air Capture}},
Journal = {{ENERGY JOURNAL}},
Year = {{2012}},
Volume = {{33}},
Number = {{1}},
Pages = {{53-81}},
Abstract = {{We conduct a sensitivity analysis to describe conditions under which
   liquid fuel producers would fund the development of a climate backstop.
   We estimate (1) the cost to develop competitively priced direct CO2 air
   capture technology, a possible climate backstop and (2) the effect of
   this technology on the value of liquid fuel reserves by country and
   fuel. Under most assumptions, development costs exceed individual
   benefits. A particularly robust result is that carbon prices generate
   large benefits for conventional oil producers making a climate backstop
   unappealing for them. Unilateral investment does become more likely
