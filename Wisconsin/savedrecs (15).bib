Author = {Rohe, Daniel P. and Allen, Matthew S.},
Book-Group-Author = {{ASME}},
Title = {{INVESTIGATION INTO THE EFFECT OF MODE SHAPE ERRORS ON VALIDATION
   EXPERIMENTS FOR EXPERIMENTAL-ANALYTICAL SUBSTRUCTURING}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
