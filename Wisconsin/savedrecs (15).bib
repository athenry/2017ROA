@inproceedings{ ISI:000302693500015,
Author = {Saha, Tapas Kumar and Lu, Mingyu and Zhao, Deyin and Ma, Zhenqiang and
   Zhou, Weidong},
Editor = {{ChangHasnain, CJ and Koyama, F and Willner, AE and Zhou, W}},
Title = {{Design of second order grating couplers to detect the angle and
   polarization of the laser beam}},
Booktitle = {{HIGH CONTRAST METASTRUCTURES}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8270}},
Note = {{Conference on High Contrast Metastructures, San Francisco, CA, JAN
   22-25, 2012}},
Organization = {{SPIE}},
Abstract = {{On-chip laser beam tracking finds innumerable applications. Popularly
   adopted quadrant photodiodes can only detect laser beam's angle
   variation up to 0.2 degrees reliably. In this paper, a novel angle
   detector is designed based on grating coupling. It consists of a grating
   layer on top of a silicon-on-insulator slab waveguide. The incident
   light is coupled into guided modes within the waveguide via the grating
   layer, and then, the incident light's angle can be determined by reading
   the outputs of light detectors within the waveguide. Performance of the
   laser angle detector in this paper is demonstrated by full-wave
   finite-difference-time-domain simulations. Numerical results show that,
   the detectable angle range can be adjusted by several design parameters
   and can reach {[}-4 degrees, 4 degrees]. The device structure in this
   paper can be straightforwardly extended to two-dimensional photonic
   crystal configurations.}},
Publisher = {{SPIE-INT SOC OPTICAL ENGINEERING}},
