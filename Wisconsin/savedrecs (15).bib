@inproceedings{ ISI:000304904000034,
Author = {Kim, TaeWan and Garrod, Toby J. and Kim, Kangho and Lee, Jaejin and
   Mawst, Luke J. and Kuech, T. F. and LaLumondiere, S. D. and Sin, Y. and
   Lotshaw, W. T. and Moss, S. C.},
Editor = {{Freundlich, A and Guillemoles, JFF}},
Title = {{Characteristics of bulk InGaAsN and InGaAsSbN materials grown by metal
   organic vapor phase epitaxy (MOVPE) for solar cell application}},
Booktitle = {{PHYSICS, SIMULATION, AND PHOTONIC ENGINEERING OF PHOTOVOLTAIC DEVICES}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8256}},
Note = {{Conference on Physics, Simulation and Photonic Engineering of
   Photovoltaic Devices, San Francisco, CA, JAN 23-26, 2012}},
Organization = {{SPIE}},
Abstract = {{Bulk, lattice-matched InGaAsSbN material has been grown by metal organic
   vapor phase epitaxy (MOVPE) for applications in concentrated
   multi-junction solar cells. By optimizing the growth conditions for high
   Sb and As partial pressures, we achieved background hole concentrations
   as low as 2 x 10(18) cm(-3). After thermal annealing, the background
   hole concentration increased from 2x10(18) to 2 x 10(19) cm(-3),
   although PL intensity increased by a factor of 7. We recently grew
   single junction (1eV) solar cells incorporating dilute-nitride materials
   and devices were fabricated and characterized for solar cell
   application. Performance characteristics of these cells without
   anti-reflection coating included the efficiency of 4.25\% under the
   AM1.5 (air mass) direct illumination, V-infinity of 0.7 V, and a
   spectral response extended to longer wavelength compared with GaAs
   cells.}},
Publisher = {{SPIE-INT SOC OPTICAL ENGINEERING}},
Address = {{1000 20TH ST, PO BOX 10, BELLINGHAM, WA 98227-0010 USA}},
Type = {{Proceedings Paper}},
