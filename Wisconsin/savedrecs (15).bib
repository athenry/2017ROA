@article{ ISI:000313182600007,
Author = {Kim, Joonhoon and Reed, Jennifer L.},
Title = {{RELATCH: relative optimality in metabolic networks explains robust
   metabolic and regulatory responses to perturbations}},
Journal = {{GENOME BIOLOGY}},
Year = {{2012}},
Volume = {{13}},
Number = {{9}},
Abstract = {{Predicting cellular responses to perturbations is an important task in
   systems biology. We report a new approach, RELATCH, which uses flux and
   gene expression data from a reference state to predict metabolic
   responses in a genetically or environmentally perturbed state. Using the
   concept of relative optimality, which considers relative flux changes
   from a reference state, we hypothesize a relative metabolic flux pattern
   is maintained from one state to another, and that cells adapt to
   perturbations using metabolic and regulatory reprogramming to preserve
   this relative flux pattern. This constraint-based approach will have
   broad utility where predictions of metabolic responses are needed.}},
