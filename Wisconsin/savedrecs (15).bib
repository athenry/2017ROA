@article{ ISI:000298203300003,
Author = {Zheng, Jie and Mehndiratta, Shomik and Guo, Jessica Y. and Liu, Zhi},
Title = {{Strategic policies and demonstration program of electric vehicle in
   China}},
Journal = {{TRANSPORT POLICY}},
Year = {{2012}},
Volume = {{19}},
Number = {{1}},
Pages = {{17-25}},
Month = {{JAN}},
Abstract = {{Recent Chinese economic and energy policies recognize the transportation
   sector as a key element in the nation's effort to meet its energy and
   air quality goals. The development of alternative fuel vehicle (AFV) has
   been considered as a particularly promising strategy. AN-related
   policies can be traced back to the eighth Five-Year Plan period (i.e.,
   1991-1995). All the work during the last twenty years has cumulatively
   prompted the transition of AFV development from policy-making to actual
