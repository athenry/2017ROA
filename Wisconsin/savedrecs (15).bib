Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
Language = {{English}},
Affiliation = {{Rawlings, JB (Reprint Author), Univ Wisconsin, Dept Chem \& Biol Engn, Madison, WI 53706 USA.
   Rawlings, James B.; Bates, Cuyler N., Univ Wisconsin, Dept Chem \& Biol Engn, Madison, WI 53706 USA.
   Angeli, David, Imperial Coll London, Dept Elect \& Elect Engn, London, England.}},
ISSN = {{0743-1546}},
ISBN = {{978-1-4673-2066-5}},
