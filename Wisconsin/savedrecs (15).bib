@inproceedings{ ISI:000335091200039,
Author = {Kuether, Robert J. and Allen, Matthew S.},
Book-Group-Author = {{ASME}},
Title = {{COMPUTING NONLINEAR NORMAL MODES USING NUMERICAL CONTINUATION AND FORCE
   APPROPRIATION}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
   CONFERENCES AND COMPUTERS AND INFORMATION IN ENGINEERING CONFERENCE
   2012, VOL 1, PTS A AND B}},
