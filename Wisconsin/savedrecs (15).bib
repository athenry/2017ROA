@inproceedings{ ISI:000320654000118,
Author = {Boston, Nigel},
Book-Group-Author = {{IEEE}},
Title = {{On the Belgian Chocolate Problem and Output Feedback Stabilization:
   Efficacy of Algebraic Methods}},
