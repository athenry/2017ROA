@inproceedings{ ISI:000309943200130,
Author = {Fonseca, Jr., Benedito J. B. and Gubner, John A.},
Book-Group-Author = {{IEEE}},
Title = {{LEAST FAVORABLE DISTRIBUTIONS FOR THE DESIGN OF SENSOR DETECTION SYSTEMS
   IN NON-CIRCULAR REGIONS OF INTEREST}},
Booktitle = {{2012 IEEE STATISTICAL SIGNAL PROCESSING WORKSHOP (SSP)}},
Year = {{2012}},
Pages = {{516-519}},
Note = {{IEEE Statistical Signal Processing Workshop (SSP), Ann Arbor, MI, AUG
   05-08, 2012}},
Organization = {{IEEE}},
Abstract = {{When designing a sensor system to detect the presence of an emitter
   whose random location has an unknown distribution, least favorable
   distributions can be used to avoid complications associated with
   composite hypotheses. Furthermore, sufficient conditions exist under
   which the least favorable distribution for the emitter location also
   makes the measurements conditionally independent and identically
