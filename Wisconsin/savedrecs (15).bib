@inproceedings{ ISI:000310776203159,
Author = {Barmish, B. Ross and Primbs, James A.},
Book-Group-Author = {{IEEE}},
Title = {{On Market-Neutral Stock Trading Arbitrage Via Linear Feedback}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
