@article{ ISI:000323914800009,
Author = {El-Guebaly, L. and Huhn, T. and Rowcliffe, A. and Malang, S. and
   ARIES-ACT Team},
Title = {{DESIGN CHALLENGES AND ACTIVATION CONCERNS FOR ARIES VACUUM VESSEL}},
Journal = {{FUSION SCIENCE AND TECHNOLOGY}},
Year = {{2013}},
Volume = {{64}},
Number = {{3}},
Pages = {{449-454}},
Month = {{SEP}},
