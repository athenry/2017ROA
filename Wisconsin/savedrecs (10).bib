@article{ ISI:000324747900002,
Author = {Lin, Shih-kang and Yeh, Chao-kuei and Xie, Wei and Liu, Yu-chen and
   Yoshimura, Masahiro},
Title = {{Ab initio-aided CALPHAD thermodynamic modeling of the Sn-Pb binary
   system under current stressing}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2013}},
Volume = {{3}},
Month = {{SEP 24}},
Abstract = {{Soldering is an ancient process, having been developed 5000 years ago.
   It remains a crucial process with many modern applications. In
   electronic devices, electric currents pass through solder joints. Anew
   physical phenomenon - the supersaturation of solders under high electric
   currents - has recently been observed. It involves (1) un-expected
   supersaturation of the solder matrix phase, and (2) the formation of
   unusual ``ring-shaped{''} grains. However, the origin of these phenomena
   is not yet understood. Here we provide a plausible explanation of these
   phenomena based on the changes in the phase stability of Pb-Sn solders.
   Ab initio-aided CALPHAD modeling is utilized to translate the electric
   current-induced effect into the excess Gibbs free energies of the
   phases. Hence, the phase equilibrium can be shifted by current
   stressing. The Pb-Sn phase diagrams with and without current stressing
   clearly demonstrate the change in the phase stabilities of Pb-Sn solders
   under current stressing.}},
