@article{ ISI:000324720700031,
Author = {Guo, Jintang and Hong, Hao and Chen, Guojun and Shi, Sixiang and Zheng,
   Qifeng and Zhang, Yin and Theuer, Charles P. and Barnhart, Todd E. and
   Cai, Weibo and Gong, Shaoqin},
Title = {{Image-guided and tumor-targeted drug delivery with radiolabeled
   unimolecular micelles}},
Journal = {{BIOMATERIALS}},
Year = {{2013}},
Volume = {{34}},
Number = {{33}},
Pages = {{8323-8332}},
Month = {{NOV}},
Abstract = {{Unimolecular micelles formed by dendritic amphiphilic block copolymers
   poly(amidoamine) poly(L-lactide)-b-poly(ethylene glycol) conjugated with
   anti-CD105 monoclonal antibody (TRC105) and 1,4,7-triazacyclononane-N,
   N', N-triacetic acid (NOTA, a macrocyclic chelator for Cu-64)
   (abbreviated as PAMAM PLA-b-PEG TRC105) were synthesized and
   characterized. Doxorubicin (DOX), a model anticancer drug, was loaded
   into the hydrophobic core of the unimolecular micelles formed by PAMAM
   and PLA via physical encapsulation. The unimolecular micelles exhibited
   a uniform size distribution and pH-sensitive drug release behavior.
   TRC105-conjugated unimolecular micelles showed a CD105-associated
   cellular uptake in human umbilical vein endothelial cells (HUVEC)
   compared with non-targeted unimolecular micelles, which was further
   validated by cellular uptake in CD105-negative MCF-7 cells. In 4T1
   murine breast tumor-bearing mice, Cu-64-labeled targeted micelles
   exhibited a much higher level of tumor accumulation than Cu-64-labeled
   non-targeted micelles, measured by serial non-invasive positron emission
   tomography (PET) imaging and confirmed by biodistribution studies. These
   unimolecular micelles formed by dendritic amphiphilic block copolymers
   that synergistically integrate passive and active tumor-targeting
   abilities with pH-controlled drug release and PET imaging capabilities
   provide the basis for future cancer theranostics. (C) 2013 Elsevier Ltd.
   All rights reserved.}},
