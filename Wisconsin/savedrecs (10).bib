@inproceedings{ ISI:000379987000080,
Author = {Li, Kang and Qian, Xiaoping and Martin, Caitlin and Sun, Wei},
Book-Group-Author = {{ASME}},
Title = {{TOWARD PATIENT-SPECIFIC COMPUTATIONAL STUDY OF AORTIC DISEASES : A
   POPULATION BASED SHAPE MODELING APPROACH}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
