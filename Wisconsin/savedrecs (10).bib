@article{ ISI:000326910600014,
Author = {Yang, Liang and Guo, Gu-Qing and Chen, Lian-Yi and LaQua, Brian and
   Jiang, Jian-Zhong},
Title = {{Tuning local structures in metallic glasses by cooling rate}},
Journal = {{INTERMETALLICS}},
Year = {{2014}},
Volume = {{44}},
Pages = {{94-100}},
Month = {{JAN}},
Abstract = {{The Zr48Cu45Al7 amorphous alloy samples quenched under cooling rates of
   about 2 x 10(6) K/s and 1 x 10(2) K/s were prepared by melt spinning and
   copper-mold suction casting, respectively. Synchrotron-radiation based
   experiments, combined with a series of calculations, were performed to
   study the microstructures in both samples. It was found that although
   the short-range orderings are similar in Zr-centered clusters for both
   samples, the atom arrangements and distributions in Cu- and Al-centered
   clusters are very different in terms of atomic-packing efficiencies and
   regularity of clusters in these two samples. A quantitative analysis
   revealed that the lower cooling rate leads to the higher packing
   efficiency and the higher regularity of clusters. This revealed how the
   cooling rate during quenching fine-tunes the atomic- and cluster-level
   microstructures in amorphous alloys with the same composition, which may
   be the structural basis to address the issue why macroscopic properties
   change with the cooling rate. (C) 2013 Elsevier Ltd. All rights
   reserved.}},
Publisher = {{ELSEVIER SCI LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, OXON, ENGLAND}},
Type = {{Article}},
