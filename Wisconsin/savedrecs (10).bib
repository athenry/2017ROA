@article{ ISI:000324850400024,
Author = {Parker, G. J. and Hitchon, W. N. G.},
Title = {{A kinetic theory of micromagnetic time evolution}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2013}},
Volume = {{377}},
Number = {{37}},
Pages = {{2388-2392}},
Month = {{NOV 8}},
Abstract = {{A semi-analytic description of recording media grains' magnetization
   flipping in a magnetic field provides hysteresis loops, efficiently,
   with all parameters found by following orbits, and in close agreement
   with existing results. Integrating the Landau-Lifshitz-Gilbert (LLG)
   equations numerically gives the average energy diffusion coefficient
   D-epsilon, energy loss rate a(epsilon) and the barrier height which must
   be overcome. These allow solution of the kinetic equation for the
   probability density f and the escape rate gamma of a grain trapped in an
   energy well, as well as other, similar, very stiff systems. gamma is
   used in Monte Carlo simulation of hysteresis. The essential physics
   governing the grains' behavior is outlined. (C) 2013 The Authors.
   Published by Elsevier B.V. All rights reserved.}},
