@inproceedings{ ISI:000392944400062,
Author = {Wang, Qing and Xu, Ke and Izard, Ryan and Kribbs, Benton and Porter,
   Joseph and Wang, Kuang-Ching and Prakash, Aditya and Ramanathan, Parmesh},
