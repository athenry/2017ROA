@article{ ISI:000333736400001,
Author = {Tan, Huachun and Wu, Yuankai and Cheng, Bin and Wang, Wuhong and Ran,
   Bin},
Title = {{Robust Missing Traffic Flow Imputation Considering Nonnegativity and
   Road Capacity}},
Journal = {{MATHEMATICAL PROBLEMS IN ENGINEERING}},
Year = {{2014}},
Abstract = {{There are increasing concerns about missing traffic data in recent
   years. In this paper, a robust missing traffic flow data imputation
   approach based on matrix completion is proposed. In the proposed method,
   the similarity of traffic flow from day to day is exploited to impute
   missing data by the low-rank hypothesis of constructed traffic flow
   matrix. And the physical limitation of road capacity and nonnegativity
   is also considered through the optimization process, which avoids the
   possibility of producing negative and overcapacity values. Moreover, the
   proposed algorithm can impute missing data and recover outlier in a
   unify framework. The experiment results show that the proposed method is
   more accurate, stable, and reasonable.}},
