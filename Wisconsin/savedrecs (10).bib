@article{ ISI:000321176900015,
Author = {Sekido, N. and Hildal, K. and Sakidja, R. and Perepezko, J. H.},
Title = {{Stability of the Nb5Si3 phase in the Nb-Mo-Si system}},
Journal = {{INTERMETALLICS}},
Year = {{2013}},
Volume = {{41}},
Pages = {{104-112}},
Month = {{OCT}},
Abstract = {{Phase equilibrium between the alpha-Nb5Si3 and beta-Nb5Si3 phases has
   been studied in the Nb-Mo-Si ternary system. The high temperature
   beta-Nb5Si3 phase is stabilized by Mo additions to yield a relatively
   narrow alpha/beta two-phase field in the Nb-Mo-Si ternary system. The
   enthalpy of the alpha-beta phase transformation has been evaluated from
   the experimentally determined two-phase field boundaries lines in
   Nb-xMo-37.5Si (x = 0-10) alloys as 21 (+/- 3) kJ/mol-atom. The
   beta-Nb5Si3 phase exhibits a temperature dependent solubility, which
   results in the formation of Nb-ss precipitates both in the alpha- and
   beta-Nb5Si3 matrices. The orientation relationship between Nb-ss and
   Nb5Si3 has been identified by transmission electron microscopy. Lattice
   parameter variation of the beta-Nb5Si3 phase suggests that the departure
   of the beta-Nb5Si3 phase from stoichiometry toward (Nb + Mo)
   compositions is derived from the anti-site substitution of Nb and Mo
   atoms for Si sites. (C) 2013 Elsevier Ltd. All rights reserved.}},
