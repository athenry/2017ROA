@article{ ISI:000327479200004,
Author = {Yu, Hai-Jun and Wang, Jun-Qiang and Shi, Xue-Tao and Louzguine-Luzgin,
   Dmitri V. and Wu, Hong-Kai and Perepezko, John H.},
Title = {{Ductile Biodegradable Mg-Based Metallic Glasses with Excellent
   Biocompatibility}},
Journal = {{ADVANCED FUNCTIONAL MATERIALS}},
Year = {{2013}},
Volume = {{23}},
Number = {{38}},
Pages = {{4793-4800}},
Month = {{OCT 11}},
Abstract = {{Magnesium-based metallic glasses (MMGs) show intriguing potentials for
   application as implantable biomaterials owing to their disordered atomic
   structure, good biodegradability, low elastic modulus, high strength,
   and large elasticity. However, despite of all these advantages, their
   brittleness is their Achilles' heel, which severely limits their
   application as biomedical materials. In the current study, a
   significantly improved ductility of MMGs under bending and tensile
   loading through minor alloying with rare-earth element ytterbium (Yb) at
   an atomic concentration of 2 and 4\% is reported. The enhanced ductility
   is attributed to the increased density of shear bands close to fracture
   end and larger plastic zones on the fracture surface. In comparison with
   that of Yb-free control, in vitro cell culture study confirms an
   improved biocompatibility of MMGs alloyed with Yb as determined by MTT,
   live-dead, and cytoskeleton staining assays, respectively.}},
