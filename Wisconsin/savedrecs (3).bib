Author = {Ramanathan, Parameswaran and Saluja, Kewal K.},
Book-Group-Author = {{IEEE}},
Title = {{Crypt-Delay: Encrypting IP Cores with Capabilities for Gate-level Logic
   and Delay Simulations}},
Booktitle = {{2016 IEEE 25TH ASIAN TEST SYMPOSIUM (ATS)}},
