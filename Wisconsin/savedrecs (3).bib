@inproceedings{ ISI:000382315800003,
Author = {Hatt, Charles R. and Wagner, Martin and Raval, Amish N. and Speidel,
   Michael A.},
Editor = {{Webster, RJ and Yaniv, ZR}},
Title = {{Dynamic tracking of prosthetic valve motion and deformation from
   bi-plane x-ray views: feasibility study}},
Booktitle = {{MEDICAL IMAGING 2016: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
Series = {{Proceedings of SPIE}},
Year = {{2016}},
Volume = {{9786}},
Note = {{Conference on SPIE Image-Guided Procedures, Robotic Interventions, and
   Modeling, San Diego, CA, FEB 28-MAR 01, 2016}},
Organization = {{SPIE; Modus Med Devices Inc; Bruker; Poco Graphite; imXPAD; Siemens; Nat
   Diagnost Imaging}},
Abstract = {{Transcatheter aortic valve replacement (TAVR) requires navigation and
   deployment of a prosthetic valve within the aortic annulus under
   fluoroscopic guidance. To support improved device visualization in this
   procedure, this study investigates the feasibility of frame-by-frame 3D
   reconstruction of a moving and expanding prosthetic valve structure from
   simultaneous bi-plane x-ray views. In the proposed method, a dynamic 3D
   model of the valve is used in a 2D/3D registration framework to obtain a
   reconstruction of the valve. For each frame, valve model parameters
   describing position, orientation, expansion state, and deformation are
   iteratively adjusted until forward projections of the model match both
   bi-plane views. Simulated bi-plane imaging of a valve at different
   signal-difference-to-noise ratio (SDNR) levels was performed to test the
   approach. 20 image sequences with 50 frames of valve deployment were
   simulated at each SDNR. The simulation achieved a target registration
   error (TRE) of the estimated valve model of 0.93 +/- 2.6 mm (mean +/-
   S.D.) for the lowest SDNR of 2. For higher SDNRs (5 to 50) a TRE of 0.04
   mm +/- 0.23 mm was achieved. A tabletop phantom study was then conducted
   using a TAVR valve. The dynamic 3D model was constructed from high
   resolution CT scans and a simple expansion model. TRE was 1.22 +/- 0.35
   mm for expansion states varying from undeployed to fully deployed, and
   for moderate amounts of inter-frame motion. Results indicate that it is
   feasible to use bi-plane imaging to recover the 3D structure of
   deformable catheter devices.}},
