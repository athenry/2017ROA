@inproceedings{ ISI:000400601400116,
Author = {Boston, Nigel and Hao, Jing},
Book-Group-Author = {{IEEE}},
Title = {{Quasi-Quadratic Residue Codes and Their Weight Distributions}},
Booktitle = {{2016 54TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2016}},
Pages = {{822-829}},
Note = {{54th Annual Allerton Conference on Communication, Control, and Computing
   (Allerton), Monticello, IL, SEP 27-30, 2016}},
Organization = {{Coordinated Sci Lab; Univ Illinois, Dept Elect \& Comp Engn; IEEE
   Control Syst Soc; IEEE Informat Theory Soc}},
Abstract = {{Quasi-Quadratic Residue Codes (QQR Codes) are a family of double
   circulant codes. They are important in at least two respects:
   Firstly, they have very good minimum distances when p  3 (mod 8),
   providing a critical test case for Goppa's Conjecture.
   Secondly, they serve as a powerful tool for studying point distributions
   on hyperelliptic curves. We will use the result of their weight
   distributions to prove a variant on a result of Larsen {[}1] on
   asymptotic normal distribution of numbers of points on hyperelliptic
   curves.
   In this paper, we will show new results on QQR Codes from several
   perspectives. We will observe (and prove when p  7 (mod 8)) that the
   weight polynomial of a QQR Code is at least divisible by (x(2) +
   y(2))(d-1), where d is its minimum distance.
   We also provide new results about their structures. When p  7 (mod 8),
   it is the even weight subcode of the direct sum of the quadratic residue
   code with itself. Its automorphism group contains the invertible affine
   transformations on F-p.
   We also answer a question of Joyner {[}2] asking whether they satisfy
   the Riemann hypothesis.}},
