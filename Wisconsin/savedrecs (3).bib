@article{ ISI:000366961100003,
Author = {Huang, Haoyuan and Hernandez, Reinier and Geng, Jumin and Sun, Haotian
   and Song, Wentao and Chen, Feng and Graves, Stephen A. and Nickles,
   Robert J. and Cheng, Chong and Cai, Weibo and Lovell, Jonathan F.},
Title = {{A porphyrin-PEG polymer with rapid renal clearance}},
Journal = {{BIOMATERIALS}},
Year = {{2016}},
Volume = {{76}},
Pages = {{25-32}},
Month = {{JAN}},
Abstract = {{Tetracarboxylic porphyrins and polyethylene glycol (PEG) diamines were
   crosslinked in conditions that gave rise to a water-soluble porphyrin
   polyamide. Using PEG linkers 2 kDa or larger prevented fluorescence
   self-quenching. This networked porphyrin mesh was retained during
   dialysis with membranes with a 100 kDa pore size, yet passed through the
   membrane when centrifugal filtration was applied. Following intravenous
   administration, the porphyrin mesh, but not the free porphyrin, was
   rapidly cleared via renal excretion. The process could be monitored by
   fluorescence analysis of collected urine, with minimal background due to
   the large Stokes shift of the porphyrin (230 nm separating excitation
   and emission peaks). In a rhabdomyolysis mouse model of renal failure,
   porphyrin mesh urinary clearance was significantly impaired. This led to
   slower accumulation in the bladder, which could be visualized
   non-invasively via fluorescence imaging. Without further modification,
   the porphyrin mesh was chelated with Cu-64 for dynamic whole body
   positron emission tomography imaging of renal clearance. Together, these
   data show that small porphyrin-PEG polymers can serve as effective
   multimodal markers of renal function. (c) 2015 Elsevier Ltd. All rights
   reserved.}},
