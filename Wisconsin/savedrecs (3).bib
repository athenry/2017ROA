@inproceedings{ ISI:000388377100563,
Author = {Luyen, Hung and Hagness, Susan C. and Behdad, Nader},
Book-Group-Author = {{IEEE}},
Title = {{A Minimally Invasive, Coax-Fed Microwave Ablation Antenna with a
   Tapered-Slot Balun}},
Booktitle = {{2016 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2016}},
Pages = {{1163-1164}},
