@article{ ISI:000392163000012,
Author = {Chen, Danjue and Ahn, Soyoung and Bang, Soohyuk and Noyce, David},
Title = {{Car-Following and Lane-Changing Behavior Involving Heavy Vehicles}},
Journal = {{TRANSPORTATION RESEARCH RECORD}},
Year = {{2016}},
Number = {{2561}},
Pages = {{89-97}},
Abstract = {{This paper presents empirical findings on car-following and
   lane-changing behavior involving heavy vehicles; trajectory data from
   the next generation simulation program were used in the study. It was
   found that when following passenger cars, heavy vehicles tended to
   reduce speed variations caused by traffic disturbances and thereby
   dampened traffic oscillations. In contrast, passenger cars following
   heavy vehicles tended to amplify traffic disturbances, although with
   lower probability and magnitude compared with the dampening effect.
   Moreover, heavy vehicles tended to discourage lane changes, especially
   behind them. This finding has convoluted implications: although reduced
   lane changes can improve traffic stability by preventing or reducing
   disturbances, large gaps can persist behind heavy vehicles and
   contribute to underutilization of road capacity.}},
