@inproceedings{ ISI:000378352900141,
Author = {Zhang, Huayu and Xing, Yuxiang},
Editor = {{Kontos, D and Flohr, TG and Lo, JY}},
Title = {{Reconstruction of Limited-angle Dual-Energy CT Using Mutual Learning and
   Cross-estimation (MLCE)}},
Booktitle = {{MEDICAL IMAGING 2016: PHYSICS OF MEDICAL IMAGING}},
