Title = {{PREDICTION OF ISOLATED RESONANCE CURVES USING NONLINEAR NORMAL MODES}},
Booktitle = {{INTERNATIONAL DESIGN ENGINEERING TECHNICAL CONFERENCES AND COMPUTERS AND
   INFORMATION IN ENGINEERING CONFERENCE, 2015, VOL 6}},
Year = {{2016}},
