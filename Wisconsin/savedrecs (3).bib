@inproceedings{ ISI:000388377100584,
Author = {Abadi, Seyed Mohamad Amin Momeni Hasan and Behdad, Nader},
Book-Group-Author = {{IEEE}},
Title = {{True-Time-Delay UWB Reflectarray with Linear to Circular Polarization
   Transformation}},
Booktitle = {{2016 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2016}},
Pages = {{1205-1206}},
Note = {{IEEE-Antennas-and-Propagation-Society International Symposium, Fajardo,
   PR, JUN 26-JUL 01, 2016}},
Organization = {{Inst Elect \& Elect Engineers Antennas \& Propagat Soc; Inst Elect \&
   Elect Engineers}},
Abstract = {{We present a new technique for designing low-profile
   circularly-polarized reflectarrays with broadband true-time-delay (TTD)
   responses and linearly-polarized feeds. The proposed reflectarray
   exploits the unit cells of ground-plane-backed, anisotropic
   miniaturized-element frequency selective surfaces as its spatial
   time-delay units (TDUs). Each TDU is composed of a stack of non-resonant
   rectangular-shaped capacitive patches. Besides providing the necessary
   time delay to create beam collimation, each TDU creates a broadband 90
   degrees phase difference between the two orthogonal components of the
   incident field. This results in a wideband, TTD reflectarray that
   converts a linearly polarized spherical incident field to a
   circularly-polarized collimated beam. A prototype of such a reflectarray
   operating at X-band is designed, fabricated, and characterized.
   Measurement results show consistent radiation characteristics, excellent
   time domain performance, and an axial ratio lower than 1.5 over at least
   40\% bandwidth. The offset-fed versions of the prototype were also shown
   to be capable of providing off-axis, TTD UWB beams within the wide field
   of view of +/- 45 degrees.}},
