@article{ ISI:000367481900088,
Author = {Tillotson, Benjamin J. and Goulatis, Loukas I. and Parenti, Isabelle and
   Duxbury, Elizabeth and Shusta, Eric V.},
Title = {{Engineering an Anti-Transferrin Receptor ScFv for pH-Sensitive Binding
   Leads to Increased Intracellular Accumulation}},
Journal = {{PLOS ONE}},
Year = {{2015}},
Volume = {{10}},
Number = {{12}},
Month = {{DEC 29}},
Abstract = {{The equilibrium binding affinity of receptor-ligand or antibody-antigen
   pairs may be modulated by protonation of histidine side-chains, and such
   pH-dependent mechanisms play important roles in biological systems,
   affecting molecular uptake and trafficking. Here, we aimed to manipulate
   cellular transport of single-chain antibodies (scFvs) against the
   transferrin receptor (TfR) by engineering pH-dependent antigen binding.
   An anti-TfR scFv was subjected to histidine saturation mutagenesis of a
   single CDR. By employing yeast surface display with a pH-dependent
   screening pressure, scFvs having markedly increased dissociation from
   TfR at pH 5.5 were identified. The pH-sensitivity generally resulted
   from a central cluster of histidine residues in CDRH1. When soluble,
   pH-sensitive, scFv clone M16 was dosed onto live cells, the internalized
   fraction was 2.6-fold greater than scFvs that lacked pH-sensitive
   binding and the increase was dependent on endosomal acidification.
   Differences in the intracellular distribution of M16 were also observed
   consistent with an intracellular decoupling of the scFv M16-TfR complex.
   Engineered pH-sensitive TfR binding could prove important for increasing
   the effectiveness of TfR-targeted antibodies seeking to exploit
   endocytosis or transcytosis for drug delivery purposes.}},
