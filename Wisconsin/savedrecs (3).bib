@inproceedings{ ISI:000387431500042,
Author = {VanDamme, Christopher I. and Allen, Matthew S.},
Editor = {{DeClerck, J and Epp, DS}},
Title = {{Using NNMs to Evaluate Reduced Order Models of Curved Beam}},
Booktitle = {{ROTATING MACHINERY, HYBRID TEST METHODS, VIBRO-ACOUSTICS AND LASER
