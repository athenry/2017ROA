@article{ ISI:000351889600014,
Author = {Mortensen, Nicolai B. and Johnson, Jay A. and Shturmakov, Alexander J.},
Title = {{Precision cable winch level wind for deep ice-coring systems}},
Journal = {{ANNALS OF GLACIOLOGY}},
Year = {{2014}},
Volume = {{55}},
Number = {{68}},
Pages = {{99-104}},
Abstract = {{In deep ice-coring, as in many other disciplines, a winching system is
   involved in the overall operation of the drilling activities. The need
   to efficiently store the cable on the winch drum is well recognized, and
   the `orthocyclically wound' approach is often used. This is accomplished
   by means of a `Lebus groove', along with a level winding scheme of some
   description. The level wind is usually implemented in one of several
   ways using some mechanism to synchronize the position of the level wind
   with the point where the cable meets the winch drum. A novel method
   using a feedback control system is presented in this paper, introducing
   a virtually error-free approach to the surprisingly difficult task of
   level winding.}},
