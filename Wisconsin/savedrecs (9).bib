@article{ ISI:000334847800021,
Author = {Garrison, L. M. and Kulcinski, G. L.},
Title = {{Irradiation resistance of grains near \{001\} on polycrystalline
   tungsten under 30 keV He+ bombardment at 1173 K}},
Journal = {{PHYSICA SCRIPTA}},
Year = {{2014}},
Volume = {{T159}},
Month = {{APR}},
Note = {{14th International Conference on Plasma-Facing Materials and
   Components(PFMC), undefined, GERMANY, MAY, 2013}},
Abstract = {{Samples of polycrystalline tungsten were irradiated at normal incidence
   by 30 keV(4) He+ at 1173 K from fluences of 6x10(21) to 6x10(22) He
   m(-2). Zones on each sample were marked using a focused ion beam so they
   could be easily found after irradiation. Electron backscatter
   diffraction was used to analyze the orientation of the grains in the
   marked zones before and after irradiation. Grains with orientations
   similar to 4 degrees to similar to 8 degrees tilt from \{0 0 1\}
   remained smoother after irradiation and were less eroded. Grains with
   all other orientations farther from \{0 0 1\} experienced complete
   morphology change and severe erosion due to the ion bombardment. A grain
   with orientation (0 0 1) was also highly eroded. Mass loss of each
   sample was measured and the sample at the highest fluence had 12 g m(-2)
   mass loss normalized to irradiated area.}},
