@inproceedings{ ISI:000361554400101,
Author = {Ghaemi, Kasra and Behdad, Nader},
Book-Group-Author = {{IEEE}},
Title = {{A Low-Profile, Vertically-Polarized, Compact, Ultra-Wideband Antenna
   with a 10: 1 Bandwidth}},
Booktitle = {{2014 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM
   (APSURSI)}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2014}},
