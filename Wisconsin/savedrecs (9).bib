@article{ ISI:000333648300001,
Author = {Huang, Kai and Szlufarska, Izabela},
Title = {{Green-Kubo relation for friction at liquid-solid interfaces}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2014}},
Volume = {{89}},
Number = {{3}},
Month = {{MAR 18}},
Abstract = {{We have developed a Green-Kubo relation that enables accurate
   calculations of friction at solid-liquid interfaces directly from
   equilibrium molecular dynamics (MD) simulations and that provides a
   pathway to bypass the time-scale limitations of typical nonequilibrium
   MD simulations. The theory has been validated for a number of different
   interfaces and it is demonstrated that the liquid-solid slip is an
   intrinsic property of an interface. Because of the high numerical
   efficiency of our method, it can be used in the design of interfaces for
   applications in aqueous environments, such as nano- and microfluidics.}},
