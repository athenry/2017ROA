@inproceedings{ ISI:000348859000028,
Author = {Brady, John and Thomas, Paul and Virgilio, David and Sayeed, Akbar},
Book-Group-Author = {{IEEE}},
Title = {{Beamspace MIMO Prototype for Low-Complexity Gigabit/s Wireless
   Communication}},
