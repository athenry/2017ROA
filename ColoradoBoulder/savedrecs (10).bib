@article{ ISI:000314461500003,
Author = {Shriver, Patrick M. and Palo, Scott E. and Zenick, Raymond G. and Balas,
   Mark J.},
Title = {{Reward-Lifetime Scheduling Approach to the Autonomous Spacecraft Power
   Management Problem}},
Journal = {{JOURNAL OF AEROSPACE COMPUTING INFORMATION AND COMMUNICATION}},
Year = {{2012}},
Volume = {{9}},
Number = {{2}},
Pages = {{58-67}},
Abstract = {{A high-level characterization of spacecraft operations and subsystems is
   used to formally define the spacecraft power management problem. Two
   objectives of maximizing the payload reward and minimizing the battery
   discharge are optimized in the Pareto-optimal sense. These objectives
   are subject to one constraint of maintaining a minimum battery voltage.
   Even with simplifying assumptions, this formulation is shown to be
   computationally intractable. Nonetheless, a formal proof of optimal
   substructure is used to develop an optimal dynamic programming solution.
   This algorithm is intended to reduce the labor-intensive operations
   problem of manually generating a `good enough' schedule to one of
   selecting an optimal schedule.}},
