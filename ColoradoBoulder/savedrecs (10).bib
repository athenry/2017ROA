@article{ ISI:000304401600008,
Author = {Rangan, Meghana and Yung, Matthew M. and Medlin, J. Will},
Title = {{NiW and NiRu Bimetallic Catalysts for Ethylene Steam Reforming:
   Alternative Mechanisms for Sulfur Resistance}},
Journal = {{CATALYSIS LETTERS}},
