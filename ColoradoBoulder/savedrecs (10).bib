@inproceedings{ ISI:000308787200006,
Author = {Stiles, Laura A. and Seubert, Carl R. and Schaub, Hanspeter},
Editor = {{McAdams, JV and McKinley, DP and Berry, MM and Jenkins, KL}},
Title = {{EFFECTIVE COULOMB FORCE MODELING IN A SPACE ENVIRONMENT}},
