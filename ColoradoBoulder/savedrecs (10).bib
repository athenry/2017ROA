Author = {Nazari, Morad and Samiei, Ehsan and Butcher, Eric A. and Schaub,
   Hanspeter},
Editor = {{McAdams, JV and McKinley, DP and Berry, MM and Jenkins, KL}},
Title = {{SPACECRAFT ATTITUDE STABILIZATION USING NONLINEAR DELAYED ACTUATOR
   CONTROL WITH AN INVERSE DYNAMICS APPROACH}},
Booktitle = {{SPACEFLIGHT MECHANICS 2012}},
