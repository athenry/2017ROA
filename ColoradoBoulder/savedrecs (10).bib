@article{ ISI:000300594900050,
Author = {Burnley, P. C. and Getting, I. C.},
Title = {{Creating a high temperature environment at high pressure in a gas piston
   cylinder apparatus}},
Journal = {{REVIEW OF SCIENTIFIC INSTRUMENTS}},
Year = {{2012}},
Volume = {{83}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{An internal heater capable of 1400 K has been developed for use with a
   gas piston cylinder apparatus capable of achieving in excess of 3 GPa
   with an argon pressure medium. The heated gas piston cylinder produces a
   truly hydrostatic environment for samples up to 3 mm in diameter and 8
   mm in length. The apparatus can be used to study systems that are
   sensitive to stress or samples that cannot withstand shear tractions.
   The gas piston cylinder apparatus was developed in an effort to reduce
   experimental uncertainty in the pressure scale and has been used to
