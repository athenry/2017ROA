@inproceedings{ ISI:000321933400054,
Author = {Jasper, Lee E. Z. and Schaub, Hanspeter},
Editor = {{Schaub, H and Gunter, BC and Russell, RP and Cerven, WT}},
Title = {{EFFECTIVE SPHERE MODELING FOR ELECTROSTATIC FORCES ON THREE-DIMENSIONAL
   SPACECRAFT SHAPES}},
