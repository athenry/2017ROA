@inproceedings{ ISI:000300433100102,
Author = {Urquhart, Landon and Santos, Marcelo and Nievinski, Felipe},
Editor = {{Kenyon, SC and Pacino, MC and Marti, UJ}},
Title = {{Fitting of NWM Ray-Traced Slant Factors to Closed-Form Tropospheric
   Mapping Functions}},
