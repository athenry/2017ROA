@inproceedings{ ISI:000309117700002,
Author = {Norris, Mark and Maksimovic, Dragan},
Book-Group-Author = {{IEEE}},
Title = {{10 MHz Large Signal Bandwidth, 95\% Efficient Power Supply for 3G-4G
   Cell Phone Base Stations}},
Booktitle = {{2012 TWENTY-SEVENTH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC)}},
