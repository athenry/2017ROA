
@article{ ISI:000306737500008,
Author = {Sato, Kohei and Sadamoto, Tomoko and Hirasawa, Ai and Oue, Anna and
   Subudhi, Andrew W. and Miyazawa, Taiki and Ogoh, Shigehiko},
Title = {{Differential blood flow responses to CO2 in human internal and external
   carotid and vertebral arteries}},
