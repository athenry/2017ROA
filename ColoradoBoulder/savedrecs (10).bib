@article{ ISI:000304888700031,
Author = {Zhou, Yun and Muhich, Christopher L. and Neltner, Brian T. and Weimer,
   Alan W. and Musgrave, Charles B.},
Title = {{Growth of Pt Particles on the Anatase TiO2 (101) Surface}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2012}},
Volume = {{116}},
Number = {{22}},
Pages = {{12114-12123}},
Month = {{JUN 7}},
Abstract = {{Growth of Pt-n (n <= 37) clusters on the defect-free TiO2 anatase (101)
   surface has been studied using ab initio pseudopotential calculations
   based on density functional theory. Several initial configurations for
   clusters of 1, 2, 7, 10, and 37 atoms were relaxed to determine the most
   stable structures. All final optimized structures are three dimensional,
   suggesting that formation of island-like particles is favored over
   planar monolayers, as verified experimentally using Pt atomic layer
   deposition and high-resolution transmission electron microscopy.
   Diffusion barriers of a single Pt adatom on TiO2 were calculated to
   understand the mobility of Pt atoms on the TiO2 surface. Activation
   barriers of 0.86 and 1.41 eV were calculated for diffusion along the
   {[}010] and {[}10 (1) over bar] directions, respectively, indicating
   that Pt atoms are relatively mobile along the {[}010] direction at
