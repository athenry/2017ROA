@article{ ISI:000304056200007,
Author = {Cornman, L. B. and Goodrich, R. K. and Axelrad, P. and Barlow, E.},
Title = {{Progress in turbulence detection via GNSS occultation data}},
Journal = {{ATMOSPHERIC MEASUREMENT TECHNIQUES}},
Year = {{2012}},
Volume = {{5}},
Number = {{4}},
Pages = {{789-808}},
Abstract = {{The increased availability of radio occultation (RO) data offers the
   ability to detect and study turbulence in the Earth's atmosphere. An
   analysis of how RO data can be used to determine the strength and
   location of turbulent regions is presented. This includes the derivation
   of a model for the power spectrum of the log-amplitude and phase
   fluctuations of the permittivity (or index of refraction) field. The
   bulk of the paper is then concerned with the estimation of the model
   parameters. Parameter estimators are introduced and some of their
   statistical properties are studied. These estimators are then applied to
   simulated log-amplitude RO signals. This includes the analysis of global
   statistics derived from a large number of realizations, as well as case
   studies that illustrate various specific aspects of the problem.
   Improvements to the basic estimation methods are discussed, and their
   beneficial properties are illustrated. The estimation techniques are
   then applied to real occultation data. Only two cases are presented, but
   they illustrate some of the salient features inherent in real data.}},
