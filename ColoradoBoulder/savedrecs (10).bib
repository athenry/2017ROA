@inproceedings{ ISI:000320611800270,
Author = {Li, Na and Gan, Lingwen and Chen, Lijun and Low, Steven H.},
Book-Group-Author = {{IEEE}},
Title = {{An optimization-based demand response in radial distribution networks}},
Booktitle = {{2012 IEEE GLOBECOM WORKSHOPS (GC WKSHPS)}},
Series = {{IEEE Globecom Workshops}},
Year = {{2012}},
Pages = {{1474-1479}},
Note = {{IEEE Globecom Workshops (GC Wkshps), Anaheim, CA, DEC 03-07, 2012}},
Organization = {{IEEE}},
Abstract = {{Demand response has recently become a topic of active research. Most of
   work however considers only the balance between aggregate load and
   supply, and abstracts away the underlying power network and the
   associated power flow constraints and operating constraints. In this
   paper, we study demand response in a radial distribution network, by
   formulating it as an optimal power flow problem that maximizes the
   aggregate user utilities and minimizes the power line losses, subject to
   the power flow constraints and operating constraints. As the resulting
   problem is non-convex and difficult to solve, we propose a convex
   relaxation that is usually exact for the real-world distribution
   circuits. We then propose a distributed algorithm for the load-serving
   entity to set the price signal to coordinate the users' demand response
   so as to achieve the optimum. Numerical examples show that the proposed
   algorithm converges fast for real-world distribution systems.}},
