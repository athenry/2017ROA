@article{ ISI:000296933200014,
Author = {Liang, Xinhua and Li, Nai-Hong and Weimer, Alan W.},
Title = {{Template-directed synthesis of porous alumina particles with precise
   wall thickness control via atomic layer deposition}},
Journal = {{MICROPOROUS AND MESOPOROUS MATERIALS}},
Year = {{2012}},
Volume = {{149}},
Number = {{1}},
Pages = {{106-110}},
Month = {{FEB 1}},
Abstract = {{Highly porous alumina particles with precise wall thickness control were
   synthesized by atomic layer deposition (AID) of alumina on highly porous
   poly(styrene-divinylbenzene) (PS-DVB) particle templates. Alumina ALD
   was carried out using alternating reactions of trimethylaluminum and
   water at 33 degrees C. The growth rate of alumina was similar to 0.3 nm
   per coating cycle. The wall thickness can be precisely controlled by
   adjusting the number of ALD coating cycles. Thermo-gravimetric analysis,
   X-ray diffraction, nitrogen adsorption, scanning electron microscopy,
   and transmission electron microscopy were used to characterize the
   fabricated porous alumina particles. The effect of number of ALD coating
   cycles and calcination temperature on the mesoporous structure of the
   alumina particles was investigated. gamma-Alumina was formed at
   temperature above 600 degrees C. Porous alumina particles with a surface
   area of 80-100 m(2)/g were obtained and thermally stable at 800 degrees
   C. The pore volume of the porous particles can be as high as 1 cm(3)/g
   after calcination at 800 degrees C. Such porous alumina particles may
   find wide application in nanotechnology and catalysis. (C) 2011 Elsevier
   Inc. All rights reserved.}},
