@article{ ISI:000303577400007,
Author = {Tarumi, Ryuichi and Ledbetter, Hassel and Shibutani, Yoji},
Title = {{Some remarks on the range of Poisson's ratio in isotropic linear
   elasticity}},
Journal = {{PHILOSOPHICAL MAGAZINE}},
Year = {{2012}},
Volume = {{92}},
Number = {{10}},
Pages = {{1287-1299}},
Abstract = {{We study the range of Poisson's ratio nu in the theory of isotropic
   linear elasticity. Using the ratio of the Lame constants chi =
   lambda/mu, we defined the parametrized Poisson's ratio as nu = nu(chi)
   and found that it represents a standard hyperbola with center (chi, nu)
   = (-1/3, 1/2) and eccentricity e = root 2. One of the focal points of
   the hyperbola represents a Born instability criterion (or the equivalent
   strong ellipticity condition) and the other determines the sign of nu.
   The hyperbola has a vertex point in the admissible range of nu, and,
   hence, it naturally divides the range into the two subranges: -1 < nu <=
   (1 - root 2)/2 and (1 - root 2)/2 <= nu < 1/2. We also found that there
   are three equivalent formulas among the nine elastic-constant ratios
