@article{ ISI:000329038800060,
Author = {Olalla, Carlos and Deline, Chris and Maksimovic, Dragan},
Title = {{Performance of Mismatched PV Systems With Submodule Integrated
   Converters}},
Journal = {{IEEE JOURNAL OF PHOTOVOLTAICS}},
Year = {{2014}},
Volume = {{4}},
Number = {{1}},
Pages = {{396-404}},
Month = {{JAN}},
Abstract = {{Mismatch power losses in photovoltaic (PV) systems can be reduced by the
   use of distributed power electronics at the module or submodule level.
   This paper presents an experimentally validated numerical model that can
   be used to predict power production with distributed maximum power point
   tracking (DMPPT) down to the cell level. The model allows the
   investigations of different DMPPT architectures, as well as the impact
   of conversion efficiencies and power constraints. Results are presented
   for annual simulations of three representative partial shading scenarios
   and two scenarios where mismatches are due to aging over a period of 25
   years. It is shown that DMPPT solutions that are based on submodule
   integrated converters offer 6.9-11.1\% improvements in annual energy
   yield relative to a baseline centralized MPPT scenario.}},
