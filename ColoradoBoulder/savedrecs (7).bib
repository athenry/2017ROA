@inproceedings{ ISI:000392912200329,
Author = {Dani, Asmita and Coffey, Michael and Popovic, Zoya},
Book-Group-Author = {{IEEE}},
Title = {{4W X-Band High Efficiency MMIC PA with Output Harmonic Injection}},
Booktitle = {{2014 44TH EUROPEAN MICROWAVE CONFERENCE (EUMC)}},
