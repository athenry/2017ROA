@article{ ISI:000327232100021,
Author = {Thurairajah, Brentha and Bailey, Scott M. and Siskind, David E. and
   Randall, Cora E. and Taylor, Michael J. and Russell, III, James M.},
Title = {{Case study of an ice void structure in polar mesospheric clouds}},
Journal = {{JOURNAL OF ATMOSPHERIC AND SOLAR-TERRESTRIAL PHYSICS}},
Year = {{2013}},
Volume = {{104}},
Pages = {{224-233}},
Month = {{NOV}},
Abstract = {{The Cloud Imaging and Particle Size (CIPS) instrument aboard the
   Aeronomy of Ice in the Mesosphere (AIM) mission produces detailed images
   of Polar Mesospheric Clouds (PMCs) from space and has revealed several
   distinct types of cloud structures. One such commonly observed structure
   is an `ice void'-a nearly circular ice-free region with diameter varying
   from tens to thousands of kilometers. We document the morphology of
   large ice voids (diameter of similar to 500-1000 km) as seen by CIPS in
   the northern hemisphere 2007 PMC season. The meteorology of the
   mesosphere when these voids occur is described by the Navy Operational
   Global Atmospheric Prediction System-Advanced Level Physics High
