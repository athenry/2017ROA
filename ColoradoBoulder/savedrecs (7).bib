@article{ ISI:000325497400002,
Author = {Yang, Jianping and Tan, Chunping and Zhang, Tingjun},
Title = {{Spatial and temporal variations in air temperature and precipitation in
   the Chinese Himalayas during the 1971-2007}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
Year = {{2013}},
Volume = {{33}},
Number = {{12}},
Pages = {{2622-2632}},
Month = {{OCT}},
Abstract = {{The Chinese Himalayas (CH) is highly sensitive to global climate change.
   The analyses of monthly mean air temperature and monthly precipitation
   data during the 1971-2007 from 24 meteorological stations show a
   significant increase of 0.31 degrees C per decade or about 1.15 degrees
   C for annual mean air temperature over the entire region. The
   temperature rise occurs primarily during the period of 1991-2007, up to
   0.73 degrees C per decade. Climate warming is characterized by all-year
   temperature increase in particular during winters with the maximum value
   of 0.38 degrees C per decade during the 1971-2007, up to 1.24 degrees C
   per decade during 1991-2007. Distributions of annual temperature trends
   show significant south-to-north, low-to-high and east-to-west warming
   trends to increase with the latitude and elevation while to decrease
   with the longitude in the CH, respectively. Annual precipitation
   increases, but not significantly during the 1971-2007. Seasonal
   precipitation has no significant trend during the same period, either.
   Spatial discrepancy of the variations in precipitation is particularly
   great. It is evident that the climate was significantly warming and
   drying in the western CH over the past 37 years.}},
