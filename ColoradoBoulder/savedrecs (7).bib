Editor = {{Broschart, SB and Turner, JD and Howell, KC and Hoots, FR}},
Title = {{IMPULSIVE GUIDANCE FOR OPTIMAL MANIFOLD-BASED TRANSFERS TO EARTH-MOON L1
   HALO ORBITS}},
Booktitle = {{ASTRODYNAMICS 2013, PTS I-III}},
