@article{ ISI:000327514700003,
Author = {Tarumi, R. and Ledbetter, H. and Ogi, H. and Hirao, M.},
Title = {{Low-temperature elastic constants of monocrystal corundum (alpha-Al2O3)}},
Journal = {{PHILOSOPHICAL MAGAZINE}},
Year = {{2013}},
Volume = {{93}},
Number = {{36}},
Pages = {{4532-4543}},
Month = {{DEC 1}},
Abstract = {{A complete set of low-temperature second-order elastic constants C-ij of
   corundum (alpha-Al2O3) was studied by resonant ultrasound spectroscopy.
   At ambient temperature, the obtained C-ij show excellent agreement with
   those reported by Gladden et al. Our C-14 exhibit monotonic elastic
   softening upon cooling, but stay positive over the entire temperature
   range considered. From a lattice-dynamics analysis, the softening can be
   attributed to an internal displacement that has two-dimensional E-g
   symmetry. The acoustic Debye temperature and Gruneisen parameter are
   calculated to be Theta(D) = 1040 K and gamma = 1.66, respectively. The
   Einstein temperatures, estimated from C-ij (T) curves, are relatively
   small compared to Theta(D) indicating that low-frequency modes are
   dominant in the zone-centred mode lattice vibrations. Also C-12 is
   essentially temperature independent.}},
