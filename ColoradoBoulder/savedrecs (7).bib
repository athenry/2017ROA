@article{ ISI:000335847600030,
Author = {Yersak, Alexander S. and Lee, Yung C. and Spencer, Joseph A. and Groner,
   Markus D.},
Title = {{Atmospheric pressure spatial atomic layer deposition web coating with in
   situ monitoring of film thickness}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY A}},
Year = {{2014}},
Volume = {{32}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{Spectral reflectometry was implemented as a method for in situ thickness
   monitoring in a spatial atomic layer deposition (ALD) system. Al2O3
   films were grown on a moving polymer web substrate at 100 degrees C
   using an atmospheric pressure ALD web coating system, with film growth
   of 0.11-0.13 nm/cycle. The modular coating head design and the in situ
   monitoring allowed for the characterization and optimization of the
   trimethylaluminum and water precursor exposures, purge flows, and web
   speed. A thickness uniformity of +/- 2\% was achieved across the web.
   ALD cycle times as low as 76 ms were demonstrated with a web speed of 1
   m/s and a vertical gap height of 0.5 mm. This atmospheric pressure ALD
   system with in situ process control demonstrates the feasibility of
   low-cost, high throughput roll-to-roll ALD. (C) 2014 American Vacuum
   Society.}},
