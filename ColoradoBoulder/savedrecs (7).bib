@article{ ISI:000323013400015,
Author = {Mostafa, Simon and Rosario-Ortiz, Fernando L.},
Title = {{Singlet Oxygen Formation from Wastewater Organic Matter}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2013}},
Volume = {{47}},
Number = {{15}},
Pages = {{8179-8186}},
Month = {{AUG 6}},
Abstract = {{Singlet oxygen (102) plays an important role in the inactivation of
   pathogens and the degradation of organic contaminants. The present study
   looks at the surface steady-state concentration of O-1(2) and quantum
   yields (Phi(so)) for organic matter present in or derived from
   wastewater (WVVOM), including those that are partially treated and after
   undergoing oxidation. The surface steady state concentrations of O-1(2)
   ranged from 1.23 to 1.43 x 10(-13) M for bulk wastewaters under
   simulated sunlight. The Phi(so) values for these samples varied from
   2.8\% to 4.7\% which was higher than the values observed for the natural
   organic matter isolates evaluated (1.6-2.1\%). Size fractionation of
   WWOM resulted in Phi(so) increases, with a value of up to 8.6\% for one
   of the <1 kDa fractions. Furthermore, oxidation of WWOM by hypochlorous
   acid (Hod) and molecular ozone also resulted in an increase in Phi(so),
   with the highest measured value being 9.3\%. This research further
   explores the correlations between the photosensitizing properties of
   WWOM and optical characteristics (e.g., absorbance, E2:E3 ratio). Making
   use of easily measurable absorbance values, a model for the prediction
   of O-1(2) steady-state concentrations is proposed.}},
