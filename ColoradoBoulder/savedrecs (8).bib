Author = {Shenoy, Raveesh and Tibbitt, Mark W. and Anseth, Kristi S. and Bowman,
   Christopher N.},
Title = {{Formation of Core-Shell Particles by Interfacial Radical Polymerization
   Initiated by a Glucose Oxidase-Mediated Redox System}},
