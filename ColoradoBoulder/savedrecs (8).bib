@article{ ISI:000319339600049,
Author = {Lee, Taewoo and Mundoor, Haridas and Gann, Derek G. and Callahan,
   Timothy J. and Smalyukh, Ivan I.},
Title = {{Imaging of director fields in liquid crystals using stimulated Raman
   scattering microscopy}},
Journal = {{OPTICS EXPRESS}},
Year = {{2013}},
Volume = {{21}},
Number = {{10}},
Pages = {{12129-12134}},
Month = {{MAY 20}},
Abstract = {{We demonstrate an approach for background-free three-dimensional imaging
   of director fields in liquid crystals using stimulated Raman scattering
   microscopy. This imaging technique is implemented using a single
   femtosecond pulsed laser and a photonic crystal fiber, providing Stokes
   and pump frequencies needed to access Raman shifts of different chemical
   bonds of molecules and allowing for chemically selective and broadband
   imaging of both pristine liquid crystals and composite materials. Using
   examples of model three-dimensional structures of director fields, we
   show that the described technique is a powerful tool for mapping of
   long-range molecular orientation patterns in soft matter via polarized
   chemical-selective imaging. (C) 2013 Optical Society of America}},
