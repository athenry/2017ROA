@inproceedings{ ISI:000328460400040,
Author = {Linnenberger, Anna and Fiedler, Callie and Roberts, Justine J. and
   Skaalure, Stacey C. and Bryant, Stephanie J. and Cole, Michael C. and
   McLeod, Robert R.},
Editor = {{Dholakia, K and Spalding, GC}},
Title = {{Optical trapping for tissue scaffold fabrication}},
Booktitle = {{OPTICAL TRAPPING AND OPTICAL MICROMANIPULATION X}},
Series = {{Proceedings of SPIE}},
Year = {{2013}},
Volume = {{8810}},
Note = {{Conference on Optical Trapping and Optical Micromanipulation X, San
   Diego, CA, AUG 25-29, 2013}},
Organization = {{SPIE; Elliot Sci Ltd; nPoint Inc; Thorlabs Inc}},
Abstract = {{We investigate holographic optical trapping combined with
   step-and-repeat maskless projection stereolithography for fine control
   of 3D position of living cells within a 3D microstructured hydrogel.
   C2C12 myoblast cells were chosen as a demonstration platform because
   their development into multinucleated myotubes requires linear
   arrangements of myoblasts. C2C12 cells are positioned in the monomer
   solution with multiple optical traps at 1064 nm and then are
   encapsulated by photopolymerization of monomer via projection of a
   512x512 spatial light modulator (SLM) illuminated at 405 nm. High 405 nm
   sensitivity and complete insensitivity to 1064 nm is enabled by a
   lithium acylphosphinate LAP) salt photoinitiator. Use of a polyethylene
   glycol dimethacrylate PEGDMA) based monomer is compared to that of
   polyethylene glycol PEG) hydrogels formed by thiol-ene photo-click
   chemistry for patterning structures with cellular resolution, and for
   maintaining cell viability. Cells patterned in thiol-ene with RGD are
   shown to retain viability up to 4 days after the trapping and
   encapsulation procedure. Further, cells patterned in thiol-ene with RGD
   and a degradable ester link, are shown to fuse, indicating the initial
   stages of development of multi-nucleated cells.}},
