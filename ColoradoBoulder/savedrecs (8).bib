@inproceedings{ ISI:000323182500023,
Author = {Nardi, Damiano and Hoogeboom-Pot, Kathleen M. and Hernandez-Charpak,
   Jorge N. and Tripp, Marie and King, Sean W. and Anderson, Erik H. and
   Murnane, Margaret M. and Kapteyn, Henry C.},
