@inproceedings{ ISI:000327210204051,
Author = {Chen, Hua and Li, Yang and Pao, Lucy Y.},
Book-Group-Author = {{IEEE}},
Title = {{Intersample Ripple Resulting from Discrete-Time Feedforward Control}},
Booktitle = {{2013 AMERICAN CONTROL CONFERENCE (ACC)}},
