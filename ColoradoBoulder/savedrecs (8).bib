Title = {{High Frequency Synchronous Buck Converter using GaN-on-SiC HEMTs}},
Booktitle = {{2013 IEEE ENERGY CONVERSION CONGRESS AND EXPOSITION (ECCE)}},
Series = {{IEEE Energy Conversion Congress and Exposition}},
Year = {{2013}},
