@article{ ISI:000320622600003,
Author = {Barsic, Anthony and Piestun, Rafael},
Title = {{Super-resolution of dense nanoscale emitters beyond the diffraction
   limit using spatial and temporal information}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{102}},
Number = {{23}},
Month = {{JUN 10}},
Abstract = {{We propose a super-resolution technique for dense clusters of blinking
   emitters. The method relies on two basic assumptions: the emitters are
   statistically independent and a model of the imaging system is known. We
   numerically analyze the performance limits of the method as a function
   of emitter density and noise level. Numerical simulations show that five
   closely packed emitters can be resolved and localized to a precision of
   17 nm. The experimental resolution of five quantum dots located within a
   diffraction-limited spot confirms the applicability of this approach.
   Statistical tests validate the independence of our quantum dots
   separated by nanoscale distances. (C) 2013 AIP Publishing LLC.}},
