@article{ ISI:000317426200003,
Author = {Jordan, Elizabeth and Javernick-Will, Amy},
Title = {{Indicators of Community Recovery: Content Analysis and Delphi Approach}},
Journal = {{NATURAL HAZARDS REVIEW}},
