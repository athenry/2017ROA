@inproceedings{ ISI:000371705200197,
Author = {Bielefeldt, Angela R.},
Book-Group-Author = {{IEEE}},
Title = {{Characteristics of Engineering Faculty Engaged in the Scholarship of
   Teaching and Learning}},
