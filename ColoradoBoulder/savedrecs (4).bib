Author = {Lu, Jie and Perreault, David J. and Afridi, Khurram K.},
Book-Group-Author = {{IEEE}},
Title = {{Impedance Control Network Resonant dc-dc Converter for Wide-Range
   High-Efficiency Operation}},
Booktitle = {{2015 THIRTIETH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC 2015)}},
Series = {{Annual IEEE Applied Power Electronics Conference and Exposition (APEC)}},
Year = {{2015}},
