@inproceedings{ ISI:000380529100051,
Author = {Wang, J. and Pak, R. Y. S.},
Editor = {{Soga, K and Kumar, K and Biscontin, G and Kuo, M}},
Title = {{Impact of seismic intensity on deformation of road embankment
   constructed by granular soil}},
