@article{ ISI:000349726900017,
Author = {Narisawa, Masaki and Terauds, Kalvis and Ma, Guangyu and Hokazono,
   Hiroki and Raj, Rishi and Iwase, Akihiro},
Title = {{Evaluation of high temperature resistance of white Si-O-C(-H) ceramics
   in an inert atmosphere}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{2015}},
Volume = {{410}},
Pages = {{106-111}},
Month = {{FEB 15}},
Abstract = {{Two kinds of white Si-O-C(-H) ceramic powders, named S900 and S1100,
   were prepared from cross-linked polysiloxane particles with an average
   diameter of 6 mu m. S900 was the product obtained in a hydrogen gas flow
   at 900 degrees C with a holding time of 1 h, whereas S1100 was the
   product obtained at 1100 degrees C with a holding time of 3 h. Heat
   stability of these ceramic powders in graphite boats were examined
   within the temperature range of 1200-1600 degrees C in an Ar gas flow.
   The powder substantially began to degrade beyond 1400 degrees C, which
   was accompanied by mass loss, fusing of particles and partial
   cristobalite precipitation. The color of S900, however, became black
   after the heat treatment at 1400 degrees C before the substantial mass
   loss, whereas the color of S1100 showed small change to light gray after
   the heat treatment at 1400 degrees C. The apparent color change of the
   samples during the heat treatment possibly corresponded to defect
   incorporation in the Si-O-C(-H) network, whereas the change in chemical
   composition was minor. At high temperature region, SiO and CO were
   evolved as a result of SiC-SiO2 reaction, although SiC and SiO2 were
   defined as virtual phases in the Si-O-C(-H) amorphous. (C) 2014 Elsevier
   B.V. All rights reserved.}},
