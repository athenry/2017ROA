@article{ ISI:000364457000043,
Author = {Lee, Eunjung and Manteuffel, Thomas A. and Westphal, Chad R.},
Title = {{FOSLL{*} FOR NONLINEAR PARTIAL DIFFERENTIAL EQUATIONS}},
