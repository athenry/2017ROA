@article{ ISI:000357788700008,
Author = {Dailing, Eric A. and Setterberg, Whitney K. and Shah, Parag K. and
   Stansbury, Jeffrey W.},
Title = {{Photopolymerizable nanogels as macromolecular precursors to covalently
   crosslinked water-based networks}},
Journal = {{SOFT MATTER}},
Year = {{2015}},
Volume = {{11}},
Number = {{28}},
Pages = {{5647-5655}},
Abstract = {{We present a strategy for directly and efficiently polymerizing aqueous
   dispersions of reactive nanogels into covalently crosslinked polymer
   networks with properties that are determined by the initial chemical and
   physical nanogel structure. This technique can extend the range of
   achievable properties and architectures for networks formed in solution,
   particularly in water where monomer selection for direct polymerization
   and the final network properties are quite limited. Nanogels were
   initially obtained from a solution polymerization of a hydrophilic
   monomethacrylate and either a hydrophilic PEG-based dimethacrylate or a
   more hydrophobic urethane dimethacrylate, which produced globular
   particles with diameters of 10-15 nm with remarkably low polydispersity
   in some cases. Networks derived from a single type of nanogel or a blend
   of nanogels with different chemistries when dispersed in water gelled
   within minutes when exposed to low intensity UV light. Modifying the
   nanogel structure changes both covalent and non-covalent secondary
   interactions in the crosslinked networks and reveals critical design
   criteria for the development of networks from highly internally
   branched, nanoscale prepolymer precursors.}},
