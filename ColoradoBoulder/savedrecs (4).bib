@inproceedings{ ISI:000371647200163,
Author = {Bennett, Trevor and Schaub, Hanspeter},
Editor = {{Furfaro, R and Cassotto, S and Trask, A and Zimmer, S}},
Title = {{TOUCHLESS ELECTROSTATIC DETUMBLING WHILE TUGGING LARGE AXI-SYMMETRIC GEO
   DEBRIS}},
Booktitle = {{SPACEFLIGHT MECHANICS 2015, PTS I-III}},
Series = {{Advances in the Astronautical Sciences}},
