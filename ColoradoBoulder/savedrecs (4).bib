@article{ ISI:000350224900054,
Author = {Xia, Jianyang and Niu, Shuli and Ciais, Philippe and Janssens, Ivan A.
   and Chen, Jiquan and Ammann, Christof and Arain, Altaf and Blanken,
   Peter D. and Cescatti, Alessandro and Bonal, Damien and Buchmann, Nina
