@article{ ISI:000349062100008,
Author = {Swartzlander, Mark D. and Blakney, Anna K. and Amer, Luke D. and
   Hankenson, Kurt D. and Kyriakides, Themis R. and Bryant, Stephanie J.},
Title = {{Immunomodulation by mesenchymal stem cells combats the foreign body
   response to cell-laden synthetic hydrogels}},
Journal = {{BIOMATERIALS}},
Year = {{2015}},
Volume = {{41}},
Pages = {{79-88}},
Month = {{FEB}},
Abstract = {{The implantation of non-biological materials, including scaffolds for
   tissue engineering, ubiquitously leads to a foreign body response (FBR).
   We recently reported that this response negatively impacts fibroblasts
   encapsulated within a synthetic hydrogel and in turn leads to a more
   severe FBR, suggesting a cross-talk between encapsulated cells and
   inflammatory cells. Given the promise of mesenchymal stem cells (MSCs)
   in tissue engineering and recent evidence of their immunomodulatory
   properties, we hypothesized that MSCs encapsulated within poly(ethylene
   glycol) (PEG) hydrogels will attenuate the FBR. In vitro, murine MSCs
   encapsulated within PEG hydrogels attenuated classically activated
   primary murine macrophages by reducing gene expression and protein
   secretion of pro-inflammatory cytokines, most notably tumor necrosis
   factor-alpha. Using a COX2 inhibitor, prostaglandin E2 (PGE2) was
   identified as a mediator of MSC immunomodulation of macrophages. In
   vivo, hydrogels laden with MSCs, osteogenically differentiating MSCs, or
   no cells were implanted subcutaneously into C57BL/6 mice for 28 days to
   assess the impact of MSCs on the fibrotic response of the FBR. The
   presence of encapsulated MSCs reduced fibrous capsule thickness compared
   to acellular hydrogels, but this effect diminished with osteogenic
   differentiation. The use of MSCs prior to differentiation in tissue
   engineering may therefore serve as a dynamic approach, through
   continuous cross-talk between MSCs and the inflammatory cells, to
   modulate macrophage activation and attenuate the FBR to implanted
   synthetic scaffolds thus improving the long-term tissue engineering
   outcome. (C) 2014 Elsevier Ltd. All rights reserved.}},
