Title = {{Classifications of Citrus Fruit Using Image Processing -GLCM Parameters}},
Booktitle = {{2015 INTERNATIONAL CONFERENCE ON COMMUNICATIONS AND SIGNAL PROCESSING
   (ICCSP)}},
