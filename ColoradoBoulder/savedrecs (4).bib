@article{ ISI:000348928200004,
Author = {Verdin, Andrew and Rajagopalan, Balaji and Kleiber, William and Katz,
   Richard W.},
Title = {{Coupled stochastic weather generation using spatial and generalized
   linear models}},
Journal = {{STOCHASTIC ENVIRONMENTAL RESEARCH AND RISK ASSESSMENT}},
Year = {{2015}},
Volume = {{29}},
Number = {{2}},
Pages = {{347-356}},
Month = {{FEB}},
Abstract = {{We introduce a stochastic weather generator for the variables of minimum
   temperature, maximum temperature and precipitation occurrence.
   Temperature variables are modeled in vector autoregressive framework,
   conditional on precipitation occurrence. Precipitation occurrence arises
   via a probit model, and both temperature and occurrence are spatially
   correlated using spatial Gaussian processes. Additionally, local climate
   is included by spatially varying model coefficients, allowing spatially
   evolving relationships between variables. The method is illustrated on a
   network of stations in the Pampas region of Argentina where
   nonstationary relationships and historical spatial correlation challenge
   existing approaches.}},
