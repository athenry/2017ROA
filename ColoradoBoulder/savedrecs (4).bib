@article{ ISI:000351765900083,
Author = {Krogstad, Molly R. and Ahn, Sungmo and Park, Wounjhang and Gopinath,
   Juliet T.},
Title = {{Nonlinear characterization of Ge28Sb12Se60 bulk and waveguide devices}},
Journal = {{OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{23}},
Number = {{6}},
Pages = {{7870-7878}},
Month = {{MAR 23}},
Abstract = {{Single-mode Ge28Sb12Se60 strip waveguides, fabricated with thermal
   evaporation and lift-off, were demonstrated at 1.03 mu m. The linear and
   nonlinear optical properties of these waveguides were shown to be
   similar to bulk samples, with differences attributed to small variations
   in composition of similar to 4 atomic \% or less. From z-scan
   measurements at 1.03 mu m using circularly polarized, similar to 200 fs
   pulses at 374 kHz, Ge28Sb12Se60 was found to have a nonlinear refractive
   index similar to 130 x fused silica and a two-photon absorption
   coefficient of 3.5 cm/GW. Given the large two-photon absorption
   coefficient, this material shows promise for optical limiting
   applications at 1 mu m. (C) 2015 Optical Society of America}},
