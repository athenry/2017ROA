Author = {Takahashi, Yu and Scheeres, Daniel J.},
Editor = {{Tanygin, S and Park, RS and Starchville, TF and Newman, LK}},
Title = {{GENERALIZED DENSITY DISTRIBUTION ESTIMATION FOR SMALL BODIES}},
