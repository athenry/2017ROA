@article{ ISI:000311424600009,
Author = {Weidman, P. D.},
Title = {{Obliquely-intersecting Hiemenz flows: a new interpretation of Howarth
   stagnation-point flows}},
Journal = {{FLUID DYNAMICS RESEARCH}},
Year = {{2012}},
Volume = {{44}},
Number = {{6}},
Month = {{DEC}},
Abstract = {{The interaction of two obliquely-intersecting Hiemenz planar stagnation
   point flows normal to a flat plate is studied. One flow of strain rate a
   aligned along the x-axis intersects the other of strain rate b at angle
   phi from the x-axis. Transformed to principal axes, a similarity
   reduction of the Navier-Stokes equation yields two ordinary differential
   equations with coefficients depending on the strain rate ratio sigma =
   b/a and phi. These equations are then transformed into Howarth's
   equations via a two-parameter mapping, thereby giving an alternative
   interpretation of Howarth's problem. The large-sigma asymptotic behavior
   of solutions for the limiting angles phi = 0 degrees and 90 degrees are
   determined. Direct numerical solutions of the principal axes equations
   for -1 <= sigma <= 2 for each phi = 0 degrees, 15 degrees, 30 degrees,
   45 degrees, 60 degrees, 75 degrees and 90 degrees compare precisely with
   those obtained from the two-parameter mapping. Plots for the wall shear
   stress parameters and normalized displacement thicknesses are presented
   along with sample similarity velocity profiles.}},
