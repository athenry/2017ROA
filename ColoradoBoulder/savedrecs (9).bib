@article{ ISI:000314298900022,
Author = {Reveyrand, T. and Ramos, I. and Popovic, Z.},
Title = {{Time-reversal duality of high-efficiency RF power amplifiers}},
Journal = {{ELECTRONICS LETTERS}},
Year = {{2012}},
Volume = {{48}},
Number = {{25}},
Month = {{DEC 6}},
Abstract = {{The similarity between RF power amplifiers and rectifiers is discussed.
   It is shown that the same high-efficiency harmonically-terminated power
   amplifier can be operated in a dual rectifier mode. Nonlinear
   simulations with a GaN HEMT transistor model show the time-reversal
   intrinsic voltage and current waveform relationship between a class-F
   amplifier and rectifier. Measurements on a class-F-1 amplifier and
   rectifier at 2.14 GHz demonstrate over 80\% efficiency in both cases.}},
