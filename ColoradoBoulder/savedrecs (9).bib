Address = {{1801 ALEXANDER BELL DRIVE, STE 500, RESTON, VA 22091-4344 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Jung, TP (Reprint Author), Univ Colorado, McAnally Fac, Campus Box 429, Boulder, CO 80309 USA.
   Jung, Timothy P.; Starkey, Ryan P., Univ Colorado, McAnally Fac, Boulder, CO 80309 USA.
   Argrow, Brian, Univ Colorado, Res \& Engn Ctr Unmanned Vehicles, Boulder, CO 80309 USA.}},
DOI = {{10.2514/1.C031709}},
