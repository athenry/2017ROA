@article{ ISI:000306017700021,
Author = {Weidman, P. D.},
Title = {{Non-axisymmetric Homann stagnation-point flows}},
Journal = {{JOURNAL OF FLUID MECHANICS}},
Year = {{2012}},
Volume = {{702}},
Pages = {{460-469}},
Month = {{JUL 10}},
Abstract = {{A modification of Homann's axisymmetric outer potential stagnation-point
   flow of strain rate a is obtained by adding periodic radial and
   azimuthal velocities of the form b r sin 2 theta and b r cos 2 theta,
   respectively, where b is a shear rate. This leads to the discovery of a
   new family of asymmetric viscous stagnation-point flows depending on the
   shear-to-strain-rate ratio gamma = b/a that exist over the range
   -infinity < gamma < infinity. Numerical solutions for the wall shear
   stress parameters and the displacement thicknesses are given and
   compared with their large-gamma asymptotic behaviours. Sample similarity
   velocity profiles are also presented.}},
