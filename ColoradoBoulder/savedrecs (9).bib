@article{ ISI:000314435300020,
Author = {Hogan, Erik A. and Schaub, Hanspeter},
Title = {{Relative Motion Control For Two-Spacecraft Electrostatic Orbit
   Corrections}},
Journal = {{JOURNAL OF GUIDANCE CONTROL AND DYNAMICS}},
Year = {{2013}},
Volume = {{36}},
Number = {{1}},
Pages = {{240-249}},
Month = {{JAN-FEB}},
Note = {{AAS/AIAA Astrodynamics Specialist Conference, AK, JUL 31-AUG 04, 2011}},
Organization = {{AAS; AIAA}},
Abstract = {{The charged relative-motion dynamics and control of a two-craft system
   is investigated if one vehicle is performing a low-thrust orbit
   correction using inertial thrusters. The nominal motion is an
   along-track configuration where active electrostatic charge control is
   maintaining an attractive force between the two vehicles. In this study
   the charging is held fixed and the inertial thruster of the tugging
   vehicle is controlled to stabilize the relative motion to a nominal
   fixed separation distance. Using a candidate Lyapunov function, the
   relative orbit control law is shown to be asymptotically stable.
   Analysis of the control system gains is performed in order to achieve a
   desired settling time and damping ratio. The effects of uncertainties in
   the vehicle charges are also examined. Using numerical simulation, the
   performance of the proposed control system is investigated for a
   formation in geosynchronous earth orbit.}},
