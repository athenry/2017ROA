@inproceedings{ ISI:000375255607013,
Author = {Bielefeldt, Angela R. and Paterson, Kurt and Swan, Chris and Pierrakos,
   Olga and Kazmer, David O. and Soisson, Annie},
Book-Group-Author = {{ASEE}},
Title = {{Spectra of Learning Through Service Programs}},
Booktitle = {{2013 ASEE ANNUAL CONFERENCE}},
Series = {{ASEE Annual Conference \& Exposition}},
Year = {{2013}},
Note = {{ASEE Annual Conference, Atlanta, GA, JUN 23-26, 2013}},
Organization = {{ASEE}},
Abstract = {{Engineering students have a growing number of opportunities to engage in
   service-learning and co-curricular service activities. Because of the
   range of opportunities that exist for students to engage in community
   service activities, the umbrella term Learning Through Service (LTS) has
   been created. As these types of opportunities increase, the range and
   diversity of these initiatives has not been well documented. While some
   more established programs have published extensively on their program
   characteristics and assessment outcomes (i.e. SLICE at the University of
   Massachusetts Lowell and EPICS at Purdue), many newer and emerging
   programs have not yet have published any information. In fact, some of
   these LTS activities are not routinely included in courses but are
   integrated intermittently by motivated faculty - sometimes without
   widespread recognition within their own college or university. This
   paper presents an initial summary of some of the lesser publicized LTS
   activities based on a literature search and two recent workshops
   associated with the NSF-grant on Engineering Faculty Engagement in
   Learning Through Service (EFELTS). Emerging LTS patterns, opportunities,
   challenges, and resources are summarized to provide an expanded view of
   the landscape of what is currently happening in engineering.}},
