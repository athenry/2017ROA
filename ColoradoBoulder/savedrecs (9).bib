@inproceedings{ ISI:000352223502031,
Author = {Borowski, Holly and Marden, Jason R. and Leslie, David S. and Frew, Eric
   W.},
Book-Group-Author = {{IEEE}},
Title = {{Coarse Resistance Tree Methods For Stochastic Stability Analysis}},
Booktitle = {{2013 IEEE 52ND ANNUAL CONFERENCE ON DECISION AND CONTROL (CDC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2013}},
Pages = {{1860-1865}},
