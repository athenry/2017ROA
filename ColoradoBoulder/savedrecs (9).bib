@inproceedings{ ISI:000377554800003,
Author = {Handzo, Ryan and Gold, Kenn and Born, George and Davies, Michael},
Editor = {{Hardaway, LR}},
Title = {{USING SIGNALS OF OPPORTUNITY FOR DEEP SPACE SATELLITE NAVIGATION}},
Booktitle = {{GUIDANCE AND CONTROL 2013}},
Series = {{Advances in the Astronautical Sciences}},
Year = {{2013}},
Volume = {{149}},
Pages = {{33-46}},
Note = {{36th Annual American-Astronautical-Society Rocky Mountain Section
   Guidance and Control Conference, Rocky Mt Sect, Breckenridge, CO, FEB
   01-06, 2013}},
Organization = {{Amer Astronaut Soc}},
Abstract = {{Satellite navigation traditionally uses GPS signals, or when GPS is
   unavailable, dedicated ground based signals. The use of GPS is limited
   to specific orbital regimes and has very limited availability beyond
   geosynchronous altitudes, while dedicated ground-based signals are
   expensive and specific to each mission. The use of generic terrestrially
   broadcast signals, or Signals of Opportunity, for use in satellite
   navigation has not yet been studied in great depth. This paper presents
   analysis that shows that these Signals of Opportunity are useful signals
   that can be picked up in GPS challenged environments as well as for
   deep-space orbital tracking. Furthermore, it is shown that not only are
   the signals available in these GPS challenged regimes, but they include
   many properties that allow satellite navigation in these GPS challenged
   regimes; including number of signals seen, uniqueness of each signal,
   and ability to range each signal to determine source of signal and
   ephemeris of receiver. These Signals of Opportunity present a new and
   economical method for satellite positioning, navigation, and timing in a
   wide variety of orbits out to lunar distance and beyond.}},
