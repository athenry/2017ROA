@article{ ISI:000339053600015,
Author = {Danaci, Burcin and Anil, Mehmet Ali and Erzan, Ayse},
Title = {{Motif statistics of artificially evolved and biological networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2014}},
Volume = {{89}},
Number = {{6}},
Month = {{JUN 30}},
Abstract = {{Topological features of gene regulatory networks can be successfully
   reproduced by a model population evolving under selection for short
   dynamical attractors. The evolved population of networks exhibit motif
   statistics, summarized by significance profiles, which closely match
   those of E. coli, S. cerevsiae, and B. subtilis, in such features as the
   excess of linear motifs and feedforward loops, and deficiency of
   feedback loops. The slow relaxation to stasis is a hallmark of a rugged
   fitness landscape, with independently evolving populations exploring
   distinct valleys strongly differing in network properties.}},
