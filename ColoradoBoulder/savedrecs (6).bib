Editor = {{Holland, WS and Zmuidzinas, J}},
Title = {{Development and characterization of the readout system for POLARBEAR-2}},
Booktitle = {{MILLIMETER, SUBMILLIMETER, AND FAR-INFRARED DETECTORS AND
   INSTRUMENTATION FOR ASTRONOMY VII}},
