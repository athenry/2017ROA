@article{ ISI:000338332800004,
Author = {Hariri-Ardebili, M. A.},
Title = {{Impact of Foundation Nonlinearity on the Crack Propagation of High
   Concrete Dams}},
Journal = {{SOIL MECHANICS AND FOUNDATION ENGINEERING}},
Year = {{2014}},
Volume = {{51}},
Number = {{2}},
Pages = {{72-82}},
Month = {{MAY}},
Abstract = {{In the present paper the seismic stability of gravity and arch dams is
   evaluated under earthquake ground motion considering the foundation
   nonlinearities. For this purpose, the finite element models of the 103 m
   Koyna gravity dam and the 203 m Dez arch dam are prepared. A fixed
   smeared crack model with the Willam-Warnke failure criterion is used for
   modeling the concrete cracking. Also the elasto-plastic model with the
   Drucker-Prager yield criterion is used for foundation cracking. Viscous
   boundary models are used for the far-end areas of the foundation medium
   to absorb the outgoing waves. It was found that considering the
   nonlinear foundation model increases the cracked areas on the dam body,
   especially in the vicinity of the foundation, and reduces the total
   stability of the coupled system.}},
