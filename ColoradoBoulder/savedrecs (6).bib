@inproceedings{ ISI:000346492605159,
Author = {Borowski, Holly and Marden, Jason R.},
Book-Group-Author = {{IEEE}},
Title = {{Fast Convergence for Time-Varying Semi-Anonymous Potential Games}},
Booktitle = {{2014 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
