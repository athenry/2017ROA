@article{ ISI:000332262400017,
Author = {Terauds, Kalvis and Raj, Rishi},
Title = {{Dramatic influence of interface chemical potentials on the oxidation of
   silicon and carbon based compounds}},
Journal = {{JOURNAL OF THE EUROPEAN CERAMIC SOCIETY}},
Year = {{2014}},
Volume = {{34}},
