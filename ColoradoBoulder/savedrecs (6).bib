@incollection{ ISI:000342786100003,
Author = {Gonzalez, Jose A. and Park, K. C. and Abascal, Ramon},
Editor = {{RodriguezTembleque, L and Aliabadi, F}},
Title = {{A partitioned formulation for FEM/BEM coupling in contact problems using
   localized Lagrange multipliers}},
Booktitle = {{WEAR AND CONTACT MECHANICS}},
Series = {{Key Engineering Materials}},
Year = {{2014}},
Volume = {{618}},
Pages = {{23-48}},
Abstract = {{This paper presents a state-of-the-art in the use of localized Lagrange
   multipliers (LLMs) for 3D frictional contact problems coupling the
   Finite Element Method (FEM) and the Boundary Element Method (BEM).
   Resolution methods for the contact problem between non-matching meshes
   have traditionally been based on a direct coupling of the contacting
   solids using classical Lagrange multipliers. These methods tend to
   generate strongly coupled systems that require a deep knowledge of the
   discretization characteristics on each side of the contact zone
   complicating the process of mixing different numerical techniques. In
   this work a displacement contact frame is inserted between the FE and BE
   interface meshes, discretized and finally connected to the contacting
   substructures using LLMs collocated at the mesh-interface nodes. This
   methodology will provide a partitioned formulation which preserves
   software modularity and facilitates the connection of non-matching FE
   and BE meshes.}},
