@inproceedings{ ISI:000360870800051,
Author = {Hussein, Mahmoud I. and Frazier, Michael J.},
Book-Group-Author = {{ASME}},
Title = {{METADAMPING IN DISSIPATIVE METAMATERIALS}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL MECHANICAL ENGINEERING CONGRESS
   AND EXPOSITION, 2013, VOL 14}},
Year = {{2014}},
Note = {{ASME International Mechanical Engineering Congress and Exposition
   (IMECE2013), San Diego, CA, NOV 15-21, 2013}},
Organization = {{ASME}},
Abstract = {{Using a generalized form of Bloch's theorem, we derive the dispersion
   relation of a viscously damped locally resonant metamaterial modeled as
   an infinite mass-in-mass lumped parameter chain. For comparison, we
   obtain the dispersion relation for a statically equivalent
   Bragg-scattering mass-spring chain that is also viscously damped. For
   the two chains, we prescribe identical damping levels in the dashpots
   and compare the damping ratio associated with all propagating Bloch
   modes. We find that the locally resonant metamaterial exhibits higher
   dissipation throughout the spectrum which indicates a damping emergence
   phenomena due to the presence of local resonance. This phenomenon, which
   we define as metadamping, provides a new paradigm for the design of
   material systems that display both high damping and high stiffness. We
   conclude our investigation by quantifying the degree of metadamping as a
   function of the long-wave speed of sound in the medium or the static
   stiffness.}},
