@inproceedings{ ISI:000361554401225,
Author = {Sammeta, Rohit and Filipovic, Dejan S.},
Book-Group-Author = {{IEEE}},
Title = {{A Low-Profile Sinuous Antenna}},
Booktitle = {{2014 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM
   (APSURSI)}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2014}},
Pages = {{1333-1334}},
Note = {{IEEE Antennas-and-Propagation-Society International Symposium (APSURSI),
   Memphis, TN, JUL 06-11, 2014}},
Organization = {{Inst Elect Elect Engineers; Inst Elect Elect Engineers Antennas \&
   Propagat Soc}},
Abstract = {{A lens loaded ferrite backed sinuous antenna is demonstrated where the
   metallic cavity side wall is eliminated to increase its operational
   bandwidth. The sinuous aperture forms the fundamental radiator and is
   loaded with an electrically small lens in order to redirect the power
   towards the upper hemisphere. The antenna is backed by a ferrite ground
   plane and is center-fed with 75 Omega cable bundle to obtain good match.
   The presented configuration has active VSWR < 3, circular polarized gain
   > 0 dBiC and wobble of wave (WoW) < 2dB in the frequency range of 500
   MHz to 2.5 GHz while being only lambda/3.5 (at 500 MHz) in diameter and
   height making it the smallest among reported or commercially available
