@article{ ISI:000332496300024,
Author = {Kim, Ji Woo and Travis, Jonathan J. and Hu, Enyuan and Nam, Kyung-Wan
   and Kim, Seul Cham and Kang, Chan Soon and Woo, Jae-Ha and Yang,
   Xiao-Qing and George, Steven M. and Oh, Kyu Hwan and Cho, Sung-Jin and
