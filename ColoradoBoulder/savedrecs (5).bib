@article{ ISI:000343769000003,
Author = {Zarzana, Kyle J. and Cappa, Christopher D. and Tolbert, Margaret A.},
Title = {{Sensitivity of Aerosol Refractive Index Retrievals Using Optical
   Spectroscopy}},
Journal = {{AEROSOL SCIENCE AND TECHNOLOGY}},
Year = {{2014}},
Volume = {{48}},
Number = {{11}},
Pages = {{1133-1144}},
Month = {{NOV 2}},
Abstract = {{Accurate refractive index values are required to determine the effects
   of aerosol particles on direct radiative forcing. Theoretical retrievals
   using extinction data alone or extinction plus absorption data have been
   simulated to determine the sensitivity of each retrieval. A range of
   aerosol types with a range of different refractive indices were
   considered. The simulations showed that the extinction-only retrieval
   was not able to accurately or precisely retrieve refractive index
   values, even for purely scattering compounds, but the addition of a
   simulated absorption measurement greatly improved the retrieval.
