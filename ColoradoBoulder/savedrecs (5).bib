@article{ ISI:000347021400037,
Author = {Jasper, Lee and Schaub, Hanspeter},
Title = {{Tethered towing using open-loop input-shaping and discrete thrust levels}},
Journal = {{ACTA ASTRONAUTICA}},
Year = {{2014}},
Volume = {{105}},
Number = {{1}},
Pages = {{373-384}},
Month = {{DEC}},
Abstract = {{Asteroid retrieval, satellite servicing, and debris removal concepts
   often rely on a thrusting vehicle to redirect and steer a passive
   object. One effective way to tow the object is through a tether. This
   study employs a discretized tether model attached to six
   degree-of-freedom end bodies. To reduce the risk of a post-burn
   collision between the end bodies, discrete thrust input shaping profiles
   are considered including a Posicast input and a bang-off-bang thrust
   profile. These input shaping techniques attain desirable collision
   avoidance performance by inducing a tumbling or gravity gradient motion
   of the tethered formation. Their performance is compared to an earlier
   frequency notched thruster profile. (C) 2014 IAA. Published by Elsevier
   Ltd. All rights reserved.}},
