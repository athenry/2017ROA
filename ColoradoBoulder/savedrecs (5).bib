@article{ ISI:000346071900027,
Author = {Dove, Jacob D. and Mountford, Paul A. and Murray, Todd W. and Borden,
   Mark A.},
Title = {{Engineering optically triggered droplets for photoacoustic imaging and
   therapy}},
Journal = {{BIOMEDICAL OPTICS EXPRESS}},
Year = {{2014}},
Volume = {{5}},
Number = {{12}},
Pages = {{4417-4427}},
Month = {{DEC 1}},
Abstract = {{Liquid perfluorocarbon (PFC) droplets incorporating optical absorbers
   can be vaporized through photothermal heating using a pulsed laser
   source. Here, we report on the effect of droplet core material on the
   optical fluence required to produce droplet vaporization. We fabricate
   gold nanoparticle templated microbubbles filled with various PFC gases
   (C3F8, C4F10, and C5F12) and apply pressure to condense them into
   droplets. The core material is found to have a strong effect on the
   threshold optical fluence, with lower boiling point droplets allowing
   for vaporization at lower laser fluence. The impact of droplet size on
   vaporization threshold is discussed, as well as a proposed mechanism for
   the relatively broad distribution of vaporization thresholds observed
   within a droplet population with the same core material. We propose that
   the control of optical vaporization threshold enabled by engineering the
   droplet core may find application in contrast enhanced photoacoustic
   imaging and therapy. (C) 2014 Optical Society of America}},
