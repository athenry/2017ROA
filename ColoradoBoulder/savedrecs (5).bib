@article{ ISI:000339601600080,
Author = {Glover, Caitlin M. and Mezyk, Stephen P. and Linden, Karl G. and
   Rosario-Ortiz, Fernando L.},
Title = {{Photochemical degradation of Corexit components in ocean water}},
Journal = {{CHEMOSPHERE}},
Year = {{2014}},
Volume = {{111}},
Pages = {{596-602}},
Month = {{SEP}},
Abstract = {{Due to the large quantities of dispersants used during the Deepwater
   Horizon spill in 2010, there were immediate concerns with regards to the
   fate and transport of the mixture in ocean waters. Direct and sensitized
   photolysis experiments were carried out for two compounds chosen as
   surrogates for the Corexit mixture (9500 and 9527) that were applied to
   surface waters during the oil spill in the Gulf of Mexico. The results
   showed that direct photolysis did not contribute significantly to the
   overall degradation (max similar to 30\%), therefore the focus shifted
   to sensitized photolysis, specifically the degradation stemming from the
   reaction rate with hydroxyl radical (HO center dot). The direct
   photochemical degradation rates for two of the compounds, dioctyl
   sulfosuccinate (DOSS) and dipropylene glycol butyl ether (DGBE) were
   measured as 4.29 x 10(-6) s(-1) and 5.95 x 10(-6) s(-1), respectively;
   whereas the overall degradation rate in ocean water was 1.56 x 10(-5)
   s(-1) and 2.23 x 10(-5) s(-1). The formation rates and apparent quantum
   yields for Ha formation were determined for six ocean water samples. The
   values ranged from 1.81 x 10(-5) near shore to 0.061 x 10(-5) for the
   open ocean. These degradation rates suggest the possibility for
   photolysis to play a role in the overall fate of Corexit. (C) 2014
   Elsevier Ltd. All rights reserved.}},
