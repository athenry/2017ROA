@article{ ISI:000336352500031,
Author = {Naik, Kiran S. and Sglavo, Vincenzo M. and Raj, Rishi},
Title = {{Field assisted sintering of ceramic constituted by alumina and yttria
   stabilized zirconia}},
Journal = {{JOURNAL OF THE EUROPEAN CERAMIC SOCIETY}},
Year = {{2014}},
Volume = {{34}},
Number = {{10}},
Pages = {{2435-2442}},
Month = {{SEP}},
Abstract = {{We show that a two-phase 50 vol\% 3YSZ-alumina ceramic flash-sinters at
   a furnace temperature of 1060 degrees C under an electrical field of 150
   V cm(-1). In contrast undoped, single-phase alumina remains immune to
   field assisted sintering at fields up to 1000 V cm(-1), although
   single-phase 3YSZ flash sinters at 750 degrees C (furnace temperature).
   The mechanisms of field assisted sintering are divided into two regimes.
   At low fields the sintering rate increases gradually (FAST), while at
   high fields sintering occurs abruptly (FLASH). Interestingly,
   alumina/zirconia composites show a hybrid behavior such that early
   sintering occurs in FAST mode, which is then followed by
   flash-sintering. The specimens held in the flashed state, after they had
   sintered to near full density, show much higher rate of grain growth
   than in conventional experiments. These results are in contrast to
   earlier work where the rate of grain growth had been shown to be slower
   under weak electrical fields. (C) 2014 Elsevier Ltd. All rights
   reserved.}},
