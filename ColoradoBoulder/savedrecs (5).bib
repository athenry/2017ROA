@article{ ISI:000345891900039,
Author = {Williams, Rhea M. and Pang, Simon H. and Medlin, J. Will},
Title = {{Ring-Opening and Oxidation Pathways of Furanic Oxygenates on
   Oxygen-Precovered Pd(111)}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
