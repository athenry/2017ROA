@article{ ISI:000341782300002,
Author = {Limburg, Steve and Grant, David and Varanasi, Mahesh K.},
Title = {{HIGHER GENUS UNIVERSALLY DECODABLE MATRICES (UDMG)}},
Journal = {{ADVANCES IN MATHEMATICS OF COMMUNICATIONS}},
Year = {{2014}},
Volume = {{8}},
Number = {{3}},
Pages = {{257-270}},
Month = {{AUG}},
Abstract = {{We introduce the notion of Universally Decodable Matrices of Genus g
   (UDMG), which for g = 0 reduces to the notion of Universally Decodable
   Matrices (UDM) introduced in H. Fix positive K,N,L. A UDMG is a set
   \{M-i vertical bar 1 <= i <= L\} of matrices of size K x N over a finite
   field such that the rows of any matrix of K g columns formed from the
   initial segments of the M, are linearly independent. We show that UDMG
   can be used to build approximately universal codes. We then provide a
   dictionary between UDMG and linear codes under the m-metric, which
   quickly provides constructions of UDMG and places bounds on the size of
   UDMG.}},
