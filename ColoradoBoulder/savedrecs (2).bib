@inproceedings{ ISI:000389467400098,
Author = {Kumar, Ashish and Lu, Jie and Afridi, Khurram K.},
Book-Group-Author = {{IEEE}},
Title = {{Power Density and Efficiency Enhancement in Impedance Control Network
   Resonant DC-DC Converters Using Topology Morphing Control}},
Booktitle = {{2016 IEEE 17th Workshop on Control and Modeling for Power Electronics
   (COMPEL)}},
Series = {{IEEE Workshop on Control and Modeling for Power Electronics}},
Year = {{2016}},
Note = {{17th IEEE Workshop on Control and Modeling for Power Electronics
   (COMPEL), Norwegian Univ Sci \& Technol, Trondheim, NORWAY, JUN 27-30,
   2016}},
Organization = {{IEEE}},
Abstract = {{This paper introduces a new voltage regulation scheme for impedance
   control network (ICN) resonant dc-dc converters in which the rectifier
   of the ICN converter alternates between full-bridge and half-bridge
   topologies. Compared to the conventional burst-mode control, this
   topology morphing control approach substantially reduces the output
   capacitance requirement in ICN converters and improves partial-power
   efficiencies. A prototype 1-MHz, 120-W step-down ICN resonant converter
   designed for an input voltage range of 18 V to 36 V, an output voltage
   of 12 V, and a 10: 1 output power range has been designed, built and
   tested with both burst-mode control and the proposed topology morphing
   control. The topology morphing control reduces the output capacitance
   requirement in the prototype ICN converter by over 36\%, and reduces
   converter losses at partial power levels by up to 46.5\%.}},
