Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Doostan, A (Reprint Author), Univ Colorado, Dept Aerosp Engn Sci, Boulder, CO 80309 USA.
   Hadigol, Mohammad; Maute, Kurt; Doostan, Alireza, Univ Colorado, Dept Aerosp Engn Sci, Boulder, CO 80309 USA.}},
DOI = {{10.1016/j.jpowsour.2015.09.060}},
