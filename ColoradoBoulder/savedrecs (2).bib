@article{ ISI:000366319300031,
Author = {Del Prete, Massimo and Costanzo, Alessandra and Georgiadis, Apostolos
   and Collado, Ana and Masotti, Diego and Popovic, Zoya},
Title = {{A 2.45-GHz Energy-Autonomous Wireless Power Relay Node}},
Journal = {{IEEE TRANSACTIONS ON MICROWAVE THEORY AND TECHNIQUES}},
Year = {{2015}},
Volume = {{63}},
Number = {{12, 2, SI}},
Pages = {{4511-4520}},
Month = {{DEC}},
Abstract = {{This paper describes the design and experimental characterization of a
   battery-less bidirectional 2.45-GHz circuit operating in oscillator mode
   as a wireless power transmitter or in rectifier mode as an energy
   harvester, with a measured efficiency greater than 50\% in both
   operating states. The dc voltage harvested in rectifier mode provides
   the drain bias for the oscillator. The FET-gate self-bias mechanism is
   exploited in both functionalities, thus eliminating external gate bias.
   Bi-directionality is based on the time-reversal properties of a
   transistor oscillator. Energy autonomy is possible at received RF power
   levels as low as -4 dBm, by means of a bias-assisting feedback loop,
   consisting of a single matched low-power diode in shunt configuration. A
   hybrid prototype is demonstrated with the ability to operate as an
   energy-autonomous power relay node by switching between transmit and
   receive power modes.}},
