@inproceedings{ ISI:000400778400052,
Author = {Anwar, Usama and Kim, Hyeokjin and Chen, Hua and Erickson, Robert and
   Maksimovic, Dragan and Afridi, Khurram K.},
Book-Group-Author = {{IEEE}},
Title = {{A High Power Density Drivetrain-Integrated Electric Vehicle Charger}},
