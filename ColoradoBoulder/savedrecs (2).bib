@article{ ISI:000363352700033,
Author = {Perez, Marco A. and Oiler, Sergio and Felippa, Carlos A. and Gil, Lluis},
Title = {{Micro-mechanical approach for the vibration analysis of CFRP laminates
   under impact-induced damage}},
Journal = {{COMPOSITES PART B-ENGINEERING}},
Year = {{2015}},
Volume = {{83}},
Pages = {{306-316}},
Month = {{DEC 15}},
Abstract = {{This paper deals with modelling the effect of low-velocity impact damage
   upon the vibration response of CFRP laminates through a micro-mechanical
   description of the induced internal damage. The serial-parallel (SP)
   continuum approach is used to estimate the map of induced internal
   damage by considering the micro-structural interaction between the
   composite constituents and modifying their constitutive performance
   through a continuum damage formulation. An eigenvalue analysis is then
   done to determine the modal response of impacted laminates. The validity
   of the modelling approach to successfully reproduce the vibration
   response of impacted coupons is assessed through a comparison with an
   experimental test series conducted on a set of 48 CFRP laminated
   coupons. The results confirm the ability of the described approach in
   comparison to competing ones used to reproduce the experimentally
   observed behaviour. (C) 2015 Elsevier Ltd. All rights reserved.}},
