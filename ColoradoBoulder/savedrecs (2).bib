@inproceedings{ ISI:000400186900043,
Author = {Arias-Hidalgo, Mijail and Neupauer, Roseanna M. and Villa-Cox, Gonzalo
   and Lucas Barcia, Jefferson},
Editor = {{Pathak, CS and Reinhart, D}},
Title = {{Comprehending Dynamics of the Ecuadorian River Discharge Series Using
   Wavelet Analysis and Bandpass Filters}},
Booktitle = {{WORLD ENVIRONMENTAL AND WATER RESOURCES CONGRESS 2016: WATERSHED
   MANAGEMENT, IRRIGATION AND DRAINAGE, AND WATER RESOURCES PLANNING AND
   MANAGEMENT}},
