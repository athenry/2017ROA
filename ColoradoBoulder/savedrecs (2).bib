Author = {Dietrich, Ann and McMahon, Jay W.},
Editor = {{Majji, M and Turner, JD and Wawrzyniak, GG and Cerven, WT}},
Title = {{AUTONOMOUS OBSERVATION PLANNING WITH FLASH LIDAR AROUND SMALL BODIES}},
