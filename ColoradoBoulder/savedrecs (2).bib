@article{ ISI:000367280100091,
Author = {Ackerman, Paul J. and Mundoor, Haridas and Smalyukh, Ivan I. and van de
   Lagemaat, Jao},
Title = {{Plasmon-Exciton Interactions Probed Using Spatial Coentrapment of
   Nanoparticles by Topological Singularities}},
Journal = {{ACS NANO}},
Year = {{2015}},
Volume = {{9}},
Number = {{12}},
Pages = {{12392-12400}},
Month = {{DEC}},
Abstract = {{We study plasmon-exciton interaction by using topological singularities
   to spatially confine, selectively deliver, cotrap and optically probe
   colloidal semiconductor and plasmonic nanoparticles. The interaction is
   monitored in a single quantum system in the bulk of a liquid crystal
   medium where nanoparticles are manipulated and nanoconfined far from
   dielectric interfaces using laser tweezers and topological
   configurations containing singularities. When quantum dot-in-a-rod
   particles are spatially colocated with a plasmonic gold nanoburst
   particle in a topological singularity core, its fluorescence increases
   because blinking is significantly suppressed and the radiative decay
   rate increases by nearly an order of magnitude owing to the Purcell
   effect. We argue that the blinking suppression is the result of the
   radiative rate change that mitigates Auger recombination and quantum dot
   ionization, consequently reducing nonradiative recombination. Our work
   demonstrates that topological singularities are an effective platform
   for studying and controlling plasmon-exciton interactions.}},
