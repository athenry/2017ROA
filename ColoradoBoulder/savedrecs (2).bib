@inproceedings{ ISI:000400170800021,
Author = {Greene, J. A.},
Editor = {{Pathak, CS and Reinhart, D}},
Title = {{Optimization of Well Placement for Engineered Injection and Extraction
   during In Situ Groundwater Remediation}},
Booktitle = {{WORLD ENVIRONMENTAL AND WATER RESOURCES CONGRESS 2016: PROFESSIONAL
   DEVELOPMENT, INNOVATIVE TECHNOLOGY, INTERNATIONAL PERSPECTIVES, AND
   HISTORY AND HERITAGE}},
Year = {{2016}},
Pages = {{185-192}},
Note = {{16th Annual World Environmental and Water Resources Congress of the
   Environmental-and-Water-Resources-Institute (EWRI), Amer Soc Civil
   Engineers, West Palm Beach, FL, MAY 22-26, 2016}},
Organization = {{Amer Soc Civil Engineers, Environm \& Water Resources Inst}},
Abstract = {{Engineered injection and extraction (EIE) has been developed as a
   strategy to improve contaminant degradation during in situ groundwater
   remediation by enhancing contact between contaminant and treatment
   chemicals. During EIE, water is sequentially injected or extracted from
   a series of wells surrounding the contaminant plume to create transient
   flow conditions strategically reconfiguring the treatment and
   contaminant plume within the aquifer to enhance contaminant degradation.
   The separation distance and five-point spatial alignment of the wells
   relative to the contaminant plume affects the ability of treatment
   chemicals to degrade the contaminant within the aquifer. In this study,
   an evolutionary algorithm is used to optimize separation distance and
   spatial orientation of wells relative to contaminant plumes. Circular
   contaminant plumes with uniform and Gaussian initial contaminant
   concentration distributions are investigated. Simulation results yield
   well alignments around contaminant plumes that maximize contaminant
   degradation.}},
