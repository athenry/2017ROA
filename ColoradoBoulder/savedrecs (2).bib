@inproceedings{ ISI:000391615800018,
Author = {Awais, Usama and Barton, Taylor W.},
Book-Group-Author = {{IEEE}},
Title = {{High PSRR Low Drop-Out Regulator with Isolated Replica Feedback Ripple
   Cancellation Technique}},
Booktitle = {{2016 IEEE DALLAS CIRCUITS AND SYSTEMS CONFERENCE (DCAS)}},
Year = {{2016}},
Note = {{IEEE Dallas Circuits and Systems Conference (DCAS), Arlington, TX, OCT
   10, 2016}},
Organization = {{IEEE}},
Abstract = {{This paper presents a CMOS low-dropout regulator (LDO) with a unique
   power supply rejection (PSR) boosting technique. A replica device is
   used to cancel the power supply noise at the output that is coupled
   through the main LDO parasitic. The proposed LDO employs a mid-ranged
   output decoupling capacitor of 50nF and a comparatively low-complexity
   circuitry to achieve the supply rejection target while maintaining fast
   load transients. The proposed LDO is designed and simulated in 0.18 mu m
   CMOS process; it features a -42.1 dB PSRR at 1 MHz and about -51.06 dB
   at 10 MHz while driving a maximum load of 50 mA. The LDO consumes a
   quiescent current of 62 mu A with load regulation of 0.0154 mA/mV.}},
