Author = {Rieger, Samantha and Scheeres, Daniel and Barbee, Brent},
Editor = {{Zanetti, R and Russell, RP and Ozimek, MT and Bowes, AL}},
Title = {{ORBITAL STABILITY REGIONS FOR HYPOTHETICAL NATURAL SATELLITES OF 101955
   BENNU (1999 RQ(36))}},
Booktitle = {{SPACEFLIGHT MECHANICS 2016, PTS I-IV}},
Series = {{Advances in the Astronautical Sciences}},
Year = {{2016}},
