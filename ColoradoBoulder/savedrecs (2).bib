@inproceedings{ ISI:000388376100088,
Author = {Zalkind, Daniel S. and Pao, Lucy Y.},
Book-Group-Author = {{IEEE}},
Title = {{The Fatigue Loading Effects of Yaw Control for Wind Plants}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2016}},
Pages = {{537-542}},
Note = {{American Control Conference (ACC), Boston, MA, JUL 06-08, 2016}},
Organization = {{Amer Automat Control Council}},
Abstract = {{We study the fatigue loading experienced by wind turbines using yaw
   control to redirect wakes. First, through aeroelastic simulations, the
   fatigue loads of a turbine equipped with a baseline and individual pitch
   controller are analyzed for a turbine operating with a constant yaw
   misalignment. Second, a two-turbine wind plant using yaw control to
   redirect wakes is optimized for energy capture with different wind
   inflow angles. That control law is then used with a distribution of wind
   directions to determine how often and at what angle an upstream turbine
   would operate with a constant yaw misalignment. Finally, the fatigue
   data from the initial simulations are extrapolated to determine the
   aggregate fatigue damage increase on an upstream wind turbine controlled
   for optimal plant power production.}},
