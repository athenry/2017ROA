@inproceedings{ ISI:000345864000027,
Author = {Butt, Khalid and Aronsson, Jonatan and Okhmatovski, Vladimir},
Book-Group-Author = {{IEEE}},
Title = {{Multiple and Non-Existent Barnes-Hut Center-of-Charge (CoC) Solution in
   Lossy Layered Substrates: Half Space Analytic and Numerical Study}},
Booktitle = {{2013 IEEE 22ND CONFERENCE ON ELECTRICAL PERFORMANCE OF ELECTRONIC
   PACKAGING AND SYSTEMS (EPEPS)}},
Series = {{IEEE Conference on Electrical Performance of Electronic Packaging and
   Systems-EPEPS}},
Year = {{2013}},
Pages = {{119-122}},
Note = {{IEEE 22nd Conference on Electrical Performance of Electronic Packaging
   and Systems (EPEPS), San Jose, CA, OCT 27-30, 2013}},
Organization = {{IEEE; IEEE Microwave Theory \& Tech Soc; IEEE Components, Packaging \&
   Mfg Technol Soc; Ansys; Cadence; Rambus; IBM Res; IdemWorks; CST;
   Agilent; Nimbic}},
Abstract = {{For rapid extraction of capacitance in lossless substrates Barnes-Hut
   algorithm is an effective technique for evaluating N charges in O(N log
   N) operations. When substrates are lossy the hierarchical application of
   the CoC approximation, apart from resulting in unique CoC, can result in
