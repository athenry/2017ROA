@article{ ISI:000313757800004,
Author = {Wylde, J. and Chen, Y. and Leafloor, E.},
Title = {{DEVELOPMENT OF A PULSING METER FOR POCKET INJECTION OF LIQUID MANURE IN
   FORAGE FIELDS}},
Journal = {{APPLIED ENGINEERING IN AGRICULTURE}},
Year = {{2012}},
Volume = {{28}},
Number = {{6}},
Pages = {{803-812}},
Month = {{NOV}},
Abstract = {{Pocket injection of liquid manure is a new approach for low soil
   disturbance injection of liquid manure. However, equipment for pocket
   injection has not been developed. In this study, three prototypes of a
   pulsing meter were designed, fabricated, and tested. The final prototype
   of the pulsing meter was adapted onto an existing AerWay aerator to form
   a field pocket injector The AerWay aerator created soil pockets
   (cavities in soil), and the pulsing meter delivered liquid manure in
   pulses with the intention of each pulse being placed into a soil pocket.
   Laboratory tests showed that the pulsing meter had the function of
   regulating liquid as pulses, which is desired for pocket injection. The
   volume of pulse varied between 88 and 278 mL (2.98 and 9.40 oz),
   depending on the operational speed of the pulsing meter The pocket
   injector was tested in a forage field and a field with barley stubble.
   The field test results further confirmed the desired pulsing function
   and reliability of the pulsing meter The field results also showed the
   significant reduction in liquid exposure on the soil surface, when
   compared to the traditional manure delivery system of the AerWay aerator}},
