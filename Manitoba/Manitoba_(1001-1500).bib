@inproceedings{ ISI:000330569800072,
Author = {Zhang, Hong and Cai, Jun and Li, Xiaolong},
Book-Group-Author = {{IEEE}},
Title = {{Energy-efficient Base Station Control with Dynamic Clustering in
   Cellular Network}},
Booktitle = {{2013 8TH INTERNATIONAL ICST CONFERENCE ON COMMUNICATIONS AND NETWORKING
   IN CHINA (CHINACOM)}},
Year = {{2013}},
Pages = {{384-388}},
Note = {{8th International ICST Conference on Communications and Networking in
   China (CHINACOM), Guilin, PEOPLES R CHINA, AUG 14-16, 2013}},
Organization = {{European Alliance Innovat; IEEE; ICST; IEEE Comp Soc}},
Abstract = {{Due to increasing awareness of environmental and economic issues for
   network operators, energy-efficiency has received much attention lately
   and becomes one of the major design goals in wireless networks. Since
   the power consumption of base stations (BS) occupies most energy cost of
   entire cellular networks, in this paper, we propose a novel power saving
   method denoted as clustering BS-off (CBSO) scheme that all the cells are
   cooperated and dynamically adjusted to switch on/off along with space
   and time. Instead of adopting a unified and consistent BS-off scheme all
   over the network, the proposed CBSO schemes adaptively group BSs in
   several clusters based on the user traffic load fluctuations and runs
   novel BS-off matching scheme in order to find the optimal BS on/off
   mechanism in each cluster separately. Experimental results demonstrate
   the effectiveness of both proposed centralized and distributed CBSO
   algorithms in energy saving.}},
