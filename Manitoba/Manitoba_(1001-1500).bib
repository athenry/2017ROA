@article{ ISI:000311522500032,
Author = {Shi, Junbin and Ouyang, Jun and Li, Qintao and Wang, Leyu and Wu, Jun
   and Zhong, Wen and Xing, Malcolm M. Q.},
Title = {{Cell-compatible hydrogels based on a multifunctional crosslinker with
   tunable stiffness for tissue engineering}},
Journal = {{JOURNAL OF MATERIALS CHEMISTRY}},
Year = {{2012}},
Volume = {{22}},
Number = {{45}},
Pages = {{23952-23962}},
Month = {{DEC 7}},
Abstract = {{This paper reports a functionalized cationic poly(amidoamine)
   crosslinker synthesized by Michael addition of peptide mimetic agmatine
   to N,N'-bis(acryloyl)cystamine. A polyethylene glycol diacrylate (PEGDA)
   hybrid hydrogel was developed using a functionalized crosslinker with
   cell adhesion capability and tunable stiffness. This engineered hydrogel
   presents a 3-D in-growth of cells and the cryosection shows remarkable
   morphology of the gel. The tunable stiffness of this hydrogel can
   regulate the osteogenic differentiation of mesenchymal stem cells from
   RT-PCR. Both in vitro and in vivo tests demonstrate adjustable
   degradation rate and biocompatibility. The crosslinker and its hydrogels
   hold promising potential for tissue engineering applications.}},
