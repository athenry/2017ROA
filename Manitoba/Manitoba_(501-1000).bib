@article{ ISI:000361414200015,
Author = {Senthilkumar, T. and Jayas, D. S. and White, N. D. G.},
Title = {{Detection of different stages of fungal infection in stored canola using
   near-infrared hyperspectral imaging}},
Journal = {{JOURNAL OF STORED PRODUCTS RESEARCH}},
Year = {{2015}},
Volume = {{63}},
Pages = {{80-88}},
Month = {{JUL}},
Abstract = {{Near-infrared (NIR) hyperspectral imaging system was used to detect
   different stages ot fungal infections in stored canola. Artificially
   infected canola seeds (Fungi: Aspergillus glaucus and Penicillium spp)
   were subjected to hyperspectral imaging in the range between 1000 and
   1600 nm at 61 evenly distributed wavelengths. Four wavelengths 1100,
   1130, 1250 and 1300 nm were identified as significant wavelengths and
   were used in statistical discriminant analysis. Pair-wise, two-class and
   six-class classification models were developed to classify the healthy
   and different stages of fungal infected samples. Linear, quadratic and
   Mahalanobis discriminant classifiers were used to classify healthy, five
   stages of A. glaucus and five stages of Penicillium spp infected canola
   seeds. All the three classifiers classified healthy and fungal infected
   canola seeds with a classification accuracy of more than 95\% for
   healthy canola seeds and more than 90\% for the initial stages of A.
   glaucus and Penicillium spp infected canola seeds. The classification
