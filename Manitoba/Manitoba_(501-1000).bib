Title = {{Channel Assignments in Wireless Networks with Time-Varying Traffic
   Behaviors}},
Booktitle = {{2015 8TH IFIP WIRELESS AND MOBILE NETWORKING CONFERENCE (WMNC)}},
Year = {{2015}},
Pages = {{285-292}},
