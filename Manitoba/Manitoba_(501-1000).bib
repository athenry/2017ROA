@inproceedings{ ISI:000380459900469,
Author = {Zhang, Hong and Cai, Jun and Li, Xiaolong},
Book-Group-Author = {{IEEE}},
Title = {{Self-organized Virtual Small Networking for Energy Saving and Load
   Balancing in Cellular Networks}},
Booktitle = {{2015 IEEE INTERNATIONAL CONFERENCE ON COMMUNICATION WORKSHOP}},
