@article{ ISI:000356531600029,
Author = {Geeganage, Janath and Annakkage, U. D. and Weekes, Tony and Archer,
   Brian A.},
Title = {{Application of Energy-Based Power System Features for Dynamic Security
   Assessment}},
Journal = {{IEEE TRANSACTIONS ON POWER SYSTEMS}},
Year = {{2015}},
Volume = {{30}},
Number = {{4}},
Pages = {{1957-1965}},
Month = {{JUL}},
Abstract = {{This paper presents a novel approach to enable frequent computational
   cycles in online dynamic security assessment by using the terms of the
   transient energy function (TEF) as input features to a machine learning
   algorithm. The aim is to train a single classifier that is capable of
   classifying stable and unstable operating points independent of the
   contingency. The network is trained based on the current system topology
   and the loading conditions. The potential of the proposed approach is
   demonstrated with the New England 39-bus test power system model using
   the support vector machine as the machine learning technique. It is
   shown that the classifier can be trained using a small set of data when
   the terms of the TEF are used as input features. The prediction accuracy
   of the proposed scheme was tested under the balanced and unbalanced
   faults with the presence of voltage sensitive and dynamic loads for
   different operating points.}},
