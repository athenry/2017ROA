@article{ ISI:000354542500109,
Author = {Wu, Jiandong and Hillier, Craig and Komenda, Paul and de Faria, Ricardo
   Lobato and Levin, David and Zhang, Michael and Lin, Francis},
Title = {{A Microfluidic Platform for Evaluating Neutrophil Chemotaxis Induced by
   Sputum from COPD Patients}},
Journal = {{PLOS ONE}},
Year = {{2015}},
Volume = {{10}},
Number = {{5}},
Month = {{MAY 11}},
Abstract = {{Chronic Obstructive Pulmonary Disease (COPD) is a common lung disease
   characterized by breathing difficulty as a consequence of narrowed
   airways. Previous studies have shown that COPD is correlated with
   neutrophil infiltration into the airways through chemotactic migration.
   However, whether neutrophil chemotaxis can be used to characterize and
   diagnose COPD is not well established. In the present study, we
   developed a microfluidic platform for evaluating neutrophil chemotaxis
   to sputum samples from COPD patients. Our results show increased
   neutrophil chemotaxis to COPD sputum compared to control sputum from
   healthy individuals. The level of COPD sputum induced neutrophil
   chemotaxis was correlated with the patient's spirometry data. The cell
   morphology of neutrophils in a COPD sputum gradient is similar to the
   morphology displayed by neutrophils exposed to an IL-8 gradient, but not
   a fMLP gradient. In competing gradients of COPD sputum and fMLP,
   neutrophils chemotaxis and cell morphology are dominated by fMLP.}},
