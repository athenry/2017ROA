@article{ ISI:000352924600013,
Author = {Kim, Yail J. and Al Wakeel, Shahlaa and Gaddafi, Adel and Cha, Young-Jin},
Title = {{In-Place Performance of Severely Deteriorated Pervious Concrete: A Case
   Study}},
Journal = {{ACI MATERIALS JOURNAL}},
Year = {{2015}},
Volume = {{112}},
Number = {{2}},
Pages = {{295-304}},
Month = {{MAR-APR}},
Abstract = {{This paper presents a case study for examining the in-place performance
   of pervious concrete in a medium-size parking Tot in Denver, CO. Of
   interest are the surface characteristics and hydraulic properties of the
   concrete dependent upon the degree of deterioration. Test results
   include pavement texture depth, skid resistance, surface profile,
   infiltration, and traffic volume. A statistical approach, analysis of
   variance, is conducted to evaluate the distinct surface properties of
   various locations in the parking lot. Mutual independency between the
   surface texture depth and skid resistance is discussed. A
   practice-oriented predictive model is proposed to estimate an
   international roughness index of pervious concrete using a texture depth
   value and skid resistance. A relationship between the infiltration of
   the pervious concrete and traffic attributes is established. The
   performance reliability of the parking lot is evaluated in conjunction
   with critical rainfall intensity.}},
