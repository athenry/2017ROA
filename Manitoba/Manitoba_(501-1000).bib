@inproceedings{ ISI:000361017900217,
Author = {Dang, Hieu V. and Kinsner, Witold},
Book-Group-Author = {{IEEE}},
Title = {{Multiobjective Memetic Optimization for Spectrum Sensing and Power
   Allocation in Cognitive Wireless Networks}},
Booktitle = {{2014 IEEE 27TH CANADIAN CONFERENCE ON ELECTRICAL AND COMPUTER
   ENGINEERING (CCECE)}},
