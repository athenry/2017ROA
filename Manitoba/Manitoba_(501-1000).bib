Editor = {{Shi, C and Shen, X}},
Title = {{AUTOMATION OF A MIX PROPORTIONING METHOD DEVELOPED FOR CONCRETES WITH
   INDIGENOUS MATERIALS OF PAKISTAN USING KNOWLEDGE-BASED GUIS IN MATLAB}},
Booktitle = {{ADVANCES IN CHEMICALLY-ACTIVATED MATERIALS (CAM'2014)}},
