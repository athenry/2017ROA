@article{ ISI:000348054700044,
Author = {McClarty, Megan M. and Bruce, Jared P. and Freund, Michael S. and
   Oliver, Derek R.},
Title = {{Piezoresistive characterization of bottom-up, n-type silicon microwires
   undergoing bend deformation}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{106}},
Number = {{2}},
Month = {{JAN 12}},
Abstract = {{The piezoresistance of silicon has been studied over the past few
   decades in order to characterize the material's unique electromechanical
   properties and investigate their wider applicability. While bulk and
   top-down (etched) micro- and nano-wires have been studied extensively,
   less work exists regarding bottom-up grown microwires. A facile method
   is presented for characterizing the piezoresistance of released,
   phosphorus-doped silicon microwires that have been grown, bottom-up, via
   a chemical vapour deposition, vapour-liquid-solid process. The method
   uses conductive tungsten probes to simultaneously make electrical
   measurements via direct ohmic contact and apply mechanical strain via
   bend deformation. These microwires display piezoresistive coefficients
   within an order of magnitude of those expected for bulk n-type silicon;
   however, they show an anomalous response at degenerate doping
   concentrations (similar to 10(20) cm(-3)) when compared to lower doping
   concentrations (similar to 10(17) cm(-3)), with a stronger
   piezoresistive coefficient exhibited for the more highly doped wires.
   This response is postulated to be due to the different growth mechanism
   of bottom-up microwires as compared to top-down. (C) 2015 AIP Publishing
   LLC.}},
