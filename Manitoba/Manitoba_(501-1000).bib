@article{ ISI:000333525200005,
Author = {Bigvand, A. Ghobadi and Ojo, Olanrewaju A.},
Title = {{On Asymmetric Diffusional Solidification During Transient Liquid Phase
   Bonding}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS A-PHYSICAL METALLURGY AND
   MATERIALS SCIENCE}},
Year = {{2014}},
Volume = {{45A}},
Number = {{4}},
Pages = {{1670-1674}},
Month = {{APR}},
Abstract = {{The underlying cause of asymmetric diffusion solidification which alters
   microstructure during transient liquid phase bonding under low
   temperature gradient was studied. A new solute-conserving asymmetric
   numerical model coupled with experimental verification showed that a
   transition from bi-directional to unidirectional solidification, under a
   constant temperature gradient, is controlled by competition between
   liquid and solid-state diffusion at one of the two liquid-solid
   interfaces. This mechanistic understanding would aid a more effective
   use of the process.}},
