@incollection{ ISI:000386074500017,
Author = {Pawlak, Miroslaw},
Editor = {{Matwin, S and Mielniczuk, J}},
Title = {{Semiparametric Inference in Identification of Block-Oriented Systems}},
Booktitle = {{CHALLENGES IN COMPUTATIONAL STATISTICS AND DATA MINING}},
Series = {{Studies in Computational Intelligence}},
Year = {{2016}},
Volume = {{605}},
Pages = {{297-331}},
Abstract = {{In this paper, we give the semiparametric statistics perspective on the
   problem of identification of a class of nonlinear dynamic systems. We
   present a framework for identification of the so-called block-oriented
   systems that can be represented by finite-dimensional parameters and an
   infinite-dimensional set of nonlinear characteristics that run typically
   through a nonparametric class of univariate functions. We consider
   systems which are expressible exactly in this form and the case when
   they are approximative models. In the latter case, we derive
   projections, that is, solutions which minimize the mean L-2 error. The
