Language = {{English}},
Affiliation = {{Naushath, MH (Reprint Author), Univ Manitoba, Dept Elect \& Comp Engn, Winnipeg, MB, Canada.
   Naushath, M. H.; Rajapakse, A. D., Univ Manitoba, Dept Elect \& Comp Engn, Winnipeg, MB, Canada.}},
ISBN = {{978-1-5090-1546-7}},
Keywords = {{Voltage Source Converters (VSC); IGBT switching loss; fitted switching
   loss model; empirical IGBT switching loss models}},
