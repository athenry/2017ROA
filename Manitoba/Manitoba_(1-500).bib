@inproceedings{ ISI:000390296800012,
Author = {Siddiqui, Sana and Khan, Muhammad Salman and Ferens, Ken and Kinsner,
   Witold},
Book-Group-Author = {{ACM}},
Title = {{Detecting Advanced Persistent Threats using Fractal Dimension based
   Machine Learning Classification}},
