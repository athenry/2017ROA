@article{ ISI:000365378700001,
Author = {Blunt, W. and Taillefer, M. and Gapes, D. and Sparling, R. and Levin, D.
   B. and Cicek, N.},
Title = {{EVALUATING SUPERSATURATION OF IN SITU PRODUCED GASES DURING THERMOPHILIC
   CELLOBIOSE FERMENTATION WITH CLOSTRIDIUM THERMOCELLUM}},
Journal = {{TRANSACTIONS OF THE ASABE}},
Year = {{2015}},
Volume = {{58}},
Number = {{5}},
Pages = {{1149-1157}},
Month = {{SEP-OCT}},
Abstract = {{In fermentation processes, it is typically assumed, but seldom
   justified, that the off-gas composition is an adequate indicator of the
   dissolved gas concentrations to which the microbes are exposed. The
   objective of this study was to measure dissolved gas concentrations and
   assess the impact of elevated H-2(aq) and CO2(aq) (due to mass transfer
   limitations) on carbon balances, electron balances, and metabolic flux
   during thermophlilic fermentation with Clostridium thermocellum. A
   membrane inlet mass spectrometer (MIMS) was used for direct on-line
   measurement of dissolved gases (H-2 and CO2) during fermentation of
   cellobiose in a bioreactor sparged with N-2 gas. Two conditions were
   evaluated: (1) 10 mL min(-1) N-2 sparging, 100 rpm mixing, no surface
   mixing, and a corresponding Reynolds number (Re) of 12,000; and (2) 100
   mL min(-1) N-2 sparging, 200 rpm mixing, surface mixing, and a
   corresponding Re of 23,000. When Re = 12,000, both H-2 and CO2 were
   supersaturated. At Re = 23,000, the H-2 supersaturation ratio decreased,
   while CO2 showed no significant supersaturation. This re-evaluation of
   the actual concentrations of H-2(aq) and CO2(aq) led to improvements in
   both the carbon recovery and the redox balance, and may help explain the
   distribution of soluble end-products observed. Major conclusions were
   that dissolved gas species (both H-2 and to a lesser extent CO2) tend to
   be underestimated when assumed to be at equilibrium with the gas phase
   even during varied hydrodynamic conditions, and elevated {[}H-2]((aq))
   and {[}CO2]((aq)) can impact carbon balances, redox balances, and
   metabolic flux, which is partly controlled by mass transfer at the
   liquid-to-gas interface.}},
