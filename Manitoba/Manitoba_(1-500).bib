@inproceedings{ ISI:000400778402059,
Author = {He, Yuanbin and Chung, Henry Shu-hung and Ho, Carl Ngai-man and Wu,
   Weimin},
Book-Group-Author = {{IEEE}},
Title = {{Current-Mode Boundary Controller with Reduced Number of Current Sensors
   for a Three-Phase Inverter}},
Booktitle = {{2016 IEEE ENERGY CONVERSION CONGRESS AND EXPOSITION (ECCE)}},
