@article{ ISI:000364603200002,
Author = {Zhou, H. and Xu, Y. and Zhang, C. and Jayas, D. S. and Zhou, X.},
Title = {{IMPLEMENTATION OF LOW-POWER WIRELESS SENSOR NETWORK PROTOCOL STACK FOR
   REAL-TIME MONITORING OF WAREHOUSES}},
Journal = {{APPLIED ENGINEERING IN AGRICULTURE}},
Year = {{2015}},
Volume = {{31}},
Number = {{5}},
Pages = {{697-706}},
Month = {{SEP}},
Abstract = {{With recent advances in technical standards of wireless sensor networks
   (WSNs) and semiconductor manufacturing technology, WSNs are used for
   large-scale industrial applications, especially for real-time monitoring
   of warehouses containing grains, medicines, tobaccos, and other
   products. To satisfy, these application demands while maintaining
   critical support for low-energy operation, a multiprocessor node
   hardware platform and a WSNs protocol stack based on IEEE 802.15.4 was
   proposed and developed Our design is characterized with low-power and
   rapid self-organization; and can enable the sensor nodes to awaken on
   radio (WOR) or wake themselves on time, the router nodes to sleep, and
   the gateway node to control the sensor nodes in real time. The protocol
   stack and hardware platform are compatible with commercial WSNs real
   time monitoring for a broad class of applications with low cost. One
   system with 4 gateway nodes and 85 sensor nodes has been successfully
   applied in warehouses of tobacco-alcoholization process storage in
   Chengdu since 15 January 2014. The almost full year-long test results
   indicated that networks could be established rapidly and the system
   could run stably with good scalability and high energy saving.}},
Publisher = {{AMER SOC AGRICULTURAL \& BIOLOGICAL ENGINEERS}},
Address = {{2950 NILES RD, ST JOSEPH, MI 49085-9659 USA}},
Type = {{Article}},
