Title = {{Maximization of signal-to-noise ratio in optical coherence tomography
   using a depth-dependent matched filter}},
Booktitle = {{PHOTONICS NORTH 2012}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8412}},
Note = {{Conference on Photonics North, Montreal, CANADA, JUN 06-08, 2012}},
