@article{ ISI:000367732700069,
Author = {He, Qiyang and Shi, Hao and Gu, Huaxiang and Naka, Gilda and Ding,
   Huaihai and Li, Xun and Zhang, Yu and Hu, Bo and Wang, Fei},
Title = {{Immobilization of Rhizopus oryzae LY6 onto Loofah Sponge as a Whole-Cell
   Biocatalyst for Biodiesel Production}},
Journal = {{BIORESOURCES}},
Year = {{2016}},
Volume = {{11}},
Number = {{1}},
Pages = {{850-860}},
Month = {{FEB}},
Abstract = {{Whole cell biocatalysts for biodiesel production have garnered
   significant attention in recent years, as they can help avoid the
   complex procedures of isolation, purification, and immobilization of
   extracellular lipase. Because of its renewability and biodegradability,
   loofah (Luffa cylindrica) sponge is an advantageous substitute for
   traditional biomass carriers in whole cell immobilization. Rhizopus
   oryzae mycelia can spontaneously attach onto loofah sponge particles
   (LSPs) during cell cultivation. The highest immobilized R. oryzae cells
   concentration can reach up to 1.40 g/1 g of LSPs. The effects of
   biocatalyst addition and water content on methanolysis for biodiesel
   production were investigated in this paper. The operational stability of
   glutaraldehyde-treated biocatalyst at 35 degrees C, using a 1: 1
   oil-to-methanol ratio, was assayed, revealing a 3.4-fold increase in
   half-life compared with the untreated biocatalyst. Under optimized
   conditions, the yield of methyl esters in the reaction mixture reached
   82.2\% to 92.2\% in each cycle. These results suggested that loofah
   sponge is a potential fungi carrier for an immobilized whole-cell
   biocatalyst.}},
Publisher = {{NORTH CAROLINA STATE UNIV DEPT WOOD \& PAPER SCI}},
Address = {{CAMPUS BOX 8005, RALEIGH, NC 27695-8005 USA}},
Type = {{Article}},
