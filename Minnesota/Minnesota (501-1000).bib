@article{ ISI:000375453600010,
Author = {Jena, Sampreeti and Suryanarayanan, Raj and Aksan, Alptekin},
Title = {{Mutual Influence of Mannitol and Trehalose on Crystallization Behavior
   in Frozen Solutions}},
