@article{ ISI:000368563000090,
Author = {Wang, Zaifei and Macosko, Christopher W. and Bates, Frank S.},
Title = {{Fluorine-Enriched Melt-Blown Fibers from Polymer Blends of Poly(butylene
   terephthalate) and a Fluorinated Multiblock Copolyester}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2016}},
Volume = {{8}},
Number = {{1}},
Pages = {{754-761}},
Month = {{JAN 13}},
Abstract = {{Melt-blown fibers (d(av) similar to 1 mu m) were produced from blends of
   poly(butylene terephthalate) (PBT) and a partially fluorinated random
   multiblock copolyester (PFCE) leading to enhanced hydrophobicity and
   even superhydrophobicity (static water contact angle = 157 +/- 3
   degrees) of the associated fiber mats. XPS measurements demonstrated
   quantitatively that the surface fluorine content increased
   systematically with the bulk loading of PFCE, rising to nearly 20 atom
   \%, which corresponds to 41 wt \% PFCE at a bulk loading of 10 wt \%.
   The PBT/PFCE fibers exhibit greater fluorine surface segregation than
   either melt-blown PBT/poly(ethylene-co-chlorotrifluoroethylene)
   (PBT/PECTFE) fibers or electrospun fibers obtained from blends of
   poly(styrene) and fluoroalkyl end-capped polystyrene (PS/PSCF). Dynamic
   contact angle measurements further demonstrated decreased surface
   adhesion energy of the melt-blown PBT/PFCE fiber mats due to the
   blooming of PFCE to the surface.}},
