@article{ ISI:000369774900075,
Author = {Sullivan, Mark M. and Bhan, Aditya},
Title = {{Acetone Hydrodeoxygenation over Bifunctional Metallic-Acidic Molybdenum
   Carbide Catalysts}},
Journal = {{ACS CATALYSIS}},
Year = {{2016}},
Volume = {{6}},
Number = {{2}},
Pages = {{1145-1152}},
Month = {{FEB}},
Abstract = {{Metallic-acidic bifunctionality of molybdenum carbidic catalytic
   formulations can be tuned by oxygen cofeed and reductive pretreatments
   to control carbonyl hydrodeoxygenation (HDO). Acetone is deoxygenated
   over activated Mo2C via sequential hydrogenation of acetone to
   equilibrium and subsequent dehydration of isopropyl alcohol (IPA).
   Dehydration to propylene occurs over Bronsted acid sites with an
   intrinsic activation energy of 103 +/- 1 kJ mol(-1) and a
   rate-determining step of beta-hydrogen scission, as inferred from a
   kinetic isotope effect (KIE) of 1.85. HDO rate-determining dehydration
   rates were kinetically independent of H-2 and oxygenate pressure from 10
   to 82 kPa and from 0.05 to 4 kPa, respectively. Both the kinetics and
   the deoxygenation reaction pathway were shown to be similar for the
   aldehyde group of propanal. Oxygen treatment of activated carbides via
   13.5 kPa O-2 cofeed was shown to decrease the catalyst surface area from
   68 to 9 m(2) g(-1) and to suppress metallic hydrogenation of acetone to
   IPA. IPA dehydration rates per gram of catalyst could be altered by a
   factor of similar to 60; a longer activation time under H-2 flow at 773
   K increased dehydration activation energies from 103 to 140 kJ mol(-1),
   decreased Bronsted acid site densities, and decreased
   2,6-di-tert-butylpyridine-normalized dehydration turnover frequencies,
   indicative of a reduction-induced change in the number and nature of the
   acid sites.}},
Publisher = {{AMER CHEMICAL SOC}},
Address = {{1155 16TH ST, NW, WASHINGTON, DC 20036 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Bhan, A (Reprint Author), Univ Minnesota Twin Cities, Dept Chem Engn \& Mat Sci, 421 Washington Ave SE, Minneapolis, MN 55455 USA.
   Sullivan, Mark M.; Bhan, Aditya, Univ Minnesota Twin Cities, Dept Chem Engn \& Mat Sci, 421 Washington Ave SE, Minneapolis, MN 55455 USA.}},
DOI = {{10.1021/acscatal.5b02656}},
ISSN = {{2155-5435}},
Keywords = {{biomass; hydrodeoxygenation; bifunctional catalysis; molybdenum carbide;
