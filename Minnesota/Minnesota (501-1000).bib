@inproceedings{ ISI:000402739900009,
Author = {Li, Bingzhe and Najafi, M. Hassan and Lilja, David J.},
Book-Group-Author = {{ACM}},
Title = {{Using Stochastic Computing to Reduce the Hardware Requirements for a
   Restricted Boltzmann Machine Classifier}},
Booktitle = {{PROCEEDINGS OF THE 2016 ACM/SIGDA INTERNATIONAL SYMPOSIUM ON
   FIELD-PROGRAMMABLE GATE ARRAYS (FPGA'16)}},
