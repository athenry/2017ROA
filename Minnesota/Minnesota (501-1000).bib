@inproceedings{ ISI:000400048104009,
Author = {Farooq, Hamza and Chen, Yongxin and Georgiou, Tryphon T. and Lenglet,
   Christophe},
Book-Group-Author = {{IEEE}},
Title = {{Some geometric ideas for feature enhancement of diffusion tensor fields}},
Booktitle = {{2016 IEEE 55TH CONFERENCE ON DECISION AND CONTROL (CDC)}},
