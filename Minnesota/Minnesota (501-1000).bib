@inproceedings{ ISI:000401795900060,
Author = {Zhao, Wenfeng and Sun, Biao and Wu, Tong and Yang, Zhi},
Book-Group-Author = {{IEEE}},
Title = {{Hardware Efficient, Deterministic QCAC Matrix Based Compressed Sensing
   Encoder Architecture for Wireless Neural Recording Application}},
Booktitle = {{PROCEEDINGS OF 2016 IEEE BIOMEDICAL CIRCUITS AND SYSTEMS CONFERENCE
   (BIOCAS)}},
Series = {{Biomedical Circuits and Systems Conference}},
