@inproceedings{ ISI:000406057400029,
Author = {Liu, Yin and Parhi, Megha and Riedel, Marc D. and Parhi, Keshab K.},
Editor = {{Matthews, MB}},
Title = {{Synthesis of Correlated Bit Streams for Stochastic Computing}},
Booktitle = {{2016 50TH ASILOMAR CONFERENCE ON SIGNALS, SYSTEMS AND COMPUTERS}},
Series = {{Conference Record of the Asilomar Conference on Signals Systems and
