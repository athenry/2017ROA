@article{ ISI:000359375700046,
Author = {Wu, Kai and Liu, Jinming and Wang, Yi and Ye, Clark and Feng, Yinglong
   and Wang, Jian-Ping},
Title = {{Superparamagnetic nanoparticle-based viscosity test}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{107}},
Number = {{5}},
Month = {{AUG 3}},
Abstract = {{Hyperviscosity syndrome is triggered by high blood viscosity in the
   human body. This syndrome can result in retinopathy, vertigo, coma, and
   other unanticipated complications. Serum viscosity is one of the
   important factors affecting whole blood viscosity, which is regarded as
   an indicator of general health. In this letter, we propose and
   demonstrate a Brownian relaxation-based mixing frequency method to test
   human serum viscosity. This method uses excitatory and detection coils
   and Brownian relaxation-dominated superparamagnetic nanoparticles, which
   are sensitive to variables of the liquid environment such as viscosity
   and temperature. We collect the harmonic signals produced by magnetic
   nanoparticles and estimate the viscosity of unknown solutions by
   comparison to the calibration curves. An in vitro human serum viscosity
   test is performed in less than 1.5 min. (C) 2015 AIP Publishing LLC.}},
