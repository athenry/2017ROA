@article{ ISI:000354339000009,
Author = {Xu, Zhihua and Gao, Feng and Malcarova, Elena A. and Heikal, Ahmed A.
   and Nemykin, Victor N.},
Title = {{Energy Transfer from Colloidal Quantum Dots to Near-Infrared-Absorbing
