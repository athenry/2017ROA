@article{ ISI:000356531500001,
Author = {Qu, T. and Victora, R. H.},
Title = {{Phase-Lock Requirements in a Serial Array of Spin Transfer
   Nano-Oscillators}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2015}},
Volume = {{5}},
Month = {{JUN 18}},
Abstract = {{The most promising approach to attain a narrow linewidth and a large
   output power simultaneously in spin torque oscillators is
   self-phase-locking of an array of oscillators. Two long range coupling
   mechanisms, magnetostatic interaction and self-induced current, are
   explored. Synchronization occurs with MR ratio similar to 14\% and
   volume similar to 2.1 x 10(-5) mu m(3) at room temperature for an
   experimental frequency dispersion, when only the self-induced microwave
   current is present. The dipole interaction decreases the MR ratio
   requirement when the elements are properly spaced.}},
