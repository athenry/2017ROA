Author = {He, Li and Chen, Jun-Yang and Wang, Jian-Ping and Li, Mo},
Title = {{All-optical switching of magnetoresistive devices using telecom-band
   femtosecond laser}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{107}},
Number = {{10}},
Month = {{SEP 7}},
Abstract = {{Ultrafast all-optical switching of the magnetization of various magnetic
   systems is an intriguing phenomenon that can have tremendous impact on
   information storage and processing. Here, we demonstrate all-optical
   switching of GdFeCo alloy films using a telecom-band femtosecond fiber
   laser. We further fabricate Hall cross devices and electrically readout
   all-optical switching by measuring anomalous Hall voltage changes. The
   use of a telecom laser and the demonstrated all-optical switching of
   magnetoresistive devices represent the first step toward integration of
   opto-magnetic devices with mainstream photonic devices to enable novel
   optical and spintronic functionalities. (C) 2015 AIP Publishing LLC.}},
