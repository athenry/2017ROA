@article{ ISI:000353931200034,
Author = {Jogwar, Sujit S. and Daoutidis, Prodromos},
Title = {{Dynamic Characteristics of Energy-Integrated Batch Process Systems:
   Insights from Two Case Studies}},
Journal = {{INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH}},
Year = {{2015}},
Volume = {{54}},
Number = {{16}},
Pages = {{4326-4336}},
Month = {{APR 29}},
Abstract = {{This paper assesses the impact of energy integration on the dynamics of
   batch process systems. Using a prototype example of an integrated batch
   reactor, it is shown that such systems give rise to network-level
   dynamics-a characteristic feature of integrated process networks. The
   network dynamics is nonlinear, evolves at a slower time scale, and is
   more pronounced in the limit of tight energy integration. Furthermore,
   it is illustrated that these dynamic characteristics are independent of
   the (direct or indirect) mode of energy integtation. Lastly, the
   significance of this network dynamics on the optimal operatitin of suth
   Systems is also demonstrated.}},
