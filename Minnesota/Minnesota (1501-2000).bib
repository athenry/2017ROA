@article{ ISI:000361074500019,
Author = {Ludwig, Michael and Berrier, Seth and Tetzlaff, Michael and Meyer, Gary},
Title = {{3D shape and texture morphing using 2D projection and reconstruction}},
Journal = {{COMPUTERS \& GRAPHICS-UK}},
Year = {{2015}},
Volume = {{51}},
Number = {{SI}},
Pages = {{146-156}},
Month = {{OCT}},
Note = {{Shape Modeling International Conference (SMI 2015), Lille, FRANCE, JUN
   24-26, 2015}},
Abstract = {{We present an algorithm for morphing shape and view-dependent texture
   that utilizes an unstructured lumigraph representation to (1) gracefully
   handle topological changes, (2) create plausible appearance
   transformations, and (3) provide user control independent from the
   underlying mesh structure. It works by reducing the otherwise complex
   problem of a 3D shape and material morph into multiple simpler 2D
   morphs. The shape morph operates on 2D depth maps and then reconstructs
   a conventional mesh, making it insensitive to the input geometry
   quality. Our morphing algorithm robustly handles topology changes and
