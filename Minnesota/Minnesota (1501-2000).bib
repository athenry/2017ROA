@article{ ISI:000364098900027,
Author = {Zhang, S. and Sobota, A. and van Veldhuizen, E. M. and Bruggeman, P. J.},
Title = {{Temporally resolved ozone distribution of a time modulated RF
   atmospheric pressure argon plasma jet: flow, chemical reaction, and
   transient vortex}},
Journal = {{PLASMA SOURCES SCIENCE \& TECHNOLOGY}},
Year = {{2015}},
Volume = {{24}},
Number = {{4}},
Month = {{AUG}},
Abstract = {{The ozone density distribution in the effluent of a time modulated RF
   atmospheric pressure plasma jet (APPJ) is investigated by time and
   spatially resolved by UV absorption spectroscopy. The plasma jet is
   operated with an averaged dissipated power of 6.5 W and gas flow rate 2
   slm argon +2\% O-2. The modulation frequency of the RF power is 50 Hz
   with a duty cycle of 50\%. To investigate the production and destruction
   mechanism of ozone in the plasma effluent, the atomic oxygen and gas
   temperature is also obtained by TALIF and Rayleigh scattering,
   respectively. A temporal increase in ozone density is observed close to
   the quartz tube exit when the plasma is switched off due to the decrease
   in O density and gas temperature. Ozone absorption at different axial
   positions indicates that the ozone distribution is dominated by the
   convection induced by the gas flow and allows estimating the on-axis
   local gas velocity in the jet effluent. Transient vortex structures
   occurring during the switch on and off of the RF power also
   significantly affect the ozone density in the far effluent.}},
