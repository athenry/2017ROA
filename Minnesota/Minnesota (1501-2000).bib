@article{ ISI:000360293400018,
Author = {Kruziki, Max A. and Bhatnagar, Sumit and Woldring, Daniel R. and Duong,
   Vandon T. and Hackel, Benjamin J.},
Title = {{A 45-Amino-Acid Scaffold Mined from the PDB for High-Affinity Ligand
   Engineering}},
Journal = {{CHEMISTRY \& BIOLOGY}},
Year = {{2015}},
Volume = {{22}},
Number = {{7}},
Pages = {{946-956}},
Month = {{JUL 23}},
Abstract = {{Small protein ligands can provide superior physiological distribution
   compared with antibodies, and improved stability, production, and
   specific conjugation. Systematic evaluation of the PDB identified a
   scaffold to push the limits of small size and robust evolution of
   stable, high-affinity ligands: 45-residue T7 phage gene 2 protein (Gp2)
   contains an alpha helix opposite a beta sheet with two adjacent loops
   amenable to mutation. De novo ligand discovery from 10 8 mutants and
   directed evolution toward four targets yielded target-specific binders
   with affinities as strong as 200 +/- 100 pM, T(m)s from 65 degrees C +/-
   3 degrees C to 80 degrees C +/- 1 degrees C, and retained activity after
   thermal denaturation. For cancer targeting, a Gp2 domain for epidermal
   growth factor receptor was evolved with 18 +/- 8 nM affinity,
   receptor-specific binding, and high thermal stability with refolding.
   The efficiency of evolving new binding function and the size, affinity,
   specificity, and stability of evolved domains render Gp2 a uniquely
   effective ligand scaffold.}},
