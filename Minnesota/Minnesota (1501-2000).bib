@article{ ISI:000356123400015,
Author = {Sun, Changquan Calvin},
Title = {{Dependence of ejection force on tableting speed-A compaction simulation
   study}},
Journal = {{POWDER TECHNOLOGY}},
Year = {{2015}},
Volume = {{279}},
Pages = {{123-126}},
Month = {{JUL}},
Abstract = {{Using a compaction simulator, it has been shown that tablet ejection
   force increases with increasing tableting speed during compression of
   powders lubricated with 1\% magnesium stearate. Faster tableting speed
   corresponds to higher coefficient of friction, A but does not affect
   residual radial die wall (RDW) stress. The higher mu suggests the
   presence of less lubricant at the tablet-die wall interface due to the
   shorter time available for migration of lubricant to the interface. This
   mechanism can also explain the sudden occurrence of powder sticking
   problem upon formulation scale up. The elevated ejection force due to
   higher mu also results in higher RDW stress at the moment of slip
   initiation, which proportionally increases ejection force. (C) 2015
