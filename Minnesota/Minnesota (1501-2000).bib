@article{ ISI:000354906000060,
Author = {Kumar, Anshuman and Low, Tony and Fung, Kin Hung and Avouris, Phaedon
   and Fang, Nicholas X.},
Title = {{Tunable Light-Matter Interaction and the Role of Hyperbolicity in
   Graphene-hBN System}},
Journal = {{NANO LETTERS}},
Year = {{2015}},
Volume = {{15}},
Number = {{5}},
Pages = {{3172-3180}},
Month = {{MAY}},
Abstract = {{Hexagonal boron nitride (hBN) is a natural hyperbolic material, which
   can also accommodate highly dispersive surface phonon-polariton modes.
   In this paper, we examine theoretically the mid-infrared optical
   properties of graphenehBN heterostructures derived from their coupled
   plasmonphonon modes. We find that the graphene plasmon couples
   differently with the phonons of the two Reststrahlen bands, owing to
   their different hyperbolicity. This also leads to distinctively
   different interaction between an external quantum emitter and the
   plasmonphonon modes in the two bands, leading to substantial
   modification of its spectrum. The coupling to graphene plasmons allows
   for additional gate tunability in the Purcell factor and narrow dips in
   its emission spectra.}},
