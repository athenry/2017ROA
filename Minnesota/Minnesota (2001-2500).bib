@inproceedings{ ISI:000370259200085,
Author = {Zare, Armin and Jovanovic, Mihailo R. and Georgiou, Tryphon T.},
Book-Group-Author = {{IEEE}},
Title = {{Alternating direction optimization algorithms for covariance completion
   problems}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2015}},
Pages = {{515-520}},
Note = {{American Control Conference, Chicago, IL, JUL 01-03, 2015}},
