@inproceedings{ ISI:000380573400075,
Author = {Yuan, Xiang and Wu, Chenggang and Wang, Zhenjiang and Li, Jianjun and
   Yew, Pen-Chung and Huang, Jeff and Feng, Xiaobing and Lan, Yanyan and
   Chen, Yunji and Guan, Yong},
