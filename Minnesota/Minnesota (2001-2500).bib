Title = {{Executing Model-based Tests on Platform-specific Implementations}},
Booktitle = {{2015 30TH IEEE/ACM INTERNATIONAL CONFERENCE ON AUTOMATED SOFTWARE
   ENGINEERING (ASE)}},
