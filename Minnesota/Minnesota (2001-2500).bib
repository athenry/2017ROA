@article{ ISI:000352138900017,
Author = {Chen, Yi-Chao and Fosdick, Roger and Fried, Eliot},
Title = {{Representation for a Smooth Isometric Mapping from a Connected Planar
   Domain to a Surface}},
Journal = {{JOURNAL OF ELASTICITY}},
Year = {{2015}},
Volume = {{119}},
Number = {{1-2}},
Pages = {{335-350}},
Month = {{APR}},
Abstract = {{A representation theorem for a smooth isometric mapping of a flat,
   connected domain in two-dimensional Euclidean point space into a surface
   in three-dimensional Euclidean point space is presented. The form of the
   mapping is shown to be necessary and sufficient to describe any such
   smooth isometry. Importantly, this work is not based upon the hypothesis
   that the mapped surface is ruled. In general, a mapping from a flat
   planar domain into a ruled surface is far from being isometric, and the
   property of being ruled is a partial consequence of our representation
   theorem.}},
