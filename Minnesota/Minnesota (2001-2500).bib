@article{ ISI:000345895200046,
Author = {Bui, Minh-Phuong N. and Abbas, Abdennour},
Title = {{Simple and rapid colorimetric detection of p-nitrophenyl substituent
   organophosphorous nerve agents}},
Journal = {{SENSORS AND ACTUATORS B-CHEMICAL}},
Year = {{2015}},
Volume = {{207}},
Number = {{A}},
Pages = {{370-374}},
Month = {{FEB}},
Abstract = {{We report a very simple and rapid chemical method for the detection of
   paraoxon and its p-nitrophenyl organophosphorous derivatives. The method
   uses only sodium borohydride followed by thermal enhancement that
   increases the sensitivity by two orders of magnitude. Visible, yellow
   color change of the neurotoxins caused by p-nitrophenolate ions can be
