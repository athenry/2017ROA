@article{ ISI:000368341900016,
Author = {Moriasi, D. N. and Zeckoski, R. W. and Arnold, J. G. and Baffaut, C. B.
   and Malone, R. W. and Daggupati, P. and Guzman, J. A. and Saraswat, D.
   and Yuan, Y. and Wilson, B. W. and Shirmohammadi, A. and Douglas-Mankin,
   K. R.},
