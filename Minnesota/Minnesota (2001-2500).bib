@article{ ISI:000355241700011,
Author = {Gao, Xiang and Jiang, Bo and Tao, Shaozhe},
Title = {{RECOVERING LOW CP/TUCKER RANKED TENSORS, WITH APPLICATIONS IN TENSOR
   COMPLETION}},
Journal = {{PACIFIC JOURNAL OF OPTIMIZATION}},
Year = {{2015}},
Volume = {{11}},
Number = {{2, SI}},
Pages = {{385-402}},
Month = {{APR}},
Abstract = {{In this paper, we propose a method to find tensor decompositions with
   low CP rank. In our approach, no prior knowledge is assumed regarding
   the rank of the tensor to be recovered. The key underlying idea is to
   take advantage of group sparsity structure. The L-1,L-2-norm
   regularization and the L-1,L-infinity-norm regularization are proposed
   to achieve the group sparse effect. Then the problem reduce to the
   multi-block optimization, which can be efficiently solved by the
   so-called Block Coordinate Decent (BCD) method. We also show that our
   approach can be used to solve the tensor low CP/Tucker rank completion
   problem. Simulation results demonstrate that our new method performs
   well numerically, especially when the tensor to be recovered indeed has
   a low rank structure.}},
