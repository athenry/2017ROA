@article{ ISI:000346792500027,
Author = {Bhattacharyya, Susmita and Gebre-Egziabher, Demoz},
Title = {{Integrity Monitoring With Vector GNSS Receivers}},
Journal = {{IEEE TRANSACTIONS ON AEROSPACE AND ELECTRONIC SYSTEMS}},
Year = {{2014}},
Volume = {{50}},
Number = {{4}},
Pages = {{2779-2793}},
Month = {{OCT}},
Abstract = {{A receiver autonomous integrity monitoring (RAIM) algorithm for vector
   tracking loop receivers is developed. It is shown that vector RAIM is
   not a simple remapping of ideas from scalar loop RAIM. Notably, it is
   shown that pseudorange residuals are not ideal for generating the test
   statistic in a vector receiver. Also, since vector receivers with
   integrity monitoring may use a recursive, multirate filter to generate
   the navigation solution, the associated vector RAIM algorithm has to
   accommodate multirate estimators. Simulation results are used to show
   that the designed vector RAIM algorithm provides comparable performance
   as that of its scalar counterpart.}},
