@article{ ISI:000343835500009,
Author = {Perlmutter, Jason D. and Popot, Jean-Luc and Sachs, Jonathan N.},
Title = {{Molecular Dynamics Simulations of a Membrane Protein/Amphipol Complex}},
Journal = {{JOURNAL OF MEMBRANE BIOLOGY}},
Year = {{2014}},
Volume = {{247}},
Number = {{9-10, SI}},
Pages = {{883-895}},
Month = {{OCT}},
Abstract = {{Amphipathic polymers known as ``amphipols{''} provide a highly
   stabilizing environment for handling membrane proteins in aqueous
   solutions. A8-35, an amphipol with a polyacrylate backbone and
   hydrophobic grafts, has been extensively characterized and widely
   employed for structural and functional studies of membrane proteins
   using biochemical and biophysical approaches. Given the sensitivity of
   membrane proteins to their environment, it is important to examine what
   effects amphipols may have on the structure and dynamics of the proteins
   they complex. Here we present the first molecular dynamics study of an
   amphipol-stabilized membrane protein, using Escherichia coli OmpX as a
   model. We begin by describing the structure of the complexes formed by
   supplementing OmpX with increasing amounts of A8-35, in order to
   determine how the amphipol interacts with the transmembrane and
   extramembrane surfaces of the protein. We then compare the dynamics of
   the protein in either A8-35, a detergent, or a lipid bilayer. We find
   that protein dynamics on all accessible length scales is restrained by
   A8-35, which provides a basis to understanding some of the stabilizing
   and functional effects of amphipols that have been experimentally
   observed.}},
