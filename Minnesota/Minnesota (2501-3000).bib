@article{ ISI:000346252500003,
Author = {Yu, Lina and Liu, Jinming and Wu, Kai and Klein, Todd and Jiang, Yong
   and Wang, Jian-Ping},
Title = {{Evaluation of Hyperthermia of Magnetic Nanoparticles by Dehydrating DNA}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2014}},
Volume = {{4}},
Month = {{NOV 27}},
Abstract = {{A method based on the thermodynamic equilibrium reached between the
   hybridization and denaturation of double-stranded DNA (ds-DNA) is opened
   up to evaluate the hyperthermia performance of magnetic nanoparticles
   (MNPs). Two kinds of MNPs with different sizes and magnetic performance
   are chosen, and their temperature increments at the surface area under
   an alternating magnetic field (AMF) are calculated and compared through
   the concentration variation of ds-DNA modified on the surface. The
   temperature difference between the surface area of MNPs and bulk
   solution is also investigated, which can reach as high as 57.86 degrees
   C when AMF applied for 300 s. This method provides a direct path way of
   comparison hyperthermia ability of MNPs, and serves as a good reference
   to choose MNPs and decides the therapy parameters based on the unique
   drug response of individual patient.}},
