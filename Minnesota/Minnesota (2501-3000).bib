@article{ ISI:000344117700007,
Author = {Beams, Ryan and Cancado, Luiz Gustavo and Oh, Sang-Hyun and Jorio, Ado
   and Novotny, Lukas},
Title = {{Spatial Coherence in Near-Field Raman Scattering}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2014}},
Volume = {{113}},
Number = {{18}},
Month = {{OCT 28}},
Abstract = {{Inelastic light scattering in crystals has historically been treated as
   a spatially incoherent process, giving rise to incoherent optical
   radiation. Here we demonstrate that Raman scattering can be spatially
   coherent, in which case it depends on the dimensionality and symmetry of
   the scatterer. Using near-field spectroscopy, we measure a correlation
   length of similar to 30 nm for the optical phonons in graphene, the
   results varying with vibrational symmetries and spatial confinement of
   the phonons.}},
