@article{ ISI:000346724100089,
Author = {Li, Yulong and Porter, Warren M. and Kshirsagar, Chaitanya and Roth,
   Ivan and Su, Yang and Reynolds, Margaret A. and Gerbi, Bruce J. and
   Koester, Steven J.},
Title = {{Fully-Depleted Silicon-on-Insulator Devices for Radiation Dosimetry in
   Cancer Therapy}},
Journal = {{IEEE TRANSACTIONS ON NUCLEAR SCIENCE}},
Year = {{2014}},
Volume = {{61}},
Number = {{6, 1}},
Pages = {{3443-3450}},
Month = {{DEC}},
Note = {{51st Annual IEEE International Nuclearand Space Radiation Effects
   Conference (NSREC), Paris, FRANCE, JUL 14-18, 2014}},
Organization = {{IEEE}},
Abstract = {{The potential of using fully-depleted silicon-on-insulator (FDSOI)
   devices as dosimeters for radiation cancer therapy is investigated. The
   response of long-channel FDSOIMOSFETs to radiation from a linear
   accelerator under various clinical conditions has been measured. Linear
   response of the device threshold voltage, Vth, is observed for typical
   interventional energy X-rays up to absorbed doses as high as 160 Gy. The
   device response is observed to be strongly influenced by the application
   of a substrate bias, V-bg, with sensitivity improvements of up to 20x
   observed for V-bg = +5 V compared to V-bg = 0. The results have been
   used to calibrate numerical simulations that show the design space for
   using FDSOI variable capacitors (varactors) as passive wireless
   radiation dosimeters suitable for in vivo implantation. Simulation
   studies show that a design space exists to realize wireless sensors with
   both high quality factor (Q > 20 at 1 GHz) and good sensitivity (Delta
   f/f = 0.12 \%/Gy). These results indicate the promise of FDSOI to
   realize a new, convenient class of in vivo dosimeters for radiation
   therapy.}},
Publisher = {{IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC}},
Address = {{445 HOES LANE, PISCATAWAY, NJ 08855-4141 USA}},
Type = {{Article; Proceedings Paper}},
Language = {{English}},
Affiliation = {{Li, YL (Reprint Author), Univ Minnesota, Dept Elect \& Comp Engn, Minneapolis, MN 55455 USA.
