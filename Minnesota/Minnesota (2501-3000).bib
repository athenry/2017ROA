@article{ ISI:000346908800013,
Author = {Powell, Nathaniel J. and Jang, Albert and Park, Jang-Yeon and Valette,
   Julien and Garwood, Michael and Marjanska, Malgorzata},
Title = {{Gradient Rotating Outer Volume Excitation (GROOVE): A Novel Method for
