@inproceedings{ ISI:000391061600004,
Author = {Varduhn, Vasco and Mundani, Ralf-Peter and Rank, Ernst},
Editor = {{Breunig, M and AlDoori, M and Butwilowski, E and Kuper, PV and Benner, J and Haefele, KH}},
