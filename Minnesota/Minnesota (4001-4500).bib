@article{ ISI:000332272300009,
Author = {Gazquez, J. and Bose, Shameek and Sharma, M. and Torija, M. A. and
   Pennycook, S. J. and Leighton, C. and Varela, M.},
Title = {{Lattice mismatch accommodation via oxygen vacancy ordering in epitaxial
   La0.5Sr0.5CoO3-delta thin films}},
Journal = {{APL MATERIALS}},
Year = {{2013}},
Volume = {{1}},
Number = {{1}},
Month = {{JUL}},
Abstract = {{The properties of complex oxide films depend sensitively on epitaxial
   strain. This strain affects bond lengths and angles, and defect types
   and densities, thus impacting physical properties. In this work we
   perform detailed characterization of depth-dependent strain in epitaxial
   La0.5Sr0.5CoO3-delta (LSCO) films on SrTiO3(001), SrTiO3(110), and
   LaAlO3(001) substrates, combining high resolution x-ray diffraction and
   scanning transmission electron microscopy, in addition to geometric
