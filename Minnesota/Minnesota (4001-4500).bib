@article{ ISI:000317014300003,
Author = {Kim, Se Hyun and Hong, Kihyon and Xie, Wei and Lee, Keun Hyung and
   Zhang, Sipei and Lodge, Timothy P. and Frisbie, C. Daniel},
Title = {{Electrolyte-Gated Transistors for Organic and Printed Electronics}},
Journal = {{ADVANCED MATERIALS}},
Year = {{2013}},
Volume = {{25}},
Number = {{13, SI}},
Pages = {{1822-1846}},
Month = {{APR 4}},
Abstract = {{Here we summarize recent progress in the development of
   electrolyte-gated transistors (EGTs) for organic and printed
   electronics. EGTs employ a high capacitance electrolyte as the gate
   insulator; the high capacitance increases drive current, lowers
   operating voltages, and enables new transistor architectures. Although
   the use of electrolytes in electronics is an old concept going back to
   the early days of the silicon transistor, new printable, fast-response
   polymer electrolytes are expanding the potential applications of EGTs in
   flexible, printed digital circuits, rollable displays, and conformal
   bioelectronic sensors. This report introduces the structure and
   operation mechanisms of EGTs and reviews key developments in electrolyte
   materials for use in printed electronics. The bulk of the article is
   devoted to electrical characterization of EGTs and emerging
   applications.}},
