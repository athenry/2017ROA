@article{ ISI:000322723000097,
Author = {Zou, Yunlong and Holmes, Russell J.},
Title = {{Influence of a MoOx interlayer on the open-circuit voltage in organic
   photovoltaic cells}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{103}},
Number = {{5}},
Month = {{JUL 29}},
Abstract = {{Metal-oxides have been used as interlayers at the anode-organic
   interface in organic photovoltaic cells (OPVs) to increase the
   open-circuit voltage (V-OC). We examine the role of MoOx in determining
   the maximum V-OC in a planar heterojunction OPV and find that the
   interlayer strongly affects the temperature dependence of V-OC. Boron
   subphthalocyanine chloride (SubPc)-C-60 OPVs that contain no interlayer
   show a maximum V-OC of 1.2V at low temperature, while those with MoOx
   show no saturation, reaching V-OC > 1.4 V. We propose that the
   MoOx-SubPc interface forms a Schottky junction that provides an
   additional contribution to V-OC at low temperature. (C) 2013 AIP
   Publishing LLC.}},
