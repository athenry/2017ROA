@article{ ISI:000316262700036,
Author = {Fang, Jianxin and Sapatnekar, Sachin S.},
Title = {{The Impact of BTI Variations on Timing in Digital Logic Circuits}},
Journal = {{IEEE TRANSACTIONS ON DEVICE AND MATERIALS RELIABILITY}},
Year = {{2013}},
Volume = {{13}},
Number = {{1}},
Pages = {{277-286}},
Month = {{MAR}},
Abstract = {{A new framework for analyzing the impact of bias temperature instability
   (BTI) variations on timing in large-scale digital logic circuits is
   proposed in this paper. This approach incorporates both the
   reaction-diffusion model and the charge-trapping model for BTI and
   embeds these into a temporal statistical static timing analysis
   framework capturing process variations and path correlations.
   Experimental results on 32-, 22-, and 16-nm technology models, which
   were verified through Monte Carlo simulation, confirm that the proposed
   approach is fast, accurate, and scalable and indicate that BTI
   variations make a significant contribution to circuit-level timing
   variations.}},
