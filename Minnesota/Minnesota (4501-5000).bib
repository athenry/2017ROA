@inproceedings{ ISI:000327210204104,
Author = {Yoon, Yongsoon and Sun, Zongxuan and Zhang, Shupeng and Zhu, Guoming G.},
Book-Group-Author = {{IEEE}},
Title = {{Development of Control-oriented Charge Mixing Model and Experimental
   Validation Using Graphical Analysis}},
Booktitle = {{2013 AMERICAN CONTROL CONFERENCE (ACC)}},
