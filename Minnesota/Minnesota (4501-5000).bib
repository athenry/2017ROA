Title = {{The influence of metallic and plastic vapors on dielectric breakdown for
   low-voltage circuit breaker after current-zero}},
Booktitle = {{PROCEEDINGS OF 2013 IEEE 59TH HOLM CONFERENCE ON ELECTRICAL CONTACTS
   (HOLM)}},
Series = {{Electrical Contacts-IEEE Holm Conference on Electrical Contacts}},
