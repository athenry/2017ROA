@article{ ISI:000311967000077,
Author = {Menke, S. Matthew and Pandey, Richa and Holmes, Russell J.},
Title = {{Tandem organic photodetectors with tunable, broadband response}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2012}},
Volume = {{101}},
Number = {{22}},
Month = {{NOV 26}},
Abstract = {{Broadband photodetection is achieved by integrating three electron donor
   materials with complementary absorption into an organic photodetector
   (OPD). While a single donor-acceptor heterojunction can show broadband
   response, the spectral tunability is intrinsically limited to the
   absorption profiles of the active materials. Here, we demonstrate
   broadband OPDs consisting of multiple bulk heterojunctions arranged in
   tandem. These OPDs show high responsivity under moderate reverse bias
   from the ultraviolet to the near-infrared. By combining materials with
   complementary absorption in a tandem OPD, we demonstrate that the
   response from each band can be separately tuned with manipulation of the
   heterojunction thicknesses or composition. (C) 2012 American Institute
   of Physics. {[}http://dx.doi.org/10.1063/1.4768807]}},
