@inproceedings{ ISI:000331259200092,
Author = {Xu, Tingting and Stephane, Massoud and Parhi, Keshab K.},
Book-Group-Author = {{IEEE}},
Title = {{Classification of Single-Trial MEG during Sentence Processing for
   Automated Schizophrenia Screening}},
