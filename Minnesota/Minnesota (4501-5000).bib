@inproceedings{ ISI:000341772900046,
Author = {Roychowdhury, Sohini and Koozekanani, Dara D. and Parhi, Keshab K.},
Editor = {{Matthews, MB}},
Title = {{Automated Denoising and Segmentation of Optical Coherence Tomography
   Images}},
Booktitle = {{2013 ASILOMAR CONFERENCE ON SIGNALS, SYSTEMS AND COMPUTERS}},
