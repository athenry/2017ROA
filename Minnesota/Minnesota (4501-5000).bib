@article{ ISI:000314558000003,
Author = {Aggarwal, Roshan L. and Farrar, Lewis W. and Greeneltch, Nathan G. and
   Van Duyne, Richard P. and Polla, Dennis L.},
Title = {{Measurement of the Surface-Enhanced Coherent Anti-Stokes Raman
   Scattering (SECARS) Due to the 1574 cm(-1) Surface-Enhanced Raman
   Scattering (SERS) Mode of Benzenethiol Using Low-Power (< 20 mW) CW
   Diode Lasers}},
Journal = {{APPLIED SPECTROSCOPY}},
Year = {{2013}},
Volume = {{67}},
Number = {{2}},
Pages = {{132-135}},
Month = {{FEB}},
Abstract = {{The surface-enhanced coherent anti-Stokes Raman scattering (SECARS) from
   a self-assembled monolayer (SAM) of benzenethiol on a silver-coated
   surface-enhanced Raman scattering (SERS) substrate has been measured for
   the 1574 cm(-1) SERS mode. A value of 9.6 +/- 1.7 x 10(-14) W was
   determined for the resonant component of the SECARS signal using 17.8 mW
   of 784.9 nm pump laser power and 7.1 mW of 895.5 nm Stokes laser power;
   the pump and Stokes lasers were polarized parallel to each other but
   perpendicular to the grooves of the diffraction grating in the
   spectrometer. The measured value of resonant component of the SECARS
   signal is in agreement with the calculated value of 9.3 x 10(-14) W
   using the measured value of 8.7 +/- 0.5 cm(-1) for the SERS linewidth
   Gamma (full width at half-maximum) and the value of 5.7 +/- 1.4 x 10(-7)
   for the product of the Raman cross section sigma(SERS) and the surface
   concentration N-s of the benzenethiol SAM. The chi chi chi chi component
   of the resonant part of the third-order nonlinear optical susceptibility
   vertical bar 3 chi((3)R)(chi chi chi chi)vertical bar for the 1574
   cm(-1) SERS mode has been determined to be 4.3 +/- 1.1 x 10(-5)
   cm.g(-1).s(2). The SERS enhancement factor for the 1574 cm(-1) mode was
   determined to be 3.6 +/- 0.9 X 10(7) using the value of 1.8 x 10(15)
   molecules/cm(2) for N-s.}},
