Title = {{Automating Document Annotation Using Open Source Knowledge}},
Booktitle = {{2013 IEEE/WIC/ACM INTERNATIONAL JOINT CONFERENCES ON WEB INTELLIGENCE
   (WI) AND INTELLIGENT AGENT TECHNOLOGIES (IAT), VOL 1}},
