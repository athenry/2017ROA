@article{ ISI:000314191100118,
Author = {Gorman, J. M. and Sparrow, E. M. and Abraham, J. P. and Mowry, G. S.},
Title = {{Operating characteristics and fabrication of a uniquely compact helical
   heat exchanger}},
Journal = {{APPLIED THERMAL ENGINEERING}},
Year = {{2013}},
Volume = {{50}},
Number = {{1}},
Pages = {{1070-1075}},
Month = {{JAN 10}},
Abstract = {{A two-fluid heat exchanger of unusual compactness and high thermal
   performance is set forth in this paper. The underlying principle of the
   exchanger is a very tight helical intertwining of two small-diameter
   tubes (e.g., hypodermic tubes) such that tubes mutually envelope each
   other. A high-conductivity thermal bridge between the tubes is assured
   by the use of a braze material insinuated into interstices created by
   the helical windings. Heat transfer and pressure drop experiments
   carried out to validate the operational performance of the device are
   described. The uniqueness of the device has motivated a description of
   both the underlying design issues and fabrication processes. It is shown
   that the device can be manufactured by means of remarkably simple
   operations and machine tools. (C) 2012 Elsevier Ltd. All rights
   reserved.}},
