@inproceedings{ ISI:000329611504124,
Author = {Mehanna, Omar and Sidiropoulos, Nicholas D.},
Book-Group-Author = {{IEEE}},
Title = {{ADAPTIVE THRESHOLDING FOR DISTRIBUTED POWER SPECTRUM SENSING}},
Booktitle = {{2013 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
Series = {{International Conference on Acoustics Speech and Signal Processing
   ICASSP}},
