@article{ ISI:000311521900036,
Author = {Zhang, Liting and Wu, Songtao and Buthe, Andreas and Zhao, Xueyan and
   Jia, Hongfei and Zhang, Songping and Wang, Ping},
Title = {{Poly(ethylene glycol) Conjugated Enzyme with Enhanced Hydrophobic
   Compatibility for Self-Cleaning Coatings}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2012}},
Volume = {{4}},
Number = {{11}},
Pages = {{5981-5987}},
Month = {{NOV}},
Abstract = {{Enzyme-based smart materials constitute a rapidly growing group of
   functional materials. Often the natively evolved enzymes are not
   compatible with hydrophobic synthetic materials, thus significantly
   limiting the performance of enzymes. This work investigates the use of a
   polyethylene glycol (PEG)-conjugated detergent enzyme for self-cleaning
   coatings. As a result, PEG conjugated a-amylase demonstrated a much more
   homogeneous distribution in polyurethane coatings than the parent native
   enzyme as detected by both fluorescent microscopy and scanning electron
   microscopy (SEM) equipped with energy-dispersive X-ray spectroscopy
   (SEM-EDX). Additionally, the conjugated enzyme showed enhanced retention
   in the coating and much improved thermal stability with a halflife of 20
   days detected at 80 degrees C and over 350 days under room temperature.
   Such coating-incorporated enzyme afforded interesting self-cleaning
   functionality against starch-based stains as examined through a slipping
   drop test.}},
