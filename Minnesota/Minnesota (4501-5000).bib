@article{ ISI:000314162600009,
Author = {Sohn, Hweerin and Victora, R. H.},
Title = {{Transition Noise Analysis of Recording Media With a Soft Underlayer
   (SUL) and an Antiferromagnetic Soft Underlayer (AF-SUL)}},
Journal = {{IEEE TRANSACTIONS ON MAGNETICS}},
Year = {{2013}},
Volume = {{49}},
Number = {{2, 2}},
Pages = {{824-828}},
Month = {{FEB}},
Abstract = {{A perpendicular magnetic recording system has been modeled for the
   purpose of analyzing transition noise, particularly ``jitter.{''}
   Conventional perpendicular media and ``soft{''} exchange coupled
   composite (ECC) media with a conventional soft underlayer (SUL) and an
   antiferromagnetic soft underlayer (AF-SUL) have been investigated using
   micromagnetic simulation. The fast Fourier transform (FFT) technique and
   graphics processing unit (GPU)-based computing have been used to reduce
   the intensive computation time for magneto-static interactions between
   the head, SUL, and recording layer. Interestingly, the jitter is always
   less dependent on reader offset from track center with the AF-SUL.
   Jitter for ECC media is also shown to depend less strongly on reader
   offset than for conventional media. The transition center deviation at
   the optimal anisotropy for both recording layers is lower with the
   AF-SUL at both linear densities considered. We further find that the
   track center moves alternately with direction of fringing field as
   expected from magnetostatic considerations.}},
