@article{ ISI:000314770300062,
Author = {Wang, Hao and Zhao, Haibao and Quarterman, P. and Wang, Jian-Ping},
Title = {{Embedded mask patterning: A nanopatterning process to fabricate FePt
   magnetic media}},
