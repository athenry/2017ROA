@article{ ISI:000389699500001,
Author = {Meng, Jianjun and Zhang, Shuying and Bekyo, Angeliki and Olsoe, Jaron
   and Baxter, Bryan and He, Bin},
Title = {{Noninvasive Electroencephalogram Based Control of a Robotic Arm for
   Reach and Grasp Tasks}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2016}},
Volume = {{6}},
Month = {{DEC 14}},
Abstract = {{Brain-computer interface (BCI) technologies aim to provide a bridge
   between the human brain and external devices. Prior research using
   non-invasive BCI to control virtual objects, such as computer cursors
   and virtual helicopters, and real-world objects, such as wheelchairs and
   quadcopters, has demonstrated the promise of BCI technologies. However,
   controlling a robotic arm to complete reach-and-grasp tasks efficiently
   using non-invasive BCI has yet to be shown. In this study, we found that
   a group of 13 human subjects could willingly modulate brain activity to
   control a robotic arm with high accuracy for performing tasks requiring
   multiple degrees of freedom by combination of two sequential low
   dimensional controls. Subjects were able to effectively control reaching
   of the robotic arm through modulation of their brain rhythms within the
   span of only a few training sessions and maintained the ability to
   control the robotic arm over multiple months. Our results demonstrate
   the viability of human operation of prosthetic limbs using non-invasive
   BCI technology.}},
