@article{ ISI:000395775300008,
Author = {Gan, Zhuo and Ding, Liya and Burckhardt, Christoph J. and Lowery, Jason
   and Zaritsky, Assaf and Sitterley, Karlyndsay and Mota, Andressa and
   Costigliola, Nancy and Starker, Colby G. and Voytas, Daniel F. and
   Tytell, Jessica and Goldman, Robert D. and Danuser, Gaudenz},
Title = {{Vimentin Intermediate Filaments Template Microtubule Networks to Enhance
   Persistence in Cell Polarity and Directed Migration}},
Journal = {{CELL SYSTEMS}},
Year = {{2016}},
Volume = {{3}},
Number = {{3}},
Pages = {{252+}},
Month = {{SEP 28}},
Abstract = {{Increased expression of vimentin intermediate filaments (VIFs) enhances
   directed cell migration, but the mechanism behind VIFs' effect on
   motility is not understood. VIFs interact with microtubules, whose
   organization contributes to polarity maintenance in migrating cells.
   Here, we characterize the dynamic coordination of VIF and microtubule
   networks in wounded monolayers of retinal pigment epithelial cells. By
   genome editing, we fluorescently labeled endogenous vimentin and
   a-tubulin, and we developed computational image analysis to delineate
   architecture and interactions of the two networks. Our results show that
   VIFs assemble an ultrastructural copy of the previously polarized
   microtubule network. Because the VIF network is long-lived compared to
   the microtubule network, VIFs template future microtubule growth along
   previous microtubule tracks, thus providing a feedback mechanism that
   maintains cell polarity. VIF knockdown prevents cells from polarizing
   and migrating properly during wound healing. We suggest that VIFs'
   templating function establishes a memory in microtubule organization
   that enhances persistence in cell polarization in general and migration
   in particular.}},
