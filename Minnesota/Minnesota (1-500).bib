@article{ ISI:000388896600004,
Author = {Yang, Bo and Li, Jingwei and Han, Qiaoni and He, Tian and Chen, Cailian
   and Guan, Xinping},
Title = {{Distributed Control for Charging Multiple Electric Vehicles with
   Overload Limitation}},
