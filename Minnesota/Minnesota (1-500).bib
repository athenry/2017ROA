@article{ ISI:000382603300031,
Author = {Demissie, Abel T. and Haugstad, Greg and Frisbie, C. Daniel},
Title = {{Quantitative Surface Coverage Measurements of Self-Assembled Monolayers
   by Nuclear Reaction Analysis of Carbon-12}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY LETTERS}},
Year = {{2016}},
Volume = {{7}},
Number = {{17}},
Pages = {{3477-3481}},
Month = {{SEP 1}},
Abstract = {{We report surface coverage measurements by Rutherford back scattering
   spectrometry (RBS) of self-assembled monolayers (SAMs) of both alkyl
   thiols and oligophenylene thiols on Au-coated mica, Si, and pyrolytic
   graphite. The C-12 atom concentration was probed at 4.266 MeV He-4(2)2+
   primary beam energy, which enhances the He-4(2)2 scattering cross
   section by exciting C-12 nuclear resonance states; this is a submode of
   RBS commonly referred to as nuclear reaction analysis (NRA). The surface
   coverage of C-12 increased linearly with the number of C-12 atoms in
   each SAM. The consistency of the C-12 atom coverage values obtained by
   NRA was cross-checked by measuring the S-32 atom concentration by
   conventional RBS. From these data, we obtained an average coverage of
   3.5 +/- 0.2 molecules/nm(2) for both alkyl thiols and oligophenylene
   thiols on polycrystalline Au surfaces. The results show the utility of
   NRA for quantitative analysis of SAM coverage on Au.}},
