@article{ ISI:000390245900010,
Author = {Yu, Qiang and Le, Jia-Liang and Hubler, Mija H. and Wendner, Roman and
   Cusatis, Gianluca and Bazant, Zdenek P.},
Title = {{Comparison of main models for size effect on shear strength of
   reinforced and prestressed concrete beams}},
Journal = {{STRUCTURAL CONCRETE}},
Year = {{2016}},
Volume = {{17}},
Number = {{5}},
Pages = {{778-789}},
Month = {{DEC}},
Abstract = {{This paper presents a critical comparison of the existing code
   provisions for the shear strength of concrete beams. The comparison is
   based on the computerized filtering-out of the in-evitable statistical
   bias from the available multivariate database on shear strength, on an
   examination of the predicted size effects on shear strength and their
   underlying hypotheses and on the results of recent high-fidelity
   numerical simulations of shear failure. In addition to examining the
   existing models, the present comparison also provides several key
   considerations for testing the scientific soundness of any model of
   shear failure in concrete beams, which is necessary for future revisions
