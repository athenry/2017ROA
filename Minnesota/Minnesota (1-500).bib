@article{ ISI:000383784400027,
Author = {Zhang, Bo and Zhong, Zhaoping and Xie, Qinglong and Liu, Shiyu and Ruan,
   Roger},
Title = {{Two-step fast microwave-assisted pyrolysis of biomass for bio-oil
   production using microwave absorbent and HZSM-5 catalyst}},
Journal = {{JOURNAL OF ENVIRONMENTAL SCIENCES}},
Year = {{2016}},
Volume = {{45}},
Pages = {{240-247}},
Month = {{JUL}},
Abstract = {{A novel technology of two-step fast microwave-assisted pyrolysis (fMAP)
   of corn stover for bio-oil production was investigated in the presence
   of microwave absorbent (SiC) and HZSM-5 catalyst. Effects of fMAP
   temperature and catalyst-to-biomass ratio on bio-oil yield and chemical
   components were examined. The results showed that this technology,
   employing microwave, microwave absorbent and HZSM-5 catalyst, was
   effective and promising for biomass fast pyrolysis. The fMAP temperature
   of 500 degrees C was considered the optimum condition for maximum yield
   and best quality of bio-oil. Besides, the bio-oil yield decreased
   linearly and the chemical components in bio-oil were improved
   sequentially with the increase of catalyst-to-biomass ratio from 1:100
   to 120. The elemental compositions of bio-char were also determined.
   Additionally, compared to one-step fMAP process, two-step fMAP could
   promote the bio-oil quality with a smaller catalyst-to-biomass ratio.
   (C) 2016 The Research Center for Eco-Environmental Sciences, Chinese
   Academy of Sciences. Published by Elsevier B.V.}},
