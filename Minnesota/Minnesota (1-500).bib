@article{ ISI:000379280500016,
Author = {Hu, Jia and Shao, Yunli and Sun, Zongxuan and Wang, Meng and Bared, Joe
   and Huang, Peter},
Title = {{Integrated optimal eco-driving on rolling terrain for hybrid electric
   vehicle with vehicle-infrastructure communication}},
Journal = {{TRANSPORTATION RESEARCH PART C-EMERGING TECHNOLOGIES}},
Year = {{2016}},
Volume = {{68}},
Pages = {{228-244}},
Month = {{JUL}},
Abstract = {{This research presents an integrated optimal controller to maximize the
   fuel efficiency of a Hybrid Electric Vehicle (HEV) traveling on rolling
   terrain. The controller optimizes both the vehicle acceleration and the
   hybrid powertrain operation. It takes advantage of the emerging
   Connected Vehicle (CV) technology and utilizes present and future
   information as optimization input, which includes road topography, and
   dynamic speed limit. The optimal control problem was solved using
   Pontryagin's Minimum Principle (PMP). Efforts were made to reduce the
   computational burden of the optimization process. The evaluation shows
   that the benefit of the proposed optimal controller is significant
   compared to regular HEV cruising at the speed limit on rolling terrain.
   The benefit ranges from 5.0\% to 8.9\% on mild slopes and from 15.7\% to
   16.9\% on steep slopes. The variation is caused by the change of hilly
   road density. (C) 2016 Elsevier Ltd. All rights reserved.}},
