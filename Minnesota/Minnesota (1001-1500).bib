@article{ ISI:000365683000005,
Author = {Hamilton, Kathryn E. and Chatterji, Priya and Lundsmith, Emma T. and
   Andres, Sarah F. and Giroux, Veronique and Hicks, Philip D. and
   Noubissi, Felicite K. and Spiegelman, Vladimir S. and Rustgi, Anil K.},
Title = {{Loss of Stromal IMP1 Promotes a Tumorigenic Microenvironment in the
   Colon}},
Journal = {{MOLECULAR CANCER RESEARCH}},
Year = {{2015}},
Volume = {{13}},
Number = {{11}},
Pages = {{1478-1486}},
Month = {{NOV}},
Abstract = {{The colon tumor microenvironment is becoming increasingly recognized as
   a complex but central player in the development of many cancers.
   Previously, we identified an oncogenic role for the mRNA-binding protein
   IMP1 (IGF2BP1) in the epithelium during colon tumorigenesis. In the
   current study, we reveal the contribution of stromal IMP1 in the context
   of colitis-associated colon tumorigenesis. Interestingly, stromal
   deletion of Imp1 (Dermo1Cre;Imp1(LoxP/LoxP), or Imp1 Delta(Mes)) in the
   azoxymethane/dextran sodium sulfate (AOM/DSS) model of
   colitis-associated cancer resulted in increased tumor numbers of larger
   size and more advanced histologic grade than controls. In addition,
   Imp1(Delta Mes) mice exhibited a global increase in protumorigenic
   microenvironment factors, including enhanced inflammation and stromal
   components. Evaluation of purified mesenchyme from AOM/DSS-treated
   Imp1(Delta Mes) mice demonstrated an increase in hepatocyte growth
   factor (HGF), which has not been associated with regulation via IMP1.
   Genetic knockdown of Imp1 in human primary fibroblasts confirmed an
   increase in HGF with Imp1 loss, demonstrating a specific,
   cell-autonomous role for Imp1 loss to increase HGF expression. Taken
   together, these data demonstrate a novel tumor-suppressive role for IMP1
