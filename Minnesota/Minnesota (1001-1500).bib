@inproceedings{ ISI:000390715800087,
Author = {Al-Digs, Abdullah and Dhople, Sairaj V. and Chen, Yu Christine},
Book-Group-Author = {{IEEE}},
Title = {{Estimating Feasible Nodal Power Injections in Distribution Networks}},
Booktitle = {{2016 IEEE POWER \& ENERGY SOCIETY INNOVATIVE SMART GRID TECHNOLOGIES
   CONFERENCE (ISGT)}},
Year = {{2016}},
Note = {{IEEE Power-and-Energy-Society Innovative Smart Grid Technologies
   Conference (ISGT), Minneapolis, MN, SEP 06-09, 2016}},
Organization = {{IEEE Power \& Energy Soc}},
Abstract = {{This paper proposes a set-theoretic method to estimate feasible nodal
   injections (generation or load) in a distribution network, while
   respecting power system performance requirements. In the setting that we
   study, performance requirements are constraints in the form of interval
   ranges on the values that line flows can take, which are modelled by a
   convex poloytope. At its core, the proposed method relies on the
   solution of a linearly constrained least-squares optimization problem,
   which is formulated using sensitivity factors computed by linearizing
   the power flow equations around the operating point. Since this
   optimization problem admits a linear closedform solution, the polytope
   of permissible line flows is propagated through the solution, via set
   operations, to obtain the set of feasible nodal injections.}},
