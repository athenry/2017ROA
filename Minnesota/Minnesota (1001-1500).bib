@article{ ISI:000364770500331,
Author = {Ahmed, Rizvi and Victora, R. H.},
Title = {{Possible Explanation for Observed Effectiveness of Voltage-Controlled
   Anisotropy in CoFeB/MgO MTJ}},
Journal = {{IEEE TRANSACTIONS ON MAGNETICS}},
Year = {{2015}},
Volume = {{51}},
Number = {{11}},
Month = {{NOV}},
Note = {{IEEE International Magnetics Conference (Intermag), Beijing, PEOPLES R
   CHINA, MAY 11-15, 2015}},
Organization = {{IEEE; Tsinghua Univ; Inst Phys; Chinese Acad Sci; Beijing Univ; Chinese
   Phys Soc; Chinese Inst Elect; Chinese Mat Res Soc}},
Abstract = {{Voltage-controlled switching of CoFeB/MgO magnetic tunnel junctions has
   been analyzed using micromagnetic simulation that includes thermal
   fluctuations and surface roughness. It is shown that the large samples
   typically studied experimentally switch by domain wall motion, nucleated
   at points where the CoFeB is slightly thicker. This is a consequence of
   small thickness variations producing large percentage changes in the
   anisotropy owing to the near balance of interface anisotropy and shape
   anisotropy in these films. It is also found that these films are likely
   never saturated at the experimental fields employed owing to the
   importance of thermal fluctuations in the near 2-D geometry.}},
