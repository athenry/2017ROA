@article{ ISI:000366509300003,
Author = {Choi, Soo-Hyung and Lee, Won Bo and Lodge, Timothy P. and Bates, Frank
   S.},
Title = {{Structure of Poly(styrene-b-ethylene-alt-propylene) Diblock Copolymer
   Micelles in Binary Solvent Mixtures}},
Journal = {{JOURNAL OF POLYMER SCIENCE PART B-POLYMER PHYSICS}},
Year = {{2016}},
