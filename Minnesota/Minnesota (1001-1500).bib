@article{ ISI:000367123100012,
Author = {Jeong, Jaehoon Paul and Kim, Jinyong and Hwang, Taehwan and Xu, Fulong
   and Guo, Shuo and Gu, Yu Jason and Cao, Qing and Liu, Ming and He, Tian},
Title = {{TPD: Travel Prediction-based Data Forwarding for light-traffic vehicular
   networks}},
Journal = {{COMPUTER NETWORKS}},
Year = {{2015}},
Volume = {{93}},
Number = {{1}},
Pages = {{166-182}},
Month = {{DEC 24}},
Abstract = {{This paper proposes Travel Prediction-based Data forwarding (TPD),
   tailored and optimized for multihop vehicle-to-vehicle communications.
   The previous schemes forward data packets mostly utilizing statistical
   information about road network traffic, which becomes much less accurate
   when vehicles travel in a light-traffic vehicular network. In this
   light-traffic vehicular network, highly dynamic vehicle mobility can
   introduce a large variance for the traffic statistics used in the data
   forwarding process. However, with the popularity of GPS navigation
   systems, vehicle trajectories become available and can be utilized to
   significantly reduce this uncertainty in the road traffic statistics.
   Our TPD takes advantage of these vehicle trajectories for a better data
   forwarding in light-traffic vehicular networks. Our idea is that with
   the trajectory information of vehicles in a target road network, a
   vehicle encounter graph is constructed to predict vehicle encounter
   events (i.e., timing for two vehicles to exchange data packets in
   communication range). With this encounter graph, TPD optimizes data
   forwarding process for minimal data delivery delay under a specific
   delivery ratio threshold. Through extensive simulations, we demonstrate
   that our TPD significantly outperforms existing legacy schemes in a
   variety of road network settings. (C) 2015 Elsevier B.V. All rights
   reserved.}},
