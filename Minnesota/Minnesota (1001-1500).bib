Language = {{English}},
Affiliation = {{Smith, S (Reprint Author), Univ Minnesota, Dept Comp Sci \& Engn, Minneapolis, MN 55455 USA.
   Smith, Shaden; Karypis, George, Univ Minnesota, Dept Comp Sci \& Engn, Minneapolis, MN 55455 USA.}},
DOI = {{10.1109/IPDPS.2016.113}},
ISSN = {{1530-2075}},
ISBN = {{978-1-5090-2140-6}},
