@inproceedings{ ISI:000388376103022,
Author = {Findeisen, Rolf and Grover, Martha A. and Wagner, Christian and Maiworm,
   Michael and Temirov, Ruslan and Tautz, F. Stefan and Salapaka, Murti V.
   and Salapaka, Srinivasa and Braatz, Richard D. and Moheimani, S. O. Reza},
