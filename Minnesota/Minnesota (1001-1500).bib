@article{ ISI:000367010800030,
Author = {Quarterman, P. and Wang, Hao and Qiu, Jiao-Ming and Guo, Honghua and Ma,
   Bin and Liu, Xiaoqi and Wang, Jian-Ping},
Title = {{Observation and elimination of broken symmetry in L1(0) FePt
   nanostructures}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{107}},
Number = {{23}},
Month = {{DEC 7}},
Abstract = {{An unexplained surface anisotropy effect was observed and confirmed in
   the magnetization reversal process of both L1(0) phase FePt
   nanoparticles with octahedral shape and (001) textured L10 FePt thin
   films with island nanostructures. We suggest that the nature of the
   observed surface effect is caused by broken symmetry on the FePt
   surface, which results in weakened exchange coupling for surface atoms.
   Furthermore, we propose, and experimentally demonstrate, a method to
   repair the broken symmetry by capping the FePt islands with a Pt layer,
   which could prove invaluable in understanding fundamental limitations of
   magnetic nanostructures. (C) 2015 AIP Publishing LLC.}},
