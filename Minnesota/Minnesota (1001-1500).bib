@article{ ISI:000365361400096,
Author = {Liu, Zhao and Li, Jun and Feng, Zhenping and Simon, Terrence},
Title = {{Numerical study on the effect of jet nozzle aspect ratio and jet angle
   on swirl cooling in a model of a turbine blade leading edge cooling
   passage}},
Journal = {{INTERNATIONAL JOURNAL OF HEAT AND MASS TRANSFER}},
Year = {{2015}},
Volume = {{90}},
Pages = {{986-1000}},
Month = {{NOV}},
Abstract = {{In this paper, a numerical simulation is performed to predict swirl
   cooling of an internal leading edge cooling passage model for a gas
   turbine blade. The swirling cooling performance and its effectiveness
   are investigated for cases of two rectangular section inlets that cause
   flow to impinge tangentially on the internal surface of the circular
   cooling passage. The effects of aspect ratio of the jet nozzle, jet
   angle and Reynolds numbers on the local and average flows and heat
   transfer are investigated. The results indicate that the pressure loss
   and global area weighted average Nusselt number on the swirl chamber
   increases with increases of Reynolds number, and increases with
   decreases of the jet nozzle aspect ratio. A correlation of the area
   weighted average Nusselt number over a range of parameters is suggested.
   The distribution of Nusselt number over the swirl chamber shows that the
   thermal stresses will be lowest with a 60 degrees jet nozzle angle. (C)
   2015 Elsevier Ltd. All rights reserved.}},
