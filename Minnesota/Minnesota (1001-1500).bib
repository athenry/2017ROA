@article{ ISI:000382025300023,
Author = {Kang, Minje and Bhan, Aditya},
Title = {{Kinetics and mechanisms of alcohol dehydration pathways on alumina
   materials}},
Journal = {{CATALYSIS SCIENCE \& TECHNOLOGY}},
Year = {{2016}},
Volume = {{6}},
Number = {{17}},
Pages = {{6667-6678}},
Abstract = {{The steady state rates of ethene and diethyl ether formation in parallel
   ethanol dehydration reactions at 573 and 623 K are mechanistically and
   kinetically described by the same rate expression on different alumina
   materials (alpha-, gamma-, and eta-Al2O3), implying that alumina
   materials have similar surface sites under reaction environments. In
   situ chemical titration using pyridine as a titrant elucidates similar
   site densities (similar to 0.12 sites nm(-2) and similar to 0.07 sites
   nm(-2) for ethene formation and similar to 0.14 sites nm(-2) and similar
   to 0.09 sites nm(-2) for diethyl ether formation on gamma- and
   eta-Al2O3, respectively) on gamma- and eta-Al2O3 indicating that similar
   surface features exist on both gamma- and eta-Al2O3. Pyridine-ethanol
   co-feed experiments show that pyridine inhibited the formation of ethene
   to a greater extent than diethyl ether suggesting that the two parallel
   dehydration reactions are not catalyzed by a common active site.}},
