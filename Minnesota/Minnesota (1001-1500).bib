@inproceedings{ ISI:000390249700043,
Author = {Murugesan, Anitha and Whalen, Michael W. and Ghassabani, Elaheh and
   Heimdahl, Mats P. E.},
Book-Group-Author = {{IEEE}},
Title = {{Complete Traceability for Requirements in Satisfaction Arguments}},
Booktitle = {{2016 IEEE 24TH INTERNATIONAL REQUIREMENTS ENGINEERING CONFERENCE (RE)}},
