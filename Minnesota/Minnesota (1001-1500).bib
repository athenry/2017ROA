@inproceedings{ ISI:000390154400056,
Author = {Dai, Haipeng and Liu, Yunhuai and Liu, Alex X. and Kong, Lingtao and
   Chen, Guihai and He, Tian},
Book-Group-Author = {{IEEE}},
Title = {{Radiation Constrained Wireless Charger Placement}},
