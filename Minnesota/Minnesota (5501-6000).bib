Editor = {{Santamaria, I and ArenasGarcia, J and CampsValls, G and Erdogmus, D and PerezCruz, F and Larsen, J}},
Title = {{EXTRACTION OF SPARSE SPATIAL FILTERS USING OSCILLATING SEARCH}},
Booktitle = {{2012 IEEE INTERNATIONAL WORKSHOP ON MACHINE LEARNING FOR SIGNAL
   PROCESSING (MLSP)}},
