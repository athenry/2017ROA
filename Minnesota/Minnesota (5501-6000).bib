@inproceedings{ ISI:000313296506141,
Author = {Kroll, Mark W. and Walcott, Gregory P. and Ideker, Raymond E. and
   Graham, Michael A. and Calkins, Hugh and Lakkireddy, Dhanunjaya and
   Luceri, Richard M. and Panescu, Dorin},
Book-Group-Author = {{IEEE}},
Title = {{The Stability of Electrically Induced Ventricular Fibrillation}},
Booktitle = {{2012 ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN MEDICINE
   AND BIOLOGY SOCIETY (EMBC)}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
Year = {{2012}},
