@inproceedings{ ISI:000312381402049,
Author = {Soni, Akshay and Haupt, Jarvis},
Book-Group-Author = {{IEEE}},
Title = {{LEARNING SPARSE REPRESENTATIONS FOR ADAPTIVE COMPRESSIVE SENSING}},
