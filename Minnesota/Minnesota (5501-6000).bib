@article{ ISI:000299210600044,
Author = {Jaeger, Stefanie and Kim, Dong Young and Hultquist, Judd F. and Shindo,
   Keisuke and LaRue, Rebecca S. and Kwon, Eunju and Li, Ming and Anderson,
   Brett D. and Yen, Linda and Stanley, David and Mahon, Cathal and Kane,
   Joshua and Franks-Skiba, Kathy and Cimermancic, Peter and Burlingame,
   Alma and Sali, Andrej and Craik, Charles S. and Harris, Reuben S. and
   Gross, John D. and Krogan, Nevan J.},
Title = {{Vif hijacks CBF-beta to degrade APOBEC3G and promote HIV-1 infection}},
Journal = {{NATURE}},
Year = {{2012}},
Volume = {{481}},
Number = {{7381}},
Pages = {{371-375}},
Month = {{JAN 19}},
Abstract = {{Restriction factors, such as the retroviral complementary DNA deaminase
   APOBEC3G, are cellular proteins that dominantly block virus
   replication(1-3). The AIDS virus, human immuno deficiency virus type 1
   (HIV-1), produces the accessory factor Vif, which counteracts the host's
   antiviral defence by hijacking a ubiquitin ligase complex, containing
   CUL5, ELOC, ELOB and a RING-box protein, and targeting APOBEC3G for
   degradation(4-10). Here we reveal, using an affinity tag/purification
   mass spectrometry approach, that Vif additionally recruits the
   transcription cofactor CBF-beta to this ubiquitin ligase complex.
   CBF-beta, which normally functions in concert with RUNX DNA binding
   proteins, allows the reconstitution of a recombinant six-protein
   assembly that elicits specific polyubiquitination activity with
   APOBEC3G, but not the related deaminase APOBEC3A. Using RNA knockdown
   and genetic complementation studies, we also demonstrate that CBF-beta
   is required for Vif-mediated degradation of APOBEC3G and therefore for
   preserving HIV-1 infectivity. Finally, simian immunodeficiency virus
   (SIV) Vif also binds to and requires CBF-beta to degrade rhesus macaque
   APOBEC3G, indicating functional conservation. Methods of disrupting the
   CBF-beta-Vif interaction might enable HIV-1 restriction and provide a
   supplement to current antiviral therapies that primarily target viral
   proteins.}},
