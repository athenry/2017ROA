@article{ ISI:000300921600008,
Author = {Shieh, Ian C. and Waring, Alan J. and Zasadzinski, Joseph A.},
Title = {{Visualizing the Analogy between Competitive Adsorption and Colloid
   Stability to Restore Lung Surfactant Function}},
Journal = {{BIOPHYSICAL JOURNAL}},
Year = {{2012}},
Volume = {{102}},
Number = {{4}},
Pages = {{777-786}},
Month = {{FEB 22}},
Abstract = {{We investigated a model of acute respiratory distress syndrome in which
   the serum protein albumin adsorbs to an air-liquid interface and
   prevents the thermodynamically preferable adsorption of the clinical
   lung surfactant Survanta by inducing steric and electrostatic energy
   barriers analogous to those that prevent colloidal aggregation. Chitosan
   and polyethylene glycol (PEG), two polymers that traditionally have been
   used to aggregate colloids, both allow Survanta to quantitatively
   displace albumin from the interface, but through two distinct
   mechanisms. Direct visualization with confocal microscopy shows that the
   polycation chitosan coadsorbs to interfacial layers of both Survanta and
   albumin, and also colocalizes with the anionic domains of Survanta at
   the air-liquid interface, consistent with it eliminating the
   electrostatic repulsion by neutralizing the surface charges on albumin
   and Survanta. In contrast, the PEG distribution does not change during
   the displacement of albumin by Survanta, consistent with PEG inducing a
   depletion attraction sufficient to overcome the repulsive energy barrier
   toward adsorption.}},
