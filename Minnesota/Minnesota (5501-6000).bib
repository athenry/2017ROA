@inproceedings{ ISI:000310776205138,
Author = {Seiler, Peter and Ozdemir, Ahmet Arda and Balas, Gary J.},
Book-Group-Author = {{IEEE}},
Title = {{Performance Limits With Preview Information and Actuator Rate
   Constraints}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
