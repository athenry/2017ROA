@inproceedings{ ISI:000310623800164,
Author = {Gopalakrishnan, Balasubramanian and Sidiropoulos, Nicholas D.},
Title = {{JOINT BACK-PRESSURE POWER CONTROL AND INTERFERENCE CANCELLATION IN
   WIRELESS MULTI-HOP NETWORKS}},
