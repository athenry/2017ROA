@inproceedings{ ISI:000312416000023,
Author = {Ballard, John R. and Casper, Andrew J. and Liu, Dalong and Haritonova,
   Alyona and Shehata, Islam A. and Troutman, Mitchell and Ebbini, Emad S.},
