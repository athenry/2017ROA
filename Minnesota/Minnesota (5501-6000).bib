@inproceedings{ ISI:000326642600004,
Author = {Derby, Jeffrey J. and Zhang, Nan and Yeckel, Andrew},
Editor = {{James, RB and Burger, A and Franks, LA and Fiederle, M}},
Title = {{Improving the growth of CZT crystals for radiation detectors: A modeling
   perspective}},
Booktitle = {{HARD X-RAY, GAMMA-RAY, AND NEUTRON DETECTOR PHYSICS XIV}},
Series = {{Proceedings of SPIE}},
