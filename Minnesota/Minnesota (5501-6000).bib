@inproceedings{ ISI:000310776205014,
Author = {Fardad, Makan and Lin, Fu and Jovanovic, Mihailo R.},
Book-Group-Author = {{IEEE}},
Title = {{On the Optimal Synchronization of Oscillator Networks via Sparse
   Interconnection Graphs}},
