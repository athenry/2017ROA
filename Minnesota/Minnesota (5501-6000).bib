@article{ ISI:000308127800003,
Author = {Wheeler, V. M. and Masuri, S. and Tamma, K. K. and Zhou, X. and Sellier,
   M.},
Title = {{ON THE APPLICABILITY OF AN ISOCHRONOUS INTEGRATION FRAMEWORK FOR
   PARABOLIC/HYPERBOLIC HEAT CONDUCTION TYPE PROBLEMS}},
Journal = {{NUMERICAL HEAT TRANSFER PART A-APPLICATIONS}},
Year = {{2012}},
Volume = {{62}},
Number = {{5, SI}},
Pages = {{372-392}},
Note = {{Minisymposium on Computational Radiative and Convective Heat Transfer in
   Honor of Professor George D. Raithby/11th United States National
   Congress on Computational Mechanics, Minneapolis, MN, JUL 25-29, 2011}},
Organization = {{ANSYS; Elsevier}},
Abstract = {{We demonstrate the applicability of a new isochronous integration
   (iIntegration) computational framework for the analysis of transient
   first/second-order systems via the new generalized single step single
   solve (GS4) family of algorithms, capable of simultaneously being
   applied to first- and second-order systems in time while providing
   attractive numerical and order-preserving attributes to capture the
   problem physics. To illustrate, we consider a recently developed model
   of heat conduction that was proposed to govern thermal transport from
   nano- to macro-scales, and is known in the literature as the C- and
   F-processes heat conduction model. This model inherits unique features
   and can represent either first- or second-order parabolic/hyperbolic
   heat conduction governing equations. A derivation of the model is first
   summarized, followed by the numerical discretization procedure and
   illustrative examples demonstrating the practicality and convenience of
   the new unified approach for modeling and analysis.}},
