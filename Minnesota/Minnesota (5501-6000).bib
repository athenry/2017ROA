@inproceedings{ ISI:000321985400048,
Author = {Gillella, Pradeep and Sun, Zongxuan},
Book-Group-Author = {{ASME}},
Title = {{SPOOL VALVE WITH ARBITRARY AREA-SCHEDULE AND APPLICATION TO A CAMLESS
   ENGINE VALVE ACTUATOR}},
Booktitle = {{PROCEEDINGS OF THE ASME DYNAMIC SYSTEMS AND CONTROL CONFERENCE AND
   BATH/ASME SYMPOSIUM ON FLUID POWER AND MOTION CONTROL (DSCC 2011), VOL 2}},
