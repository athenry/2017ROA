Author = {Gupta, Saket and Sapatnekar, Sachin S.},
Book-Group-Author = {{IEEE}},
Title = {{GNOMO: Greater-than-NOMinal V-dd Operation for BTI Mitigation}},
Booktitle = {{2012 17TH ASIA AND SOUTH PACIFIC DESIGN AUTOMATION CONFERENCE (ASP-DAC)}},
