@incollection{ ISI:000299709400009,
Author = {Ermentrout, G. Bard and Beverlin, II, Bryce and Netoff, Theoden},
Editor = {{Schultheiss, NW and Prinz, AA and Butera, RJ}},
Title = {{Phase Response Curves to Measure Ion Channel Effects on Neurons}},
Booktitle = {{PHASE RESPONSE CURVES IN NEUROSCIENCE: THEORY, EXPERIMENT, AND ANALYSIS}},
Series = {{Springer Series in Computational Neuroscience}},
Year = {{2012}},
Volume = {{6}},
Pages = {{207-236}},
Abstract = {{In this chapter, we explore how different common ionic currents change
   the shape of the PRC using several well-known conductance-based models.
   By also studying the simplest possible network - two pulse coupled
   neural oscillators we can use the shape of the PRC to quantify the
   degree of synchrony in such a pair. We find a number of qualitative
   effects that include changing the sign of the PRC during certain phases
   and altering the skew of the PRC. Using bifurcation theory, we trace
   these effects back to the underlying dynamics that govern the onset of
   repetitive firing as the neuron is depolarized by a constant current.
   The key bifurcation involves the so-called Takens-Bogdanov bifurcation.
   We show that many of the shape changes of the PRC can be understood by
   examining the dynamics of the neuron near this bifurcation.}},
