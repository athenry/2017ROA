Author = {Zhou, Wenguang and Hu, Bing and Li, Yecong and Min, Min and Mohr,
   Michael and Du, Zhenyi and Chen, Paul and Ruan, Roger},
Title = {{Mass Cultivation of Microalgae on Animal Wastewater: a Sequential
   Two-Stage Cultivation Process for Energy Crop and Omega-3-Rich Animal
   Feed Production}},
