@article{ ISI:000304855900012,
Author = {Qin, Jian and Morse, David C.},
Title = {{Fluctuations in Symmetric Diblock Copolymers: Testing Theories Old and
   New}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2012}},
Volume = {{108}},
Number = {{23}},
Month = {{JUN 6}},
Abstract = {{Computer simulations are used to study composition fluctuations in
   disordered diblock copolymer melts over a range of values of the chain
   length N, and test several theories for the structure factor S(q).
   Specifically, we test the random-phase approximation (RPA), which is
   based on a self-consistent field treatment of fluctuations, the
   Fredrickson-Helfand theory, which was designed to describe fluctuations
   near the order-disorder transition, and the relatively new renormalized
   one-loop (ROL) theory. The results confirm claims that the RPA is exact
   in the limit N -> infinity and that the ROL theory yields the dominant
   corrections to the RPA within a systematic expansion in powers of N-1/2,
   and show that the ROL theory is much more accurate than either older
   theory.}},
