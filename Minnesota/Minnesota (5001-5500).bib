@article{ ISI:000302625800004,
Author = {Tang, Jok M. and Saad, Yousef},
Title = {{A probing method for computing the diagonal of a matrix inverse}},
Journal = {{NUMERICAL LINEAR ALGEBRA WITH APPLICATIONS}},
Year = {{2012}},
Volume = {{19}},
Number = {{3}},
Pages = {{485-501}},
Month = {{MAY}},
Abstract = {{The computation of some entries of a matrix inverse arises in several
   important applications in practice. This paper presents a probing method
   for determining the diagonal of the inverse of a sparse matrix in the
   common situation when its inverse exhibits a decay property, i.e. when
   many of the entries of the inverse are small. A few simple properties of
   the inverse suggest a way to determine effective probing vectors based
   on standard graph theory results. An iterative method is then applied to
   solve the resulting sequence of linear systems, from which the diagonal
   of the matrix inverse is extracted. The results of numerical experiments
   are provided to demonstrate the effectiveness of the probing method.
