@article{ ISI:000301609400009,
Author = {Lew, Christopher M. and Rajabbeigi, Nafiseh and Tsapatsis, Michael},
Title = {{Tin-containing zeolite for the isomerization of cellulosic sugars}},
Journal = {{MICROPOROUS AND MESOPOROUS MATERIALS}},
Year = {{2012}},
Volume = {{153}},
Pages = {{55-58}},
Month = {{MAY 1}},
Abstract = {{Isomerization reactions starting with dihydroxyacetone (DHA), xylose,
   and glucose in water were catalyzed by Sn-MFI and Sn-BEA zeolites.
   Sn-SEA was more active than Sn-MFI for glucose, while both catalysts
   were able to isomerize dihydroxyacetone and xylose. After 210 min at 90
   degrees C, the yields of lactic acid, xylulose, and fructose using
   Sn-BEA were 54\%, 24\%, and 34\%, respectively, at conversions of 93\%,
   81\%, and 65\%, respectively. Using Sn-MFI, the yields were 65\%, 19\%,
   and 4\%, respectively, at conversions of 98\%, 40\%, and 9\%,
   respectively. Pore size limitations, substrate sizes, and catalyst
   surface area all contribute to the strong activity of Sn-SEA for all
   three sugars; however, Sn-MFI is easier to make and may be suitable for
   reactions involving smaller molecules. (C) 2011 Elsevier Inc. All rights
   reserved.}},
