@article{ ISI:000311301600019,
Author = {Lindquist, Nathan C. and Johnson, Timothy W. and Jose, Jincy and Otto,
   Lauren M. and Oh, Sang-Hyun},
Title = {{Ultrasmooth metallic films with buried nanostructures for backside
   reflection-mode plasmonic biosensing}},
Journal = {{ANNALEN DER PHYSIK}},
