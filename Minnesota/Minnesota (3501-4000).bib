@inproceedings{ ISI:000343655307066,
Author = {Gopalakrishnan, Balasubramanian and Sidiropoulos, Nicholas D.},
Book-Group-Author = {{IEEE}},
Title = {{COGNITIVE TRANSMIT BEAMFORMING FROM BINARY LINK QUALITY FEEDBACK FOR
   POINT TO POINT MISO CHANNELS}},
Booktitle = {{2014 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
