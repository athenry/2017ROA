@article{ ISI:000209429300012,
Author = {Daoutidis, Prodromos and Kelloway, Adam and Marvin, W. Alex and
   Rangarajan, Srinivas and Torres, Ana I.},
Title = {{Process systems engineering for biorefineries: new research vistas}},
Journal = {{CURRENT OPINION IN CHEMICAL ENGINEERING}},
Year = {{2013}},
Volume = {{2}},
Number = {{4}},
Pages = {{442-447}},
Month = {{NOV}},
Abstract = {{Biorefineries are envisioned as complex, integrated plants capable of
   using biomass feedstocks to produce a broad spectrum of chemical and
   fuel products. Their development is favored due to ample availability of
   biomass resources, and policy and economic incentives. Their design and
   deployment requires choices on scale, location, product portfolio and
   technologies to be used. Biomass processing also requires new chemical
   transformations and process designs, and is subject to unique logistical
   challenges. These factors open up exciting research vistas for process
   systems engineering research, ranging from enterprise-level analysis to
   process modeling and design. This paper highlights recent progress and
   future research opportunities in this emerging field.}},
