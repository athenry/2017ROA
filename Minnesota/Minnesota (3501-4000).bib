@article{ ISI:000346388200003,
Author = {Jiang, Jingkun and Kim, Chungman and Wang, Xiaoliang and Stolzenburg,
   Mark R. and Kaufman, Stanley L. and Qi, Chaolong and Sem, Gilmore J. and
   Sakurai, Hiromu and Hama, Naoya and McMurry, Peter H.},
Title = {{Aerosol Charge Fractions Downstream of Six Bipolar Chargers: Effects of
   Ion Source, Source Activity, and Flowrate}},
Journal = {{AEROSOL SCIENCE AND TECHNOLOGY}},
Year = {{2014}},
Volume = {{48}},
Number = {{12}},
Pages = {{1207-1216}},
Abstract = {{Bipolar diffusion charging is used routinely in aerosol electrical
   mobility size distribution measurements. In this study, aerosol charge
   fractions produced by six bipolar chargers (neutralizers) were measured
   using a tandem differential mobility analyzer system. Factors that were
   studied include the type of ion source (Po-210, Kr-85, Am-241, and soft
   X-ray), source activity, charger design, and aerosol flowrate. It was
   found that all six types of neutralizers achieve stationary state charge
   distributions when the source activity is sufficiently high. For Po-210
   neutralizers with an initial radioactivity of 18.5 MBq (0.5 mCi),
   stationary state charge distributions are achieved when the source is
   less than 3.25 years old (residual activity no less than 0.0527 MBq).
   Stationary state was achieved for Kr-85 neutralizers having residual
   radioactivity greater than 70 MBq. Source activities of Am-241 and soft
   X-ray neutralizers are discussed. Aerosol charge fractions for six
   neutralizers remain reasonably invariant over a wide range of flowrates.
   The positive charge fractions achieved by the soft X-ray neutralizer are
   higher than those by the other five neutralizers using radioactive
   sources while negative charge fractions for all neutralizers studied are
   all in a similar range. This study also raises questions about bipolar
   charging fractions used for data inversion in some scanning mobility
   particle spectrometer (SMPS) systems, and underscores the need to better
   understand bipolar charging to achieve more accurate measurements of
   particle size distributions.}},
Publisher = {{TAYLOR \& FRANCIS INC}},
Address = {{530 WALNUT STREET, STE 850, PHILADELPHIA, PA 19106 USA}},
Type = {{Article}},
Language = {{English}},
