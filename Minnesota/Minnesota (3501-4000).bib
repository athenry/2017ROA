@article{ ISI:000326570300036,
Author = {Chiang, Hung-Sheng and Leger, James R. and Nilsson, Johan and Sahu,
   Jayanta},
Title = {{Direct observation of Kramers-Kronig self-phasing in coherently combined
   fiber lasers}},
Journal = {{OPTICS LETTERS}},
Year = {{2013}},
Volume = {{38}},
Number = {{20}},
Pages = {{4104-4107}},
Month = {{OCT 15}},
Abstract = {{A highly stable coherent beam-combining system has been designed to
   measure self-phasing in fiber lasers due to nonlinear effects. Whereas
   self-phasing in previous coherent combination experiments has been
   principally attributed to wavelength shifting, these wavelength effects
   have been efficiently suppressed in our experiment by using a dual-core
   fiber with closely balanced optical path lengths. The self-phasing from
   nonlinear effects could then be measured independently and directly by
   common-path interferometry with a probe laser. The Kramers-Kronig effect
   in the fiber gain media was observed to induce a phase shift that
   effectively canceled the applied path length errors, resulting in
   efficient lasing under all phase conditions. This process was
   demonstrated to result in robust lasing over a large range of pump
   conditions. (C) 2013 Optical Society of America}},
