@article{ ISI:000331077600001,
Author = {Ebtehaj, Ardeshir M. and Zupanski, Milija and Lerman, Gilad and
   Foufoula-Georgiou, Efi},
Title = {{Variational data assimilation via sparse regularisation}},
Journal = {{TELLUS SERIES A-DYNAMIC METEOROLOGY AND OCEANOGRAPHY}},
Year = {{2014}},
Volume = {{66}},
Abstract = {{This paper studies the role of sparse regularisation in a properly
   chosen basis for variational data assimilation (VDA) problems.
   Specifically, it focuses on data assimilation of noisy and down-sampled
   observations while the state variable of interest exhibits sparsity in
   the real or transform domains. We show that in the presence of sparsity,
   the l(1)-norm regularisation produces more accurate and stable solutions
   than the classic VDA methods. We recast the VDA problem under the
   l(1)-norm regularisation into a constrained quadratic programming
   problem and propose an efficient gradient-based approach, suitable for
   large-dimensional systems. The proof of concept is examined via
   assimilation experiments in the wavelet and spectral domain using the
   linear advection-diffusion equation.}},
