@article{ ISI:000391107600476,
Author = {Ge, Quanbo and Wen, Chenglin and Chen, Shaodong and Sun, Ruoyu and Li,
   Yuan},
Title = {{Adaptive Cubature Strong Tracking Information Filter Using Variational
   Bayesian Method}},
Journal = {{IFAC PAPERSONLINE}},
Year = {{2014}},
