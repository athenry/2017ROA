@article{ ISI:000326788300003,
Author = {Morales, Angel D. Cortes and Economou, Ioannis G. and Peters, Cornelis
   J. and Siepmann, J. Ilja},
Title = {{Influence of simulation protocols on the efficiency of Gibbs ensemble
   Monte Carlo simulations}},
Journal = {{MOLECULAR SIMULATION}},
Year = {{2013}},
Volume = {{39}},
Number = {{14-15, SI}},
Pages = {{1135-1142}},
Month = {{DEC 1}},
Abstract = {{The Gibbs ensemble Monte Carlo (GEMC) method is a versatile approach for
   the prediction of fluid phase equilibria from particle-based
   simulations. For a one-component system, a GEMC simulation utilises two
   separate simulation boxes for the vapour and the liquid phases and a
   significant fraction of the computational effort is expended on special
   trial moves that transfer (swap) particles and exchange volume between
   the two boxes. The user needs to specify the frequency of swap and
   volume moves and the overall volume that controls the phase ratio. In
   this study, the efficiency of GEMC simulation protocols that yield three
   different frequencies of accepted swap and volume moves and three
   different phase ratios is assessed for the computation of the saturated
   vapour pressure and liquid density of n-octane and water at three
   reduced temperatures. Differences in the simulation efficiency of up to
   an order of magnitude are observed, and recommendations are made for
   suitable GEMC simulation protocols.}},
