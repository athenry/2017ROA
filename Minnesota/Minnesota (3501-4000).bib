@article{ ISI:000330535500004,
Author = {Ye, Lei and Zhang, Pengyuan and Duval, Sue and Su, Liping and Xiong,
   Qiang and Zhang, Jianyi},
Title = {{Thymosin beta 4 Increases the Potency of Transplanted Mesenchymal Stem
   Cells for Myocardial Repair}},
Journal = {{CIRCULATION}},
Year = {{2013}},
Volume = {{128}},
Number = {{11, 1}},
Pages = {{S32-S41}},
Month = {{SEP 10}},
Note = {{Scientific Sessions of the American-Heart-Association (AHA), Los
   Angeles, CA, NOV 03-07, 2012}},
Organization = {{Amer Heart Assoc}},
Abstract = {{Background-Thymosin beta 4 (T beta 4) has been shown to enhance the
   survival of cultured cardiomyocytes. Here, we investigated whether the
   cytoprotective effects of T beta 4 can increase the effectiveness of
   transplanted swine mesenchymal stem cells (sMSCs) for cardiac repair in
   a rat model of myocardial infarction (MI).
   Methods and Results-Under hypoxic conditions, cellular damage (lactate
   dehydrogenase leakage), apoptosis (terminal deoxynucleotidyl transferase
   dUTP nick end labelingc cells), and caspase-8 activity were
   significantly lower, whereas B-cell lymphoma-extra large protein
   expression was significantly higher, in sMSCs cultured with T beta 4 (1
   mu g/mL) than in sMSCs cultured without T beta 4, and T beta 4 also
   increased sMSC proliferation. For in vivo experiments, animals were
   treated with basal medium (MI: n=6), a fibrin patch (Patch: n=6), a
   patch containing sMSCs (sMSC: n=9), or a patch containing sMSCs and T
   beta 4 (sMSC/T beta 4: n=11); T beta 4 was encapsulated in gelatin
   microspheres to extend T beta 4 delivery. Four weeks after treatment,
   echocardiographic assessments of left-ventricular ejection fraction and
   fractional shortening were significantly better (P<0.05) in sMSC/T beta
   4 animals (left-ventricular ejection fraction=51.7 +/- 1.1\%; fractional
   shortening=26.7 +/- 0.7\%) than in animals from MI (39 +/- 3\%; 19.5 +/-
   1.7\%) and Patch (43 +/- 1.4\%; 21.6 +/- 0.9\%) groups. Histological
   assessment of infarct wall thickness was significantly higher (P<0.05)
   in sMSC/T beta 4 animals (50\%, {[}45\%, 80\%]) than in animals from MI
   (25\%, {[}20\%, 25\%]) group. Measurements in sMSC (left-ventricular
   ejection fraction=45 +/- 2.6\%; fractional shortening=22.9 +/- 1.6\%;
   TH=43\% {[}25\%, 45\%]), Patch, and MI animals were similar. T beta 4
   administration also significantly increased vascular growth, the
   retention/survival of the transplanted sMSCs, and the recruitment of
   endogenous c-Kit(+) progenitor cells to the infarcted region.
   Conclusions-Extended-release T beta 4 administration improves the
   retention, survival, and regenerative potency of transplanted sMSCs
   after myocardial injury.}},
