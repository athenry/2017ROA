@article{ ISI:000326669400041,
Author = {Pustulka, Kevin M. and Wohl, Adam R. and Lee, Han Seung and Michel,
   Andrew R. and Han, Jing and Hoye, Thomas R. and McCormick, Alon V. and
   Panyam, Jayanth and Macosko, Christopher W.},
Title = {{Flash Nanoprecipitation: Particle Structure and Stability}},
Journal = {{MOLECULAR PHARMACEUTICS}},
Year = {{2013}},
Volume = {{10}},
Number = {{11}},
Pages = {{4367-4377}},
Month = {{NOV}},
Abstract = {{Flash nanoprecipitation (FNP) is a process that, through rapid mixing,
   stabilizes an insoluble low molecular weight compound in a nanosized,
   polymer-stabilized delivery vehicle. The polymeric components are
   typically amphiphilic diblock copolymers (BCPs). In order to fully
   exploit the potential of FNP, factors affecting particle structure,
   size, and stability must be understood. Here we show that polymer type,
   hydrophobicity and crystallinity of the small molecule, and small
   molecule loading levels all affect particle size and stability. Of the
   four block copolymers (BCP) that we have studied here, poly(ethylene
   glycol)-b-poly(lactic-co-glycolic acid) (PEG-b-PLGA) was most suitable
   for potential drug delivery applications due to its ability to give rise
   to stable nanoparticles, its biocompatibility, and its degradability. We
   found little difference in particle size when using PLGA block sizes
   over the range of 5 to 15 IcDa. The choice of hydrophobic small molecule
   was important, as molecules with a calculated water-octanol partition
   coefficient (clogP) below 6 gave rise to particles that were unstable
   and underwent rapid Ostwald ripening. Studies probing the internal
   structure of nanoparticles were also performed. Analysis of differential
   scanning calorimetry (DSC), cryogenic transmission electron microscopy
   (cryo-TEM), and H-1 NMR experiments support a three-layer
   core-shell-corona nanoparticle structure.}},
