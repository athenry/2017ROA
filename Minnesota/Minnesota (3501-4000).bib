@article{ ISI:000340195100002,
Author = {Falchetto, Augusto Cannone and Marasteanu, Mihai O. and Balmurugan,
   Sreelatha and Negulescu, Ioan I.},
Title = {{Investigation of asphalt mixture strength at low temperatures with the
   bending beam rheometer}},
Journal = {{ROAD MATERIALS AND PAVEMENT DESIGN}},
Year = {{2014}},
Volume = {{15}},
Number = {{1, SI}},
Pages = {{28-44}},
Abstract = {{A new strength test method for asphalt mixture at low temperatures is
   presented in this paper. The method uses a modified bending beam
   Rheometer (BBR), capable of applying loads at different rates. Two major
   factors, that can significantly affect strength, were analysed: cooling
   medium and specimen size. Similar strength values were obtained in air
   and potassium acetate and significantly lower values were obtained in
   ethanol. Preliminary investigation with sodium fluorescein (FL)
   indicates the fluorescing marker diffused in mixtures when ethanol was
   used. The failure distributions of BBR mixture beams and of larger beams
   (LBs) were analysed using histogram testing and size effect theory.
   Different Weibull moduli were obtained for BBR and for the LB, which
   indicates that BBR beams may be too small to capture the representative
   volume element of the material.}},
