@article{ ISI:000336777300034,
Author = {Jiang, Xinlong and Yang, Liqun and Yang, Qing and Xiong, Chunhua and
   Jiang, Jianxiong and Jia, Qian and Chen, Qing},
Title = {{Adsorption Behaviour of Co(II) from Aqueous Solutions onto D113-III
   Resin}},
Journal = {{ASIAN JOURNAL OF CHEMISTRY}},
Year = {{2014}},
Volume = {{26}},
Number = {{7, A}},
Pages = {{2022-2026}},
Month = {{APR}},
Abstract = {{In this study, the adsorption and desorption properties of Co(II) on
   D113:III resin have been studied. The different variables: affecting the
   adsorption capacity Such as pH of the solution; temperature and
   contact-time have been investigated, which indicate that D113-III resit
   could adsorb Co(l) effectively from aqueous solution. The results show
   that the optimal adsorption condition of D113-III resin for CO(II) is
   achieved at pH = 6.5. The maximum saturated adsorption Capacity of
   CO(II) is 192.1 mg.g(-1) D113-III resin rat 298 K. The thermodynamic
   parameters such as Delta G degrees which were all negative and Delta H
   degrees Which was positive, indicated that the adsorption of Co(II) ion
   onto D113-III resin was spontaneous and endothermic, respectively.
   Positive Delta S degrees value of Co(II) adsorption process indicates an
   irregular increase of the randomness at the D113-III resin-solution
   interface during adsorption. Adsorption isotherms correlated well with
   the Langmuir model. Experimental data were also evaluated to find out
   kinetic characteristics Of the adsorption process. Desorption: studies
   revealed that Co(II) ion could be eluted effectively by using the
   0.25-1.0 mol L-1 HCl solution. Thomas Model was applied to experimental
   data to predict the breakthrough curves and to determine the
   characteristics parameters of the column Useful for process design.}},
