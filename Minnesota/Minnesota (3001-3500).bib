@inproceedings{ ISI:000346492605030,
Author = {Cheong, Kai Loon and Du, Zhekang and Li, Perry Y. and Chase, Thomas R.},
Book-Group-Author = {{IEEE}},
Title = {{Hierarchical Control Strategy for a Hybrid Hydro-mechanical Transmission
   (HMT) Power-Train}},
Booktitle = {{2014 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
