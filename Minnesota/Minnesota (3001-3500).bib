@article{ ISI:000338089400020,
Author = {Barrett, John S. F. and Abdala, Ahmed A. and Srienc, Friedrich},
Title = {{Poly(hydroxyalkanoate) Elastomers and Their Graphene Nanocomposites}},
Journal = {{MACROMOLECULES}},
