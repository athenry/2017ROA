Author = {Gladen, Adam C. and Davidson, Jane H. and Mantell, Susan C.},
Book-Group-Author = {{ASME}},
Title = {{THE EFFECT OF A THERMOTROPIC MATERIAL ON THE OPTICAL EFFICIENCY AND
