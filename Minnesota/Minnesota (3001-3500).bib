@article{ ISI:000333383000003,
Author = {Sun, Tao and Zhang, Dong-Bo and Wentzcovitch, Renata M.},
Title = {{Dynamic stabilization of cubic CaSiO3 perovskite at high temperatures
   and pressures from ab initio molecular dynamics}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2014}},
Volume = {{89}},
Number = {{9}},
Month = {{MAR 26}},
Abstract = {{The stability of cubic CaSiO3 perovskite (CaPv) at high temperatures and
   pressures is investigated by vibrational normal-mode analysis. We
   compute power spectra of mode autocorrelation functions using a recently
   developed hybrid approach combining ab initio molecular dynamics with
   lattice dynamics. These power spectra, together with the probability
   distributions of atomic displacements, indicate that cubic CaPv is
   stabilized at T similar to 600 K and P similar to 26 GPa. We then
   utilize the concept of phonon quasiparticles to characterize the
   vibrational properties of cubic CaPv at high temperature and obtain
   anharmonic phonon dispersions through the whole Brillouin zone. Such
   temperature-dependent phonon dispersions pave the way for more accurate
   calculations of free-energy, thermodynamic, and thermoelastic properties
   of cubic CaPv at Earth's lower mantle conditions.}},
