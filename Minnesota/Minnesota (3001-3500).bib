@article{ ISI:000335795800003,
Author = {Nikiforov, I. and Dontsova, E. and James, R. D. and Dumitrica, T.},
Title = {{Tight-binding theory of graphene bending}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2014}},
Volume = {{89}},
Number = {{15}},
Month = {{APR 30}},
Abstract = {{We model bent graphene as a large-radius (> 1 nm) single-walled carbon
   nanotube in order to uncover the origin of its bending rigidity. An
   analytic formula based on the Huckel molecular orbital theory connects
   the finite resistance to bending directly to the curvature-induced
   torsional misalignment of the pi orbitals no longer of pure p character.
   The analytical predictions are supported by direct density functional
   theory-based tight-binding objective molecular dynamics calculations.
   Our tight-binding theory also gives useful insight into modeling
   graphene bending with continuum plate mechanics and classical
   interatomic potentials.}},
