@article{ ISI:000333489500002,
Author = {Ilias, Samia and Bhan, Aditya},
Title = {{The mechanism of aromatic dealkylation in methanol-to-hydrocarbons
   conversion on H-ZSM-5: What are the aromatic precursors to light
   olefins?}},
