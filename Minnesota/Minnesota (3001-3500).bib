@inproceedings{ ISI:000348253202007,
Author = {Ulavi, Tejas and Hebrink, Tim and Davidson, Jane H.},
Editor = {{Lentz, A and Renne, D}},
Title = {{Analysis of a Hybrid Solar Window for Building Integration}},
Booktitle = {{2013 ISES SOLAR WORLD CONGRESS}},
Series = {{Energy Procedia}},
Year = {{2014}},
Volume = {{57}},
Pages = {{1941-1950}},
Note = {{ISES Solar World Congress (SWC), Cancun, MEXICO, NOV 03-07, 2013}},
Organization = {{ISES}},
Abstract = {{The technical performance of a hybrid `solar window' that provides
   heating in addition to daylighting is evaluated. A wavelength selective
   film is coupled with a compound parabolic concentrator (CPC) to reflect
   and concentrate the infrared portion of the solar spectrum onto a
   tubular absorber while transmitting the visible portion of the spectrum
   into the interior space. The optical performance of the CPC/selective
   film is predicted using a Monte Carlo Ray-Tracing model. An adaptive
   concentrator geometry based on asymmetrical truncation of CPCs is
   analyzed for vertical windows. The predicted visible transmittance is
   0.61 to 0.67 for double glazed windows. The solar heat gain coefficient
   and the U-factor are comparable to existing glazing technology. The
   annual thermal efficiency for double glazed windows based on use in
   Minneapolis, MN is 19-24\%. (C) 2014 Published by Elsevier Ltd.}},
