@inproceedings{ ISI:000380904701106,
Author = {Rawat, Ankit Singh and Song, Zhao and Dimakis, Alexandros G. and Gal,
   Anna},
Book-Group-Author = {{IEEE}},
Title = {{Batch Codes through Dense Graphs without Short Cycles}},
Booktitle = {{2015 IEEE INTERNATIONAL SYMPOSIUM ON INFORMATION THEORY (ISIT)}},
Series = {{IEEE International Symposium on Information Theory}},
Year = {{2015}},
Pages = {{1477-1481}},
Note = {{IEEE International Symposium on Information Theory (ISIT), Hong Kong,
