@article{ ISI:000349847000009,
Author = {Matthews, Devin A. and Stanton, John F.},
Title = {{Non-orthogonal spin-adaptation of coupled cluster methods: A new
   implementation of methods including quadruple excitations}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{2015}},
Volume = {{142}},
Number = {{6}},
Month = {{FEB 14}},
Abstract = {{The theory of non-orthogonal spin-adaptation for closed-shell molecular
   systems is applied to coupled cluster methods with quadruple excitations
   (CCSDTQ). Calculations at this level of detail are of critical
   importance in describing the properties of molecular systems to an
   accuracy which can meet or exceed modern experimental techniques. Such
   calculations are of significant (and growing) importance in such fields
   as thermodynamics, kinetics, and atomic and molecular spectroscopies.
   With respect to the implementation of CCSDTQ and related methods, we
   show that there are significant advantages to non-orthogonal
   spin-adaption with respect to simplification and factorization of the
   working equations and to creating an efficient implementation. The
   resulting algorithm is implemented in the CFOUR program suite for CCSDT,
   CCSDTQ, and various approximate methods (CCSD(T), CC3, CCSDT-n, and
   CCSDT(Q)). (C) 2015 AIP Publishing LLC.}},
