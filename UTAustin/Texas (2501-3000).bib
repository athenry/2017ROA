@inproceedings{ ISI:000371708104093,
Author = {Lin, Xingqin and Heath, Jr., Robert W. and Andrews, Jeffrey G.},
Book-Group-Author = {{IEEE}},
Title = {{Spectral Efficiency of Massive MIMO Systems with D2D Underlay}},
