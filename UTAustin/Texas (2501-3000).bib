@inproceedings{ ISI:000380564900012,
Author = {Zhu, Yuhao and Halpern, Matthew and Reddi, Vijay Janapa},
Book-Group-Author = {{IEEE}},
Title = {{Event-Based Scheduling for Energy-Efficient QoS (eQoS) in Mobile Web
   Applications}},
