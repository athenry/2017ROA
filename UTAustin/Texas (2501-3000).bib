@article{ ISI:000351681300018,
Author = {Weathers, Annie and Khan, Zia Ullah and Brooke, Robert and Evans, Drew
   and Pettes, Michael T. and Andreasen, Jens Wenzel and Crispin, Xavier
   and Shi, Li},
Title = {{Significant Electronic Thermal Transport in the Conducting Polymer
   Poly(3,4-ethylenedioxythiophene)}},
Journal = {{ADVANCED MATERIALS}},
Year = {{2015}},
Volume = {{27}},
Number = {{12}},
Pages = {{2101-2106}},
Month = {{MAR 25}},
Abstract = {{Suspended microdevices are employed to measure the in-plane electrical
   conductivity, thermal conductivity, and Seebeck coefficient of suspended
   poly(3,4-ethylenedioxythiophene) (PEDOT) thin films. The measured
   thermal conductivity is higher than previously reported for PEDOT and
