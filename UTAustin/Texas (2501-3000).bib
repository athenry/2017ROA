@inproceedings{ ISI:000381084300014,
Author = {Huaco, G. D. and Jirsa, J. O.},
Editor = {{Leon, RT}},
Title = {{Modeling the Performance of Rehabilitated Extremely Damaged Concrete
   Columns and a Masonry Wall for Analysis and Design}},
Booktitle = {{IMPROVING THE SEISMIC PERFORMANCE OF EXISTING BUILDINGS AND OTHER
   STRUCTURES 2015}},
