@article{ ISI:000349726400004,
Author = {Khan, Mubassira and Abdel-Rahim, Ahmed and Williams, Christopher J.},
Title = {{Potential crash reduction benefits of shoulder rumble strips in two-lane
   rural highways}},
Journal = {{ACCIDENT ANALYSIS AND PREVENTION}},
Year = {{2015}},
Volume = {{75}},
Pages = {{35-42}},
Month = {{FEB}},
Abstract = {{This paper reports the findings from a study aimed at examining the
   effectiveness of shoulder rumble strips in reducing run-off-the-road
   (ROR) crashes on two-lane rural highways using the empirical Bayes (EB)
   before-and-after analysis method. Specifically, the study analyzed the
   effects of traffic volume, roadway geometry and paved right shoulder
   width on the effectiveness of shoulder rumble strips. The results of
   this study demonstrate the safety benefits of shoulder rumble strips in
   reducing the ROR crashes on two-lane rural highways using the state of
   Idaho 2001-2009 crash data. This study revealed a 14\% reduction in all
   ROR crashes after the installation of shoulder rumble strips on 178.63
   miles of two-lane rural highways in Idaho. The results indicate that
   shoulder rumble strips were most effective on roads with relatively
   moderate curvature and right paved shoulder width of 3 feet and more.
   (C) 2014 Elsevier Ltd. All rights reserved.}},
