@article{ ISI:000350878500143,
Author = {Zou, Yi and Chakravarty, Swapnajit and Wray, Parker and Chen, Ray T.},
Title = {{Experimental demonstration of propagation characteristics of
   mid-infrared photonic crystal waveguides in silicon-on-sapphire}},
Journal = {{OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{23}},
Number = {{5}},
Pages = {{6965-6975}},
Month = {{MAR 9}},
Abstract = {{We provide the first experimental demonstration of optical transmission
   characteristics of a W1 photonic crystal waveguide in silicon on
   sapphire at mid infrared wavelength of 3.43 mu m. Devices are studied as
   a function of lattice constant to tune the photonic stop band across the
   single wavelength of the source laser. The shift in the transmission
   profile as a function of temperature and refractive index is
   experimentally demonstrated and compared with simulations. In addition
   to zero transmission in the stop gap, propagation losses less than 20
   dB/cm are observed for group indices greater than 20 below the light
   line while more than 300 dB/cm propagation losses are observed above the
   light line, characteristic of the waveguiding behavior of photonic
   crystal line defect modes. 2015 Optical Society of America}},
