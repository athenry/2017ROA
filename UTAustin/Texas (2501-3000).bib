@inproceedings{ ISI:000370720100107,
Author = {Arapostathis, Ari and Biswas, Anup and Pang, Guodong},
Book-Group-Author = {{IEEE}},
Title = {{Optimal Scheduling of A Large-Scale Multiclass Parallel Server System
   with Ergodic Cost}},
