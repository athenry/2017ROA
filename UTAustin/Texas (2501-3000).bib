@inproceedings{ ISI:000380564900009,
Author = {Kim, Jungrae and Sullivan, Michael and Erez, Mattan},
Book-Group-Author = {{IEEE}},
Title = {{Bamboo ECC: Strong, Safe, and Flexible Codes for Reliable Computer
   Memory}},
