@article{ ISI:000353931200026,
Author = {Yu, Guangzhe and Dong, Jiannan and Foster, Lynn M. and Metaxas, Athena
   E. and Truskett, Thomas M. and Johnston, Keith P.},
Title = {{Breakup of Oil Jets into Droplets in Seawater with Environmentally
