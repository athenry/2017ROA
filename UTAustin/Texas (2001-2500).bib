@article{ ISI:000360448700009,
Author = {Li, Xiao and Heath, Jr., Robert W. and Linehan, Kevin and Butler, Ray},
Title = {{Metrocell Antennas The Positive Impact of a Narrow Vertical Beamwidth
   and Electrical Downtilt}},
Journal = {{IEEE VEHICULAR TECHNOLOGY MAGAZINE}},
Year = {{2015}},
Volume = {{10}},
Number = {{3}},
Pages = {{51-59}},
Month = {{SEP}},
Abstract = {{This article discusses the positive impact that metrocell antennas with
   a narrow vertical beamwidth and electrical downtilt can have on
   heterogeneous cellular networks. Using a model of random cell placement
   based on a Poisson distribution, along with an innovative
   three-dimensional (3-D) building model that quantifies blockage due to
   shadowing, it is demonstrated that the network spectral efficiency and
   average user throughput both increase as the vertical beamwidth
   decreases and downtilt is applied to metrocell transmission. Moreover,
   the network becomes more energy efficient. Importantly, these additional
   gains in network performance can be achieved without any cooperation or
   exchange of information between macrocell base stations (BSs) and
   metrocells.}},
