@article{ ISI:000356755300035,
Author = {Bhandari, Rohit and Anderson, Rachel M. and Stauffer, Shannon and Dylla,
   Anthony G. and Henkeman, Graeme and Stevenson, Keith J. and Crooks,
   Richard M.},
Title = {{Electrochemical Activity of Dendrimer-Stabilized Tin Nanoparticles for
   Lithium Alloying Reactions}},
Journal = {{LANGMUIR}},
Year = {{2015}},
Volume = {{31}},
Number = {{23}},
Pages = {{6570-6576}},
Month = {{JUN 16}},
Abstract = {{The synthesis and characterization of Sn nanoparticles in organic
   solvents using sixth-generation dendrimers modified on their periphery
   with hydrophobic groups as stabilizers are repotted. Sn2+:dendrimer
   ratios of 147 and 225 were employed for the synthesis, corresponding to
   formation Of Sn-147 and Sn-225 dendritner-Stabilized nanoparticles
   (DSNs). Transmission electron microscopy analysis indicated the presence
   of Ultrasmall Sn nanoparticles having an average size of 3.0-5.0 rim.
   X-ray absorption spectroscopy suggested the presence of Sn nanoparticles
   with only partially oxidized surfaces. Cyclic voltammetry studies of the
   So DSNs for Li alloying/dealloying reactions demonstrated good
   reversibility. Control experiments carried out in the absence of DSNs
   clearly indicated that these ultrasmall Sn DSNs react directly with Li
