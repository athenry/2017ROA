@article{ ISI:000353176100021,
Author = {Su, Norman C. and Smith, Zachary P. and Freeman, Benny D. and Urban,
   Jeffrey J.},
Title = {{Size-Dependent Permeability Deviations from Maxwell's Model in Hybrid
   Cross-Linked Poly(ethylene glycol)/Silica Nanoparticle Membranes}},
Journal = {{CHEMISTRY OF MATERIALS}},
