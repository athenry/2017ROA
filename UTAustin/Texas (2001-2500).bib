@article{ ISI:000357623500008,
Author = {Dang, Hoang X. and Rettie, Alexander J. E. and Mullins, C. Buddie},
Title = {{Visible-Light-Active NiV2O6 Films for Photoelectrochemical Water
   Oxidation}},
