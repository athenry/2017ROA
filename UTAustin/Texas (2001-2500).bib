@article{ ISI:000365412900121,
Author = {Yu, Haiyang and Xu, Yibo and Cheng, Shiqing and Zhang, Tiantian and
   Kang, Xiaodong and Zeng, Yang},
Title = {{The Effect of Injection Velocity on Injectivity Profile Reverse
   Phenomenon in Polymer Flooding Reservoirs}},
Journal = {{JOURNAL OF COMPUTATIONAL AND THEORETICAL NANOSCIENCE}},
Year = {{2015}},
Volume = {{12}},
Number = {{9}},
Pages = {{2817-2820}},
Month = {{SEP}},
Abstract = {{Polymer flooding, as an enhanced oil recovery (EOR) method, has been
   widely used in petroleum engineering. However, as polymer flooding in
   oil reservoirs enters mid-and-late stages, `injectivity profile reverse
   phenomenon' appears in some blocks with strong anisotropy where more
   fluids transport through the formation with higher permeability than
   that in water flooding reservoirs, while the formation with lower
   permeability absorb fewer fluids. This phenomenon dramatically affects
   the performance of polymer flooding, resulting in unsatisfied EOR. In
   this work, the regularity of injectivity profile reverse phenomenon is
   analyzed, and the effects of injection velocity and molecular weight of
   polymer are investigated, The results show that bigger molecular weight
   of polymer and higher injection velocity result in earlier appearance of
   injectivity profile reverse phenomenon and decreased cumulative
   imbibition rate of low-permeability layer, which leads to lower sweep
   efficiency of polymer flooding under experimental conditions. This work
   explores the feasible application of proper methods to relieve the
   injectivity profile reverse phenomenon and to improve oil recovery, such
   as multi-layer polymer injection, injection velocity control,
   polymer-water alternative flooding, and deep profile control ahead of
   polymer injection.}},
