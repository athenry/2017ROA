@article{ ISI:000357693200013,
Author = {Tymchenko, M. and Gavrikov, V. K. and Spevak, I. S. and Kuzmenko, A. A.
   and Kats, A. V.},
Title = {{Quasi-resonant enhancement of a grazing diffracted wave and deep
   suppression of specular reflection on shallow metal gratings in
   terahertz}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{106}},
Number = {{26}},
Month = {{JUN 29}},
Abstract = {{We report on a previously unexamined anomaly at diffraction of THz
   radiation on metal gratings. This anomaly consists in a nearly complete
   redirection of energy to a non-specular homogeneous diffraction order
   propagating at a specific grazing angle and deep suppression of the
   specular reflection. We show that this anomaly is located far enough
   from the well-known one caused by the resonant excitation of the surface
   plasmon-polariton. The effect under consideration is of general nature
   and can exist in all spectrum regions; however, it is especially
   pronounced in THz region, where it should be taken into account when
   analyzing relevant experimental results. (C) 2015 AIP Publishing LLC.}},
