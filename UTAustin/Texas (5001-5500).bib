@article{ ISI:000324510100014,
Author = {Narayanamoorthy, Sriram and Paleti, Rajesh and Bhat, Chandra R.},
Title = {{On accommodating spatial dependence in bicycle and pedestrian injury
   counts by severity level}},
Journal = {{TRANSPORTATION RESEARCH PART B-METHODOLOGICAL}},
Year = {{2013}},
Volume = {{55}},
Pages = {{245-264}},
Month = {{SEP}},
Abstract = {{This paper proposes a new spatial multivariate count model to jointly
   analyze the traffic crash-related counts of pedestrians and bicyclists
   by injury severity. The modeling framework is applied to predict injury
   counts at a Census tract level, based on crash data from Manhattan, New
   York. The results highlight the need to use a multivariate modeling
   system for the analysis of injury counts by road-user type and injury
   severity level, while also accommodating spatial dependence effects in
