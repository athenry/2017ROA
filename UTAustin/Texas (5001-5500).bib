@article{ ISI:000322236400035,
Author = {Hoang, Son and Ngo, Thong Q. and Berglund, Sean P. and Fullon, Raymond
   R. and Ekerdt, John G. and Mullins, C. Buddie},
Title = {{Improvement of Solar Energy Conversion with Nb-Incorporated TiO2
   Hierarchical Microspheres}},
Journal = {{CHEMPHYSCHEM}},
Year = {{2013}},
Volume = {{14}},
Number = {{10, SI}},
Pages = {{2270-2276}},
Month = {{JUL 22}},
Abstract = {{Niobium-modified (2) hierarchical spherical micrometer-size particles,
   which consist of many nanowires, are synthesized by solvothermal
   synthesis and studied as photoelectrodes for water photo-oxidation and
   dye-sensitized solar cell (DSSC) applications. Incorporation of Nb leads
   to a rutile-to-anatase (2) phase transition in the (2) hierarchical
   spheres (HSs), with the anatase percentage increasing from 0\% for the
   pristine (2) HSs to 47.6\% for the 1.82 at.\% Nb-incorporated (2)
   sample. Incorporation of Nb leads to significant improvements in water
   photo-oxidation with the photocurrents reaching 70.5 Acm(-2) at 1.23 V
   versus the reversible hydrogen electrode, compared with 28.3 Acm(-2) for
   the pristine (2) sample. (2) HS-based DSSCs reaches 6.09 +/- 0.15\% at
   0.25 at.\% Nb, significantly higher than that for the pristine (2) HS
   cells (3.99 +/- 0.02\%). In addition, the incident-photon-to-current
   efficiency spectra for DSSCs show that employing (2) and Nb: (2) HSs
   provides better light harvesting, especially of long-wavelength photons,
   than anatase (2) nanoparticle-based DSSCs.}},
