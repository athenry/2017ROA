@article{ ISI:000326739400005,
Author = {Cholette, Michael E. and Celen, Merve and Djurdjanovic, Dragan and
   Rasberry, John D.},
Title = {{Condition Monitoring and Operational Decision Making in Semiconductor
   Manufacturing}},
