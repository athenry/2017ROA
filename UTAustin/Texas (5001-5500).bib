@article{ ISI:000326714100002,
Author = {Bayer, Carolyn L. and Kelvekar, Juili and Emelianov, Stanislav Y.},
Title = {{Influence of nanosecond pulsed laser irradiance on the viability of
   nanoparticle-loaded cells: implications for safety of contrast-enhanced
   photoacoustic imaging}},
Journal = {{NANOTECHNOLOGY}},
Year = {{2013}},
Volume = {{24}},
Number = {{46}},
Month = {{NOV 22}},
Abstract = {{Photoacoustic imaging, a promising new diagnostic medical imaging
   modality, can provide high contrast images of molecular features by
   introducing highly- absorbing plasmonic nanoparticles. Currently, it is
   uncertain whether the absorption of low fluence pulsed light by
   plasmonic nanoparticles could lead to cellular damage. In our studies we
   have shown that low fluence pulsed laser excitation of accumulated
   nanoparticles at low concentration does not impact cell growth and
   viability, while we identify thresholds at which higher nanoparticle
   concentrations and fluences produce clear evidence of cell death. The
   results provide insights for improved design of photoacoustic contrast
   agents and for applications in combined imaging and therapy.}},
