@article{ ISI:000326279100020,
Author = {Ye, Dexin and Wang, Zhiyu and Xu, Kuiwen and Li, Huan and Huangfu,
   Jiangtao and Wang, Zheng and Ran, Lixin},
Title = {{Ultrawideband Dispersion Control of a Metamaterial Surface for
   Perfectly-Matched-Layer-Like Absorption}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2013}},
Volume = {{111}},
Number = {{18}},
Month = {{OCT 29}},
Abstract = {{Narrow bandwidth is a fundamental issue plaguing practical applications
   of metamaterial absorbers. In this Letter, we show that by deliberately
   controlling the dispersion and dissipation of a metamaterial, an
   ultrawideband perfect metamaterial absorber with complex-valued
   constitutive parameters strictly satisfying the modified model of a
   perfectly matched layer, can be achieved. The nearly perfect power
   absorption, better than 99\%, was experimentally observed in an
   unprecedented bandwidth of 39\%, approaching the theoretical Rozanov
   limit. We expect a wide range of applications to emerge from this
   general concept.}},
