@article{ ISI:000325284500019,
Author = {Kweon, Kyoung E. and Hwang, Gyeong S.},
Title = {{Surface structure and hole localization in bismuth vanadate: A first
   principles study}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{103}},
Number = {{13}},
Month = {{SEP 23}},
Abstract = {{The monoclinic and tetragonal phases of bismuth vanadate (BiVO4) have
   been found to exhibit significantly different photocatalytic activities
   for water splitting. To assess a possible surface effect on the
   phase-dependent behavior, we calculate and compare the geometries and
   electronic structures of the monoclinic and tetragonal BiVO4 (001)
   surfaces using hybrid density functional theory. The relaxed atomic
   configurations of these two surfaces are found to be nearly identical,
   while an excess hole shows a relatively stronger tendency to localize at
   the surface than the bulk in both phases. Possible factors for the
   phase-dependent photocatalytic activity of BiVO4 are discussed. (C) 2013
   AIP Publishing LLC.}},
