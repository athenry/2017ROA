@article{ ISI:000326137500005,
Author = {Landsberger, S. and Brabec, C. and Canion, B. and Hashem, J. and Lu, C.
   and Millsap, D. and George, G.},
Title = {{Determination of Ra-226, Ra-228 and Pb-210 in NORM products from oil and
   gas exploration: Problems in activity underestimation due to the
   presence of metals and self-absorption of photons}},
Journal = {{JOURNAL OF ENVIRONMENTAL RADIOACTIVITY}},
Year = {{2013}},
Volume = {{125}},
Number = {{SI}},
Pages = {{23-26}},
Month = {{NOV}},
Note = {{6th International Symposium on In Situ Nuclear Metrology as a Tool for
   Radioecology (INSINUME), Brussels, BELGIUM, JUN 11-15, 2012}},
Abstract = {{Typical calibration of solid environmental samples for the determination
   of Ra-226, Ra-228 and Pb-210 entails the use of standard reference
   materials which have a very similar matrix. However, TENORM samples from
   the oil and gas exploration contain unusually high amounts of calcium,
   strontium and barium which can severely attenuate the photons of Pb-210
   and Ra-226 with their characteristic 46.1 keV and 186.2 key gamma-rays,
   respectively and to some extent Ra-228 with the characteristic
   gamma-rays of 911.2 keV and 969.0 keV. We used neutron activation
   analysis to evaluate the content of TENORM for calcium, barium and
   strontium and then used a software program SELABS to determine the
   self-absorption. Our results confirm that even in Petrie (R) containers
   with small dimensions the Pb-210 can be underestimated by almost by a
   factor of four while Ra-226 can be underestimated by 5\%. The Ra-228
   activities are virtually unaffected due to the higher energy gamma-rays.
   However, the implications for TENORM studies that employ large Marinelli
   (R) containers having sample sizes between 0.25 and 1.0 L may be
   severely compromised by the presence of high Z elements in elevated
   concentrations. The usual spectral interferences on Ra-226, Ra-228 and
   Pb-210 coming from other radionuclides in the U-254, U-235 and U-238
   decay chains are virtually nonexistent due the very high activity levels
   of Ra-226, Ra-228 and Pb-210 in the tens of thousands of Bq/kg. (C) 2013
   Elsevier Ltd. All rights reserved.}},
