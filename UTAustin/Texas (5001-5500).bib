@article{ ISI:000327591900078,
Author = {Lee, Yongjin and Hwang, Gyeong S.},
Title = {{Microsegregation effects on the thermal conductivity of
   silicon-germanium alloys}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2013}},
Volume = {{114}},
Number = {{17}},
Month = {{NOV 7}},
Abstract = {{A silicon-germanium (SiGe) alloy is a promising candidate for
   thermoelectric materials; while it shows a significantly reduced thermal
   conductivity (k) as compared to pure Si and Ge, the k values obtained
   from previous experiments and computations tend to be widely scattered.
   We present here a computational analysis of thermal transport in SiGe,
   particularly the effects of the local segregation (microsegregation) of
   alloying elements. Our nonequilibrium molecular dynamics simulations
   confirm the strong dependence of k on the Si: Ge ratio and the
   occurrence of the minimum k around Si0.8Ge0.2, consistent with existing
   experimental observations. Moreover, our study clearly demonstrates that
   the k of Si0.8Ge0.2 increases substantially and monotonically as Ge
   atoms undergo segregation; that is, the magnitude of alloy scattering is
   found to be sensitive to homogeneity in the distribution of alloying
   elements. Nonequilibrium Green's function analysis also shows that such
   microsegregation enhances phonon transmission due to the reduced number
   of scattering centers. The findings highlight that distribution
   homogeneity, along with composition, can be a critical factor in
   determining the k of SiGe alloys. (C) 2013 AIP Publishing LLC.}},
