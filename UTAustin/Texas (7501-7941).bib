Title = {{Stream Cipher Hash based Execution Monitoring (SCHEM) Framework for
   Intrusion Detection on Embedded Processors}},
Booktitle = {{2012 IEEE 18TH INTERNATIONAL ON-LINE TESTING SYMPOSIUM (IOLTS)}},
Series = {{IEEE International On-Line Testing Symposium}},
Year = {{2012}},
