@inproceedings{ ISI:000320654000086,
Author = {Yoo, Yongseok and Koyluoglu, O. Ozan and Vishwanath, Sriram and Fiete,
   Ila},
Book-Group-Author = {{IEEE}},
Title = {{Dynamic shift-map coding with side information at the decoder}},
