@inproceedings{ ISI:000304770700012,
Author = {Trivedi, Tanuj and Raffaeli, Frank and Pasupathy, Praveen and
   Abu-Yousef, Ali E. and Chen, Ye and Neikirk, Dean and Wood, Sharon},
Editor = {{Gyekenyesi, AL and Yu, TY and Shull, PJ and Diaz, AA and Wu, HF}},
Title = {{A reflectometer-based reader for passive wireless structural
   surveillance sensors}},
Booktitle = {{NONDESTRUCTIVE CHARACTERIZATION FOR COMPOSITE MATERIALS, AEROSPACE
   ENGINEERING, CIVIL INFRASTRUCTURE, AND HOMELAND SECURITY 2012}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8347}},
