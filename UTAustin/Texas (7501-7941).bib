Title = {{Performance of an Autonomous Multi-Maneuver Algorithm for Lunar
   Trans-Earth Injection}},
Journal = {{JOURNAL OF SPACECRAFT AND ROCKETS}},
Year = {{2012}},
Volume = {{49}},
Number = {{1}},
Pages = {{165-174}},
