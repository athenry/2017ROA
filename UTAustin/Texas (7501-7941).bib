@inproceedings{ ISI:000309117700127,
Author = {Yu, Sheng-Yang and Kwasinski, Alexis},
Book-Group-Author = {{IEEE}},
Title = {{Analysis of a soft-switching technique for isolated time-sharing
   multiple-input converters}},
Booktitle = {{2012 TWENTY-SEVENTH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC)}},
Series = {{Annual IEEE Applied Power Electronics Conference and Exposition (APEC)}},
Year = {{2012}},
