@inproceedings{ ISI:000302580900076,
Author = {Elahi, Sahar and Mancuso, J. Jacob and Feldman, Marc D. and Dijkstra,
   Jouke and Milner, Thomas E.},
Editor = {{Kollias, N and Choi, B and Zeng, H and Kang, HW and Knudsen, BE and Wong, BJF and Ilgner, JF and Izdebski, K and Suter, MJ and Lam, S and Brenner, M and Gregory, KW and Tearney, GJ and Marcu, L and Hirschberg, H and Madsen, S and MahadevanJansen, A and Jansen, ED and Mandelis, A}},
Title = {{Effect of stent surface-scattering properties on IV-OCT images}},
Booktitle = {{PHOTONIC THERAPEUTICS AND DIAGNOSTICS VIII, PTS 1 AND 2}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8207}},
Note = {{Conference on Photonic Therapeutics and Diagnostics VIII, San Francisco,
   CA, JAN 21-24, 2012}},
Organization = {{SPIE}},
Abstract = {{Effect of stent surface-scattering properties on the appearance of stent
   struts in IV-OCT images was examined by simulation of light-stent
   interaction by an optical design software package. A phantom blood
   vessel was constructed from a mix of polydimethylsiloxane (PDMS) and
   titanium dioxide to simulate the elastic and optical scattering
   properties of the arterial wall. A Cordis CYPHER (R) sirolimus-eluting
   stent was deployed within the phantom vessel and high resolution
   Micro-CT images of the stent strut were recorded to create a
   three-dimensional representation that was imported into software. A
   Gaussian surface-scattering model (bi-directional scattering
   distribution function) was assumed for the strut. Simulation of IV-OCT
   catheter and reflection of light from the stent strut was implemented
   for different surface scattering properties. A model of IV-OCT catheter
   was defined in the optical model and the rotation of the light beam over
   the stent strut was simulated. The measured parameters included:
   fraction of the reflected rays returning to the catheter and coordinate
