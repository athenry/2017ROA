@inproceedings{ ISI:000323918000058,
Author = {Rao, Prashant and Deshpande, Ashish D.},
Book-Group-Author = {{ASME}},
Title = {{EFFECTS OF MOMENT ARM VARIATIONS ON THE JOINT STIFFNESS CHARACTERISTICS
   OF A MUSCULO-TENDON DRIVEN JOINT}},
Booktitle = {{PROCEEDINGS OF THE ASME 5TH ANNUAL DYNAMIC SYSTEMS AND CONTROL DIVISION
