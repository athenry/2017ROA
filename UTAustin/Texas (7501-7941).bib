Editor = {{Kollias, N and Choi, B and Zeng, H and Kang, HW and Knudsen, BE and Wong, BJF and Ilgner, JF and Izdebski, K and Suter, MJ and Lam, S and Brenner, M and Gregory, KW and Tearney, GJ and Marcu, L and Hirschberg, H and Madsen, S and MahadevanJansen, A and Jansen, ED and Mandelis, A}},
Title = {{Intraoperative laser speckle contrast imaging for monitoring cerebral
   blood flow: results from a 10-patient pilot study}},
