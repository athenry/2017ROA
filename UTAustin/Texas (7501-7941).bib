@inproceedings{ ISI:000321076700104,
Author = {Fyffe, John R. and Cohen, Stuart M. and Webber, Michael E.},
Book-Group-Author = {{ASME}},
Title = {{COMPARING FLEXIBLE CO2 CAPTURE IN GAS- AND COAL-DOMINATED ELECTRICITY
