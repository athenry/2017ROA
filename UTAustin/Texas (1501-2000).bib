@article{ ISI:000368835300004,
Author = {Lee, Jongbok and Rajeeva, Bharath Bangalore and Yuan, Tianyu and Guo,
   Zi-Hao and Lin, Yen-Hao and Al-Hashimi, Mohammed and Zheng, Yuebing and
   Fang, Lei},
Title = {{Thermodynamic synthesis of solution processable ladder polymers}},
Journal = {{CHEMICAL SCIENCE}},
Year = {{2016}},
Volume = {{7}},
Number = {{2}},
Pages = {{881-889}},
Abstract = {{The synthesis of a carbazole-derived, well-defined ladder polymer was
   achieved under thermodynamic control by employing reversible
   ring-closing olefin metathesis. This unique approach featured mild
   conditions and excellent efficiency, affording the ladder polymer
   backbone with minimum levels of unreacted defects. Rigorous NMR analysis
   on a C-13 isotope-enriched product revealed that the main-chain
   contained less than 1\% of unreacted precursory vinyl groups. The rigid
   conformation of the ladder-type backbone was confirmed by photophysical
   analysis, while the extended rod-like structure was visualized under
   scanning tunneling microscope. Excellent solubility of this polymer in
   common organic solvents allowed for feasible processing of thin films
   using solution-casting techniques. Atomic force microscopy and grazing
   incident X-ray scattering revealed a uniform and amorphous morphology of
   these films, in sharp contrast to the polycrystalline thin films of its
   small molecular counterpart.}},
