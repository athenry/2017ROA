@article{ ISI:000367378500003,
Author = {Chen, Pai-Yen and Le, Khai Q. and Alu, Andrea},
Title = {{Nonlinear nanocircuitry based on quantum tunneling effects}},
Journal = {{MRS COMMUNICATIONS}},
Year = {{2015}},
Volume = {{5}},
Number = {{4}},
Pages = {{565-571}},
Month = {{DEC}},
Abstract = {{Metatronics, or metamaterial-inspired optical nanocircuitry, has
   provided a powerful toolset to tailor and implement modular quasi-static
   circuit functionalities in the optical regime. So far, these concepts
   have been mostly limited to linear operations, while many of the
   relevant operations in integrated circuits require nonlinear responses.
   In this work, we introduce nonlinear infrared nanocircuit elements
   exploiting large quantum conductance driven by photon-assisted tunneling
   and enhanced by hybrid plasmonic nanojunctions. Based on these concepts,
   we present infrared lumped nanocircuit mixers and switches for
   second-harmonic generation, and wide-spectrum self-amplitude modulators
   based on nanorods.}},
