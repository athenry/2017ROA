@inproceedings{ ISI:000377658000016,
Author = {Wang, Zheng and Xu, Xiaochuan and Fan, D. L. and Wang, Yaguo and Chen,
   Ray T.},
Editor = {{Schroder, H and Chen, RT}},
Title = {{High quality factor trapezoidal subwavelength grating waveguide
   micro-ring resonator}},
Booktitle = {{OPTICAL INTERCONNECTS XVI}},
