@article{ ISI:000368404500050,
Author = {Landsberger, S. and Lara, R. and Landsberger, S. G.},
Title = {{NON-DESTRUCTIVE DETERMINATION OF URANIUM, THORIUM AND K-40 IN TOBACCO
   AND THEIR IMPLICATION ON RADIATION DOSE LEVELS TO THE HUMAN BODY}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{2015}},
Volume = {{167}},
Number = {{1-3}},
Pages = {{243-246}},
Month = {{NOV}},
Note = {{9th International Symposium on the Natural Radiation Environment (NRE),
   Hirosaki, JAPAN, SEP 22-26, 2014}},
Organization = {{Hirosaki Univ; Int Atomic Energy Agcy}},
Abstract = {{The naturally occurring radionuclides of U-235, U-238 and Th-232 and
   their daughter products are a potential major source of anthropogenic
   radiation to tobacco smokers. Often overlooked is the presence of K-40
   in tobacco and its implication to radiation dose accumulation in the
   human body. In this study, these three radiation sources have been
   determined in four typical US cigarettes using neutron activation
   analysis (NAA). The NAA reactions of U-238(n, g) U-239, Th-232(n, gamma)
   Th-233 and K-41(n, gamma) K-42 were used to determine U-235, U-238 and
   Th-232 and K-40, respectively. The activity of U-238 can easily be
   determined by epithermal NAA of the U-238(n, gamma) 239U reaction, and
   the activity of (235), (234)(U) can easily be deduced. Using isotopic
   ratios, the activity due to K-40 was found by the determined
   concentrations of K-41 (also by epithermal neutrons) in the bulk
   material. Each gram of total potassium yields 30 Bq of K-40. The annual
   effective dose for smokers using 20 cigarettes per day was calculate to
   be 14.6, 137 and 9 mu Sv y(-1) for U-238,U-235,U-234, Th-232 and K-40,
   respectively. These values are significantly lower that the dose
   received from Po-210 except for Th-232.}},
