@article{ ISI:000366142800002,
Author = {Graham, J. and Kanov, K. and Yang, X. I. A. and Lee, M. and Malaya, N.
   and Lalescu, C. C. and Burns, R. and Eyink, G. and Szalay, A. and Moser,
   R. D. and Meneveau, C.},
Title = {{A Web services accessible database of turbulent channel flow and its use
   for testing a new integral wall model for LES}},
Journal = {{JOURNAL OF TURBULENCE}},
Year = {{2016}},
Volume = {{17}},
Number = {{2}},
Pages = {{181-215}},
Abstract = {{The output from a direct numerical simulation (DNS) of turbulent channel
   flow at Re approximate to 1000 is used to construct a publicly and Web
   services accessible, spatio-temporal database for this flow. The
   simulated channel has a size of 8h x 2h x 3h, where h is the channel
   half-height. Data are stored at 2048 x 512 x 1536 spatial grid points
   for a total of 4000 time samples every 5 time steps of the DNS. These
   cover an entire channel flow-through time, i.e. the time it takes to
   traverse the entire channel length 8h at the mean velocity of the bulk
   flow. Users can access the database through an interface that is based
   on the Web services model and perform numerical experiments on the
   slightly over 100 terabytes (TB) DNS data on their remote platforms,
   such as laptops or local desktops. Additional technical details about
   the pressure calculation, database interpolation, and differentiation
   tools are provided in several appendices. As a sample application of the
   channel flow database, we use it to conduct an a-priori test of a
   recently introduced integral wall model for large eddy simulation of
   wall-bounded turbulent flow. The results are compared with those of the
   equilibrium wall model, showing the strengths of the integral wall model
   as compared to the equilibrium model.}},
