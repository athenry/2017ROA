@article{ ISI:000385640900001,
Author = {Estakhri, Nasim Mohammadi and Alu, Andrea},
Title = {{Wave-front Transformation with Gradient Metasurfaces}},
Journal = {{PHYSICAL REVIEW X}},
Year = {{2016}},
Volume = {{6}},
Number = {{4}},
Month = {{OCT 14}},
Abstract = {{Relying on abrupt phase discontinuities, metasurfaces characterized by a
   transversely inhomogeneous surface impedance profile have been recently
   explored as an ultrathin platform to generate arbitrary wave fronts over
   subwavelength thicknesses. Here, we outline fundamental limitations of
   passive gradient metasurfaces in molding the impinging wave and show
   that local phase compensation is essentially insufficient to realize
   arbitrary wave manipulation, but full-wave designs should be considered.
   These findings represent a critical step towards realistic and highly
   efficient conformal wave manipulation beyond the scope of ray optics,
   enabling unprecedented nanoscale light molding.}},
