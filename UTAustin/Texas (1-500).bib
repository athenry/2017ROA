@article{ ISI:000388835700003,
Author = {Lorenzo, Guillermo and Scott, Michael A. and Tew, Kevin and Hughes,
   Thomas J. R. and Zhang, Yongjie Jessica and Liu, Lei and Vilanova,
   Guillermo and Gomez, Hector},
Title = {{Tissue-scale, personalized modeling and simulation of prostate cancer
   growth}},
Journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA}},
Year = {{2016}},
Volume = {{113}},
Number = {{48}},
Pages = {{E7663-E7671}},
Month = {{NOV 29}},
Abstract = {{Recently, mathematical modeling and simulation of diseases and their
   treatments have enabled the prediction of clinical outcomes and the
   design of optimal therapies on a personalized (i.e., patient-specific)
   basis. This new trend in medical research has been termed ``predictive
   medicine.{''} Prostate cancer (PCa) is a major health problem and an
   ideal candidate to explore tissue-scale, personalized modeling of cancer
   growth for two main reasons: First, it is a small organ, and, second,
   tumor growth can be estimated by measuring serum prostate-specific
   antigen (PSA, a PCa biomarker in blood), which may enable in vivo
   validation. In this paper, we present a simple continuous model that
   reproduces the growth patterns of PCa. We use the phase-field method to
   account for the transformation of healthy cells to cancer cells and use
   diffusion-reaction equations to compute nutrient consumption and PSA
   production. To accurately and efficiently compute tumor growth, our
   simulations leverage isogeometric analysis (IGA). Our model is shown to
   reproduce a known shape instability from a spheroidal pattern to
   fingered growth. Results of our computations indicate that such shift is
   a tumor response to escape starvation, hypoxia, and, eventually,
   necrosis. Thus, branching enables the tumor to minimize the distance
   from inner cells to external nutrients, contributing to cancer survival
   and further development. We have also used our model to perform
