@article{ ISI:000381243200012,
Author = {Spann, Andrew P. and Campbell, James E. and Fitzgibbon, Sean R. and
   Rodriguez, Armando and Cap, Andrew P. and Blackbourne, Lorne H. and
   Shaqfeh, Eric S. G.},
Title = {{The Effect of Hematocrit on Platelet Adhesion: Experiments and
   Simulations}},
Journal = {{BIOPHYSICAL JOURNAL}},
Year = {{2016}},
Volume = {{111}},
Number = {{3}},
Pages = {{577-588}},
Month = {{AUG 9}},
Abstract = {{The volume fraction of red blood cells (RBCs) in a capillary affects the
   degree to which platelets are promoted to marginate to near a vessel
   wall and form blood clots. In this work we investigate the relationship
   between RBC hematocrit and platelet adhesion activity. We perform
   experiments flowing blood samples through a microfluidic channel coated
   with type 1 collagen and observe the rate at which platelets adhere to
   the wall. We compare these results with three-dimensional boundary
   integral simulations of a suspension of RBCs and platelets in a periodic
   channel where platelets can adhere to the wall. In both cases, we find
   that the rate of platelet adhesion varies greatly with the RBC
   hematocrit. We observe that the relative decrease in platelet activity
   as hematocrit falls shows a similar profile for simulation and
   experiment.}},
