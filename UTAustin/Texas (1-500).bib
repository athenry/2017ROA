@article{ ISI:000381452300013,
Author = {Chen, Yunshen and Elhag, Amro S. and Worthen, Andrew J. and Reddy,
   Prathima P. and Ou, Anne Marie and Hirasaki, George J. and Nguyen, Quoc
   P. and Biswal, Sibani L. and Johnston, Keith P.},
Title = {{High Temperature CO2-in-Water Foams Stabilized with Cationic Quaternary
   Ammonium Surfactants}},
Journal = {{JOURNAL OF CHEMICAL AND ENGINEERING DATA}},
Year = {{2016}},
Volume = {{61}},
Number = {{8}},
Pages = {{2761-2770}},
Month = {{AUG}},
Abstract = {{The design of surfactants for stabilizing CO2-in-water (brine) (C/W)
   foams at high temperature is challenging given the low density (solvent
   strength) of CO2, limited surfactant solubility in brine, and a lack of
   knowledge of the interfacial and rheological properties. Herein, the
   tail length of trimethylammonium cationic surfactants was optimized to
   provide the desired phase behavior and interfacial properties for
   formation and stabilization of the C/W foams. The headgroup was properly
   balanced with a C12-14 hydrocarbon tail to achieve aqueous solubility in
   22\% total dissolved solids (TDS) brine up to 393 K (120 degrees C)
   along with high surfactant adsorption (area/surfactant molecule of 154
   angstrom(2)) at the CO, water (C-W) interface which reduced the
   interfacial tension from similar to 40 mN/m to similar to 6 mN/m. For
   C12-14N(CH3)(3)Cl, these properties enabled stabilization of a C/W foam
   with an apparent viscosity of 14 mPa.s at 393 K in both a crushed
   calcium carbonate packed bed (75 mu m(2) or 76 Darcy) and a capillary
   tube downstream of the bed. In addition, the partition coefficient of
   the surfactant between oil and 22\% TDS (255 kg/m(3)) brine was less
   than 0.15, which would be beneficial for minimizing the loss of the
   surfactant to an oil phase in applications including enhanced oil
   recovery and hydraulic fracturing.}},
