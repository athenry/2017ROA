@article{ ISI:000387067800001,
Author = {Yang, Ruiyue and Huang, Zhongwei and Yu, Wei and Li, Gensheng and Ren,
   Wenxi and Zuo, Lihua and Tan, Xiaosi and Sepehrnoori, Kamy and Tian,
