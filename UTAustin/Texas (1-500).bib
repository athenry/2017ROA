@article{ ISI:000388495100013,
Author = {Jarrah, Ahmad I. and Qi, Xiangtong and Bard, Jonathan F.},
Title = {{The Destination-Loader-Door Assignment Problem for Automated Package
   Sorting Centers}},
Journal = {{TRANSPORTATION SCIENCE}},
Year = {{2016}},
Volume = {{50}},
