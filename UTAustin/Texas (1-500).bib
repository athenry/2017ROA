@article{ ISI:000391080900013,
Author = {Blachut, Gregory and Sirard, Stephen M. and Maher, Michael J. and Asano,
   Yusuke and Someya, Yasunobu and Lane, Austin P. and Durand, William J.
   and Bates, Christopher M. and Dinhobl, Andrew M. and Gronheid, Roel and
   Hymes, Diane and Ellison, Christopher J. and Willson, C. Grant},
Title = {{A Hybrid Chemo-/Grapho-Epitaxial Alignment Strategy for Defect Reduction
   in Sub-10 nm Directed Self-Assembly of Silicon Containing Block
   Copolymers}},
