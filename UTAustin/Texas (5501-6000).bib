@article{ ISI:000314723600011,
Author = {Lai, Wei-Cheng and Chakravarty, Swapnajit and Zou, Yi and Guo, Yunbo and
   Chen, Ray T.},
Title = {{Slow light enhanced sensitivity of resonance modes in photonic crystal
   biosensors}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{102}},
Number = {{4}},
Month = {{JAN 28}},
Abstract = {{We demonstrate experimentally that in photonic crystal sensors with a
   side-coupled cavity-waveguide configuration, group velocity of the
   propagating mode in the coupled waveguide at the frequency of the
   resonant mode plays an important role in enhancing the sensitivity. In
   linear L13 photonic crystal microcavities, with nearly same resonance
   mode quality factors similar to 7000 in silicon-on-insulator devices,
   sensitivity increased from 57 nm/RIU to 66 nm/RIU as group index in the
   coupled waveguide increased from 10.2 to 13.2. Engineering for highest
   sensitivity in such planar integrated sensors, thus, requires careful
   slow light design for optimized sensor sensitivity. (C) 2013 American
   Institute of Physics. {[}http://dx.doi.org/10.1063/1.4789857]}},
