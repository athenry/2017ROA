@inproceedings{ ISI:000341702104038,
Author = {Lee, Juhun and Fingeret, Michelle C. and Teo, Irene and Hanasono,
   Matthew M. and Skoracki, Roman J. and Markey, Mia K.},
Book-Group-Author = {{IEEE}},
Title = {{Quantitative Measures of Facial Expression for Patients with Head and
   Neck Cancer}},
Booktitle = {{2013 35TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
