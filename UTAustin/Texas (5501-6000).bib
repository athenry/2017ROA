@article{ ISI:000316569600044,
Author = {Fay, A. G. and Biegalski, S. R.},
Title = {{Contributions to the Ar-37 background by research reactor operations}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY}},
Year = {{2013}},
Volume = {{296}},
Number = {{1}},
Pages = {{273-277}},
Month = {{APR}},
Abstract = {{Radioargon has been identified as a useful nuclide for verifying
   compliance with the Comprehensive Nuclear-Test-Ban Treaty. Use of Ar-37
   to identify a nuclear explosion requires quantification of contributions
   to the Ar-37 background at a potential measurement site. A method of
   estimating Ar-37 release activities using isotopes of radioxenon and
   radioargon has been developed in this paper. Numerical solutions to the
   system of equations describing air-activation in a reactor were used to
   determine ratios of release activities for Xe-135/Xe-133,
   Xe-133m/Xe-131m, and Ar-37 /Ar-41 as function of irradiation time and
   off-gas residence time prior to measurement and release. Published
   radioactive noble gas effluent data for the High Flux Isotope Reactor,
   HFIR (ORNL) from the year 1996 to 2010 were compiled as a test data set
   to predict the Ar-37 release on a yearly basis. An average Ar-37 release
   rate of 1.86 x 10(10) Bq per year was calculated. The estimated release
   rate was used as a source term for atmospheric transport to run a test
   case for Ar-37 release over a typical HFIR operation cycle. Results
   showed that ground-level concentrations of Ar-37 did not exceed the
   minimum detectable concentration for a Ar-37 field measurement system
   beyond the immediate vicinity of the release point.}},
