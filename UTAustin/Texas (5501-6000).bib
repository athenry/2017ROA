@article{ ISI:000318810300002,
Author = {Argyropoulos, Christos and Le, Khai Q. and Mattiucci, Nadia and
   D'Aguanno, Giuseppe and Alu, Andrea},
Title = {{Broadband absorbers and selective emitters based on plasmonic Brewster
   metasurfaces}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2013}},
Volume = {{87}},
Number = {{20}},
Month = {{MAY 10}},
Abstract = {{We discuss the possibility of realizing utlrabroadband omnidirectional
   absorbers and angularly selective coherent thermal emitters based on
   properly patterned plasmonic metastructures. Instead of relying on
   resonant concentration effects that inherently limit the bandwidth, we
   base our design on the combination of two inherently nonresonant
   effects: plasmonic Brewster funneling and adiabatic plasmonic focusing.
   Using this approach, we propose compact, broadband absorption and
   emission spanning terahertz, infrared, and optical frequencies, ideal
   for various energy and defense applications.}},
