@article{ ISI:000319361400023,
Author = {Kim, Jin-Young and Lee, Junbok and Yu, SeGi},
Title = {{Field-emission energy distribution of BN-coated carbon nanotubes}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2013}},
Volume = {{62}},
Number = {{9}},
Pages = {{1354-1357}},
Month = {{MAY}},
Abstract = {{The field-emission energy distribution of BN-coated multiwalled carbon
   nanotubes was investigated by measuring the energy distribution of
   emitted electrons. The field-emission energy distribution curve suddenly
   changes from a single peak to two peaks after the buildup process of
   carbon nanotube field emitters. The movement of the peaks is different
   from that in previous reports on coated carbon nanotubes. A possible
   explanation for this peak movement is proposed here - a thermal effect
   due to a high emission current during the measurements.}},
