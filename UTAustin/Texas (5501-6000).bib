@inproceedings{ ISI:000345500500052,
Author = {Sherman, Brent and Chen, Xi and Thu Nguyen and Xu, Qing and Rafique,
   Humera and Freeman, Stephanie A. and Voice, Alexander K. and Rochelle,
   Gary T.},
Editor = {{Dixon, T and Yamaji, K}},
Title = {{Carbon Capture with 4 m Piperazine/4 m 2-Methylpiperazine}},
Booktitle = {{GHGT-11}},
Series = {{Energy Procedia}},
Year = {{2013}},
Volume = {{37}},
Pages = {{436-447}},
Note = {{International Conference on Greenhouse Gas Technologies (GHGT), Kyoto,
   JAPAN, NOV 18-22, 2012}},
Abstract = {{An equimolar diamine blend of 4 m 2-methylpiperazine (2MPZ) with 4 m
   piperazine (PZ) is shown to be an attractive solvent for CO2 capture.
   This blend overcomes the difficulties posed by the narrow solid
   solubility window of pure 8 m PZ while preserving its benefits. The
   solid solubility window at 20 degrees C broadens from P-CO2({*}) = 0.01
   to P-CO2({*}) = 0.011. As the blend viscosity is nearly double that of
   pure PZ, normalizing the capacity by viscosity shows a practical
   capacity comparable to MEA at 0.63 mol CO2/ kg solvent. The CO2
   absorption rate of the blend is lower, with at k(g,avg)' = 7.1 x 10(7)
   mol/s . Pa . m(2) at 40 degrees C, 84\% that of PZ. The heats of CO2
   absorption of the blend and PZ are equal at Delta H-abs = -70 kJ/mol.
   While the blend thermal stability is decreased, T-max = 155 compared to
   163 degrees C, oxidative stability is similar. Lastly, their
   volatilities are nearly equal with amine Henry's constant near 23 Pa at
   40 degrees C. In short, the equimolar blend of 4 m 2MPZ with 4 m PZ is a
   competitive solvent for amine scrubbing. (C) 2013 The Authors. Published
   by Elsevier Ltd.}},
