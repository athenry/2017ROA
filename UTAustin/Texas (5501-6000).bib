@article{ ISI:000316569600035,
Author = {Egnatuk, C. M. and Biegalski, S. R.},
Title = {{Large volume gas irradiation procedure developed at The University of
   Texas at Austin}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY}},
Year = {{2013}},
Volume = {{296}},
Number = {{1}},
Pages = {{223-226}},
Month = {{APR}},
Abstract = {{A large volume, in-core gas irradiation technique was developed at The
   University of Texas at Austin MARK II TRIGA reactor to produce a sample
   of Ar-42 with an activity above 1 mBq. The method requires a large
   volume, 1.4 L, of natural Ar gas (99.6003 \% Ar-40) at about 1 atm and
   three 12 h irradiation periods. The production of Ar-42 requires a
   double capture to be produced from the stable Ar-40 isotope. This method
   produced 940 kBq of Ar-39, 3.08 MBq Ar-37, 114 GBq Ar-41, and 0.311 Bq
   Ar-42 at the end of the final irradiation period.}},
