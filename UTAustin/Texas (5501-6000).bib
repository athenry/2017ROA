@inproceedings{ ISI:000345500501088,
Author = {Chen, Eric and Madan, Tarun and Sachde, Darschan and Walters, Matthew S.
   and Nielsen, Paul and Rochelle, Gary T.},
Editor = {{Dixon, T and Yamaji, K}},
Title = {{Pilot Plant Results with Piperazine}},
Booktitle = {{GHGT-11}},
Series = {{Energy Procedia}},
Year = {{2013}},
Volume = {{37}},
Pages = {{1572-1583}},
Note = {{International Conference on Greenhouse Gas Technologies (GHGT), Kyoto,
   JAPAN, NOV 18-22, 2012}},
Abstract = {{Concentrated (40 wt \%) piperazine (PZ) with two-stage flash
   regeneration is an advanced second generation amine-based process for
   CO2 capture. Two pilot campaigns with the two-stage flash process were
   completed in 2011 and successfully demonstrated high temperature
   stripping at 150 degrees C with concentrated piperazine. Data
   reconciliation for the absorber with intercooling return spray was
   completed using Aspen Plus (R) Data-Fit. Data reconciliation for the
   two-stage flash with warm rich bypass was completed using Aspen Plus (R)
   Data-Fit and a dynamic model developed in MATLAB (R). The results of
   data reconciliation analysis found that the CO2 loading in the lean and
   rich streams needed to be systematically shifted upward, 4.6-4.7 \% in
   the stripper analysis and 7.5\% in the absorber analysis.
   An analysis of the October 2011 campaign pilot plant data identified the
   two configurations with the lowest equivalent work as the high
