@article{ ISI:000317186100009,
Author = {Evans, John A. and Hughes, Thomas J. R.},
Title = {{Isogeometric divergence-conforming B-splines for the unsteady
   Navier-Stokes equations}},
Journal = {{JOURNAL OF COMPUTATIONAL PHYSICS}},
Year = {{2013}},
Volume = {{241}},
Pages = {{141-167}},
Month = {{MAY 15}},
Abstract = {{Divergence-conforming B-splines are developed for application to the
   incompressible Navier-Stokes equations on geometrically mapped domains.
   These enable smooth, point-wise divergence-free solutions and thus
   satisfy mass conservation in the strongest possible sense. Semi-discrete
   methods based on divergence-conforming B-splines are shown to conserve
   linear and angular momentum and satisfy balance laws for energy,
   vorticity, enstrophy, and helicity. These are geometric
   structure-preserving quantities and numerical simulations that are
   sensitive to them are shown to be qualitatively correct and
   quantitatively accurate. The methods developed are anticipated to open
   new doors to the practical calculation of complex flows and to studies
   of their physical behavior. (C) 2013 Elsevier Inc. All rights reserved.}},
