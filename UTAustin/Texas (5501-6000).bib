@inproceedings{ ISI:000341772900313,
Author = {Puljiz, Zrinka and Vikalo, Haris},
Editor = {{Matthews, MB}},
Title = {{A Message Passing Algorithm for Haplotype Assembly}},
Booktitle = {{2013 ASILOMAR CONFERENCE ON SIGNALS, SYSTEMS AND COMPUTERS}},
