@article{ ISI:000318319700009,
Author = {Yu, Guihua and Xie, Xing and Pan, Lijia and Bao, Zhenan and Cui, Yi},
Title = {{Hybrid nanostructured materials for high-performance electrochemical
   capacitors}},
