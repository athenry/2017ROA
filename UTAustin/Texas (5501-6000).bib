Author = {Alves, Marco A. Z. and Villavieja, Carlos and Diener, Matthias and
   Navaux, Philippe O. A.},
Book-Group-Author = {{IEEE}},
Title = {{Energy Efficient Last Level Caches via Last Read/Write Prediction}},
Booktitle = {{2013 25TH INTERNATIONAL SYMPOSIUM ON COMPUTER ARCHITECTURE AND HIGH
   PERFORMANCE COMPUTING (SBAC-PAD)}},
Series = {{International Symposium on Computer Architecture and High Performance
   Computing}},
Year = {{2013}},
