@inproceedings{ ISI:000403301400036,
Author = {Restrepo, Ricardo L. and Russell, Ryan P.},
Editor = {{Zanetti, R and Russell, RP and Ozimek, MT and Bowes, AL}},
Title = {{THE SHADOW TRAJECTORY MODEL FOR FAST LOW-THRUST INDIRECT OPTIMIZATION}},
