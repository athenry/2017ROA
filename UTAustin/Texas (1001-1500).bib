@article{ ISI:000368523200009,
Author = {Duncan, Juliana and Harjunmaa, Ari and Terrell, Rye and Drautz, Ralf and
   Henkelman, Graeme and Rogal, Jutta},
Title = {{Collective Atomic Displacements during Complex Phase Boundary Migration
   in Solid-Solid Phase Transformations}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2016}},
Volume = {{116}},
Number = {{3}},
Month = {{JAN 19}},
Abstract = {{The A15 to bcc phase transition is simulated at the atomic scale based
   on an interatomic potential for molybdenum. The migration of the phase
   boundary proceeds via long-range collective displacements of entire
   groups of atoms across the interface. To capture the kinetics of these
   complex atomic rearrangements over extended time scales we use the
   adaptive kinetic Monte Carlo approach. An effective barrier of 0.5 eV is
   determined for the formation of each new bcc layer. This barrier is not
   associated with any particular atomistic process that governs the
   dynamics of the phase boundary migration. Instead, the effective layer
   transformation barrier represents a collective property of the complex
   potential energy surface.}},
