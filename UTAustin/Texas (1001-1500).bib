@article{ ISI:000369900600033,
Author = {Wu, Wenzhi and He, Feng and Wang, Yaguo},
Title = {{Reversible ultrafast melting in bulk CdSe}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2016}},
Volume = {{119}},
Number = {{5}},
Month = {{FEB 7}},
Abstract = {{In this work, transient reflectivity changes in bulk CdSe have been
   measured with two-color femto-second pump-probe spectroscopy under a
   wide range of pump fluences. Three regions of reflectivity change with
   pump fluences have been consistently revealed for excited carrier
   density, coherent phonon amplitude, and lattice temperature. For laser
   fluences from 13 to 19.3 mJ/cm(2), ultrafast melting happens in first
   several picoseconds. This melting process is purely thermal and
   reversible. A complete phase transformation in bulk CdSe may be reached
   when the absorbed laser energy is localized long enough, as observed in
   nanocrystalline CdSe. (C) 2016 AIP Publishing LLC.}},
