Author = {de Almeida, Marcelino M. and Akella, Maruthi},
Editor = {{Zanetti, R and Russell, RP and Ozimek, MT and Bowes, AL}},
Title = {{DISCRETE ADAPTIVE ANGULAR VELOCITY ESTIMATION - AN EXPERIMENTAL ANALYSIS}},
