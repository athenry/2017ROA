@inproceedings{ ISI:000388376106144,
Author = {LeSage, Jonathan R. and Longoria, Raul G.},
Book-Group-Author = {{IEEE}},
Title = {{Mobile System Shutdown Prevention via Energy Storage-aware Predictive
   Control}},
