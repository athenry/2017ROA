@inproceedings{ ISI:000390852200110,
Author = {Li, Z. C. and Chen, X. L. and Gao, M. T. and Jiang, H.},
Editor = {{Kim, YH}},
Title = {{Simulation Near-field Ground Motion in Kumamoto Earthquake, Japan
   M(JMA)7.3 by Empirical Green Function Method}},
Booktitle = {{PROCEEDINGS OF THE 2016 5TH INTERNATIONAL CONFERENCE ON CIVIL,
   ARCHITECTURAL AND HYDRAULIC ENGINEERING (ICCAHE 2016)}},
Series = {{AER-Advances in Engineering Research}},
