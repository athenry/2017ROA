@inproceedings{ ISI:000392361000042,
Author = {Tran, Jason and Cameron, Christopher G. and Sirohi, Jayant},
Book-Group-Author = {{ASME}},
Title = {{MEASUREMENT OF FLAPPING WING DEFORMATION USING PIEZOELECTRIC MODAL
   SENSORS}},
Booktitle = {{PROCEEDINGS OF THE ASME CONFERENCE ON SMART MATERIALS, ADAPTIVE
   STRUCTURES AND INTELLIGENT SYSTEMS, 2016, VOL 1}},
