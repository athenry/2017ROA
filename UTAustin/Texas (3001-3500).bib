Editor = {{Ryan, TP}},
Title = {{Utility And Translatability Of Mathematical Modeling, Cell Culture And
   Small And Large Animal Models In Magnetic Nanoparticle Hyperthermia
   Cancer Treatment Research}},
Booktitle = {{ENERGY-BASED TREATMENT OF TISSUE AND ASSESSMENT VIII}},
