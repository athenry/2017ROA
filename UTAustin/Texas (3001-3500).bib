Title = {{Co-focused ultrasound and optical coherence elastography system for the
   study of age-related changes of biomechanical properties of crystalline
   lens in rabbit eyes}},
Booktitle = {{OPHTHALMIC TECHNOLOGIES XXV}},
Series = {{Proceedings of SPIE}},
