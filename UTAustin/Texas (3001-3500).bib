@article{ ISI:000343750300040,
Author = {Zhao, Yu and Ding, Yu and Song, Jie and Li, Gang and Dong, Guangbin and
   Goodenough, John B. and Yu, Guihua},
Title = {{Sustainable Electrical Energy Storage through the Ferrocene/Ferrocenium
   Redox Reaction in Aprotic Electrolyte}},
Journal = {{ANGEWANDTE CHEMIE-INTERNATIONAL EDITION}},
Year = {{2014}},
Volume = {{53}},
Number = {{41}},
Pages = {{11036-11040}},
Month = {{OCT 6}},
Abstract = {{The large-scale, cost-effective storage of electrical energy obtained
   from the growing deployment of wind and solar power is critically needed
   for the integration into the grid of these renewable energy sources.
   Rechargeable batteries having a redox-flow cathode represent a viable
   solution for either a Li-ion or a Na-ion battery provided a suitable
   low-cost redox molecule soluble in an aprotic electrolyte can be
   identified that is stable for repeated cycling and does not cross the
   separator membrane to the anode. Here we demonstrate an environmentally
   friendly, low-cost ferrocene/ferrocenium molecular redox couple that
   shows about 95\% energy efficiency and about 90\% capacity retention
   after 250 full charge/discharge cycles.}},
