@inproceedings{ ISI:000393287300061,
Author = {Zhu, Yuhao and Richins, Daniel and Halpern, Matthew and Reddi, Vijay
   Janapa},
Book-Group-Author = {{ACM}},
Title = {{Microarchitectural Implications of Event-driven Server-side Web
   Applications}},
Booktitle = {{PROCEEDINGS OF THE 48TH ANNUAL IEEE/ACM INTERNATIONAL SYMPOSIUM ON
   MICROARCHITECTURE (MICRO-48)}},
Year = {{2015}},
Pages = {{762-774}},
Note = {{48th Annual IEEE/ACM International Symposium on Microarchitecture
   (MICRO), Waikiki, HI, DEC 05-09, 2015}},
Organization = {{IEEE; ACM; ARM; IBM; Intel; Microsoft; NetApp; Qualcomm; HP; ACM
   SIGMICRO}},
Abstract = {{Enterprise Web applications are moving towards server-side scripting
   using managed languages. Within this shifting context, event-driven
   programming is emerging as a crucial programming model to achieve
   scalability. In this paper, we study the microarchitectural implications
   of server-side scripting, JavaScript in particular, from a unique
   event-driven programming model perspective. Using the Node.js framework,
   we come to several critical microarchitectural conclusions. First,
   unlike traditional server-workloads such as CloudSuite and BigDataBench
   that are based on the conventional thread-based execution model,
   event-driven applications are heavily single-threaded, and as such they
   require significant single-thread performance. Second, the single-thread
   performance is severely limited by the front-end inefficiencies of
   today's server processor microarchitecture, ultimately leading to
   overall execution inefficiencies. The front-end inefficiencies stem from
   the unique combination of limited intra-event code reuse and large
   inter-event reuse distance. Third, through a deep understanding of
   event-specific characteristics, architects can mitigate the front-end
   inefficiencies of the managed-language-based event-driven execution via
   a combination of instruction cache insertion policy and prefetcher.}},
