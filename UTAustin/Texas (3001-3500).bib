@article{ ISI:000354879000010,
Author = {Wu, Zilong and Song, Nan and Menz, Ryan and Pingali, Bharadwaj and Yang,
   Ying-Wei and Zheng, Yuebing},
Title = {{Nanoparticles functionalized with supramolecular host-guest systems for
   nanomedicine and healthcare}},
Journal = {{NANOMEDICINE}},
Year = {{2015}},
Volume = {{10}},
Number = {{9}},
Pages = {{1493-1514}},
Abstract = {{Synthetic macrocyclic host compounds can interact with suitable guest
   molecules via noncovalent interactions to form functional supramolecular
   systems. With the synergistic integration of the response of molecules
   and the unique properties at the nanoscale, nanoparticles functionalized
   with the host-guest supramolecular systems have shown great potentials
   for a broad range of applications in the fields of nanoscience and
   nanotechnology. In this review article, we focus on the applications of
   the nanoparticles functionalized with supramolecular host-guest systems
   in nanomedicine and healthcare, including therapeutic delivery, imaging,
   sensing and removal of harmful substances. A large number of examples
   are included to elucidate the working mechanisms, advantages,
   limitations and future developments of the nanoparticle-supramolecule
   systems in these applications.}},
