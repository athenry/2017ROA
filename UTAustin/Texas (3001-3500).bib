@article{ ISI:000343886800020,
Author = {Kong, Inwook and Kim, Seong-Wan and Swartzlander, Jr., Earl E.},
Title = {{Design of Goldschmidt Dividers with Quantum-Dot Cellular Automata}},
Journal = {{IEEE TRANSACTIONS ON COMPUTERS}},
Year = {{2014}},
Volume = {{63}},
Number = {{10}},
Pages = {{2620-2625}},
Month = {{OCT}},
Abstract = {{A Goldschmidt divider implemented with semiconductor quantum-dot
   cellular automata (QCA) is described. Most Goldschmidt dividers use a
   state machine for control, but state machines are difficult to implement
   in QCAs due to the long delays between the state machines and the
   computational circuits to be controlled. To resolve this problem, a data
   tag method is used. The data tags travel with the data and local tag
   decoders generate control signals. Since each datum has a tag,
   pipelining can be employed to increase the throughput. Using the new
   architecture, fixed-point Goldschmidt dividers are implemented with QCA
   technology.}},
