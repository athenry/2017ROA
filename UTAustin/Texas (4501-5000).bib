@inproceedings{ ISI:000377326600021,
Author = {Pellegrini, Etienne and Russell, Ryan P. and Vittaldev, Vivek},
Editor = {{Broschart, SB and Turner, JD and Howell, KC and Hoots, FR}},
Title = {{F AND G TAYLOR SERIES SOLUTIONS TO THE STARK PROBLEM WITH SUNDMAN
   TRANSFORMATIONS}},
Booktitle = {{ASTRODYNAMICS 2013, PTS I-III}},
