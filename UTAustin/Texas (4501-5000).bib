@article{ ISI:000328319900012,
Author = {Arbogast, Todd and Juntunen, Mika and Pool, Jamie and Wheeler, Mary F.},
Title = {{A discontinuous Galerkin method for two-phase flow in a porous medium
   enforcing H(div) velocityand continuous capillary pressure}},
Journal = {{COMPUTATIONAL GEOSCIENCES}},
Year = {{2013}},
Volume = {{17}},
Number = {{6}},
Pages = {{1055-1078}},
Month = {{DEC}},
Abstract = {{We consider the slightly compressible two-phase flow problem in a porous
   medium with capillary pressure. The problem is solved using the implicit
   pressure, explicit saturation (IMPES) method, and the convergence is
   accelerated with iterative coupling of the equations. We use
   discontinuous Galerkin to discretize both the pressure and saturation
   equations. We apply two improvements, which are projecting the flux to
   the mass conservative H(div)-space and penalizing the jump in capillary
   pressure in the saturation equation. We also discuss the need and use of
   slope limiters and the choice of primary variables in discretization.
   The methods are verified with two- and three-dimensional numerical
   examples. The results show that the modifications stabilize the method
   and improve the solution.}},
