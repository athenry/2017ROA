@article{ ISI:000328911400001,
Author = {Chen, Peng and Huang, Yu-Yen and Hoshino, Kazunori and Zhang, Xiaojing},
Title = {{Multiscale immunomagnetic enrichment of circulating tumor cells: from
   tubes to microchips}},
Journal = {{LAB ON A CHIP}},
Year = {{2014}},
Volume = {{14}},
Number = {{3}},
Pages = {{446-458}},
Abstract = {{We review the rare cancer cell sorting technologies, with a focus on
   multiscale immunomagnetic approaches. Starting from the conventional
   magnetic activated cell sorting system, we derive the scaling laws of
   immunomagnetic assay and justify the recent trend of using downscaled
   systems for CTC studies. Furthermore, we introduce recent work on
   combining the immunomagnetic assay with microfluidic technology for
   enhanced separation. We summarize different types of in-channel
   micro-magnetic structures that can further increase the local magnetic
   field without lowering the system throughput. Related design concepts,
   principles, and microfabrication techniques are presented and evaluated.}},
