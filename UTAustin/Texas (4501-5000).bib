@inproceedings{ ISI:000393387400054,
Author = {Khawaja, Ahmed and Wang, Jiajun and Gerstlauer, Andreas and John, Lizy
   K. and Malhotra, Dhairya and Biros, George},
Book-Group-Author = {{IEEE}},
Title = {{Performance Analysis of HPC Applications with Irregular Tree Data
