Title = {{Characterization of Thermal Stresses and Plasticity in Through-Silicon
   Via Structures for Three-dimensional Integration}},
Booktitle = {{STRESS INDUCED PHENOMENA AND RELIABILITY IN 3D MICROELECTRONICS}},
Series = {{AIP Conference Proceedings}},
