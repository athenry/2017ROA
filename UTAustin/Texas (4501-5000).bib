@article{ ISI:000334998400016,
Author = {Wood, Sean M. and Klavetter, Kyle C. and Heller, Adam and Mullins, C.
   Buddie},
Title = {{Fast lithium transport in PbTe for lithium-ion battery anodes}},
Journal = {{JOURNAL OF MATERIALS CHEMISTRY A}},
Year = {{2014}},
Volume = {{2}},
Number = {{20}},
Pages = {{7238-7243}},
Abstract = {{The reversible charging of a lead chalcogenide, PbTe, was studied for
   use as the anode material in a Li-ion cell and compared to PbO. A
   similar series of Li-Pb alloys were formed but with Li2Te present
   instead of Li2O. In the presence of Li2Te, rapid Li-Pb alloying and
   dealloying were observed in the potential range of 0.01-0.7 V. In the
   potential range of 0.8-2.5 V, Li2Te formed and decomposed reversibly.
   Electrodes were cycled stably for 100 cycles at a C/5 rate in both
   potential domains. The electrodes were also cycled stably at rates up to
   10C. The presence of Li2Te reduced the overpotential required at higher
   charge and discharge rates by acting as a superionic conductor to
   improve lithium ion diffusion. These results recommend this material for
   potential use in low-power applications such as cell phones.}},
