@article{ ISI:000328636400047,
Author = {Pandav, Gunja and Ganesan, Venkat},
Title = {{Fluctuation effects on the order-disorder transition in polydisperse
   copolymer melts}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{2013}},
Volume = {{139}},
Number = {{21}},
Month = {{DEC 7}},
Abstract = {{Using single chain in mean field simulation approach, we examine the
   influence of fluctuation effects on the order-disorder transition (ODT)
   of polydisperse copolymer systems. We consider two model systems: (i)
   molecular weight polydisperse systems represented by AB diblock
   copolymer melts having monodisperse A blocks and polydisperse B blocks;
   and (ii) compositionally polydisperse symmetric diblock copolymer
   systems. In each case, we present results for the fluctuation-induced
   shift in the ODT from the corresponding mean-field predictions. In both
   models, an increase in polydispersity enhances the influence of
   fluctuations. Moreover, for compositionally polydisperse systems, we
   observe that the effects of fluctuations show similar trends in systems
   containing quenched and annealed representation of sequences. (C) 2013
   AIP Publishing LLC.}},
