@inproceedings{ ISI:000337585600033,
Author = {Wen, Gezheng and Markey, Mia K. and Muralidhar, Gautam S.},
Editor = {{MelloThoms, CR and Kupinski, MA}},
Title = {{A Stereo Matching Model Observer For Stereoscopic Viewing of 3D Medical
   Images}},
Booktitle = {{MEDICAL IMAGING 2014: IMAGE PERCEPTION, OBSERVER PERFORMANCE, AND
   TECHNOLOGY ASSESSMENT}},
Series = {{Proceedings of SPIE}},
Year = {{2014}},
Volume = {{9037}},
Note = {{Conference on Medical Imaging - Image Perception, Observer Performance,
   and Technology Assessment, San Diego, CA, FEB 16-17, 2014}},
Organization = {{SPIE; Modus Med Devices Inc; XIFIN Inc; Ventana Med Syst Inc; Intrace
   Med}},
Abstract = {{Stereoscopic viewing of 3D medical imaging data has the potential to
   increase the detection of abnormalities. We present a new stereo model
   observer inspired by the characteristics of stereopsis in human vision.
   Given a stereo pair of images of an object (i.e., left and right images
   separated by a small displacement), the model observer first finds the
   corresponding points between the two views, and then fuses them together
   to create a 2D cyclopean view. Assuming that the cyclopean view has
   extracted most of the 3D information presented in the stereo pair, a
   channelized Hotel ling observer (CHO) can be utilized to make decisions.
   We conduct a simulation study that attempts to mimic the detection of
   breast lesions on stereoscopic viewing of breast tomosynthesis
   projection images. We render voxel datasets that contain random 3D
   power-law noise to model normal breast tissues with various breast
   densities. 3D Gaussian signal is added to some of the datasets to model
   the presence of a breast lesion. By changing the separation angle
   between the two views, multiple stereo pairs of projection images are
   generated for each voxel dataset. The performance of the model is
   evaluated in terms of the accuracy of binary decisions on the presence
   of the simulated lesions.}},
