@article{ ISI:000347132600005,
Author = {Siddiqui, M. Sarwar and Rached, Marc and Fowler, David W.},
Title = {{Mixture Design for Minimizing Cement Content in Pavement Concrete}},
Journal = {{TRANSPORTATION RESEARCH RECORD}},
Year = {{2014}},
Number = {{2441}},
Pages = {{20-27}},
Abstract = {{Pavement concrete containing aggregates that do not meet ASTM C33 often
   experiences workability problems. One of the reasons is the lack of
   proper guidelines for proportioning concrete that contains aggregates
   with poor shape, texture, and grading. Commonly used methods, such as
   ACT 211, were developed for aggregates that meet ASTM C33; these methods
   are not adequate for optimizing mixtures containing aggregates such as
   manufactured sands. This paper presents a simple mixture proportioning
   method for designing pavement concrete with low slump. The proposed
   method has the potential to minimize the cement content of paving
   mixtures with any available source or combination of aggregate without
   affecting performance. Several combinations of aggregates were tested
   and the results for slump, compressive strength, and modulus of
   elasticity are presented in this paper.}},
