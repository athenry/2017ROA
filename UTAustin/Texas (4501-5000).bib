Author = {Matthews, Jordan and Klatt, Timothy and Seepersad, Carolyn C. and
   Haberman, Michael and Shahan, David},
Book-Group-Author = {{ASME}},
Title = {{BAYESIAN NETWORK CLASSIFIERS AND DESIGN FLEXIBILITY METRICS FOR
   SET-BASED, MULTISCALE DESIGN WITH MATERIALS DESIGN APPLICATIONS}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
