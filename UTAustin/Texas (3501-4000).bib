@article{ ISI:000336048000006,
Author = {Xiao, Penghao and Sheppard, Daniel and Rogal, Jutta and Henkelman,
   Graeme},
Title = {{Solid-state dimer method for calculating solid-solid phase transitions}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{2014}},
Volume = {{140}},
Number = {{17}},
Month = {{MAY 7}},
Abstract = {{The dimer method is a minimum mode following algorithm for finding
   saddle points on a potential energy surface of atomic systems. Here, the
   dimer method is extended to include the cell degrees of freedom for
   periodic solid-state systems. Using this method, reaction pathways of
   solid-solid phase transitions can be determined without having to
   specify the final state structure or reaction mechanism. Example
   calculations include concerted phase transitions between CdSe polymorphs
   and a nucleation and growth mechanism for the A15 to BCC transition in
   Mo. (C) 2014 AIP Publishing LLC.}},
