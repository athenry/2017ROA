@article{ ISI:000336914700015,
Author = {Khoshnevis, Sepideh and Nordhauser, Jennifer E. and Craik, Natalie K.
   and Diller, Kenneth R.},
Title = {{Quantitative Evaluation of the Thermal Heterogeneity on the Surface of
   Cryotherapy Cooling Pads}},
Journal = {{JOURNAL OF BIOMECHANICAL ENGINEERING-TRANSACTIONS OF THE ASME}},
Year = {{2014}},
Volume = {{136}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{We have investigated thermal operating characteristics of 13
   commercially available cryotherapy units (CTUs) and their associated
   cooling pads using IR imaging. Quantitative examination of the
   temperature profiles from pad IR images shows diverse, nonuniform
   temperature distribution patterns. The extent of heterogeneity of the
   temperature fields was quantified via standard image analysis methods,
   including thresholding, spatial gradient diagrams, and frequency
   histogram distributions. A primary conclusion of this study is that it
   is a misnomer to characterize the thermal performance of a CTU and
   cooling pad combination in terms of a single therapeutic temperature.}},
