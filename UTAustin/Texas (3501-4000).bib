@article{ ISI:000334293200023,
Author = {Kim, Mingjun and Chelikowsky, James R.},
Title = {{Simulated non-contact atomic force microscopy for GaAs surfaces based on
   real-space pseudopotentials}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{2014}},
Volume = {{303}},
Pages = {{163-167}},
Month = {{JUN 1}},
Abstract = {{We simulate non-contact atomic force microscopy (AFM) with a GaAs(1 1 0)
   surface using a real-space ab initio pseudopotential method. While most
   ab initio simulations include an explicit model for the AFM tip, our
   method does not introduce the tip modeling step. This approach results
   in a considerable reduction of computational work, and also provides
   complete AFM images, which can be directly compared to experiment. By
   analyzing tip-surface interaction forces in both our results and
   previous ab initio simulations, we find that our method provides very
   similar force profile to the pure Si tip results. We conclude that our
   method works well for systems in which the tip is not chemically active.
