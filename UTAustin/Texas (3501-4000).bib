@article{ ISI:000339472100008,
Author = {Allcorn, Eric and Manthiram, Arumugam},
Title = {{FeSb2-Al2O3-C Nanocomposite Anodes for Lithium-Ion Batteries}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2014}},
Volume = {{6}},
Number = {{14}},
Pages = {{10886-10891}},
Month = {{JUL 23}},
Abstract = {{FeSb2-Al2O3-C nanocomposite synthesized by ambient-temperature
   high-energy mechanical milling (HEMM) of Sb2O3, Fe, Al, and C has been
   investigated as an anode material for lithium-ion batteries. The
   FeSb2-Al2O3-C nanocomposites are characterized with X-ray diffraction
   (XRD), X-ray photoelectron spectroscopy (XPS), scanning electron
   microscopy (SEM), and high-resolution transmission electron microscopy
   (HRTEM). The characterization data reveal it to be composed of
   crystalline FeSb2 nanoparticles finely dispersed in an amorphous matrix
   of Al2O3 and carbon. The FeSb2-Al2O3-C nanocomposite exhibits an initial
   discharge (lithiation) capacity of 877 mAh g(-1) an initial charge
   (delithiation) capacity of 547 mAh g(-1), yielding an initial coulombic
   efficiency of 62\%. The extended cycling performance for this composite
   is far superior to that of the intermetallic FeSb2 or a similarly
   prepared FeSb2-C composite. FeSb2-Al2O3-C retains a specific capacity of
   similar to 350 mAh g(-1) after 500 lithiation/delithiation cycles.}},
