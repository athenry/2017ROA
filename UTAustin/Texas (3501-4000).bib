@article{ ISI:000341021600016,
Author = {Balakrishna, Ananya Renuka and Huber, John E. and Landis, Chad M.},
Title = {{Nano-actuator concepts based on ferroelectric switching}},
Journal = {{SMART MATERIALS AND STRUCTURES}},
Year = {{2014}},
Volume = {{23}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{The concept of a nano-actuator that uses ferroelectric switching to
   generate enhanced displacements is explored using a phase-field model.
   The actuator has a ground state in the absence of applied electric field
   that consists of polarized domains oriented to form a flux closure. When
   electric field is applied, the polarization reorients through
   ferroelectric switching and produces strain. The device is mechanically
   biased by a substrate and returns to the ground state when electric
   field is removed, giving a repeatable actuation cycle. The mechanical
   strains which accompany ferroelectric switching are several times
   greater than the strains attained due to the piezoelectric effect alone.
   We also demonstrate a second design of actuator in which the
   displacements are further increased by the bending of a ferroelectric
   beam. Phase-field modeling is used to track the evolution of domain
   patterns in the devices during the actuation cycle, and to study the
   design parameters so as to enhance the achievable actuation strains.}},
