@article{ ISI:000335609000005,
Author = {Bhat, Chandra R. and Astroza, Sebastian and Sidharthan, Raghuprasad and
   Bhat, Prerna C.},
Title = {{A multivariate hurdle count data model with an endogenous multiple
   discrete-continuous selection system}},
Journal = {{TRANSPORTATION RESEARCH PART B-METHODOLOGICAL}},
Year = {{2014}},
Volume = {{63}},
Pages = {{77-97}},
Month = {{MAY}},
Abstract = {{This paper proposes a new econometric formulation and an associated
   estimation method for multivariate count data that are themselves
   observed conditional on a participation selection system that takes a
   multiple discrete-continuous model structure. This leads to a joint
   model system of a multivariate count and a multiple discrete-continuous
   selection system in a hurdle-type model. The model is applied to analyze
   the participation and time investment of households in out-of-home
   activities by activity purpose, along with the frequency of
   participation in each selected activity. The results suggest that the
   number of episodes of activities as well as the time investment in those
   activities may be more of a lifestyle- and lifecycle-driven choice than
   one related to the availability of opportunities for activity
   participation. (C) 2014 Elsevier Ltd. All rights reserved.}},
