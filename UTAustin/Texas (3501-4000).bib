@article{ ISI:000339806400016,
Author = {Li, Bing and Louca, Despina and Hu, Biao and Niedziela, Jennifer L. and
   Zhou, Jianshi and Goodenough, John B.},
Title = {{Dynamic Distortions in the YTiO3 Ferromagnet}},
Journal = {{JOURNAL OF THE PHYSICAL SOCIETY OF JAPAN}},
Year = {{2014}},
Volume = {{83}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{The orbitally ordered YTiO3 ferromagnet is revisited to investigate the
   influence of local lattice dynamics on the magnetic transition. By
   probing the local lattice dynamics using inelastic neutron scattering
   and the dynamic pair density function (DPDF) analysis, we find that
   strong, local anharmonic fluctuations persist up to 60 meV. The spread
   in energy is expected of fluctuations that are well-defined in
   real-space. The anharmonicity is manifested in the form of a double-well
   potential, where the spectral weight of the DPDF peak involving Y-O
   correlations shifts away from the central peak to two new peaks as a
   function of energy. This transition occurs around 40meV. The weight
   distribution between the two peaks is temperature dependent that
   corresponds to a change in the nature of the local modes. The
   anharmonicity in the Y-O local modes most likely affects the Ti orbital
   overlap leading to a temperature dependent spin disorder.}},
