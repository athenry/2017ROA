@article{ ISI:000336591000009,
Author = {Sitzman, Alex J. and Stefani, Francis and Bourell, David L. and Trevino,
   Eliseo},
Title = {{Use of the Magnetic Saw Effect for Manufacturing}},
Journal = {{IEEE TRANSACTIONS ON PLASMA SCIENCE}},
Year = {{2014}},
Volume = {{42}},
Number = {{5, 1}},
Pages = {{1173-1178}},
Month = {{MAY}},
Abstract = {{This paper describes a novel technique for making fine cuts in metal
   plates and foils. The process is based on a variant of the magnetic saw
   effect, a magnetic instability associated with high pulsed magnetic
   fields. When not controlled, the magnetic saw effect can lead to failure
   in high-field pulsed conductors. By limiting energy and pulse duration,
   the authors have produced controlled cuts with kerfs as fine as 0.01 mm.
   These promising results motivated a systematic study of the parameters
   that govern the quality and reproducibility of magnetic saw cuts. This
   paper provides an overview of the technique, of some key experiments,
   and of the practical issues associated with using pulsed magnetic fields
   for manufacturing.}},
