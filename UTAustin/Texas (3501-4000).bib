@article{ ISI:000338022300010,
Author = {Satish, Rohit and Aravindan, Vanchiappan and Ling, Wong Chui and
   Goodenough, John B. and Madhavi, Srinivasan},
Title = {{Carbon-Coated Li3Nd3W2O12: A High Power and Low-Voltage Insertion Anode
   with Exceptional Cycleability for Li-Ion Batteries}},
Journal = {{ADVANCED ENERGY MATERIALS}},
Year = {{2014}},
Volume = {{4}},
Number = {{9}},
Month = {{JUN}},
Abstract = {{The synthesis of carbon-coated Li3Nd3W2O12 (C-Li3Nd3W2O12), a low
   voltage insertion anode (0.3 V vs. Li) for a Li-ion battery, is reported
   to exhibit extraordinary performance. The low voltage reversible
   insertion provides an increase in the energy density of Li-ion power
   packs. For instance, C-Li3Nd3W2O12 delivered an energy density of
   approximate to 390 Wh kg(-1) (based on cathode mass loading) when
   coupled with an LiMn2O4 cathode with an operating potential of 3.4 V.
   Furthermore, excellent cycling profiles are observed for C-Li3Nd3W2O12
   anodes both in half and full-cell configurations. The full-cell is
   capable of delivering very stable cycling profiles at high current rates
