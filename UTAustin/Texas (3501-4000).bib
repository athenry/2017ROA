@article{ ISI:000340514500016,
Author = {Wiegand, Jennifer R. and Smith, Zachary P. and Liu, Qiang and Patterson,
   Christopher T. and Freeman, Benny D. and Guo, Ruilan},
Title = {{Synthesis and characterization of triptycene-based polyimides with
