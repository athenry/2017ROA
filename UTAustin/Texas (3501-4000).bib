@article{ ISI:000337891800056,
Author = {McDaniel, Martin D. and Posadas, Agham and Ngo, Thong Q. and Karako,
   Christine M. and Bruley, John and Frank, Martin M. and Narayanan, Vijay
   and Demkov, Alexander A. and Ekerdt, John G.},
Title = {{Incorporation of La in epitaxial SrTiO3 thin films grown by atomic layer
   deposition on SrTiO3-buffered Si (001) substrates}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2014}},
Volume = {{115}},
Number = {{22}},
Month = {{JUN 14}},
Abstract = {{Strontium titanate, SrTiO3 (STO), thin films incorporated with lanthanum
   are grown on Si (001) substrates at a thickness range of 5-25 nm. Atomic
   layer deposition (ALD) is used to grow the LaxSr1-xTiO3 (La: STO) films
   after buffering the Si (001) substrate with four-unit-cells of STO
   deposited by molecular beam epitaxy. The crystalline structure and
   orientation of the La: STO films are confirmed via reflection
   high-energy electron diffraction, X-ray diffraction, and cross-sectional
   transmission electron microscopy. The low temperature ALD growth
   (similar to 225 degrees C) and post-deposition annealing at 550 degrees
   C for 5 min maintains an abrupt interface between Si (001) and the
   crystalline oxide. Higher annealing temperatures (650 degrees C) show
   more complete La activation with film resistivities of similar to 2.0 x
   10(-2) Omega cm for 20-nm-thick La: STO (x similar to 0.15); however,
   the STO-Si interface is slightly degraded due to the increased annealing
   temperature. To demonstrate the selective incorporation of lanthanum by
   ALD, a layered heterostructure is grown with an undoped STO layer
   sandwiched between two conductive La: STO layers. Based on this work, an
   epitaxial oxide stack centered on La: STO and BaTiO3 integrated with Si
   is envisioned as a material candidate for a ferroelectric field-effect
   transistor. (C) 2014 AIP Publishing LLC.}},
