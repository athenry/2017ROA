@article{ ISI:000311135800013,
Author = {Gallo, F. and Satapathy, S. and Ravi-Chandar, K.},
Title = {{Plastic deformation in electrical conductors subjected to short-duration
   current pulses}},
Journal = {{MECHANICS OF MATERIALS}},
Year = {{2012}},
Volume = {{55}},
Pages = {{146-162}},
Month = {{DEC}},
Abstract = {{In this paper, we consider the viscoplastic response of Al 6061-T6 and
   Cu-102 when subjected to a combination of mechanical load and
   high-intensity electric current. The specimens are subjected to
   mechanical loading under fixed-grip and dead-load conditions; in
   addition, the specimen is subjected to a nearly sinusoidal current pulse
   (frequency 4 kHz, duration similar to 1 ms, and intensity similar to
   10(9) A/m(2)). The resulting temperature increase causes the yield
   stress to drop and enables accumulation of plastic strain. A
   viscoplastic model is used to simulate the process; comparisons of the
   simulation results to time resolved measurements of strain and
   temperature are used to calibrate the viscoplastic model. (C) 2012
   Elsevier Ltd. All rights reserved.}},
