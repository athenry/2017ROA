@article{ ISI:000311009900001,
Author = {Ideker, Jason H. and Bentivegna, Anthony F. and Folliard, Kevin J. and
   Juenger, Maria C. G.},
Title = {{Do Current Laboratory Test Methods Accurately Predict Alkali-Silica
   Reactivity?}},
Journal = {{ACI MATERIALS JOURNAL}},
Year = {{2012}},
Volume = {{109}},
Number = {{4}},
Pages = {{395-402}},
Month = {{JUL-AUG}},
Abstract = {{Traditional laboratory methods for identifying alkali-silica reactive
   aggregates (ASTM C1260 and C1293) sometimes produce. inaccurate and
   conflicting results. To benchmark these tests and ultimately develop a
   more realistic test method(s), an outdoor exposure site was constructed
   to simulate field performance of concrete incorporating reactive
   aggregates. It was found that the performance of the aggregates in the
   laboratory and field environments varied considerably. ASTM C1260 was a
   good indicator of reactivity for some aggregates. In many cases,
   however, the results from ASTM C1260 disagreed with those from ASTM
   C1293 and the outdoor simulated field tests. In general, reactivity
   diagnosed by ASTM C1293 was found to correlate well with reactivity in
   field exposure. However, when ASTM C1293 was modified to evaluate lower
   alkali loadings, the test was unable to predict expansion in outdoor
   specimens.}},
