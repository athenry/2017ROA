@article{ ISI:000307264400029,
Author = {Castrillon, Santiago Romero-Vargas and Matysiak, Silvina and Stillinger,
   Frank H. and Rossky, Peter J. and Debenedetti, Pablo G.},
Title = {{Phase Behavior of a Lattice Hydrophobic Oligomer in Explicit Water}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY B}},
Year = {{2012}},
Volume = {{116}},
Number = {{31}},
Pages = {{9540-9548}},
Month = {{AUG 9}},
Abstract = {{We investigate the thermodynamics of hydrophobic oligomer collapse using
   a water-explicit, three-dimensional lattice model. The model captures
   several aspects of protein thermodynamics, including the emergence of
   cold- and thermal-unfolding, as well as unfolding at high solvent
   density (a phenomenon akin to pressure-induced denaturation). We show
   that over a range of conditions spanning a increase in solvent density,
   the oligomer transforms into a compact, strongly water-penetrated
   conformation at low temperature. This contrasts with thermal unfolding
   at high temperature, where the system ``denatures{''} into an extended
   random coil conformation. We report a phase diagram for hydrophobic
   collapse that correctly captures qualitative aspects of cold and thermal
   unfolding at low to intermediate solvent densities.}},
