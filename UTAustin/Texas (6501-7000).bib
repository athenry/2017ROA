@article{ ISI:000306002100021,
Author = {Zhang, Ying and Wei, Xiangmin and Tsai, Yi-Te and Zhu, Jinying and
   Fetrat, Farhad A. and Gucunski, Nenad},
Title = {{Multisensor data fusion for impact-echo testing of concrete structures}},
Journal = {{SMART MATERIALS AND STRUCTURES}},
Year = {{2012}},
Volume = {{21}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{Based on the investigation of spatial variations of impact-echo signals
   for different source and receiver locations, a simple multisensor data
   fusion strategy is proposed to increase the accuracy of nondestructive
   evaluation of concrete structures using the impact-echo test. The data
   fusion strategy fuses the ratios between spectral amplitudes of the
   delamination and concrete bottom echo signals (D/B ratios) from multiple
   source-receiver arrays. The fused D/B ratios demonstrate different
   characteristics for test locations above the delamination, above the
   sound concrete, and across the delamination boundary. These
   characteristics can be used to accurately locate the delamination and
   its boundaries without increasing testing time. The applicability of the
   multisensor data fusion for impact-echo testing is validated using both
   numerical simulation and experimental testings.}},
