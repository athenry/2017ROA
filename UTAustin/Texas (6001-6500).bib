Author = {Chatterjee, Avhishek and Rawat, Ankit Singh and Vishwanath, Sriram and
   Sanghavi, Sujay},
Book-Group-Author = {{IEEE}},
Title = {{Learning the Causal Graph of Markov Time Series}},
Booktitle = {{2013 51ST ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2013}},
