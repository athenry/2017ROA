@inproceedings{ ISI:000335000400002,
Author = {Yang, Kai and Yilmaz, Ali E.},
Book-Group-Author = {{IEEE}},
Title = {{FFT-Accelerated Analysis of Scattering from Three-Dimensional Structures
   Residing in Multiple Layers}},
Booktitle = {{2013 COMPUTATIONAL ELECTROMAGNETICS WORKSHOP (CEM'13)}},
Year = {{2013}},
Pages = {{4-8}},
Note = {{Computational Electromagnetics Workshop (CEM), Izmir, TURKEY, AUG 02-05,
   2013}},
Abstract = {{An extension of the adaptive integral method (AIM) is presented for fast
   analysis of scattering from three-dimensional (3D) structures located in
   multiple layers of a planar layered medium. The proposed scheme
   accelerates the iterative method-of-moments solution by employing 3D
   auxiliary regular grids, each of which encloses the parts of the
   structure in a different layer, and 2D auxiliary regular grids, each of
   which is located at a different interface of the layered medium. The
   auxiliary grids are used to execute the standard four-stage AIM
   procedure (anterpolation, propagation, interpolation, and correction)
   but the propagation stage of the procedure is divided into intra-layer
   and inter-layer components. Only the 3D grids and both 3D and 2D grids
   are used for intra-and inter-layer propagation stages, respectively.
   Numerical results validate the accuracy and efficiency of the proposed
   method.}},
