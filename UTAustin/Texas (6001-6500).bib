@inproceedings{ ISI:000327210205091,
Author = {Wang, Siyun and Baldea, Michael},
Book-Group-Author = {{IEEE}},
Title = {{Storage-Enhanced Thermal Management for Mobile Devices}},
Booktitle = {{2013 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2013}},
Pages = {{5344-5349}},
Note = {{American Control Conference (ACC), Washington, DC, JUN 17-19, 2013}},
