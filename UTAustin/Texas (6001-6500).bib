@inproceedings{ ISI:000325263900003,
Author = {Akinwande, Deji and Tao, Li},
Editor = {{George, T and Islam, MS and Dutta, AK}},
Title = {{Graphene-Si Heterogeneous Nanotechnology}},
Booktitle = {{MICRO- AND NANOTECHNOLOGY SENSORS, SYSTEMS, AND APPLICATIONS V}},
