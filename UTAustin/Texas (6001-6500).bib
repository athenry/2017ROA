@inproceedings{ ISI:000327210203022,
Author = {Kim, Jong S. and Powell, Kody M. and Edgar, Thomas F.},
Book-Group-Author = {{IEEE}},
Title = {{Nonlinear Model Predictive Control for a Heavy-Duty Gas Turbine Power
   Plant}},
