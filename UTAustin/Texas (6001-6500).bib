@inproceedings{ ISI:000324988601034,
Author = {Yu, Sheng-Yang and Kwasinski, Alexis},
Book-Group-Author = {{IEEE}},
Title = {{Investigation of Multiple-Input Converters Bi-Directional Power Flow
   Characteristics}},
Booktitle = {{2013 TWENTY-EIGHTH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC 2013)}},
Series = {{Annual IEEE Applied Power Electronics Conference and Exposition (APEC)}},
Year = {{2013}},
