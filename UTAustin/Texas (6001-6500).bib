@article{ ISI:000323521000018,
Author = {Obliosca, Judy M. and Liu, Cong and Yeh, Hsin-Chih},
Title = {{Fluorescent silver nanoclusters as DNA probes}},
Journal = {{NANOSCALE}},
Year = {{2013}},
Volume = {{5}},
Number = {{18}},
Pages = {{8443-8461}},
Abstract = {{Fluorescent silver nanoclusters (few atoms, quantum sized) have
   attracted much attention as promising substitutes for conventional
   fluorophores. Due to their unique environmental sensitivities, new
   fluorescent probes have been developed based on silver nanoclusters for
   the sensitive and specific detection of DNA. In this review we present
   the recent discoveries of activatable and color-switchable properties of
   DNA-templated silver nanoclusters and discuss the strategies to use
   these new properties in DNA sensing.}},
