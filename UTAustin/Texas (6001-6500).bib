Title = {{Bridging the Gap between the Total and Additional Test-Case
   Prioritization Strategies}},
Booktitle = {{PROCEEDINGS OF THE 35TH INTERNATIONAL CONFERENCE ON SOFTWARE ENGINEERING
   (ICSE 2013)}},
Year = {{2013}},
