@article{ ISI:000320245400029,
Author = {Jiang, Zhongqing and Pei, Bo and Manthiram, Arumugam},
Title = {{Randomly stacked holey graphene anodes for lithium ion batteries with
   enhanced electrochemical performance}},
Journal = {{JOURNAL OF MATERIALS CHEMISTRY A}},
Year = {{2013}},
Volume = {{1}},
Number = {{26}},
Pages = {{7775-7781}},
Abstract = {{Holey graphene (HG) synthesized by a hydrothermal method followed by
   etching with KOH and ball milling is randomly stacked to form a porous
   structure. These randomly stacked holey graphene anodes exhibit high
   rate capability with excellent cycling stability as an anode material
   for lithium-ion cells. This fascinating electrochemical performance can
   be ascribed to their specific porous structure, providing numerous
   active sites for Li+ insertion, reduced effective diffusion distance for
   the Li+ ions, high electrical conductivity, low charge-transfer
   resistance across the electrolyte-electrode interface, and improved
   structural stability against the local volume change during Li+
   insertion-extraction.}},
