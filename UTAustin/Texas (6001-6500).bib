@article{ ISI:000316375800044,
Author = {Voice, Alexander K. and Rochelle, Gary T.},
Title = {{Products and process variables in oxidation of monoethanolamine for CO2
   capture}},
Journal = {{INTERNATIONAL JOURNAL OF GREENHOUSE GAS CONTROL}},
Year = {{2013}},
Volume = {{12}},
Pages = {{472-477}},
Month = {{JAN}},
Abstract = {{Oxidative degradation of monoethanolamine (MEA) in CO2 capture systems
   was studied using two different apparatuses and a variety of
   experimental conditions. Dissolved iron, manganese, and copper were all
   potent catalysts, increasing the oxidation rate by up to a factor of
   ten. The activation energy of the oxidation rate was 68-70 kJ/mol.
   Addition of 2\% CO2 to the gas increased oxidation by a factor of five.
   The oxidation rate was found to be approximately first-order in oxygen
   partial pressure. Addition of 50 ppm NO2 center dot or SO2 to the
   reactor gas had a minimal effect on the degradation rate. The mass
   balance was closed within 5\% at high gas flow and within 20\% at low
   gas flow. Two-thirds of the degraded MEA in both systems was converted
   to ammonia. Other major products were 1-(2-hydroxyethyl)-formamide (HEF)
   and 1-(2-hydroxyethyp-imidazole (HEI). (C) 2012 Elsevier Ltd. All rights
   reserved.}},
