@inproceedings{ ISI:000350887800054,
Author = {Lee, Kyoungtae and Yoon, Yeonam and Sun, Nan},
Book-Group-Author = {{IEEE}},
Title = {{A 1.8mW 2MHz-BW 66.5dB-SNDR Delta Sigma ADC Using VCO-Based Integrators
   with Intrinsic CLA}},
Booktitle = {{2013 IEEE CUSTOM INTEGRATED CIRCUITS CONFERENCE (CICC)}},
Series = {{IEEE Custom Integrated Circuits Conference}},
Year = {{2013}},
Note = {{35th Annual IEEE Custom Integrated Circuits Conference (CICC) - The
   Showcase for Circuit Design in the Heart of Silicon Valley, San Jose,
   CA, SEP 22-25, 2013}},
Organization = {{IEEE}},
Abstract = {{This paper presents a scaling-friendly continuous-time closed-loop
   VCO-based Delta Sigma ADC. It uses the VCO as both quantizer and
   integrator, and thus, obviates the need for power-hungry
   scaling-unfriendly OTAs and precision comparators. It arranges two VCOs
   in a pseudo-differential manner, which cancels out even-order
   distortions. More importantly, it brings an intrinsic clocked averaging
   (CLA) capability that automatically addresses DAC mismatches. The
   prototype ADC in 130nm CMOS occupies a small area of 0.03mm(2) and
   achieves 66.5dB SNDR over 2MHz BW while sampling at 300MHz and consuming
   1.8mW under a 1.2V power supply. It can also operate with a low analog
   supply of 0.7V and achieves 65.8dB SNDR while consuming 1.1mW. The
   corresponding figure-of-merits (FOMs) for the two cases are 0.25pJ/step
   and 0.17pJ/step respectively.}},
