@inproceedings{ ISI:000326335201074,
Author = {Moharir, Sharayu and Shakkottai, Sanjay},
Book-Group-Author = {{IEEE}},
Title = {{MaxWeight vs. BackPressure: Routing and Scheduling in Multi-Channel
   Relay Networks}},
Booktitle = {{2013 PROCEEDINGS IEEE INFOCOM}},
