@inproceedings{ ISI:000360869000082,
Author = {Kim, E. and Manuel, L.},
Book-Group-Author = {{ASME}},
Title = {{ON THE EXTREME ROTOR AND SUPPORT STRUCTURE RESPONSE OF AN OFFSHORE WIND
   TURBINE IN AN EVOLVING HURRICANE}},
Booktitle = {{PROCEEDINGS OF THE ASME 32ND INTERNATIONAL CONFERENCE ON OCEAN, OFFSHORE
   AND ARCTIC ENGINEERING - 2013 - VOL 8}},
Year = {{2013}},
Note = {{32nd ASME International Conference on Ocean, Offshore and Arctic
   Engineering, Nantes, FRANCE, JUN 09-14, 2013}},
Organization = {{ASME, Ocean Offshore \& Arctic Engn Div}},
Abstract = {{This study examines extreme response statistics for a monopile-supported
   5-MW offshore wind turbine in 20 meters of water that is subjected to
   coupled wind and wave input fields during a hurricane. Over
   approximately 120 hours, these hurricane-induced input fields yield
   changing characteristics of the excitation and the response of a parked
   turbine. As the storm evolves, the directionality of the wind and waves
   changes; short-crested waves are simulated and associated wind velocity
   fields are generated. Aerodynamic loads on the rotor and hydrodynamic
   loads on the support structure are simulated in coupled response
   analyses. Because yaw control backup power is not assured during the
   hurricane, different assumptions on yaw misalignment are assumed in the
   turbine response simulations. Time series of various turbine response
   measures are evaluated. Response extremes are of particular interest; we
   discuss the relative importance of wind and waves on the overall turbine
   performance during the storm. We also assess the role of yaw control
   systems and the effect of loss of power to such systems during tropical
   storms by examining the turbine response for alternative situations of
   turbine misalignment. Ultimately, this study seeks to provide the
   framework for assessing turbine designs for tropical cyclone conditions.}},
