Author = {Hernandez, Sonia and Akella, Maruthi R.},
Editor = {{Tanygin, S and Park, RS and Starchville, TF and Newman, LK}},
Title = {{LYAPUNOV-BASED GUIDANCE SCHEMES FOR IN-PLANE RENDEZVOUS USING
   LEVI-CIVITA COORDINATES}},
