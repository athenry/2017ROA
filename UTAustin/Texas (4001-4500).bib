@article{ ISI:000333226700009,
Author = {Thomas, Courtney and Lampert, David and Reible, Danny},
Title = {{Remedy performance monitoring at contaminated sediment sites using
   profiling solid phase microextraction (SPME) polydimethylsiloxane (PDMS)
   fibers}},
Journal = {{ENVIRONMENTAL SCIENCE-PROCESSES \& IMPACTS}},
Year = {{2014}},
Volume = {{16}},
Number = {{3}},
Pages = {{445-452}},
Month = {{MAR}},
Abstract = {{Passive sampling using polydimethylsiloxane (PDMS) profilers was
   evaluated as a tool to assess the performance of in situ sediment
   remedies at three locations, Chattanooga Creek (Chattanooga, TN), Eagle
   Harbor (Bainbridge Island, WA) and Hunter's Point (San Francisco, CA).
   The remedy at the first two locations was capping over PAH contaminated
   sediments while at Hunter's Point, the assessment was part of an in situ
   treatment demonstration led by R. G. Luthy (Stanford University) using
   activated carbon mixed into PCB contaminated sediments. The
   implementation and results at these contaminated sediment sites were
   used to illustrate the utility and usefulness of the passive sampling
   approach. Two different approaches were employed to evaluate kinetics of
   uptake onto the sorbent fibers. At the capping sites, the passive
