@article{ ISI:000334572400055,
Author = {Larentis, Stefano and Tolsma, John R. and Fallahazad, Babak and Dillen,
   David C. and Kim, Kyounghwan and MacDonald, Allan H. and Tutuc, Emanuel},
Title = {{Band Offset and Negative Compressibility in Graphene-MoS2
   Heterostructures}},
Journal = {{NANO LETTERS}},
Year = {{2014}},
Volume = {{14}},
Number = {{4}},
Pages = {{2039-2045}},
Month = {{APR}},
Abstract = {{We use electron transport to characterize monolayer graphene-multilayer
   MoS2 heterostructures. Our samples show ambipolar characteristics and
   conductivity saturation on the electron branch that signals the onset of
   MoS2 conduction band population. Surprisingly, the carrier density in
   graphene decreases with gate bias once MoS2 is populated, demonstrating
   negative compressibility in MoS2. We are able to interpret our
   measurements quantitatively by accounting for disorder and using the
   random phase approximation (RPA) for the exchange and correlation
   energies of both Dirac and parabolic-band two-dimensional electron
   gases. This interpretation allows us to extract the energetic offset
   between the conduction band edge of MoS2 and the Dirac point of
   graphene.}},
