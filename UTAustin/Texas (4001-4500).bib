@article{ ISI:000333947600008,
Author = {Cho, Wonhui and Lee, Jietae and Edgar, Thomas F.},
Title = {{Simple Analytic Proportional-Integral-Derivative (PID) Controller Tuning
   Rules for Unstable Processes}},
Journal = {{INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH}},
Year = {{2014}},
Volume = {{53}},
Number = {{13}},
Pages = {{5048-5054}},
Month = {{APR 2}},
Abstract = {{Very simple proportional-integral-derivative (PID) controller tuning
   rules for a wide range of stable processes are available. However, for
   unstable processes, the design trend is for controllers to be more
   complex for better performances. Here, the design concept of
   ``simplicity{''} is extended to unstable processes. Simple desired
   closed-loop transfer functions for the direct synthesis method and
   simple approximations of the process time delay are utilized for
   unstable processes. Very simple tuning rules for PID controllers and
   set-point filters are obtained, yielding similar or even improved
   performances over previous more complicated PID controller tuning
   methods.}},
