@article{ ISI:000331798000057,
Author = {Kwong, David and Hosseini, Amir and Covey, John and Zhang, Yang and Xu,
   Xiaochuan and Subbaraman, Harish and Chen, Ray T.},
Title = {{On-chip silicon optical phased array for two-dimensional beam steering}},
Journal = {{OPTICS LETTERS}},
Year = {{2014}},
Volume = {{39}},
Number = {{4}},
Pages = {{941-944}},
Month = {{FEB 15}},
Abstract = {{A 16-element optical phased array integrated on chip is presented for
   achieving two-dimensional (2D) optical beam steering. The device is
   fabricated on the silicon-on-insulator platform with a 250 nm silicon
   device layer. Steering is achieved via a combination of wavelength
   tuning and thermo-optic phase shifting with a switching power of P-pi =
   20 mW per channel. Using a silicon waveguide grating with a
   polycrystalline silicon overlay enables narrow far field beam widths
   while mitigating the precise etching needed for conventional shallow
   etch gratings. Using this system, 2D steering across a 20 degrees x 15
   degrees field of view is achieved with a sidelobe level better than 10
   dB and with beam widths of 1.2 degrees x 0.5 degrees. (C) 2014 Optical
   Society of America}},
