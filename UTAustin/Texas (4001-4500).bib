@article{ ISI:000364566900007,
Author = {Lu, Nanshu and Kim, Dae-Hyeong},
Title = {{Flexible and Stretchable Electronics Paving the Way for Soft Robotics}},
Journal = {{SOFT ROBOTICS}},
Year = {{2014}},
Volume = {{1}},
Number = {{1}},
Pages = {{53-62}},
Month = {{MAR}},
Abstract = {{Planar and rigid wafer-based electronics are intrinsically incompatible
   with curvilinear and deformable organisms. Recent development of organic
   and inorganic flexible and stretchable electronics enabled sensing,
   stimulation, and actuation of/for soft biological and artificial
   entities. This review summarizes the enabling technologies of soft
   sensors and actuators, as well as power sources based on flexible and
   stretchable electronics. Examples include artificial electronic skins,
   wearable biosensors and stimulators, electronics-enabled programmable
   soft actuators, and mechanically compliant power sources. Their
   potential applications in soft robotics are illustrated in the framework
   of a five-step human-robot interaction loop. Outlooks of future
   directions and challenges are provided at the end.}},
