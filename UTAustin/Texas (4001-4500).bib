@inproceedings{ ISI:000361211500062,
Author = {Freeman, Brice and Hao, Pingjiao and Baker, Richard and Kniep, Jay and
   Chen, Eric and Ding, Junyuan and Zhang, Yue and Rochelle, Gary T.},
Editor = {{Dixon, T and Herzog, H and Twinning, S}},
Title = {{Hybrid membrane-absorption CO2 capture process}},
