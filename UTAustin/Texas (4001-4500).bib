@inproceedings{ ISI:000361554400169,
Author = {Estep, N. A. and Sounas, D. L. and Alu, A.},
Book-Group-Author = {{IEEE}},
Title = {{Magnetic-Free, Fully Integrated, Compact Microwave Circulator Using
   Angular-Momentum Biasing}},
Booktitle = {{2014 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM
   (APSURSI)}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2014}},
