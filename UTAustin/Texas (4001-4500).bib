@article{ ISI:000330971000011,
Author = {Mondal, Somnath and Sharma, Mukul M.},
Title = {{Role of flying buttresses in the jamming of granular matter through
   multiple rectangular outlets}},
Journal = {{GRANULAR MATTER}},
Year = {{2014}},
Volume = {{16}},
Number = {{1}},
Pages = {{125-132}},
Month = {{FEB}},
Abstract = {{We present a simulation study on the formation of particle arches
   supported by flying buttresses that arrest the flow of monodisperse
   spherical particles through two adjacent slots (rectangular outlets) in
   a three-dimensional (3D) anisometric flat-bottomed silo discharging
   under gravity. Most previous experimental and simulation studies on the
   jamming of a silo have focused on a single outlet, and have shown that
   the mean number of particles exiting the silo before it jams depends on
   the ratio of outlet size to particle diameter (). In this paper, we look
   at the nature of jamming if there are two adjacent outlets and discover
   some new physical insights on how sand arches are mechanically supported
   and how they interact with each other. For a fixed slot width and
   particle diameter , the distance separating the two openings is varied.
   The distribution of particles exiting the silo before both slots are
   jammed is obtained for each . From these distributions, we show that as
   becomes less than approximately , the particle arches on adjacent
   outlets start affecting each other, and mutually stable arches become
   harder to form. We show that in cases, where for a small orifice ratio a
   single outlet would have easily jammed, two adjacent outlets do not jam.
   We propose that this is due to the importance of stable particles
   resting on the base of the silo adjacent to the arches, which
   redistribute the load much like a flying buttress does in Gothic arches.}},
