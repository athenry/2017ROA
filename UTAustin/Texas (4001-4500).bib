@inproceedings{ ISI:000361211505081,
Author = {Sun, Yuhao and Bryant, Steven L.},
Editor = {{Dixon, T and Herzog, H and Twinning, S}},
Title = {{Influence of Grain Size Distribution and Varying Heterogeneity on Local
   Capillary Trapping}},
