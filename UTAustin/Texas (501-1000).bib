@article{ ISI:000387807000009,
Author = {Levko, Dmitry and Raja, Laxminarayan L.},
Title = {{Influence of field emission on microwave microdischarges}},
Journal = {{HIGH VOLTAGE}},
Year = {{2016}},
Volume = {{1}},
Number = {{1}},
Pages = {{57-59}},
Month = {{APR}},
Abstract = {{An instability in the stable operation of microdischarges sustained at
   microwave frequencies, is investigated by a self-consistent
   one-dimensional particle-in-cell Monte Carlo collisions model. The
   instability is caused by the field electron emission from electrodes and
   is triggered by the high electric fields at the sheaths in a dense
   microplasma. For an operating frequency of 9 GHz, and electrode gap of
   60 mu m, the field emission (FE) is not active at the breakdown voltage.
   However, once a stable dense plasma is produced post-breakdown, a strong
   sheath results in an electrode electric fields that exceed the threshold
   for FE causing a runaway in the electron generation in the plasma.}},
