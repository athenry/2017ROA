@article{ ISI:000385493900045,
Author = {Sui, Xin and Lenharth, Andrew and Fussell, Donald S. and Pingali, Keshav},
Title = {{Proactive Control of Approximate Programs}},
Journal = {{OPERATING SYSTEMS REVIEW}},
Year = {{2016}},
Volume = {{50}},
