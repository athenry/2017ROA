@article{ ISI:000374811700020,
Author = {Sounas, Dimitrios L. and Alu, Andrea},
Title = {{Color Separation through Spectrally-Selective Optical Funneling}},
Journal = {{ACS PHOTONICS}},
Year = {{2016}},
Volume = {{3}},
Number = {{4}},
Pages = {{620-626}},
Month = {{APR}},
Abstract = {{We present the concept and a basic design for a free-space color sorter,
   which can efficiently separate different spectral components of an
   optical beam. The structure is based on a resonant cavity formed between
   a metallic wall and a stack of partially reflecting screens. Such a
   cavity supports multiple leaky-wave resonances, which enable spectrally
   selective funneling of the incident beam through apertures in the
   metallic wall. The apertures are coupled to resonators with different
