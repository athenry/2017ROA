@article{ ISI:000374616900056,
Author = {Sun, Yujia and Zhang, Xiaobing},
Title = {{Analysis of transient conduction and radiation problems using lattice
   Boltzmann and finite volume methods}},
Journal = {{INTERNATIONAL JOURNAL OF HEAT AND MASS TRANSFER}},
Year = {{2016}},
Volume = {{97}},
Pages = {{611-617}},
Month = {{JUN}},
Abstract = {{In this paper, the lattice Boltzmann method (LBM) and the finite volume
   method (FVM) are coupled to solve transient conduction and radiation
   heat transfer problems. They are used to solve the energy equation and
   get the radiative heat source, respectively. To avoid additional
   interpolations between these two solvers when adding the radiative heat
   source to the energy equation, we propose to use the same grid systems
   for the LBM and FVM. This is achieved by adopting a half way bounce back
   boundary scheme for LBM. Good agreement with available literature is
   obtained for both 1D and 2D conditions. Effects of conduction-radiation
   parameter and scattering albedo on the transient temperature
   distribution are presented and discussed. The method proposed by this
   study is efficiently able to solve combined conduction and radiation
   problems. (C) 2016 Elsevier Ltd. All rights reserved.}},
