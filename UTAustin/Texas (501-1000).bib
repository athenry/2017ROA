@article{ ISI:000380415400011,
Author = {Soler-Crespo, Rafael A. and Gao, Wei and Xiao, Penghao and Wei, Xiaoding
   and Paci, Jeffrey T. and Henkelman, Graeme and Espinosa, Horacio D.},
Title = {{Engineering the Mechanical Properties of Monolayer Graphene Oxide at the
   Atomic Level}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY LETTERS}},
Year = {{2016}},
Volume = {{7}},
Number = {{14}},
Pages = {{2702-2707}},
Month = {{JUL 21}},
Abstract = {{The mechanical properties of graphene oxide (GO) are of great importance
   for applications in materials engineering. Previous mechanochemical
   studies of GO typically focused on the influence of the degree of
   oxidation on the mechanical behavior. In this study, using density
   functional-based tight binding simulations, validated using density
   functional theory simulations, we reveal that the deformation and
   failure of GO are strongly dependent on the relative concentrations of
   epoxide (-O-) and hydroxyl (-OH) functional groups. Hydroxyl groups
   cause GO to behave as a brittle material; by contrast, epoxide groups
   enhance material ductility through a mechanically driven
   epoxide-to-ether functional group transformation. Moreover, with
   increasing epoxide group concentration, the strain to failure and
   toughness of GO significantly increases without sacrificing material
   strength and stiffness. These findings demonstrate that GO should be
   treated as a versatile, tunable material that may be engineered by
   controlling chemical composition, rather than as a single, archetypical
   material.}},
