@article{ ISI:000368197200017,
Author = {Okamoto, Makiko and Akella, Maruthi R.},
Title = {{Avoiding the local-minimum problem in multi-agent systems with limited
   sensing and communication}},
Journal = {{INTERNATIONAL JOURNAL OF SYSTEMS SCIENCE}},
Year = {{2016}},
Volume = {{47}},
Number = {{8}},
Pages = {{1943-1952}},
Month = {{JUN 10}},
Abstract = {{In this paper, we consider a control problem for nonholonomic
   multi-agent systems in which agents and obstacles operate within a
   circular-shaped work area. We assume that agents only have limited
   sensing and communication ranges. We propose a novel control scheme
   using potential functions that drives agents from the initial to the
   goal configuration while avoiding collision with other agents,
   obstacles, and the boundary of the work area. The control scheme employs
   an avoidance strategy that ensures that the agents are never trapped at
   local minima that are typically encountered with most potential
   function-based approaches. A numerical simulation is presented to
   demonstrate the validity and effectiveness of the proposed control
   scheme.}},
