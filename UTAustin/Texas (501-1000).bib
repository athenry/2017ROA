@article{ ISI:000378469900005,
Author = {Wood, Alison and Blackhurst, Michael and Garland, Jay L. and Lawler,
   Desmond F.},
Title = {{Incentivizing Decentralized Sanitation: The Role of Discount Rates}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2016}},
Volume = {{50}},
Number = {{12}},
Pages = {{6146-6153}},
Month = {{JUN 21}},
Abstract = {{In adoption decisions for decentralized sanitation technologies, two
   decision makers are involved: the public utility and the individual
   homeowner. Standard life cycle cost is calculated from the perspective
   of the utility, which uses a market-based discount rate in these
   calculations. However, both decision-makers must be considered,
   including their differing perceptions of the time trade-offs inherent in
   a stream of costs and benefits. This study uses the discount rate as a
   proxy for these perceptions and decision-maker preferences. The results
   in two case studies emphasize the dependence on location of such
   analyses. Falmouth, Massachusetts, appears to be a good candidate for
   incentivizing decentralized sanitation while the Allegheny County
   Sanitary Authority service area in Pennsylvania appears to have no need
   for similar incentives. This method can be applied to any two-party
   decision in which the parties are expected to have different discount
   rates.}},
Publisher = {{AMER CHEMICAL SOC}},
Address = {{1155 16TH ST, NW, WASHINGTON, DC 20036 USA}},
