@article{ ISI:000401244000156,
Author = {Usova, Anastasiia A. and Polushin, Ilia G. and Patel, Rajni V.},
Title = {{A Graph Separation Stability Condition for Non-Planar Conic Systems}},
Journal = {{IFAC PAPERSONLINE}},
Year = {{2016}},
