@inproceedings{ ISI:000382315800034,
Author = {Karami, Elham and Gaede, Stewart and Lee, Ting-Yim and Samani, Abbas},
Editor = {{Webster, RJ and Yaniv, ZR}},
Title = {{Diaphragm motion characterization using chest motion data for
   biomechanics-based lung tumor tracking during EBRT}},
Booktitle = {{MEDICAL IMAGING 2016: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
