@inproceedings{ ISI:000389166200205,
Author = {Li, Y. G. and Fang, F. and Hong, H. P.},
Editor = {{Yang, Q and Yang, Y and Ru, J}},
Title = {{COMPARISON OF SEISMIC RESPONSE OF SINGLE-LAYER CYLINDRICAL LATTICE SHELL
   ESTIMATED USING TIME HISTORY ANALYSIS AND RESPONSE SPECTRUM METHOD}},
Booktitle = {{Fundamental Research in Structural Engineering: Retrospective and
   Prospective, Vols 1 and 2}},
Year = {{2016}},
Pages = {{1423-1428}},
Note = {{14th International Symposium on Structural Engineering (ISSE-14),
   Beijing, PEOPLES R CHINA, OCT 12-15, 2016}},
Organization = {{Beijing Jiaotong Univ; Natl Nat Sci Fdn}},
Abstract = {{Both the time history analysis procedure and the response spectrum
   method are frequently used to estimate the seismic response of the
   structures. One of the advantages of the time history analysis procedure
   is that it can be easily employed for actual or simulated ground motion
   records of identified scenario seismic events, although it can be
   computationally intensive. The response spectrum method is simple to
   use; its accuracy depends on dynamic characteristics of the considered
   structure.
   This study is focused on the seismic responses of single-layer
   cylindrical lattice shell. A comparison of the responses estimated by
   using the time-history analysis and the response spectrum method is
   presented. For the time-history analysis, 166 ground motion record
   components are employed, while the responses obtained by using the
   response spectrum method are estimated based on the design spectrum or
   the average spectral acceleration of scaled record components. The
   comparison indicates that the accuracy of the response spectrum method
   for such a structure depends on the type of response of interest and the
   intensity measure used to scale the record components. In general, it is
   suggested that the response spectrum method should not be employed to
   estimate the structural member forces of the cylindrical lattice shell.}},
