@inproceedings{ ISI:000388439700027,
Author = {Holness, F. Benjamin and Price, Aaron D.},
Editor = {{BarCohen, Y and Vidal, F}},
Title = {{Robotic extrusion processes for direct ink writing of 3D conductive
   polyaniline structures}},
Booktitle = {{ELECTROACTIVE POLYMER ACTUATORS AND DEVICES (EAPAD) 2016}},
