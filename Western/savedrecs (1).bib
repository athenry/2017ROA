@article{ ISI:000361792100003,
Author = {Stalnov, Oksana and Ben-Gida, Hadar and Kirchhefer, Adam J. and
   Guglielmo, Christopher G. and Kopp, Gregory A. and Liberzon, Alexander
   and Gurka, Roi},
Title = {{On the Estimation of Time Dependent Lift of a European Starling (Sturnus
   vulgaris) during Flapping Flight}},
Journal = {{PLOS ONE}},
Year = {{2015}},
Volume = {{10}},
Number = {{9}},
Month = {{SEP 22}},
Abstract = {{We study the role of unsteady lift in the context of flapping wing bird
   flight. Both aerodynamicists and biologists have attempted to address
   this subject, yet it seems that the contribution of unsteady lift still
   holds many open questions. The current study deals with the estimation
   of unsteady aerodynamic forces on a freely flying bird through analysis
   of wingbeat kinematics and near wake flow measurements using time
   resolved particle image velocimetry. The aerodynamic forces are obtained
   through two approaches, the unsteady thin airfoil theory and using the
   momentum equation for viscous flows. The unsteady lift is comprised of
   circulatory and non-circulatory components. Both approaches are
   presented over the duration of wingbeat cycles. Using long-time sampling
   data, several wingbeat cycles have been analyzed in order to cover both
   the downstroke and upstroke phases. It appears that the unsteady lift
   varies over the wingbeat cycle emphasizing its contribution to the total
   lift and its role in power estimations. It is suggested that the
   circulatory lift component cannot assumed to be negligible and should be
   considered when estimating lift or power of birds in flapping motion.}},
