Title = {{Development of 3D ultrasound needle guidance for high-dose-rate
   interstitial brachytherapy of gynaecological cancers}},
Booktitle = {{MEDICAL IMAGING 2016: ULTRASONIC IMAGING AND TOMOGRAPHY}},
Series = {{Proceedings of SPIE}},
