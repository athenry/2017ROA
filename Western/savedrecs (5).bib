Journal = {{RADIOLOGY}},
Year = {{2012}},
Volume = {{265}},
Number = {{2}},
Pages = {{600-610}},
Month = {{NOV}},
Abstract = {{Purpose: To quantitatively compare hyperpolarized helium 3 (He-3) and
   xenon 129 (Xe-129) magnetic resonance (MR) images obtained within 5
