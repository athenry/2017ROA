@article{ ISI:000306693100023,
Author = {Weiler, J. P. and Wood, J. T.},
Title = {{Modeling the tensile failure of cast magnesium alloys}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2012}},
Volume = {{537}},
Pages = {{133-140}},
Month = {{OCT 5}},
Abstract = {{An analytical tensile failure model for die-cast magnesium alloys is
   used to predict the tensile ductility of die-cast and sand-cast
   magnesium alloy specimens. The size and location of the porosity is
   measured on the fracture surfaces of die-cast AZ91 and AE44 and
   sand-cast AM60, AZ91 and AE44 specimens. The tensile elongation is
