@inproceedings{ ISI:000322002700025,
Author = {Tanguay, Jesse and Yun, Seungman and Kim, Ho Kyung and Cunningham, Ian
   A.},
Editor = {{Nishikawa, RM and Whiting, BR and Hoeschen, C}},
Title = {{Cascaded systems analyses of photon-counting x-ray detectors}},
Booktitle = {{MEDICAL IMAGING 2013: PHYSICS OF MEDICAL IMAGING}},
