@article{ ISI:000304995600007,
Author = {Buchanan, Daniel N. and Lindenmaier, Tamas and McKay, Shayna and Bureau,
   Yves and Hackam, Daniel G. and Fenster, Aaron and Parraga, Grace},
Title = {{THE RELATIONSHIP OF CAROTID THREE-DIMENSIONAL ULTRASOUND VESSEL WALL
   VOLUME WITH AGE AND SEX: COMPARISON TO CAROTID INTIMA-MEDIA THICKNESS}},
Journal = {{ULTRASOUND IN MEDICINE AND BIOLOGY}},
Year = {{2012}},
Volume = {{38}},
Number = {{7}},
Pages = {{1145-1153}},
Month = {{JUL}},
Abstract = {{The relationship of three-dimensional ultrasound (3DUS)-derived carotid
   vessel wall volume (VWV) was evaluated with respect to age and sex.
   B-mode and 3DUS images were acquired for 316 subjects from diverse
   groups including obese primary prevention, diabetic nephropathy, renal
   transplant and rheumatoid arthritis populations. The relationship for
   intima-media thickness (IMT) and VWV with age and sex were determined
   using Pearson-product-moment correlations. Mean IMT (r = 0.18, p =
   0.001) and VWV (r = 0.24, p < 0.01) correlated modestly with age. There
   were modest correlations in males (IMT, r = 0.19, p = 0.003; VWV, r =
   0.34, p < 0.001) and in females for IMT and age (r = 0.30, p = 0.007)
   but not between 3DUS VWV and age in females (r = 0.10, p = 0.4).
   Significant associations between plaque and VWV (r = 0.36, p = 0.001)
   but not IMT suggest different correlations in females that may be
   attributed to plaque. (E-mail: gep@imaging.robarts.ca) (C) 2012 World
   Federation for Ultrasound in Medicine \& Biology.}},
