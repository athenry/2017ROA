@article{ ISI:000324853700003,
Author = {Tecle, Amanuel S. and Bitsuamlak, Girma T. and Aly, Aly Mousaad},
Title = {{Internal pressure in a low-rise building with existing envelope openings
   and sudden breaching}},
Journal = {{WIND AND STRUCTURES}},
Year = {{2013}},
Volume = {{16}},
Number = {{1, SI}},
Pages = {{25-46}},
Month = {{JAN}},
Abstract = {{This paper presents a boundary-layer wind tunnel (BLWT) study on the
   effect of variable dominant openings on steady and transient responses
   of wind-induced internal pressure in a low-rise building. The paper
   presents a parametric study focusing on differences and similarities
   between transient and steady-state responses, the effects of size and
   locations of dominant openings and vent openings, and the effects of
   wind direction angle. In addition, the necessity of internal volume
   correction during sudden breaching was considered, i.e., a transient
