@article{ ISI:000302466700006,
Author = {Burkhart, Timothy A. and Andrews, David M. and Dunning, Cynthia E.},
Title = {{Failure characteristics of the isolated distal radius in response to
   dynamic impact loading}},
Journal = {{JOURNAL OF ORTHOPAEDIC RESEARCH}},
Year = {{2012}},
Volume = {{30}},
Number = {{6}},
Pages = {{885-892}},
Month = {{JUN}},
Abstract = {{We examined the mechanical response of the distal radius pre-fracture
   and at fracture under dynamic impact loads. The distal third of eight
   human cadaveric radii were potted and placed in a custom designed
   pneumatic impact system. The distal intra-articular surface of the
   radius rested against a model scaphoid and lunate, simulating 45 degrees
   of wrist extension. The scaphoid and lunate were attached to a load cell
   that in turn was attached to an impact plate. Impulsive impacts were
   applied at increasing energy levels, in 10?J increments, until fracture
   occurred. Three 45 degrees stacked strain gauge rosettes were affixed
   along the length of the radius quantifying the bone strains. The mean
   (SD) fracture energy was 45.5 (16)?J. The mean (SD) resultant impact
   reaction force (IRFr) at failure was 2,142 (1,229)?N, resulting in high
   compressive strains at the distal (2,718 (1,698)?mu e) and proximal
   radius (3,664 (1,890)?mu e). We successfully reproduced consistent
   fracture patterns in response to dynamic loads. The fracture energy and
   forces reported here are lower and the strains are higher than those
   previously reported and can likely be attributed to the controlled,
   incremental, dynamic nature of the applied loads. (c) 2011 Orthopaedic
   Research Society. Published by Wiley Periodicals, Inc. J Orthop Res
   30:885892, 2012}},
