@article{ ISI:000312571000003,
Author = {Seifu, Dawit G. and Mequanint, Kibret},
Title = {{Fabrication of Vascular Tissue Engineering Scaffolds with Enhanced
   Oxygen Diffusivity and Cell Infiltration}},
Journal = {{JOURNAL OF BIOMATERIALS AND TISSUE ENGINEERING}},
Year = {{2012}},
Volume = {{2}},
Number = {{4}},
Pages = {{280-291}},
Month = {{DEC}},
Abstract = {{Increased oxygen diffusivity through three dimensional (3D) porous
   scaffolds is an important requirement to successfully fabricate
   engineered tissue constructs. In this study, tubular and disk-shaped
   poly(carbonate urethane) (PCU) scaffolds for vascular tissue engineering
   application were fabricated by a solvent casting and particulate
   leaching method using different PCU concentration (15\%-30\% in N,
   N-dimethylformamide (DMF)) and NH4Cl porogens. Enhanced pore
   interconnectivity and oxygen diffusivity resulted from scaffolds
   fabricated using 15\% PCU in DMF. Human coronary artery smooth muscle
   cells (HCASMCs) were seeded into these scaffolds and cultured for up to
   28 days under static or flow conditions in a perfusion bioreactor. For
   both tubular and disk-shaped scaffolds, better cell infiltration depth
   and distribution was observed under flow condition than static culture
   controls as shown by hematoxylin and eosin staining and fluorescence
   staining respectively. Furthermore, preliminary Western blot data
   suggested that dynamically-cultured HCASMCs produced more elastin than
   static culture controls. The results demonstrated that scaffolds having
   high oxygen diffusivity and that support HCASMCs attachment and
   infiltration could be designed for the fabrication of tissue engineered
   vascular constructs.}},
