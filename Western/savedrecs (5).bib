@article{ ISI:000308452100006,
Author = {Biegler, Franziska and Daley, Mark and McQuillan, Ian},
Title = {{Algorithmic decomposition of shuffle on words}},
Journal = {{THEORETICAL COMPUTER SCIENCE}},
Year = {{2012}},
Volume = {{454}},
Pages = {{38-50}},
Month = {{OCT 5}},
Abstract = {{We investigate shuffle-decomposability into two words. We give an
   algorithm which takes as input a DFA M (under certain conditions) and
   determines the unique candidate decomposition into words u and v such
   that L(M) = u (sic) v if M is shuffle decomposable, in time O(vertical
   bar u vertical bar + vertical bar v vertical bar). Even though this
   algorithm does not determine whether or not the DFA is shuffle
   decomposable, the sublinear time complexity of only determining the two
   words under the assumption of decomposability is surprising given the
   complexity of shuffle, and demonstrates an interesting property of the
   operation.
   We also show that for given words u and u and a DFA M we can determine
   whether u (sic) v subset of L(M) in polynomial time. (C) 2012 Elsevier
   B.V. All rights reserved.}},
