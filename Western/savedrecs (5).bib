@article{ ISI:000300811100020,
Author = {Musa, Ahmed Y. and Jalgham, Ramzi T. T. and Mohamad, Abu Bakar},
Title = {{Molecular dynamic and quantum chemical calculations for phthalazine
   derivatives as corrosion inhibitors of mild steel in 1 M HCl}},
Journal = {{CORROSION SCIENCE}},
Year = {{2012}},
Volume = {{56}},
Pages = {{176-183}},
Month = {{MAR}},
Abstract = {{The abilities of phthalazine derivatives, including phthalazine (PT),
   phthalazone (PTO) and phthalhydrazide (PTD), to inhibit the corrosion of
   mild steel in 1 M HCl at 30 degrees C were studied using electrochemical
   impedance spectroscopy (EIS) and potentiodynamic polarization
   measurements. Theoretical calculations were performed to investigate the
   electronic structures of the PT derivatives. Our results showed that the
   inhibition efficiencies of these derivatives improved with increases in
   concentration. The data also showed that PTD < PT < PTO in terms of the
   inhibiting efficiency. Theoretical calculations also revealed that PTO
   is expected to be the best inhibitor among the studied phthalazine
   derivatives. (C) 2011 Elsevier Ltd. All rights reserved.}},
