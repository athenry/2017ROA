Title = {{Intra-operative Prostate Motion Tracking Using Surface Markers for
   Robot-Assisted Laparoscopic Radical Prostatectomy}},
Booktitle = {{MEDICAL IMAGING 2012: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
Series = {{Proceedings of SPIE}},
