@inproceedings{ ISI:000371317400047,
Author = {Ukwatta, Eranga and Yuan, Jing and Rajchl, Martin and Fenster, Aaron},
Editor = {{Ayache, N and Delingette, H and Golland, P and Mori, K}},
Title = {{Efficient Global Optimization Based 3D Carotid AB-LIB MRI Segmentation
   by Simultaneously Evolving Coupled Surfaces}},
Booktitle = {{MEDICAL IMAGE COMPUTING AND COMPUTER-ASSISTED INTERVENTION - MICCAI
   2012, PT III}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2012}},
Volume = {{7512}},
