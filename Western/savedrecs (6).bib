Title = {{Impact of initial surface parameters on the final quality of laser
   micro-polished surfaces}},
Booktitle = {{MICROMACHINING AND MICROFABRICATION PROCESS TECHNOLOGY XVII}},
Series = {{Proceedings of SPIE}},
