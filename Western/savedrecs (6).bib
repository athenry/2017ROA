@inproceedings{ ISI:000317098400007,
Author = {Crites, Christine D. and Varma, Rajiv K. and Sharma, Vinay and Milroy,
   Bill},
Book-Group-Author = {{IEEE}},
Title = {{Characterization of Harmonics in a Utility Feeder with PV Distributed
   Generation}},
Booktitle = {{2012 IEEE ELECTRICAL POWER AND ENERGY CONFERENCE (EPEC)}},
Series = {{IEEE Electrical Power and Energy Conference}},
Year = {{2012}},
Pages = {{40-45}},
Note = {{IEEE Electrical Power and Energy Conference (EPEC), London, CANADA, OCT
   10-12, 2012}},
Organization = {{IEEE}},
Abstract = {{A thorough analysis of the harmonic characteristics of a feeder in the
   London Hydro distribution system is presented in this paper. This feeder
   interconnects several solar photovoltaic (PV) arrays as well as two
   shunt capacitor banks. Harmonic measurements were acquired over one week
   at critical locations in the feeder. The analysis highlights higher than
   expected harmonic distortion levels, though still within limits
   established in IEEE 519-1992. Harmonics injected by the solar systems
   are partially additive, contributing approximately 0.1\% to the total
   harmonic distortion (THD) at the feeder terminal. Significant 3rd, 5th,
