@inproceedings{ ISI:000324506700138,
Author = {Jobehdar, Mona Hassanzadeh and Gadallah, Aly H. and Siddiqui, Kamran and
   Chishty, Wajid A.},
Book-Group-Author = {{ASME}},
Title = {{THE EFFECT OF NOZZLE SHAPE AND CONFIGURATION ON BUBBLE FORMATION IN A
   LIQUID CROSS FLOW}},
Booktitle = {{PROCEEDINGS OF THE ASME FLUIDS ENGINEERING DIVISION SUMMER MEETING,
   2012, VOL 1, PTS A AND B, SYMPOSIA}},
Series = {{ASME Fluids Engineering Division Summer Meeting}},
