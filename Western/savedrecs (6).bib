@article{ ISI:000299775500018,
Author = {Prince, Andrew and Bassi, Amarjeet S. and Haas, Christine and Zhu, Jesse
   X. and Dawe, Jennifer},
Title = {{Soy protein recovery in a solvent-free process using continuous
   liquid-solid circulating fluidized bed ion exchanger}},
Journal = {{BIOTECHNOLOGY PROGRESS}},
Year = {{2012}},
Volume = {{28}},
Number = {{1}},
Pages = {{157-162}},
Month = {{JAN-FEB}},
Abstract = {{Soy protein concentrates and soy protein isolates act as ingredients in
   bakery, meat and dairy products, baby formulas, starting materials for
   spun textured vegetable products, and other nutritional supplements. In
   this study, the effectiveness of a liquidsolid circulating fluidized bed
   (LSCFB) ion exchanger is demonstrated for the recovery of soluble soy
   proteins from full fat and defatted soy flour. Under steady-state
   operating conditions, about 50\% of the proteins could be recovered from
   the feed streams entering the ion exchanger. The LSCFB was shown to be a
   promising system for the recovery of soy protein from both defatted and
   full fat soy flour solutions. As the ion exchange process captures
   dissolved proteins, the system may offer a less damaging form of
   processing compared with the acid precipitation process where soy
   protein aggregates form and functionality is affected. In addition, the
   LSCFB allows simultaneous adsorption and desorption of the proteins
   allowing for a continuous operation. No prefiltration of feed containing
   suspended particles is required as well, because fluidization is used in
   place of packed bed technology to improve on current ion exchange
   processes. (c) 2011 American Institute of Chemical Engineers Biotechnol.
   Prog., 2012}},
