@article{ ISI:000375814200005,
Author = {Ott, Noemie and Yan, Yuanming and Ramamurthy, Sridhar and Kairy, Shravan
   and Birbilis, Nick},
Title = {{Auger electron spectroscopy analysis of grain boundary microchemistry in
   an Al-Cu-Li alloy}},
Journal = {{SCRIPTA MATERIALIA}},
Year = {{2016}},
Volume = {{119}},
Pages = {{17-20}},
Month = {{JUL 1}},
Abstract = {{In situ impact fracture combined with Auger electron spectroscopy (AES)
   was employed to determine grain boundary (GB) microchemistry, namely
   lithium (Li) concentration, for an Al-Cu-Li alloy. The Li/Al ratio of
   GBs in the naturally aged condition was revealed to be significantly
   higher than in the overaged condition (or in the bulk alloy). GB
   fracture surfaces for the naturally aged alloy were depleted in Cu,
   whereas in the overaged condition the GB fracture surfaces were depleted
   in Li and enriched in Cu relative to the bulk composition. These
   findings are discussed in the context of intergranular corrosion and the
   precipitation sequence in Al-Cu-Li alloys. (C) 2016 Elsevier Ltd. All
   rights reserved.}},
