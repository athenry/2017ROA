@article{ ISI:000371557400022,
Author = {Grolinger, Katarina and L'Heureux, Alexandra and Capretz, Miriam A. M.
   and Seewald, Luke},
Title = {{Energy Forecasting for Event Venues: Big Data and Prediction Accuracy}},
Journal = {{ENERGY AND BUILDINGS}},
Year = {{2016}},
Volume = {{112}},
Pages = {{222-233}},
Month = {{JAN 15}},
Abstract = {{Advances in sensor technologies and the proliferation of smart meters
   have resulted in an explosion of energy-related data sets. These Big
   Data have created opportunities for development of new energy services
   and a promise of better energy management and conservation. Sensor-based
   energy forecasting has been researched in the context of office
   buildings, schools, and residential buildings. This paper investigates
   sensor-based forecasting in the context of event-organizing venues,
   which present an especially difficult scenario due to large variations
   in consumption caused by the hosted events. Moreover, the significance
   of the data set size, specifically the impact of temporal granularity,
   on energy prediction accuracy is explored. Two machine-learning
   approaches, neural networks (NN) and support vector regression (SVR),
   were considered together with three data granularities: daily, hourly,
   and 15 minutes. The approach has been applied to a large entertainment
   venue located in Ontario, Canada. Daily data intervals resulted in
   higher consumption prediction accuracy than hourly or 15-min readings,
   which can be explained by the inability of the hourly and 15-min models
   to capture random variations. With daily data, the NN model achieved
   better accuracy than the SVR; however, with hourly and 15-min data,
   there was no definitive dominance of one approach over another. Accuracy
   of daily peak demand prediction was significantly higher than accuracy
   of consumption prediction. (C) 2015 The Authors. Published by Elsevier
   B.V.}},
