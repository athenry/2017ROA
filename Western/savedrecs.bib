@article{ ISI:000389164000009,
Author = {Dordizadeh, Peyman and Adamiak, Kazimierz and Castle, G. S. Peter},
Title = {{Parametric study of the characteristics of Trichel pulses in the
   needle-plane negative corona discharge in atmospheric air}},
Journal = {{JOURNAL OF ELECTROSTATICS}},
Year = {{2016}},
Volume = {{84}},
Pages = {{73-80}},
Month = {{DEC}},
Abstract = {{A numerical study of the impact of different model coefficients on the
   frequency, DC current, and the rise time of the Trichel pulses in a
   needle-plane negative corona discharge is presented here. The studied
   coefficients include the coefficients of the ionization and attachment
   reactions, the mobilities of the three charged species considered in the
   model (electrons and positive and negative ions), and the coefficient of
   the secondary emission of electrons. The characteristics of the pulses
   were also studied using another set of model coefficients which includes
   an extended number of reactions such as detachment of the electrons from
   negative ions. (C) 2016 Elsevier B.V. All rights reserved.}},
