@article{ ISI:000377487000025,
Author = {Zhang, Naibo and Hu, Mingjun and Shao, Lingmin and Yang, Jun},
Title = {{Localization of Printed Chipless RFID in 3-D Space}},
Journal = {{IEEE MICROWAVE AND WIRELESS COMPONENTS LETTERS}},
Year = {{2016}},
Volume = {{26}},
Number = {{5}},
Pages = {{373-375}},
Month = {{MAY}},
Abstract = {{A novel localization technique of printed chipless RFID in 3-D space is
   presented by using a frequency domain chipless tag. The chipless tag is
   interrogated by an ultra-wideband (UWB) signal, and the response from
   tag is analyzed for extracting the resonance information. The range
   information is used for accurate localization of chipless tag. The
   validity and accuracy of the localization method has been verified by
   moving the tag to different points in a fixed interrogation zone. The
   measured angular deviations achieved based on the proposed approach are
   less than 5 degrees when localization radius is from 10 cm to 40 cm.}},
