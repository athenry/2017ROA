@article{ ISI:000378276700009,
Author = {Chowdhury, Mohammad Monirul Islam and Kim, Mingu and Haroun, Basem
   Mikhaeil and Nakhla, George and Keleman, Michael},
Title = {{Flocculent Settling of Food Wastes}},
Journal = {{WATER ENVIRONMENT RESEARCH}},
Year = {{2016}},
Volume = {{88}},
Number = {{7}},
Pages = {{660-664}},
Month = {{JUL 1}},
Abstract = {{This study evaluated the flocculent settling in water and municipal
   wastewater (MWW) in a 10.6 ft deep column. A total of eight runs at
   three different testing conditions involving MWW alone, food waste (FW)
   alone, and FW in MWW (FW+MWW) were conducted. Total suspended solid
   (TSS), total BOD (TBOD), total COD (TCOD), total nitrogen (TN), and
   total phosphorous (TP) removal efficiencies after 3 hours of settling
   were 62\%, 46\%, 49\%, 46\% and 62\% for FW, and 50\%, 43\%, 39\%, 37\%
   and 24\% for MWW. Removal efficiencies of particulate COD (PCOD) and
   particulate BOD (PBOD) at the lowest surface overflow rate (SOR) of 1.1
   m(3)/m(2)/hr corresponding to the longest settling time of 3 hours were
   59\% and 64\% for FW, and 65\% and 70\% for FW with MWW samples. On the
