@article{ ISI:000369201100005,
Author = {Guo, Daoping and Robinson, Clare and Herrera, Jose E.},
Title = {{Mechanism of dissolution of minium (Pb3O4) in water under depleting
   chlorine conditions}},
Journal = {{CORROSION SCIENCE}},
Year = {{2016}},
Volume = {{103}},
Pages = {{42-49}},
Month = {{FEB}},
Abstract = {{Long-term batch dissolution experiments were performed on solid phase
   Pb3O4 under drinking water conditions simulating a depleting chlorine
   regime. Analytical and spectroscopic results indicate Pb3O4 is first
   dissolved in the presence of chlorine through a nucleophilic attack to
   the Pb2+ centers in the solid, and is subsequently oxidized by free
   chlorine to form beta-PbO2, with a concomitant decrease in soluble lead
   concentrations. Following chlorine depletion, Pb3O4 dissolution
   regulates the process, resulting in surface deposition of beta-PbO2.
   Formation of lead carbonates controls the final observed lead
   concentrations for Pb3O4 dissolution. A dissolution model is presented
   to rationalize these observations. (C) 2015 Elsevier Ltd. All rights
   reserved.}},
