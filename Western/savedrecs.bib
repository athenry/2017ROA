@article{ ISI:000388627200003,
Author = {Daneshvar, Poulad and Bouaanani, Najib and Goda, Katsuichiro and
   Atkinson, Gail M.},
Title = {{Damping Reduction Factors for Crustal, Inslab, and Interface Earthquakes
   Characterizing Seismic Hazard in Southwestern British Columbia, Canada}},
Journal = {{EARTHQUAKE SPECTRA}},
Year = {{2016}},
Volume = {{32}},
Number = {{1}},
Pages = {{45-74}},
Month = {{FEB}},
Abstract = {{High-damping displacement spectra and corresponding damping reduction
   factors (eta) are important ingredients in seismic design and analysis
   of structures equipped with seismic protection systems, as well as in
   displacement-based design methodologies. In this study, we investigated
   eta factors for three types of earthquake characterizing seismic hazard
   in southwestern British Columbia, Canada: shallow crustal, deep inslab,
   and interface subduction. We used a large and comprehensive database
   including records from recent relevant earthquakes, such as the 2011
   Tohoku event. Our key observations were as follows: (1) there is
   negligible dependence of eta on soil class; (2) there is significant
   dependence of eta on the frequency content and duration of ground
   motions that characterize the different record types, and (3) eta is
   dependent on period, particularly for inslab events. Period-dependent
   equations were proposed to predict eta for damping ratios between 5\%
   and 30\% corresponding to the three event types.}},
