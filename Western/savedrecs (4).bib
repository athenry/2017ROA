@inproceedings{ ISI:000341264000021,
Author = {Meskini, Sonia and Nassif, Ali Bou and Capretz, Luiz Fernando},
Book-Group-Author = {{IEEE Comp Soc}},
Title = {{Reliability Models Applied to Mobile Applications}},
Booktitle = {{2013 IEEE 7TH INTERNATIONAL CONFERENCE ON SOFTWARE SECURITY AND
   RELIABILITY - COMPANION (SERE-C)}},
Year = {{2013}},
Pages = {{156-163}},
Note = {{7th IEEE International Conference on Software Security and Reliability
   (SERE), Gaithersburg, MD, JUN 18-20, 2013}},
Organization = {{IEEE; Natl Inst Stand \& Technol; IEEE Reliabil Soc; IEEE Comp Soc}},
Abstract = {{Smartphones have become the most used electronic devices. They carried
   out most of the functionalities of desktops, allowing various useful
   applications that suit the users' needs. Therefore, instead of the
   operator, the user has become the number one controller of the device
   and its applications and thus its reliability becomes an emergent need.
   We aim to investigate and evaluate the efficacy of Software Reliability
   Growth Models (SRGMs) when applied to Smartphone application failure
   data and check whether they achieve the same success as in the
   desktop/laptop area. We selected three of the most used SRGMs and
   applied them to three different Smartphone applications. None of the
   selected models were able to account for the data satisfactorily. Their
   failure is traced back to the specific features of mobile applications
   compared to desktop applications. Thus, a suitable model for Smartphone
   applications is still needed to improve their reliability.}},
