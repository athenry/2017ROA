@article{ ISI:000318239300008,
Author = {Alecian, E. and Wade, G. A. and Catala, C. and Grunhut, J. H. and
   Landstreet, J. D. and Boehm, T. and Folsom, C. P. and Marsden, S.},
Title = {{A high-resolution spectropolarimetric survey of Herbig Ae/Be stars - II.
   Rotation}},
