@article{ ISI:000323356200014,
Author = {Zhang, Y. T. and Li, D. J. and Zhao, M. L. and Guo, M. X. and Sun, X.
   and Geng, D. and Gu, H. Q.},
Title = {{Comparative in Cell and Blood Behaviors of Pristine, Carboxyl and
   Hydroxyl Functionalized Multiwalled Carbon Nanotubes}},
Journal = {{SCIENCE OF ADVANCED MATERIALS}},
Year = {{2013}},
Volume = {{5}},
Number = {{10}},
Pages = {{1436-1443}},
Month = {{OCT}},
Abstract = {{In vitro cytocompatibility and hemocompatibility of pristine multiwalled
   carbon nanotubes (MWCNTs), hydroxyl functionalized MWCNTs (MWCNTs-OH),
   and carboxyl functionalized MWCNTs (MWCNTs-COOH) were investigated.
   Their interactions with mouse-fibroblast cells, human endothelial cells,
   and rabbit blood were characterized using cell-adhesion,
   platelet-adhesion and hemolytic-rate assays. The results showed that
   both cells cultured on the functionalized MWCNTs displayed higher cell
   proliferation, viability and adhesion than those on pristine MWCNTs,
   indicating the improved cell growth on the functionalized MWCNTs. The
   cells adhering on MWCNTs-OH showed the best growth situation among the
   three MWCNTs systems. Platelet investigation indicated that the
   anticoagulantion of three materials increased in the order: pristine
   MWCNTs < MWCNTs-OH < MWCNTs-COOH. The three MWCNTs were also
   nonhemolytic materials. Fourier transform infrared spectroscopy, X-ray
   photoelectron spectroscopy and water contact angle measurements
   indicated the existence of hydroxyl and carboxyl moieties on the
   functionalized MWCNTs, attributing superior hydrophilicity to the
   pristine MWCNTs, which might be the main reasons related to the
   improvement of cytocompatibility and hemoconnpatibility.}},
