@article{ ISI:000318495300009,
Author = {Geremew, Africa M.},
Title = {{Pore-Water Pressure Development Caused by Wave-Induced Cyclic Loading in
   Deep Porous Formation}},
Journal = {{INTERNATIONAL JOURNAL OF GEOMECHANICS}},
Year = {{2013}},
Volume = {{13}},
Number = {{1}},
Pages = {{65-68}},
Month = {{FEB}},
Abstract = {{The overall development of pore-water pressure in a deep, homogenous,
   and isotropic porous formation caused by wave-induced cyclic loading has
