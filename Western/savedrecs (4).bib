@article{ ISI:000315425800007,
Author = {Osgouee, Ahmad and Jiang, Jin},
Title = {{ROBUST NONLINEAR METHOD FOR STEAM GENERATOR LEVEL CONTROL}},
Journal = {{NUCLEAR TECHNOLOGY}},
Year = {{2013}},
Volume = {{181}},
Number = {{3}},
Pages = {{493-506}},
Month = {{MAR}},
Abstract = {{In this paper, a new, robust control method based on a multimodel
   predictive control scheme is developed for steam generator level (SGL)
   control in nuclear power plants. For a multiramp power increase from low
   to full power, the proposed controller is capable of keeping the SGL
   within the admissible range by minimizing the level transients and
   improving the stability of the control loop. Simulation results and a
   general framework for systematically studying the SGL are presented to
   demonstrate the effectiveness of the proposed control method by
   comparing the performance of the designed controller with that of a
   properly tuned conventional three-element
   proportional-integral-derivative (PID) controller. Moreover, it has been
   demonstrated that the proposed controller is more robust than a
   conventional PID controller to steam flow disturbances caused by load
   variations.}},
