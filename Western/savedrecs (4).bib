@article{ ISI:000321735400025,
Author = {Tymchyshyn, Matthew and Yuan, Zhongshun and Xu, Chunbao (Charles)},
Title = {{Direct conversion of glycerol into bio-oil via hydrotreatment using
   supported metal catalysts}},
Journal = {{FUEL}},
Year = {{2013}},
Volume = {{112}},
Pages = {{193-202}},
Month = {{OCT}},
Abstract = {{Hydrotreatment of glycerol (a byproduct of bio-diesel production) was
   conducted in the presence of a number of different supported metal
   catalysts at a temperature of 300 degrees C under 5 MPa initial pressure
   of H-2. The bio-oil products were comprehensively characterized to
   determine their physical/chemical properties. Experiments with supported
   catalyst metals revealed a positive correlation between support acidity
   and bio-oil yield. It was determined that MoCoP is the most effective
   combination of catalyst metals, and the bio-oil yield from
   hydro-treatment of glycerol with the MoCoP catalysts is strongly
   dependent on the support materials, following the trend of MgO < AC <
   Al2O3 < zeolite. With the MoCoP/zeolite catalyst, a maximum yield of
   similar to 40 wt.\% bio-oil, and >90\% glycerol conversion was achieved
   for 60 min reaction. The volatile components of the bio-oil were found
   to consist mostly of substituted phenols and ketones in the C6-C9 range.
   The bio-oils obtained had a maximum HHV of 33 MJ/kg and the majority
   (similar to 95\%) of the compounds present was found to have boiling
   points lower than 165 degrees C, making these bio-oils potential
   oxygenated fuel additives. (C) 2013 Elsevier Ltd. All rights reserved.}},
Publisher = {{ELSEVIER SCI LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, OXON, ENGLAND}},
