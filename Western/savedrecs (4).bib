@article{ ISI:000322656500020,
Author = {Gu, Renpeng and Xu, William Z. and Charpentier, Paul A.},
Title = {{Synthesis of polydopamine-coated graphene-polymer nanocomposites via
   RAFT polymerization}},
Journal = {{JOURNAL OF POLYMER SCIENCE PART A-POLYMER CHEMISTRY}},
Year = {{2013}},
Volume = {{51}},
Number = {{18}},
Pages = {{3941-3949}},
Month = {{SEP 15}},
Abstract = {{Graphene-polymer nanocomposites have significant potential in many
   applications such as photovoltaic devices, fuel cells, and sensors.
   Functionalization of graphene is an essential step in the synthesis of
   uniformly distributed graphene-polymer nanocomposites, but often results
   in structural defects in the graphitic sp 2 carbon framework. To address
   this issue, we synthesized graphene oxide (GO) by oxidative exfoliation
   of graphite and then reduced it into graphene via self-polymerization of
   dopamine (DA). The simultaneous reduction of GO into graphene, and
   polymerization and coating of polydopamine (PDA) on the reduced graphene
   oxide (RGO) surface were confirmed with XRD, UV-Vis, XPS, Raman, TGA,
   and FTIR. The degree of reduction of GO increased with increasing DA/GO
   ratio from 1/4 to 4/1 and/or with increasing temperature from room
   temperature to 60 degrees C. A RAFT agent,
   2-(dodecylthiocarbonothioylthio) 22methylpropionic acid, was linked onto
   the surface of the PDA/RGO, with a higher equivalence of RAFT agent in
   the reaction leading to a higher concentration of RAFT sites on the
   surface. Graphene-poly(methyl methacrylate), graphene-poly(tert-butyl
   acrylate), and graphene-poly(N-isopropylacrylamide) nanocomposites were
   synthesized via RAFT polymerization, showing their characteristic
   solubility in several different solvents. This novel synthetic route was
   found facile and can be readily used for the rational design of
   graphene-polymer nanocomposites, promoting their applications. (C) 2013
   Wiley Periodicals, Inc. J. Polym. Sci., Part A: Polym. Chem. 2013, 51,
   3941-3949}},
Publisher = {{WILEY-BLACKWELL}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
