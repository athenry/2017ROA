@inproceedings{ ISI:000348029400088,
Author = {Hrinivich, W. Thomas and Gibson, Eli and Gaed, Mena and Gomez, Jose A.
   and Moussa, Madeleine and McKenzie, Charles A. and Bauman, Glenn S. and
   Ward, Aaron D. and Fenster, Aaron and Wong, Eugene},
Editor = {{Yaniv, ZR and Holmes, DR}},
Title = {{A dimensionless dynamic contrast enhanced MRI parameter for
   intra-prostatic tumour target volume delineation: Initial comparison
   with histology}},
Booktitle = {{MEDICAL IMAGING 2014: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
Series = {{Proceedings of SPIE}},
Year = {{2014}},
Volume = {{9036}},
