@inproceedings{ ISI:000392740800053,
Author = {Khoshnam, Mahta and Patel, Rajni V.},
Book-Group-Author = {{IEEE}},
Title = {{Robotics-Assisted Catheter Manipulation for Improving Cardiac Ablation
   Efficiency}},
Booktitle = {{2014 5th IEEE RAS \& EMBS International Conference on Biomedical
