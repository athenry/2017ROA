Language = {{English}},
Affiliation = {{Floryan, JM (Reprint Author), Univ Western Ontario, Dept Mech \& Mat Engn, London, ON N6A 5B9, Canada.
   Inasawa, A.; Asai, M., Tokyo Metropolitan Univ, Dept Aerosp Engn, Hino, Tokyo 1910065, Japan.
   Floryan, J. M., Univ Western Ontario, Dept Mech \& Mat Engn, London, ON N6A 5B9, Canada.}},
DOI = {{10.1007/s00162-014-0321-x}},
