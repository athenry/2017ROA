@inproceedings{ ISI:000380551500044,
Author = {Delrobaei, Mehdi and Parrent, Andrew and Jog, Mandar and Rahimi,
   Fariborz and Tran, Stephanie and Ognjanovic, Kristina and Gilmore,
   Greydon and McIssac, Kenneth},
Book-Group-Author = {{IEEE}},
Title = {{Quantifying the Short-term Effects of Deep Brain Stimulation Surgery on
   Bradykinesia in Parkinson's Disease Patients}},
Booktitle = {{2014 21TH IRANIAN CONFERENCE ON BIOMEDICAL ENGINEERING (ICBME)}},
Year = {{2014}},
Pages = {{224-228}},
Note = {{21th Iranian Conference on Biomedical Engineering, Tehran, IRAN, NOV
   26-28, 2014}},
Organization = {{IEEE}},
Abstract = {{Clinical scale-based follow-up of patients undergoing bilateral
   subthalamic deep brain stimulation (STN-DBS) surgery has shown
   inconsistent effects on bradykinesia in Parkinson's disease patients.
   Furthermore, quantitative assessment of STN-DBS effect on bradykinesia
   has not been fully explored yet. Our group uses multisensory kinematic
   technologies to study short-term and long-term DBS effects on
