@article{ ISI:000330417100016,
Author = {Xiao, Biwei and Li, Xifei and Li, Xia and Wang, Biqiong and Langford,
   Craig and Li, Ruying and Sun, Xueliang},
Title = {{Graphene Nanoribbons Derived from the Unzipping of Carbon Nanotubes:
   Controlled Synthesis and Superior Lithium Storage Performance}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2014}},
Volume = {{118}},
Number = {{2}},
Pages = {{881-890}},
Month = {{JAN 16}},
Abstract = {{Graphene nanoribbons (GNRs) from chemical unzipping of carbon nanotubes
   (CNTs) have been reported to be a suitable candidate for lithium ion
   battery materials, but very few of them focused on controlling GNRs with
   different unzipping levels. Here we present a study of GNRs with
   controlled unzipping level and the prevailing factors that affect the
   lithium storage performance at early and final unzipping level; besides,
   the effect of thermal reduction has been investigated. On the basis of
   Raman and BET surface area tests, we found that the unzipping of CNTs
   starts with surface etching and then proceeds to partial and full
   unzipping and finally fragmentation and aggregation. Galvanostatic
   charge discharge reveals that defect increase is mainly responsible for
   the capacity enhancement at the early unzipping level; surface area drop
   is associated with the capacity fade at the final unzipping level.
   Surface functional groups can result in low electrical conductivity and
   therefore cause capacity drop within several cycles. The GNRs with
   controlled unzipping level display, different electrochemical behaviors
   and thus can provide rational choices for researchers who are searching
   for desired functions using GNRs as additives in lithium ion batteries.}},
