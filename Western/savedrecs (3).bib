Author = {Shahbazi, Mahya and Talebi, H. Ali and Patel, Rajni V.},
Book-Group-Author = {{IEEE}},
Title = {{Networked Dual-User Teleoperation with Time-Varying Authority
   Adjustment: A Wave Variable Approach}},
Booktitle = {{2014 IEEE/ASME INTERNATIONAL CONFERENCE ON ADVANCED INTELLIGENT
   MECHATRONICS (AIM)}},
Series = {{IEEE ASME International Conference on Advanced Intelligent Mechatronics}},
