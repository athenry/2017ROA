@article{ ISI:000328507700028,
Author = {Wijeratne, Dunisha S. and Moschopoulos, Gerry},
Title = {{A Novel Three-Phase Buck-Boost AC-DC Converter}},
Journal = {{IEEE TRANSACTIONS ON POWER ELECTRONICS}},
Year = {{2014}},
Volume = {{29}},
Number = {{3}},
Pages = {{1331-1343}},
Month = {{MAR}},
Abstract = {{A simple, low-cost, reduced-switch, three-phase ac-dc buck-boost
   converter is proposed in this paper. The converter can operate with
   input power factor correction and is suitable for applications where a
   converter needs to operate over a wide range of input ac voltages and/or
   produce a wide range of output dc voltages. The paper will examine how a
   reduced switched converter with capacitive input filter operates in the
   boost mode and how a reduced switch converter operates in buck and boost
   modes. In this paper, the converter's operation is explained and
   analyzed in detail and its design is discussed. The feasibility of the
