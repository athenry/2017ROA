@article{ ISI:000331661800030,
Author = {Xie, Jun Yu and Ding, Guang Hong and Karttunen, Mikko},
Title = {{Molecular dynamics simulations of lipid membranes with lateral force:
   Rupture and dynamic properties}},
Journal = {{BIOCHIMICA ET BIOPHYSICA ACTA-BIOMEMBRANES}},
Year = {{2014}},
Volume = {{1838}},
Number = {{3}},
Pages = {{994-1002}},
Month = {{MAR}},
Abstract = {{Membranes' response to lateral tension, and eventual rupture, remains
   poorly understood. In this study, pure dipalmitoylphosphatidylcholine
   (DPPC) lipid bilayers, under tension/pressure, were studied using
   molecular dynamics (MD) simulations. The irreversible membrane breakdown
   is demonstrated to depend on the amplitude of lateral tension, loading
   rate, and the size of the bilayer. In all of our simulations, -200 bar
   lateral pressure was found to be enough to rupture lipid membrane
   regardless of the loading rate or the membrane size. Loading rate and
   membrane size had a significant impact on rupture. A variety of dynamic
   properties of lipid molecules, probability distribution of area per
   lipid particularly, have been determined, and found to be fundamental
   for describing membrane behavior in detail, thus providing the
   quantitative description for the requirement of membrane rupture. (C)
   2014 Elsevier B.V. All rights reserved.}},
