@article{ ISI:000341269500014,
Author = {Hossain, M. Z. and Floryan, J. M.},
Title = {{Natural convection in a fluid layer periodically heated from above}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2014}},
Volume = {{90}},
Number = {{2}},
Month = {{AUG 25}},
Abstract = {{Natural convection in a horizontal layer subject to periodic heating
   from above has been studied. It is shown that the primary convection
   leads to the cooling of the bulk of the fluid below the mean temperature
   of the upper wall. The secondary convection may lead either to
   longitudinal rolls, transverse rolls, or oblique rolls. The global flow
   properties (e.g., the average Nusselt number for the primary convection
   and the critical conditions for the secondary convection) are identical
   to those of the layer heated from below. However, the flow and
   temperature patterns exhibit phase shifts in the horizontal directions.}},
