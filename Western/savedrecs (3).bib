@inproceedings{ ISI:000339426000008,
Author = {Carson, Jeffrey J. L. and Najiminaini, Mohamadreza and Vasefi, Fartash
   and Kaminska, Bozena},
Editor = {{Singh, MR}},
Title = {{Nanostructure-based optical filters for multispectral imaging
   applications}},
Booktitle = {{ELECTRONIC, PHOTONIC, PLASMONIC, PHONONIC AND MAGNETIC PROPERTIES OF
   NANOMATERIALS}},
