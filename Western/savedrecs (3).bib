@article{ ISI:000353721900001,
Author = {Hussain, S. T. and Nadeem, Sohail and Ul Haq, Rizwan},
Title = {{Model-based analysis of micropolar nanofluid flow over a stretching
   surface}},
Journal = {{EUROPEAN PHYSICAL JOURNAL PLUS}},
Year = {{2014}},
Volume = {{129}},
Number = {{8}},
Month = {{AUG 4}},
Abstract = {{This article deals with the study of micropolar nanofluids flow over a
   stretching sheet. This study uses the compatible models to deal with the
   effects of two different kinds of nanoparticles (copper (Cu) and silver
   (Ag)) within the base fluids (water and Kerosene oil). Developed
   governing boundary layer equations and the boundary conditions are
   transformed into the system of coupled nonlinear ordinary differential
   equations. Numerical results are constructed for velocity, temperature,
   skin friction coefficient and local Nusselt number by considering the
   thermo-physical properties of both base fluids and particles. Fluid flow
   behavior is analyzed through stream lines and a conclusion has been
   developed under the observation of fluid flow behavior.}},
