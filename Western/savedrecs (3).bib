@inproceedings{ ISI:000339390900031,
Author = {Guo, Fumin and Pike, Damien and Svenningsen, Sarah and Coxson, Harvey O.
   and Drozd, John J. and Yuan, Jing and Fenster, Aaron and Parraga, Grace},
Editor = {{Molthen, RC and Weaver, JB}},
Title = {{Development and application of pulmonary structure-function registration
   methods: Towards pulmonary image-guidance tools for improved airway
   targeted therapies and outcomes}},
Booktitle = {{MEDICAL IMAGING 2014: BIOMEDICAL APPLICATIONS IN MOLECULAR, STRUCTURAL,
   AND FUNCTIONAL IMAGING}},
Series = {{Proceedings of SPIE}},
