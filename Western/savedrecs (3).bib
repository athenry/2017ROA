@inproceedings{ ISI:000347686400069,
Author = {Ukwatta, Eranga and Yuan, Jing and Qiu, Wu and Wu, Katherine C. and
   Trayanova, Natalia and Vadakkumpadan, Fijoy},
Editor = {{Golland, P and Hata, N and Barillot, C and Hornegger, J and Howe, R}},
Title = {{Myocardial Infarct Segmentation and Reconstruction from 2D
