
@article{ ISI:000342168500016,
Author = {Creed, Irena F. and Spargo, Adam T. and Jones, Julia A. and Buttle, Jim
   M. and Adams, Mary B. and Beall, Fred D. and Booth, Eric G. and
   Campbell, John L. and Clow, Dave and Elder, Kelly and Green, Mark B. and
