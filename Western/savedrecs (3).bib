@inproceedings{ ISI:000348029400058,
Author = {Li, Feng P. and Rajchl, Martin and Moore, John and Peters, Terry M.},
Editor = {{Yaniv, ZR and Holmes, DR}},
Title = {{Ultrasound based mitral valve annulus tracking for off-pump beating
   heart mitral valve repair}},
Booktitle = {{MEDICAL IMAGING 2014: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
Series = {{Proceedings of SPIE}},
Year = {{2014}},
Volume = {{9036}},
