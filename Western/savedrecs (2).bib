@inproceedings{ ISI:000374710500014,
Author = {Jayarathne, Uditha L. and Luo, Xiongbiao and Chen, Elvis C. S. and
   Peters, Terry M.},
Editor = {{Linte, CA and Yaniv, Z and Fallavollita, P}},
Title = {{Simultaneous Estimation of Feature Correspondence and Stereo Object Pose
   with Application to Ultrasound Augmented Robotic Laparoscopy}},
Booktitle = {{AUGMENTED ENVIRONMENTS FOR COMPUTER-ASSISTED INTERVENTIONS, AE-CAI 2015}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2015}},
Volume = {{9365}},
Pages = {{134-144}},
Note = {{10th International Workshop on Augmented Environments for
   Computer-Assisted Interventions (AE-CAI), Munich, GERMANY, OCT09, 2015}},
Abstract = {{In-situ visualization of ultrasound in robot-assisted surgery requires
   robust, real-time computation of the pose of the intra-corporeal
   ultrasound (US) probe with respect to the stereo-laparoscopic camera.
   Image based, intrinsic methods of computing this relative pose need to
   overcome challenges due to irregular illumination, partial feature
   occlusion and clutter that are unavoidable in practical
   robotic-laparoscopy. In this paper, we extend a state-of-the-art
   simultaneous monocular pose and correspondence estimation framework to a
   stereo imaging model. The method is robust to partial feature occlusion
   and clutter, and does not require explicit feature matching. Through
   exhaustive experiments, we demonstrate that in terms of accuracy, the
   proposed method outperforms the conventional stereo pose estimation
   approach and the state-of-the-art monocular camera-based method. Both
   quantitative and qualitative results are presented.}},
