@inproceedings{ ISI:000354365300003,
Author = {Vannelli, Claire and Moore, John and McLeod, Jonathan and Ceh, Dennis
   and Peters, Terry},
Editor = {{Yaniv, ZR and Webster, RJ}},
Title = {{Dynamic heart phantom with functional mitral and aortic valves}},
Booktitle = {{MEDICAL IMAGING 2015: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
Series = {{Proceedings of SPIE}},
Year = {{2015}},
