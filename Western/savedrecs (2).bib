@inproceedings{ ISI:000358617800048,
Author = {Zhang, Zheng-Rong and Jiang, Li-Ying},
Editor = {{Zhang, Z}},
Title = {{Effect of Flexoelectricity on the Electromechanical Coupling of
   Piezoelectric Nanoplates}},
