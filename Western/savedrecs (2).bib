@article{ ISI:000345477500018,
Author = {Husain, S. Z. and Floryan, J. M.},
Title = {{Efficient over-determined implementation of the immersed boundary
   conditions method}},
Journal = {{COMPUTERS \& FLUIDS}},
Year = {{2014}},
Volume = {{105}},
Pages = {{194-203}},
Month = {{DEC 10}},
Abstract = {{An efficient implementation of the spectrally-accurate immersed boundary
   conditions method based on the over-determined formulation is presented.
   The linear system resulting from the discretization is solved using a
   special solver that takes advantage of the structure of the coefficient
   matrix. Part of the system corresponding to the field equation is solved
   exactly while the part corresponding to the boundary relations is solved
   in the least squares sense. This process permits use of a larger number
   of constraints to enforce the boundary conditions along the complex flow
   boundaries. A significant expansion of the range of geometries that can
   be handled accurately using the new implementation has been documented.
   Furthermore, a significant acceleration of computations as well as a
   significant reduction of memory requirements have been demonstrated. (C)
   2014 Elsevier Ltd. All rights reserved.}},
