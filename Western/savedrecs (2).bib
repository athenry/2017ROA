@article{ ISI:000348146600001,
Author = {Geng, Dongsheng and Ding, Ning and Hor, T. S. Andy and Liu, Zhaolin and
   Sun, Xueliang and Zong, Yun},
Title = {{Potential of metal-free ``graphene alloy{''} as electrocatalysts for
   oxygen reduction reaction}},
