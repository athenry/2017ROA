@article{ ISI:000343345900020,
Author = {Mara, T. G. and Terry, B. K. and Ho, T. C. E. and Isyumov, N.},
Title = {{Aerodynamic and peak response interference factors for an upstream
   square building of identical height}},
Journal = {{JOURNAL OF WIND ENGINEERING AND INDUSTRIAL AERODYNAMICS}},
Year = {{2014}},
Volume = {{133}},
Number = {{SI}},
Pages = {{200-210}},
Month = {{OCT}},
Note = {{12th Americas Conference on Wind Engineering (ACWE), Seattle, WA, JUN
   16-20, 2013}},
Organization = {{Amer Assoc Wind Engn}},
Abstract = {{The wind loading of tall buildings in an urban environment is often
   greatly affected by aerodynamic interference resulting from the
   interaction of approaching wind with upstream and adjacent structures.
   Interference effects are not currently considered in wind loading codes,
   primarily due to the complexity of involved variables. Advice on
   building configurations which have the potential to generate problematic
   interference effects would be useful for structural engineers and urban
   planners. Wind tunnel tests were carried out to assess the interference
   effects associated with a single upstream building having identical
   height and geometry to the building of interest. The data are presented
   as interference factors (IFs) relating the loading and response under
   interference to that of an isolated building, and the underlying
   aerodynamic mechanisms are discussed. Contour plots are presented and
   discussed for IFs corresponding to: (i) the aerodynamic wind loading
   and; (ii) the peak response. The interference factors associated with
   the peak responses are useful for the identification of potential
   interference issues in the preliminary design or siting phase of a
   project. The results indicate that, depending on combinations of
   exposure and dynamic properties, the peak response IF may approach the
   same order as the partial wind load factor in some codes. (C) 2014
   Elsevier Ltd. All rights reserved.}},
