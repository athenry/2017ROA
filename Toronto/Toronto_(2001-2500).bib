@article{ ISI:000351476200006,
Author = {Dziedzic, Rebecca and Margerm, Katelyn and Evenson, Jeff and Karney,
   Bryan W.},
Title = {{Building an Integrated Water-Land Use Database for Defining Benchmarks,
   Conservation Targets, and User Clusters}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2015}},
Volume = {{141}},
Number = {{4}},
Month = {{APR}},
Abstract = {{Water utilities have large amounts of data at their disposal, which are
   seldom being used to their full potential. Integrating water billing
   records with land-use and demographic data organizes information and
   makes inherent correlations easier to understand, facilitating
   communication to stakeholders. This data was integrated for three
   Ontario (Canada) municipalities, Barrie, Guelph, and London. A summary
   tool was created, with proposed metrics and charts, that facilitates
   comparisons between cities, definition of benchmarks, and identification
   of targets for conservation. More than 60\% of consumption in these
   cities is residential, and mostly lies below the Ontario average of 267
   L/cap . day. Water user clusters were created through self-organizing
   maps, K-means, and hierarchical clustering, and selected according to
   their pseudo-F and Rand statistics. Users within the same or similar
   property codes were found to cluster together. The application of
   data-mining methods provides actionable information for utilities
   seeking to reduce demands and increase system sustainability. (C) 2014
   American Society of Civil Engineers.}},
