@inproceedings{ ISI:000380547100028,
Author = {Sohrabi, Foad and Yu, Wei},
Book-Group-Author = {{IEEE}},
Title = {{Hybrid Beamforming with Finite-Resolution Phase Shifters for Large-Scale
   MIMO Systems}},
Booktitle = {{2015 IEEE 16th International Workshop on Signal Processing Advances in
   Wireless Communications (SPAWC)}},
Series = {{IEEE International Workshop on Signal Processing Advances in Wireless
   Communications}},
Year = {{2015}},
