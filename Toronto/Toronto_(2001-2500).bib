@article{ ISI:000354193000039,
Author = {Wu, Tongfei and Farnood, Ramin},
Title = {{A preparation method of cellulose fiber networks reinforced by
   glutaraldehyde-treated chitosan}},
Journal = {{CELLULOSE}},
Year = {{2015}},
Volume = {{22}},
Number = {{3}},
Pages = {{1955-1961}},
Month = {{JUN}},
Abstract = {{An eco-friendly polymeric additive was developed to increase the dry and
   wet strengths of cellulose fiber networks. Biopolymer chitosan was
   firstly treated with glutaraldehyde (GA) and then mixed with cellulose
   fibers in pulp suspension. GA-treated chitosan was deposited onto
   cellulose fibers by adjusting pH. Increases of 140 and 930 \% were
   achieved for dry and wet tensile strength, respectively, at 2 wt\%
   dosage which were much higher than that for the chitosan-treated samples
   (50 and 180 \%, respectively). The surface morphology of cellulose
   fibers deposited with GA-treated chitosan was examined by scanning
   electron microscopy. The effects of GA-chitosan treatment on tear
   resistance and color properties were also investigated.}},
