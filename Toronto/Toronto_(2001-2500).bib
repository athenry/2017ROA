@article{ ISI:000356631300032,
Author = {Tisato, Nicola and Quintal, Beatriz and Chapman, Samuel and
   Podladchikov, Yury and Burg, Jean-Pierre},
Title = {{Bubbles attenuate elastic waves at seismic frequencies: First
   experimental evidence}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2015}},
Volume = {{42}},
Number = {{10}},
Pages = {{3880-3887}},
Month = {{MAY 28}},
Abstract = {{The migration of gases from deep to shallow reservoirs can cause
   damageable events. For instance, some gases can pollute the biosphere or
   trigger explosions and eruptions. Seismic tomography may be employed to
   map the accumulation of subsurface bubble-bearing fluids to help
   mitigating such hazards. Nevertheless, how gas bubbles modify seismic
   waves is still unclear. We show that saturated rocks strongly attenuate
   seismic waves when gas bubbles occupy part of the pore space. Laboratory
   measurements of elastic wave attenuation at frequencies <100Hz are
   modeled with a dynamic gas dissolution theory demonstrating that the
   observed frequency-dependent attenuation is caused by
   wave-induced-gas-exsolution-dissolution (WIGED). This result is
   incorporated into a numerical model simulating the propagation of
   seismic waves in a subsurface domain containing CO2-gas bubbles. This
   simulation shows that WIGED can significantly modify the wavefield and
   illustrates how accounting for this physical mechanism can potentially
   improve the monitoring and surveying of gas bubble-bearing fluids in the
   subsurface.}},
