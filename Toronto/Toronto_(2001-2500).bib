@inproceedings{ ISI:000380547100038,
Author = {Chen, Meng-Hsi and Liang, Ben and Dong, Min},
Book-Group-Author = {{IEEE}},
Title = {{A Semidefinite Relaxation Approach to Mobile Cloud Offloading with
   Computing Access Point}},
Booktitle = {{2015 IEEE 16th International Workshop on Signal Processing Advances in
   Wireless Communications (SPAWC)}},
Series = {{IEEE International Workshop on Signal Processing Advances in Wireless
   Communications}},
Year = {{2015}},
Pages = {{186-190}},
