@inproceedings{ ISI:000381813000374,
Author = {Li, A. and Gretzinger, D.},
Editor = {{Jaffray, DA}},
Title = {{Applying an Evidence-based Approach to Managing Alarm Safety: A
   University Health Network Case Study}},
Booktitle = {{WORLD CONGRESS ON MEDICAL PHYSICS AND BIOMEDICAL ENGINEERING, 2015, VOLS
   1 AND 2}},
Series = {{IFMBE Proceedings}},
Year = {{2015}},
Volume = {{51}},
Pages = {{1538-1541}},
Note = {{World Congress on Medical Physics and Biomedical Engineering, Toronto,
   CANADA, JUN 07-12, 2015}},
Abstract = {{High reliance on medical technologies in modern medicine has resulted in
   excessive number of alarms. This translates into an increased risk of
   alarm fatigue amongst caregivers, lowered quality of care and
   interrupted patient recovery. Technologies have been developed to
   address alarm safety, but there are inherent limitations to every
   technology. It was, therefore, the aim of this case study to develop an
   evidence-based approach to alarm safety management that can be applied
   across clinical units. Two intensive care units, a Level 3 (Medical
   Surgical Neurological Intensive Care Unit (MSNICU)) and a Level 2, at
   the Toronto Western Hospital were investigated. A recommended alarms
   management program by the ECRI Institute was modified to include only
   steps that were applicable to the investigational units and their
   associated workplace cultures. Observations in the clinical units,
   nursing alarm surveys for MSNICU staff and review of incident reports
   were used to determine the areas of greatest vulnerability and establish
   priorities. Recurring themes in the textual data were indexed and
   refined. Thematic analysis revealed that alarm safety concerns centered
   around three themes of related constructs (alarm noise, alarm
   desensitization and alarm workflow). However, the relative contribution
   of each theme to alarm concerns varied on each unit. Standardized
   procedures and increased educational activities could reduce unnecessary
   noise from frequent alarms. Cultural changes are anticipated to provide
   an effective first step to alarm safety management, however, future
   studies will require analysis of alarm data (number per bed, type and
   duration) to identify high-priority areas per clinical unit in a safe,
   effective manner.}},
