@article{ ISI:000358624400005,
Author = {Ghaffari, Shahriar and Khalid, Saad and Butler, Mark and Naguib, Hani E.},
Title = {{Development of High Thermally Conductive and Electrically Insulative
   Polylactic Acid (PLA) and Hexagonal Boron Nitride (hBN) Composites for
   Electronic Packaging Applications}},
Journal = {{JOURNAL OF BIOBASED MATERIALS AND BIOENERGY}},
Year = {{2015}},
Volume = {{9}},
Number = {{2}},
Pages = {{145-154}},
Month = {{APR}},
Note = {{13th International Symposium on Bioplastics Biocomposites and
   Biorefining: Moving Towards a Sustainable Bioeconomy, Guelph, CANADA,
   MAY 19-24, 2014}},
Abstract = {{Bio-based/green polymers and multifunctional bio-based polymeric
   composites warrant a multidisciplinary approach to promote the
   development of the next generation of materials, products, and processes
   that are environmentally sustainable. The scientific challenge is to
   produce bio-based composites with suitable properties as replacements
   for current oil-based materials in different applications to create the
   demand for mass production of bio-based/green polymers. This would raise
   sustainable development of these eco-friendly materials. This research
   aims to develop a new family of multifunctional bio-based polymer
   composites with improved thermal conductivity to be used for heat
   management in the electronics industry. A series of parametric studies
   were conducted to clarify the science behind the composites' behavior
   and their structure-to-property relationships. Using bio-based polymers
   (e.g., polylactic acid (PLA)) as the matrix, heat transfer networks were
   developed and structured by embedding hexagonal boron nitride (hBN) in a
   PLA matrix. The effects of random uniform micron size thermal networks
   of hBN on improving the effective thermal conductivity (k(eff)) of
   produced composites were studied and compared. The composites were then
   characterized with respect to physical, thermal, electrical, and
   mechanical properties for practical usage in the electronics industry.
   The use of high thermally conductive filler systems, with optimized
   material formulation, was found to promote the composites' effective
   thermal conductivity more than 11-fold over neat PLA. The thermally
   conductive composite is expected to afford unique opportunities to
   injection mold three-dimensional, net-shaped, lightweight, and
   eco-friendly microelectronic enclosures with superior heat dissipation
   performance.}},
