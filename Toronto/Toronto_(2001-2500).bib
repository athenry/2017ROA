@inproceedings{ ISI:000380904702038,
Author = {Tan, Louis and Mahdaviani, Kaveh and Khisti, Ashish and Soljanin, Emina},
Book-Group-Author = {{IEEE}},
Title = {{Coding for Source-Broadcasting over Erasure Channels with Feedback}},
Booktitle = {{2015 IEEE INTERNATIONAL SYMPOSIUM ON INFORMATION THEORY (ISIT)}},
Series = {{IEEE International Symposium on Information Theory}},
Year = {{2015}},
Pages = {{2136-2140}},
