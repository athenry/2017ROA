Title = {{Cooperative Spectrum Sensing with Per-User Power Constraints}},
Booktitle = {{2012 IEEE 23RD INTERNATIONAL SYMPOSIUM ON PERSONAL INDOOR AND MOBILE
   RADIO COMMUNICATIONS (PIMRC)}},
Series = {{IEEE International Symposium on Personal Indoor and Mobile Radio
   Communications Workshops-PIMRC}},
