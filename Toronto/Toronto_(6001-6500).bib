@article{ ISI:000300390800004,
Author = {Derrible, Sybil},
Title = {{Reviewing and assessing the Toronto metro system}},
Journal = {{CANADIAN JOURNAL OF CIVIL ENGINEERING}},
Year = {{2012}},
Volume = {{39}},
Number = {{2}},
Pages = {{154-162}},
Month = {{FEB}},
Abstract = {{From a small 12-station system in 1954 to a full network of four lines
   and 69 stations today, the Toronto metro has become an integral part of
   the city's transportation system. From the opening of the second line in
   1967, ridership and vehicle kilometres operated have increased at
   similar rates at around 4.3\%. After a decrease in the 1990s, ridership
   now increases by 2.59\% annually with more than 200 million passengers
   (i.e., fares collected) in 2008. Transit mode share, however, has nearly
   stagnated at around 22\%, notably due to the significant population
   growth. Today, the system is mainly used for the home to work and home
   to school trips (69\%), which is clearly exhibited from a conventional
   bimodal passenger distribution. Finally, it seems to have performed
   relatively well to date compared to its North American peers.
   Nevertheless, it will likely need to be improved and expanded in the
   near future to accommodate the substantial population growth forecast.}},
