Author = {Dadvar, Shima and Chandra, Sanjeev and Ashgriz, Nasser and Drappel,
   Stephan},
Book-Group-Author = {{ASME}},
Title = {{ADHESION OF WAX DROPLETS TO POROUS SUBSTRATES}},
