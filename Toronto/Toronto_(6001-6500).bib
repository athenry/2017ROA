Author = {Mahanta, Mohammad Shahin and Aghaei, Amirhossein S. and Plataniotis,
   Konstantinos N.},
Book-Group-Author = {{IEEE}},
Title = {{A Bayes Optimal Matrix-variate LDA for Extraction of Spatio-Spectral
   Features from EEG Signals}},
Booktitle = {{2012 ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN MEDICINE
   AND BIOLOGY SOCIETY (EMBC)}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
Year = {{2012}},
