@inproceedings{ ISI:000324221300098,
Author = {McColl, Derek and Nejat, Goldie},
Book-Group-Author = {{ASME}},
Title = {{A SOCIALLY ASSISTIVE ROBOT THAT CAN INTERPRET BODY LANGUAGE}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
