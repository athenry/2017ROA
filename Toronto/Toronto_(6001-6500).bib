@inproceedings{ ISI:000320625800110,
Author = {Eslamian, Morteza and Ahmed, Mahmoud},
Book-Group-Author = {{ASME}},
Title = {{Modeling of Particle Formation via Emulsion Combustion Spray Method}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL MECHANICAL ENGINEERING CONGRESS
   AND EXPOSITION - 2010, VOL 5, PTS A AND B}},
