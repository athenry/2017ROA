@inproceedings{ ISI:000315988000017,
Author = {Abdelfattah, Mohamed S. and Betz, Vaughn},
Book-Group-Author = {{IEEE}},
Title = {{Design Tradeoffs for Hard and Soft FPGA-based Networks-on-Chip}},
Booktitle = {{2012 INTERNATIONAL CONFERENCE ON FIELD-PROGRAMMABLE TECHNOLOGY (FPT'12)}},
Year = {{2012}},
Pages = {{95-103}},
Note = {{11th International Conference on Field-Programmable Technology (FPT),
   Seoul Natl Univ, Seoul, SOUTH KOREA, DEC 10-12, 2012}},
Organization = {{IEEE; IEEE Circuits \& Syst Soc (CAS); IEEE Electron Devices Soc (EDS);
   Altera; Cypress Semicond; Samsung Elect; Xilinx; Hanback Elect; Huins;
   Libertron}},
Abstract = {{Incorporating Networks-on-Chip (NoC) within FPGAs has the potential not
   only to improve the efficiency of the interconnect, but also to increase
   designer productivity and reduce compile time by raising the abstraction
   level of communication. By comparing NoC components on FPGAs and ASICs
   we quantify the efficiency gap between the two platforms and use the
   results to understand the design tradeoffs in that space. The crossbar
   has the largest FPGA vs. ASIC gaps: 85x area and 4.4x delay, while the
   input buffers have the smallest: 17x area and 2.9x delay. For a soft NoC
   router, these results indicate that wide datapaths, deep buffers and a
   small number of ports and virtual channels (VC) are favorable for FPGA
   implementation. If one hardens a complete state-of-the-art VC router it
   is on average 30x more area efficient and can achieve 3.6x the maximum
   frequency of a soft implementation. We show that this hard router can be
   integrated with the soft FPGA interconnect, and still achieve an area
   improvement of 22x. A 64-node NoC of hard routers with soft interconnect
   utilizes area equivalent to 1.6\% of the logic modules in the latest
   FPGAs, compared to 33\% for a soft NoC.}},
