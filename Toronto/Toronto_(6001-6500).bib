@article{ ISI:000302503800018,
Author = {Sarkas, Ioannis and Hasch, Juergen and Balteanu, Andreea and Voinigescu,
   Sorin P.},
Title = {{A Fundamental Frequency 120-GHz SiGe BiCMOS Distance Sensor With
   Integrated Antenna}},
Journal = {{IEEE TRANSACTIONS ON MICROWAVE THEORY AND TECHNIQUES}},
