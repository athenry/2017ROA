@inproceedings{ ISI:000307676800033,
Author = {Deluce, J. and Lee, S. -C. and Vecchio, F. J.},
Editor = {{ParraMontesinos, GJ and Reinhardt, HW and Naaman, AE}},
Title = {{Crack Formation in FRC Structural Elements Containing Conventional
   Reinforcement}},
Booktitle = {{HIGH PERFORMANCE FIBER REINFORCED CEMENT COMPOSITES 6}},
Series = {{RILEM Bookseries}},
