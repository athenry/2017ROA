@article{ ISI:000300340000006,
Author = {Kamino, Brett A. and Mills, Bridget and Reali, Christopher and Gretton,
   Michael J. and Brook, Michael A. and Bender, Timothy P.},
Title = {{Liquid Triarylamines: The Scope and Limitations of Piers-Rubinsztajn
   Conditions for Obtaining Triarylamine-Siloxane Hybrid Materials}},
Journal = {{JOURNAL OF ORGANIC CHEMISTRY}},
Year = {{2012}},
Volume = {{77}},
Number = {{4}},
Pages = {{1663-1674}},
Month = {{FEB 17}},
Abstract = {{New liquid triarylamine-siloxane hybrid materials are produced using the
   Piers-Rubinsztajn reaction. Under mild conditions, liquid analogues of
   conventional and commonly crystalline triarylamines are easily
   synthesized from readily available or accessible intermediates. Using a
   diverse selection of triarylamines, we explored the effects of siloxane
   group and substitution pattern on the physical properties of these
   materials, and we have demonstrated that relatively large molecular
   liquids with desirable electrochemical properties can be produced. The
   interactions between the strongly Lewis acidic catalyst used for this
   transformation, tris(pentafluorophenyl)borane (BCF), and the Lewis basic
   triarylamine substrates were studied. Through UV-vis-NIR and F-19 NMR
   spectroscopy, we have proposed that the catalyst undergoes a reversible
   redox reaction with the substrates to produce a charge transfer complex.
   The formation of this charge transfer complex is sensitive to the
   oxidation potential of the triarylamine and can greatly affect the
   kinetics of the Piers-Rubinsztajn reaction.}},
