@inproceedings{ ISI:000309117700178,
Author = {Kobravi, K. and Iravani, R. and Kojori, H. A.},
Book-Group-Author = {{IEEE}},
Title = {{A New High Switching-Frequency MOSFET-Based Current-Source-Converter
   with Bidirectional Power-Flow Capability}},
Booktitle = {{2012 TWENTY-SEVENTH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC)}},
Series = {{Annual IEEE Applied Power Electronics Conference and Exposition (APEC)}},
Year = {{2012}},
Pages = {{1179-1186}},
Note = {{27th Annual IEEE Applied Power Electronics Conference and Exposition
   (APEC), Orlando, FL, FEB 05-09, 2012}},
Organization = {{IEEE; Power Sources Manufacturers Assoc; IEEE Ind Applicat Soc (IAS);
   IEEE Power Elect Soc (PELS)}},
Abstract = {{The objective is to develop a compact Current-Source-Converter (CSC)
   with bidirectional power flow capability (four-quadrant CSC). To
   minimize the size of the CSC, the size of the input-filter/output-filter
   of the CSC should be reduced. Therefore, the CSC should be enabled of
   operation at high switching-frequencies, e. g., above 40kHz. A
   four-quadrant CSC operating at high-switching frequency requires: (i) a
   new ac-switch and (ii) a new commutation-strategy. To achieve these
   requirements the IGBT-diode-based CSC is not a suitable option, because:
   (i) it cannot provide bidirectional power flow and (ii) it cannot
   provide a fast turn-on and turn-off switching process; therefore, the
   switching-frequency is limited. This paper proposes a new CSC, operating
   based on MOSFET switches to provide: (i) the capability of operation at
   above 40kHz switching-frequency and (ii) providing a bidirectional power
   flow capability.}},
