@article{ ISI:000299789700005,
Author = {Zhang, Hong and Andrews, Susan A.},
Title = {{Effects of phosphate-based corrosion inhibitors on the kinetics of
   chlorine degradation and haloacetic acid formation in contact with three
   metal materials}},
Journal = {{CANADIAN JOURNAL OF CIVIL ENGINEERING}},
Year = {{2012}},
Volume = {{39}},
Number = {{1}},
Pages = {{44-54}},
Month = {{JAN}},
Abstract = {{Maintaining a secondary disinfectant in the distribution system has
   caused undesirable disinfection by-product (DBP) formation, such as
   haloacetic acids (HAA(9)), and the occurrence of corrosion. This study
   investigated free chlorine degradation and HAA(9) formation as a result
   of the interactions between metals (ductile iron, lead, and copper) and
   corrosion inhibitors (orthophosphate and polyphosphate) by using
   material-specific formation potential (MS-FP) tests at bench scale.
   Interactions of phosphate-based corrosion inhibitors with metal coupons
   had no significant impact on chlorine decay for iron and lead coupons,
   but reduced the rate of free chlorine decay by 50\% for copper coupons.
   Water incubated with iron coupons had approximately 50\% less HAA
   formation compared with bulk water. Elevated HAA formation in the
   presence of copper coupons indicated that copper could catalyze HAA
   formation. Dichloroacetic acid (DCAA) was the dominant species when
   water was in contact with metal coupons, whereas comparable DCAA and
   trichloroacetic acid (TCAA) formed in the bulk water. The results of
   this study will help utilities understand the effects of interactions
   between corrosion inhibitors and metals on chlorine decay and HAA
   formation in their distribution systems, in particular when new pipes
   are installed or as water enters consumers' buildings.}},
