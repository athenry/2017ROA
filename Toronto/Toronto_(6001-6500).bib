Title = {{Detailed Numerical Simulation of Single-Walled Carbon Nanotube Synthesis
   in a Radio-Frequency Induction Thermal Plasma System}},
Booktitle = {{12TH HIGH-TECH PLASMA PROCESSES CONFERENCE (HTPP-12)}},
Series = {{Journal of Physics Conference Series}},
