@article{ ISI:000377671300001,
Author = {Wilhelm, Stefan and Tavares, Anthony J. and Dai, Qin and Ohta, Seiichi
   and Audet, Julie and Dvorak, Harold F. and Chan, Warren C. W.},
Title = {{Analysis of nanoparticle delivery to tumours}},
