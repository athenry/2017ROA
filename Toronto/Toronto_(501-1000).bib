@article{ ISI:000376550800016,
Author = {Zhang, Fangrui and Wu, Chengqing and Zhao, Xiao-Ling and Xiang, Hengbo
   and Li, Zhong-Xian and Fang, Qin and Liu, Zhongxian and Zhang, Yadong
   and Heidarpour, Amin and Packer, Jeffrey A.},
Title = {{Experimental study of CFDST columns infilled with UHPC under close-range
   blast loading}},
Journal = {{INTERNATIONAL JOURNAL OF IMPACT ENGINEERING}},
Year = {{2016}},
Volume = {{93}},
Pages = {{184-195}},
Month = {{JUL}},
Abstract = {{Concrete-filled double-skin tubes (CFDST) have been increasingly popular
   in the field of engineering in recent years. A lot of research has been
   carried out to investigate the behaviour of CFDST members under a
   variety of loading conditions. This paper presents an experimental
   investigation on ultra-high performance concrete infilled double-skin
   tube columns subjected to close-in blast loading. Two types of CFDST
   columns were investigated in the experiments - one with both inner and
   outer tubes circular and the other one with both square. The main test
   parameters included the explosive charge weight and the magnitude of
   axial load. After the blast tests, there was no visible buckling nor
   ruptures found on the steel tubes and only minor cracks, of no more than
   1mm width, were observed in the core concrete when the outer steel tube
   was removed. Based on the findings of the experiments, it is evident
   that CFDST column has excellent blast resistance. This feature has the
   potential to be used in high-value structures which may be the targets
   of terrorist attacks, such as embassies, government buildings and
   critical infrastructures. (C) 2016 Elsevier Ltd. All rights reserved.}},
