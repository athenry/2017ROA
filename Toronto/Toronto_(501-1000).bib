@article{ ISI:000378747700011,
Author = {Wang, Wei and Mandelis, Andreas},
Title = {{SNR and Contrast Enhancement Techniques for the Photoacoustic Radar
   Imaging}},
Journal = {{INTERNATIONAL JOURNAL OF THERMOPHYSICS}},
Year = {{2016}},
Volume = {{37}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{This paper presents two methods for photoacoustic signal enhancement in
   biological tissues. One such method is based on the fact that
   temperature can affect the signals of the photoacoustic radar.
   Therefore, thermally assisted methods have been used for photoacoustic
   imaging contrast improvement. Another method is based on harmonic
   wavelength modulation which results in a differential PA radar signal to
   strengthen early cancer detection. Two chirped waveforms modulated
   out-of-phase between 680 nm and 800 nm can effectively suppress the
   background noise, greatly enhance the SNR and detect small variations in
   hemoglobin oxygenation levels, thereby distinguishing pre-malignant
   tumors. Experimental results demonstrate the accuracy of the
   frequency-modulated differential measurement with sheep blood at
   different hemoglobin oxygenation (StO(2)) levels.}},
