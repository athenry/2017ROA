@article{ ISI:000377924800013,
Author = {Hajighasemi, Mahbod and Nocek, Boguslaw P. and Tchigvintsev, Anatoli and
   Brown, Greg and Flick, Robert and Xu, Xiaohui and Cui, Hong and Hai,
   Tran and Joachimiak, Andrzej and Golyshin, Peter N. and Savchenko,
   Alexei and Edwards, Elizabeth A. and Yakunin, Alexander F.},
Title = {{Biochemical and Structural Insights into Enzymatic Depolymerization of
   Polylactic Acid and Other Polyesters by Microbial Carboxylesterases}},
Journal = {{BIOMACROMOLECULES}},
Year = {{2016}},
Volume = {{17}},
Number = {{6}},
Pages = {{2027-2039}},
Month = {{JUN}},
Abstract = {{Polylactic acid (PLA) is a biodegradable polyester derived from
   renewable resources, which is a leading candidate for the replacement of
   traditional petroleum-based polymers. Since the global production of PLA
   is quickly growing, there is..an urgent need for the development of
   efficient recycling technologies, which will produce lactic acid instead
   of CO2 as the final product. After screening 90 purified microbial
   alpha/beta-hydrolases, we, identified `hydrolytic activity against
   emulsified PLA in two uncharacterized proteins, ABO2449 from Alcanivorax
   borkumnsis and RPA1511 from Rhodopseudomonas palustris. Both enzymes
   were also active against emulsified polycaprolactone and other
   polyesters as well as against soluble alpha-naphthyl and p-nitrophenyl
   monoesters. In addition, both ABO2449 and RPA1511 catalyzed complete or
   extensive hydrolysis of solid PLA with the production of lactic acid
   monomers, dimers, and larger oligomers as products. The crystal
   structure of RPA1511 was determined at 2.2 A resolution and revealed a
   classical alpha/beta-hydrolase fold with a wide-operractive site
   containing a molecule of polyethylene glycol bound near the catalytic
