@article{ ISI:000374582100001,
Author = {Stogios, Peter J. and Cox, Georgina and Spanogiannopoulos, Peter and
   Pillon, Monica C. and Waglechner, Nicholas and Skarina, Tatiana and
   Koteva, Kalinka and Guarne, Alba and Savchenko, Alexei and Wright,
   Gerard D.},
Title = {{Rifampin phosphotransferase is an unusual antibiotic resistance kinase}},
Journal = {{NATURE COMMUNICATIONS}},
Year = {{2016}},
Volume = {{7}},
Pages = {{1-12}},
Month = {{APR}},
Abstract = {{Rifampin (RIF) phosphotransferase (RPH) confers antibiotic resistance by
   conversion of RIF and ATP, to inactive phospho/RIF, AMP and Pi. Here we
   present the crystal structure of RPH from Listeria monocytogenes
   (RPH-Lm), which reveals that the enzyme is comprised of three domains:
   two substrate-binding domains (ATP-grasp and RIF-binding domains); and a
   smaller phosphate-carrying His swivel domain. Using solution small-angle
   X-ray scattering and mutagenesis, we reveal a mechanism where the swivel
   domain transits between the spatially distinct substrate-binding sites
