@article{ ISI:000378922800025,
Author = {Mosanenzadeh, Shahriar Ghaffari and Khalid, Saad and Cui, Yi and Naguib,
   Hani E.},
Title = {{High thermally conductive PLA based composites with tailored hybrid
   network of hexagonal boron nitride and graphene nanoplatelets}},
