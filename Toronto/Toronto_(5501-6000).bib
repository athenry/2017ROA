@article{ ISI:000309413600001,
Author = {Suzuki, Hitoshi and MacDonald, Jacqueline and Syed, Khajamohiddin and
   Salamov, Asaf and Hori, Chiaki and Aerts, Andrea and Henrissat, Bernard
   and Wiebenga, Ad and vanKuyk, Patricia A. and Barry, Kerrie and
