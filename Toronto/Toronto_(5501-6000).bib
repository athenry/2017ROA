@article{ ISI:000312239100012,
Author = {Ketabi, Sanaz and Lian, Keryn},
Title = {{Thermal and structural characterizations of PEO-EMIHSO4 polymer
   electrolytes}},
Journal = {{SOLID STATE IONICS}},
Year = {{2012}},
Volume = {{227}},
Pages = {{86-90}},
Month = {{OCT 29}},
Abstract = {{Solid polymer electrolytes containing poly(ethylene oxide) (PEO) and
   1-ethyl-3-methylimidazolium hydrogensulfate (EMIHSO4), an ionic liquid,
   were investigated. Thermal and structural properties of this polymer
   electrolyte were characterized using DSC, XRD, and FTIR. After addition
   of EMIHSO4, the melting point of PEO decreased by ca. 25 degrees C.
   Thermal and structural analyses indicated that the crystallinity of PEO
   substantially decreased with increasing EMIHSO4 content. The interaction
   between ionic liquid and polymer suggested further dissociation of the
   ionic liquid. Thus. EMIHSO4 acted as ionic conductor and plasticizer.
