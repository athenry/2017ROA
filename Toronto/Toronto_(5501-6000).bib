@article{ ISI:000310151000020,
Author = {Hinebaugh, J. and Challa, P. R. and Bazylak, A.},
Title = {{Accounting for low-frequency synchrotron X-ray beam position
   fluctuations for dynamic visualizations}},
Journal = {{JOURNAL OF SYNCHROTRON RADIATION}},
Year = {{2012}},
Volume = {{19}},
Number = {{6}},
Pages = {{994-1000}},
Month = {{NOV}},
Note = {{International Workshop on Improving Data Qualityand Quantity for XAFS
   Experiments, Tsukuba, JAPAN, APR 12-13, 2011}},
Abstract = {{Synchrotron X-ray radiography on beamline 05B1-1 at the Canadian Light
   Source Inc. was employed to study dynamic liquid water transport in the
   porous electrode materials of polymer electrolyte membrane fuel cells.
   Dynamic liquid water distributions were quantified for each radiograph
   in a sequence, and non-physical liquid water measurements were obtained.
   It was determined that the position of the beam oscillated vertically
   with an amplitude of similar to 25 mu m at the sample and a frequency of
   similar to 50 mHz. In addition, the mean beam position moved linearly in
   the vertical direction at a rate of 0.74 mu m s(-1). No evidence of
   horizontal oscillations was detected. In this work a technique is
   presented to account for the temporal and spatial dependence of
   synchrotron beam intensity, which resulted in a significant reduction in
   false water thickness. This work provides valuable insight into the
   treatment of radiographic time-series for capturing dynamic processes
   from synchrotron radiation.}},
