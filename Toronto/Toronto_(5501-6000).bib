@article{ ISI:000304312000001,
Author = {Galle, Michael A. and Saini, Simarjeet S. and Mohammed, Waleed S. and
   Qian, Li},
Title = {{Chromatic dispersion measurements using a virtually referenced
   interferometer}},
Journal = {{OPTICS LETTERS}},
Year = {{2012}},
Volume = {{37}},
Number = {{10}},
Pages = {{1598-1600}},
Month = {{MAY 15}},
Abstract = {{We present a technique for measuring the chromatic dispersion of
   short-length (<1 m) optical devices using unbalanced spectral
   interferometry and a virtual reference path. The technique combines the
   speed and ease of measurement of unbalanced spectral interferometry with
   the accuracy of balanced spectral interferometry. We demonstrate
   measurement accuracy for group delay and the dispersion-length product
   of similar to 10(-3) ps/m (<0.0001\% relative error) and similar to
   10(-5) ps/m (<0.5\% relative error), respectively. Measurement precision
   is demonstrated to be similar to 10(-5) ps/m (<0.15\% relative
   deviation). We validate the technique via measurement of well-known
   dispersion standards. (C) 2012 Optical Society of America}},
