@article{ ISI:000304453900004,
Author = {Fernandez-Gonzalez, Rodrigo and Zallen, Jennifer A.},
Title = {{Feeling the Squeeze: Live-Cell Extrusion Limits Cell Density in
   Epithelia}},
Journal = {{CELL}},
Year = {{2012}},
Volume = {{149}},
Number = {{5}},
Pages = {{965-967}},
Month = {{MAY 25}},
Abstract = {{Tissues develop in confined volumes that can impose mechanical
   constraints on their growth, but it is unclear how cells respond to
   these limits to regulate tissue size and shape. Two papers show that
   overcrowding and cell deformation lead to the shedding of live cells to
