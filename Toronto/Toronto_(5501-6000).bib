@article{ ISI:000308992000007,
Author = {Karout, Johnny and Kramer, Gerhard and Kschischang, Frank R. and Agrell,
   Erik},
Title = {{A Two-Dimensional Signal Space for Intensity-Modulated Channels}},
Journal = {{IEEE COMMUNICATIONS LETTERS}},
Year = {{2012}},
Volume = {{16}},
Number = {{9}},
Pages = {{1361-1364}},
Month = {{SEP}},
Abstract = {{A two-dimensional signal space for intensity-modulated channels is
   presented. Modulation formats using this signal space are designed to
   minimize average and peak power for a fixed minimum distance between
   signal points. The uncoded, high-signal-to-noise ratio, power and
   spectral efficiencies are compared to those of the best known formats.
   The new formats are simpler than existing subcarrier formats, and are
   superior if the bandwidth is measured as 90\% in-band power. Existing
   subcarrier formats are better if the bandwidth is measured as 99\%
   in-band power.}},
