@article{ ISI:000303472800014,
Author = {Golozar, Mehdi and Chien, Ken and Coyle, Thomas W.},
Title = {{Orthorhombic alpha-MoO3 Coatings with Lath-Shaped Morphology Developed
   by SPPS: Applications to Super-Capacitors}},
