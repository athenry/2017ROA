@article{ ISI:000359509700011,
Author = {Zhang, Jia-Ning and Zhao, Yan and Liu, Chao and Han, Elizabeth S. and
   Yu, Xue and Lidington, Darcy and Bolz, Steffen-Sebastian and You, Lidan},
Title = {{The role of the sphingosine-1-phosphate signaling pathway in osteocyte
   mechanotransduction}},
Journal = {{BONE}},
Year = {{2015}},
Volume = {{79}},
Pages = {{71-78}},
Month = {{OCT}},
Abstract = {{Osteocytes are proposed to be the mechanosensory cells that translate
   mechanical loading into biochemical signals during the process of bone
   adaptation. The lipid mediator sphingosine-l-phosphate (SIP) has been
   reported to play a role in the mechanotransduction process of blood
   vessels and also in the dynamic control of bone mineral homeostasis.
   Nevertheless, the potential role of SIP in bone mechanotransduction has
   yet to be elucidated. In this study, we hypothesized that a SIP cascade
   is involved in the activation of osteocytes in response to
   loading-induced oscillatory fluid flow (OFF) in bone. MLO-Y4
   osteocyte-like cells express the necessary components of a functional
   SIP cascade. To examine the involvement of SIP signaling in osteocyte
   mechanotransduction, we applied OFF (1 Pa, 1 Hz) to osteocyte-like
   MLO-Y4 cells under conditions where the SIP signaling pathway was
   modulated. We found that decreased endogenous SIP levels significantly
   suppressed the OFF-induced intracellular calcium response. Addition of
   extracellular SIP to MLO-Y4 cells enhanced the synthesis and release of
   prostaglandin E2 (PGE(2)) under static cells and amplified OFF-induced
   PGE(2) release. The stimulatory effect of OFF on the gene expression
   levels of osteoprotegerin (OPG) and receptor activator for nuclear
   factor KB ligand (RANKL) was SIP dependent. Furthermore, the S1P(2)
   receptor subtype was shown to be involved in OFF-induced PGE2 synthesis
   and release, as well as down-regulation of RANKL/OPG gene expression
   ratio. In summary, our data suggest that SIP cascade is involved in
   OFF-induced mechanotransduction in MLO-Y4 cells and that extracellular
   SIP exerts its effect partly through S1P(2) receptors. (C) 2015 Elsevier
   Inc. All rights reserved.}},
