@article{ ISI:000364252700018,
Author = {Moldoveanu, Georgiana A. and Papangelalds, Vladimiros G.},
Title = {{Strategies for calcium sulphate scale control in hydrometallurgical
   processes at 80 degrees C}},
Journal = {{HYDROMETALLURGY}},
Year = {{2015}},
Volume = {{157}},
Pages = {{133-139}},
Month = {{OCT}},
Abstract = {{In order to control calcium sulphate scale formation in
   hydrometallurgical processes, strategies for lowering the concentration
   of soluble calcium in simulated solutions at 80 degrees C (conditions in
   the Pressure Acid Leach thickener tank) below the solubility limit of
   calcium sulphate at 140 degrees C (conditions in the medium pressure
   slurry pre-heater) were investigated. Reagents known to form insoluble
   products with calcium such as: Na2CO3, H2C2O4, Na2C2O4 and Na3PO4, were
   investigated and the kinetics and selectivity of the precipitation
   process were evaluated as well. Both Na2CO3 and Na2C2O4 successfully and
   selectively lowered aqueous calcium concentration below the solubility
   limit at 80 degrees C at 1:1 stoichiometric ratios with respect to
   calcium, while excess of H2C2O4 was required in order to achieve
   advanced calcium removal; Na2CO3 in a 2:1 ratio reached even lower
   values, below the solubility limit at 140 degrees C. Na3PO4 in a 1:1
   ratio proved highly inefficient, removing only 67\% of the total
   calcium. (C) 2015 Elsevier B.V. All rights reserved.}},
