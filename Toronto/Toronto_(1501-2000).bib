@article{ ISI:000357973200005,
Author = {Del Vecchio, Ciro and Kwon, Oh-Sung and Di Sarno, Luigi and Prota,
   Andrea},
Title = {{Accuracy of nonlinear static procedures for the seismic assessment of
   shear critical structures}},
Journal = {{EARTHQUAKE ENGINEERING \& STRUCTURAL DYNAMICS}},
Year = {{2015}},
Volume = {{44}},
Number = {{10}},
Pages = {{1581-1600}},
Month = {{AUG}},
Abstract = {{The nonlinear behavior of reinforced concrete (RC) members represents a
   key issue in the seismic performance assessment of structures. Many
   structures constructed in the 1980s or earlier were designed based on
   force limits; thus they often exhibit brittle failure modes, strength
   and stiffness degradation, and severe pinching effects. Field surveys
   and experimental evidence have demonstrated that such inelastic
   responses affect the global behavior of RC structural systems. Efforts
   have been made to consider the degrading stiffness and strength in the
   simplified nonlinear static procedures commonly adopted by
   practitioners. This paper investigates the accuracy of such procedures
   for the seismic performance assessment of RC structural systems. Refined
   finite element models of a shear critical bridge bent and a
   flexure-critical bridge pier are used as reference models. The numerical
   models are validated against experimental results and used to evaluate
   the inelastic dynamic response of the structures subjected to earthquake
   ground motions with increasing amplitude. The maximum response from the
   refined numerical models is compared against the results from the
   simplified static procedures, namely modified capacity spectrum method
   and coefficient method in FEMA-440. The accuracy of the static
   procedures in estimating the displacement demand of a flexure-critical
   system and shear-critical system is discussed in detail. Copyright (c)
   2015 John Wiley \& Sons, Ltd.}},
