@article{ ISI:000364585000016,
Author = {Nourani, Amir and Spelt, Jan K.},
Title = {{Combined effect of strain-rate and mode-ratio on the fracture of
   lead-free solder joints}},
Journal = {{MATERIALS \& DESIGN}},
Year = {{2015}},
Volume = {{85}},
Pages = {{115-126}},
Month = {{NOV 15}},
Abstract = {{The critical strain energy release rate for the solder joint fracture
   was measured as a function of the strain rate and the mode ratio of
   loading. These data are useful in predicting the fracture of solder
   joints loaded under arbitrary combinations of tension and shear during
   the impact conditions typical of falling portable electronic devices. In
   this study, strain rates from quasi-static (close to 0 s(-1)) to 61
   s(-1) were investigated at phase angles from 0 to 60 degrees, typical of
   the range found in microelectronic devices. Copper-solder-copper double
   cantilever beam (DCB) model specimens were prepared using SAC305 solder
   at cooling rates and times above liquidus typical of actual ball grid
   arrays (BGAs). A drop tester was designed and built to achieve different
   strain rates at various mode ratios. The critical initiation strain
   energy release rate, Jci, increased about 70\% from quasi-static to
   intermediate strain rates, before decreasing by more than 67\% from
