@article{ ISI:000357701900011,
Author = {Rizvi, Ali and Park, Chul B. and Favis, Basil D.},
Title = {{Tuning viscoelastic and crystallization properties of polypropylene
   containing in-situ generated high aspect ratio polyethylene
   terephthalate fibrils}},
Journal = {{POLYMER}},
Year = {{2015}},
Volume = {{68}},
Pages = {{83-91}},
Month = {{JUN 26}},
Abstract = {{This study explores how the rheological and crystallization properties
   of linear polypropylene (PP) are changed by incorporating fibrillated
   polyethylene terephthalate (PET). PET domains are fibrillated by melt
   blending with PP followed by drawing. In uniaxial extensional flows, the
   PP/fibrillated-PET shows strain hardening behavior, not observed for
   PP/PET blends with spherical PET domains or for the neat PP. At PET
   fibril contents >= 3 wt\%, a plateau appears in the storage modulus at
   low frequencies, indicating the formation of a percolated physical
   network of fibrils that responds elastically over long timescales.
   Avrami analysis of the crystallization of PP/fibrillated-PET at ambient
   and elevated CO2 pressures reveals that the PET fibrils enhance the
   kinetics of isothermal crystallization of PP under both pressure
   conditions. WAXS study shows that when PP/fibrillated-PET is
   isothermally crystallized at high pressures, gamma-phase crystal
   formation of PP is promoted which is representative of superior
   mechanical properties. (C) 2015 Elsevier Ltd. All rights reserved.}},
