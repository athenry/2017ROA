@article{ ISI:000357172100012,
Author = {Shakiba, Nika and White, Carl A. and Lipsitz, Yonatan Y. and
   Yachie-Kinoshita, Ayako and Tonge, Peter D. and Hussein, Samer M. I. and
   Puri, Mira C. and Elbaz, Judith and Morrissey-Scoot, James and Li, Mira
   and Munoz, Javier and Benevento, Marco and Rogers, Ian M. and Hanna,
   Jacob H. and Heck, Albert J. R. and Wollscheid, Bernd and Nagy, Andras
   and Zandstra, Peter W.},
Title = {{CD24 tracks divergent pluripotent states in mouse and human cells}},
Journal = {{NATURE COMMUNICATIONS}},
Year = {{2015}},
Volume = {{6}},
Month = {{JUN}},
Abstract = {{Reprogramming is a dynamic process that can result in multiple
   pluripotent cell types emerging from divergent paths. Cell surface
   protein expression is a particularly desirable tool to categorize
   reprogramming and pluripotency as it enables robust quantification and
   enrichment of live cells. Here we use cell surface proteomics to
   interrogate mouse cell reprogramming dynamics and discover CD24 as a
   marker that tracks the emergence of reprogramming-responsive cells,
   while enabling the analysis and enrichment of transgene-dependent
   (F-class) and -independent (traditional) induced pluripotent stem cells
   (iPSCs) at later stages. Furthermore, CD24 can be used to delineate
   epiblast stem cells (EpiSCs) from embryonic stem cells (ESCs) in mouse
   pluripotent culture. Importantly, regulated CD24 expression is conserved
   in human pluripotent stem cells (PSCs), tracking the conversion of human
   ESCs to more naive-like PSC states. Thus, CD24 is a conserved marker for
   tracking divergent states in both reprogramming and standard pluripotent
   culture.}},
