@article{ ISI:000360344200036,
Author = {Ma, Youchun and Mills, James K. and Xiong, Jijun and Zhang, Wendong and
   Tan, Qiuling},
Title = {{Simulation of a high-speed superimposed fiber Bragg gratings
   interrogation system}},
Journal = {{APPLIED OPTICS}},
Year = {{2015}},
Volume = {{54}},
Number = {{25}},
Pages = {{7734-7739}},
Month = {{SEP 1}},
Abstract = {{This paper presents a simulation of high-speed nonuniform random
   sampling in a superimposed fiber Bragg gratings (SFBGs) interrogation
   system. The simulated Gauss SFBGs are used to generate a nonuniform
   sensing pulse train during each scanning cycle. Six different conditions
   that can cause nonuniform sampling are simulated, and a random sine-wave
   driving method to improve the driving speed is proposed. An 11.8 kHz
   dynamic strain is measured by generating an additive nonuniform randomly
   distributed 12 kHz optical sensing pulse train from a mean 2 kHz
   sinusoidal periodically changing scanning frequency and three SFBGs.
   Four conditions that can improve the sampling results are also
   simulated. (C) 2015 Optical Society of America}},
