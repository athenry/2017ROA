@article{ ISI:000365323000019,
Author = {Zhang, Jin and Meguid, S. A.},
Title = {{Piezoelectric effect on the intrinsic dissipation in oscillating gallium
   nitride nanobelts}},
Journal = {{EPL}},
Year = {{2015}},
Volume = {{112}},
Number = {{2}},
Month = {{OCT}},
Abstract = {{The intrinsic dissipation in oscillating gallium nitride (GaN) nanobelts
   subject to an electric field has been examined using molecular-dynamics
   simulations. Owing to the piezoelectric effect of GaN nanobelts, their
   quality factor is greatly influenced by the electric field and can be
   improved by over 10 times across a range of electric-field strengths. In
   addition, the temperature dependence of the quality factor is also
   significantly affected by the applied electric field. Some possible
   explanations as to how the piezoelectric effect influences the intrinsic
   energy loss are proposed based on the classical thermoelastic
   dissipation theory. Copyright (C) EPLA, 2015}},
