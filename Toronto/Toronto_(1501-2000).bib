@article{ ISI:000356935700023,
Author = {Jalali, Mohammad Sadegh and Ting, Clifford and Liang, Joshua and
   Sheikholeslami, Ali and Kibune, Masaya and Tamura, Hirotaka},
Title = {{A 3 x Blind ADC-Based CDR for a 20 dB Loss Channel}},
Journal = {{IEEE TRANSACTIONS ON CIRCUITS AND SYSTEMS I-REGULAR PAPERS}},
Year = {{2015}},
Volume = {{62}},
Number = {{6}},
Pages = {{1658-1667}},
Month = {{JUN}},
Abstract = {{This paper proposes using a 3-bit ADC to blindly sample the received
   data from a channel with 20 dB loss at Nyquist at 3x the baud rate. By
   moving from 2x to 3x sampling, we reduce the required ADC resolution
   from 5-bit to 3-bit, thereby reducing the overall power consumption by a
   factor of 2. Measurements from our test chip fabricated in Fujitsu's 65
   nm CMOS show a high frequency jitter tolerance of 0.25 UIpp for a 5 Gb/s
   PRBS31 with a 60 `' FR4 channel.}},
