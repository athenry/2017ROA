@article{ ISI:000344178300003,
Author = {Najafi, Naqi and Heuzey, Marie-Claude and Carreau, Pierre J. and
   Therriault, Daniel and Park, Chul B.},
Title = {{Rheological and foaming behavior of linear and branched polylactides}},
Journal = {{RHEOLOGICA ACTA}},
Year = {{2014}},
Volume = {{53}},
Number = {{10-11}},
Pages = {{779-790}},
Month = {{NOV}},
Abstract = {{In this work, a chain extender (CE) was added to polylactide (PLA) to
   improve its foamability. The steady and transient rheological properties
   of neat PLA and CE-treated PLA revealed that the introduction of the CE
   profoundly affected the melt viscosity and elasticity. The linear
   viscoelastic properties of CE-enriched PLA suggested that a long-chain
   branching (LCB) structure was formed from the reaction with the CE.
   LCB-PLA exhibited an increased viscosity, more shear sensitivity, and
   longer relaxation time in comparison with the linear PLA. The LCB
   structure was also found to affect the transient shear stress growth and
   elongational flow behavior. LCB-PLA exhibited a pronounced strain
   hardening, whereas no strain hardening was observed for the linear PLA.
   Batch foaming of the linear and LCB-PLAs was also examined at foaming
   temperatures of 130, 140, and 155 A degrees C. The LCB structure
   significantly increased the integrity of the cells, cell density, and
   void fraction.}},
