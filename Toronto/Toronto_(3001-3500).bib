@article{ ISI:000346125300006,
Author = {Duffy, D. and Papangelakis, V. and Davis, B.},
Title = {{Lateritic saprolite leaching in concentrated magnesium chloride media}},
Journal = {{CANADIAN METALLURGICAL QUARTERLY}},
Year = {{2014}},
Volume = {{53}},
Number = {{4}},
Pages = {{432-439}},
Month = {{OCT}},
Abstract = {{Magnesium chloride brines present a number of potential advantages for
   the processing of lateritic saprolite ores for nickel production.
   Concentrated MgCl2 solutions enhance the activity of acid used, allow
   atmospheric leaching at elevated temperature, and inhibit magnesium
   dissolution, which reduces acid consumption and increases metal
   selectivity. However, with a chloride based leach it is economically
   requisite to recover hydrochloric acid, conventionally accomplished by
   pyrohydrolysis. This work was performed in conjunction with a novel
   flowsheet for the processing on saprolite ores, which recovers HCl by
   the precipitation and subsequent decomposition of magnesium
   hydroxychlorides, alleviating some of the issues with pyrohydrolysis.
   Leaching experiments have been conducted in concentrated MgCl2 brines,
   up to 4.5 m, to determine the amenable process conditions and explore
   the kinetics of the process. It was determined that >95\% extraction of
   metals was possible using both aqueous and gaseous HCl, while
   suppressing the dissolution of Mg from the ore.}},
