@article{ ISI:000345907100053,
Author = {Tisato, Nicola and Quintal, Beatriz},
Title = {{Laboratory measurements of seismic attenuation in sandstone: Strain
   versus fluid saturation effects}},
Journal = {{GEOPHYSICS}},
Year = {{2014}},
Volume = {{79}},
Number = {{5}},
Pages = {{WB9-WB14}},
Month = {{SEP-OCT}},
Abstract = {{In the past few decades, great attention has been focused on uncovering
   the physics of seismic wave attenuation in fluid-saturated rocks.
   However, the relationship among many variables affecting attenuation is
   still not completely clear. For instance, although the role of strain in
   enhancing friction dissipation is relatively well known for dry rocks,
   it remains unclear how and how much it affects attenuation in
   fluid-saturated rocks. We experimentally measured attenuation in the
   extensional mode in Berea sandstone at strains between 7.8 x 10(-7) and
   1.9 x 10(-5), and at frequencies in the seismic bandwidth (1-100 Hz).
   These strains were similar to those typically observed in seismic
   exploration (similar to 10(-6)). We also measured the transient fluid
   pressure caused when a stepwise stress was applied resulting in such
   strains. For the studied strain range, our results indicated that: (1)
   the overall attenuation in dry Berea sandstone increased linearly with
   strain, (2) the frequency-dependent component of attenuation, which was
   associated with fluid saturation, was approximately insensitive to
   strain, and (3) the overall attenuation can be considered as a sum of a
   frequency-independent and a frequency-dependent components.}},
