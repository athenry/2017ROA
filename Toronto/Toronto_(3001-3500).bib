Author = {Alsohaily, Ahmed and Sousa, Elvino S.},
Title = {{Dynamic Spectrum Management in Multi-Radio Access Technology (RAT)
   Cellular Systems}},
Journal = {{IEEE WIRELESS COMMUNICATIONS LETTERS}},
Year = {{2014}},
Volume = {{3}},
Number = {{3}},
Pages = {{249-252}},
Month = {{JUN}},
Abstract = {{Fixed Spectrum Allocation (FSA) results in suboptimal spectrum
   utilization, unbalanced system loading and the inability to adapt to
   system traffic variations when employed in multi-Radio Access Technology
