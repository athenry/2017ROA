@article{ ISI:000339385400001,
Author = {Kazuyo, Fukura and Hong, So Yeon and Yeon, Young Joo and Joo, Jeong Chan
   and Yoo, Young Je},
Title = {{Enhancing the activity of Bacillus circulans xylanase by modulating the
   flexibility of the hinge region}},
Journal = {{JOURNAL OF INDUSTRIAL MICROBIOLOGY \& BIOTECHNOLOGY}},
Year = {{2014}},
Volume = {{41}},
Number = {{8}},
Pages = {{1181-1190}},
Month = {{AUG}},
Abstract = {{Enzymes undergo multiple conformational changes in solution, and these
   dynamics are considered to play a critical role in enzyme activity.
   Hinge-bending motions, resulting from reciprocal movements of dynamical
   quasi-rigid bodies, are thought to be related to turnover rate and are
   affected by the physical properties of the hinge regions. In this study,
   hinge identification and flexibility modification of the regions by
   mutagenesis were conducted to explore the relationship between hinge
   flexibility and catalytic activity. Bacillus circulans xylanase was
   selected for the identification and mutation of the hinge regions. As a
   result, turnover rate (V (max)) was improved approximately twofold in
   mutants that have more rigid hinge structure, despite the decrease in K
   (m) and V (max)/K (m). This result indicates that the rigidly mutated
   hinge has positive effects on B. circulans xylanase activity.}},
