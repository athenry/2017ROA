@article{ ISI:000344588600075,
Author = {Zhang, Jin and Meguid, S. A.},
Title = {{Buckling of microtubules: An insight by molecular and continuum
   mechanics}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2014}},
Volume = {{105}},
Number = {{17}},
Month = {{OCT 27}},
Abstract = {{The molecular structural mechanics method has been extended to
   investigate the buckling of microtubules (MTs) with various
   configurations. The results indicate that for relative short MTs the
   shear deformation effect, rather than the nonlocal effect, is mainly
   responsible for the limitation of their widely used Euler beam
   description and the observed length-dependence of their bending
   stiffness. In addition, the configuration effect of MTs is also studied
   and considered as an explanation for the large scattering of the
   critical buckling force and bending stiffness observed in existing
   experiments. This configuration effect is also found to mainly originate
   from the geometry of the MTs and is mainly determined by the
   protofilament number. (C) 2014 AIP Publishing LLC.}},
