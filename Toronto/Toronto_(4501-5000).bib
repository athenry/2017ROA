@inproceedings{ ISI:000341702106245,
Author = {Guirgis, Mirna and Chinvarun, Yotin and Carlen, Peter L. and Bardakjian,
   Berj L.},
Book-Group-Author = {{IEEE}},
Title = {{The Role of Delta-Modulated High Frequency Oscillations in Seizure State
   Classification}},
Booktitle = {{2013 35TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
   MEDICINE AND BIOLOGY SOCIETY (EMBC)}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
