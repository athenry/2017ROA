@article{ ISI:000312991000021,
Author = {Foroughi, Hooman and Acosta, Edgar J. and Kawaji, Masahiro},
Title = {{Design of a micro glass cell apparatus for pure gas-nonvolatile liquid
   phase behaviour study}},
Journal = {{CANADIAN JOURNAL OF CHEMICAL ENGINEERING}},
Year = {{2013}},
Volume = {{91}},
Number = {{2}},
Pages = {{382-390}},
Month = {{FEB}},
Abstract = {{A micro syringe is used as a constant volume cell for gasliquid
   equilibrium (GLE) study. The cell is made of glass and has a volume of
   <100?mu L. It can operate at pressures up to 13?MPa and temperatures up
   to 115 degrees C. Two different experimental procedures are presented
   for systems with nonvolatile low and high viscosity liquids. A micro
   magnetic stir bar is used to mix the gasliquid mixtures inside the cell.
   Since the internal volume of the cell is small, a short mixing time is
   sufficient for the gasliquid mixtures to reach equilibrium. The
   solubility values are measured by using the pressure decay method. The
   experimental procedures are validated by measuring the carbon dioxide
   (CO2) solubility in water and highly viscous bitumen. The experimental
   results are in good agreement with the available literature data which
   shows that the technique works well. (c) 2011 Canadian Society for
   Chemical Engineering}},
