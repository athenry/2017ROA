@article{ ISI:000315325700006,
Author = {Wang, Lin and Fu, Qing-Liang and Lee, Chi-Guhn and Zeng, Yu-Rong},
Title = {{Model and algorithm of fuzzy joint replenishment problem under
   credibility measure on fuzzy goal}},
Journal = {{KNOWLEDGE-BASED SYSTEMS}},
Year = {{2013}},
Volume = {{39}},
Pages = {{57-66}},
Month = {{FEB}},
Abstract = {{The joint replenishment problem (JRP) has received considerable
   attention and all of the work on JRP is under explicit environment. In
   fact, the decision makers often have to face vague operational
   conditions. In this paper, a novel JRP model with fuzzy minor
   replenishment cost and fuzzy inventory holding cost is developed. More
   concisely, this model is a fuzzy dependent-chance programming (DCP)
   model. Subsequently, the technique of the traditional fuzzy simulation
   (FS) approach and differential evolution algorithm (DE) are integrated
   to design a hybrid intelligent algorithm named FSDE-I to solve this
   practical fuzzy JRP. Thirdly, another intelligent algorithm named
   FSDE-II using an improved FS approach is proposed to estimate the
   credibility more precisely. Finally, FSDE-I and FSDE-II are illustrated
   with numerical examples and the results show the effectiveness of
   FSDE-II. Crown Copyright (c) 2012 Published by Elsevier B.V. All rights
   reserved.}},
