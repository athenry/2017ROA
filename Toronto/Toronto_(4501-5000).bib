@article{ ISI:000320764300010,
Author = {Ma, Xiongfeng and Xu, Feihu and Xu, He and Tan, Xiaoqing and Qi, Bing
   and Lo, Hoi-Kwong},
Title = {{Postprocessing for quantum random-number generators: Entropy evaluation
   and randomness extraction}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2013}},
Volume = {{87}},
Number = {{6}},
Month = {{JUN 21}},
Abstract = {{Quantum random-number generators (QRNGs) can offer a means to generate
   information-theoretically provable random numbers, in principle. In
   practice, unfortunately, the quantum randomness is inevitably mixed with
   classical randomness due to classical noises. To distill this quantum
   randomness, one needs to quantify the randomness of the source and apply
   a randomness extractor. Here, we propose a generic framework for
   evaluating quantum randomness of real-life QRNGs by min-entropy, and
   apply it to two different existing quantum random-number systems in the
   literature. Moreover, we provide a guideline of QRNG data postprocessing
   for which we implement two information-theoretically provable randomness
   extractors: Toeplitz-hashing extractor and Trevisan's extractor.}},
