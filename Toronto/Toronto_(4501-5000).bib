@article{ ISI:000314738700048,
Author = {Ghaffari, H. O. and Sharifzadeh, M. and Young, R. Paul},
Title = {{Complex aperture networks}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2013}},
Volume = {{392}},
Number = {{4}},
Pages = {{1028-1037}},
Month = {{FEB 15}},
Abstract = {{A complex network approach is proposed for studying the shear behavior
   of a rough rock joint. Similarities between aperture profiles are
   established, and a functional complex network-in each shear
   displacement-is constructed in two directions: parallel and
   perpendicular to the shear direction. We find that the growth of the
   clustering coefficient and that of the number of edges are approximately
   scaled with the development of shear strength and hydraulic
   conductivity, which could possibly be utilized to estimate and formulate
   a friction law and the evolution of shear distribution over asperities.
   Moreover, the frictional interface is mapped in the global-local
   parameter space of the corresponding functional friction network,
   showing the evolution path and, eventually, the residual stage.
   Furthermore, we show that with respect to shear direction, parallel
   aperture patches are more adaptable to environmental stimuli than
   perpendicular profiles. We characterize the pure-contact profiles using
   the same approach. Unlike the first case, the later networks show a
   growing trend while in the residual stage; a saturation of links is
   encoded in contact networks. (C) 2012 Elsevier B.V. All rights reserved.}},
