@inproceedings{ ISI:000341702104104,
Author = {Zulueta-Coarasa, Teresa and Kurugol, Sila and Ross, James C. and Washko,
   George G. and Estepar, Raul San Jose},
Book-Group-Author = {{IEEE}},
Title = {{Emphysema Classification Based on Embedded Probabilistic PCA}},
