@article{ ISI:000317437800017,
Author = {David, Robert and Neumann, A. Wilhelm},
Title = {{Contact Angle Hysteresis on Randomly Rough Surfaces: A Computational
   Study}},
Journal = {{LANGMUIR}},
Year = {{2013}},
Volume = {{29}},
Number = {{14}},
Pages = {{4551-4558}},
Month = {{APR 9}},
Abstract = {{Wetting is important in many applications, and the solid surfaces being
   wet invariably feature some amount of surface roughness. A free
   energy-based computational simulation is used to study the effect of
   roughness on wetting and especially contact angle hysteresis. On
   randomly rough, self-affine surfaces, it is found that hysteresis
   depends primarily on the value of the Wenzel roughness parameter r,
   increasing in proportion with r - 1. Micrometer-level roughness causes
   hysteresis of a few degrees.}},
