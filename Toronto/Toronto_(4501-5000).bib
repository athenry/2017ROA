@inproceedings{ ISI:000342297200022,
Author = {Sorkhabi, A. A. and Mercan, O.},
Editor = {{Brebbia, CA and Hernandez, S}},
Title = {{The effects of measurement errors in the restoring force feedback during
   real-time hybrid simulations}},
Booktitle = {{EARTHQUAKE RESISTANT ENGINEERING STRUCTURES IX}},
Series = {{WIT Transactions on the Built Environment}},
Year = {{2013}},
Volume = {{132}},
Pages = {{271-282}},
