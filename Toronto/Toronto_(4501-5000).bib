@article{ ISI:000321233900036,
Author = {Stefanovic, Nem and Pavel, Lacra},
Title = {{Robust power control of multi-link single-sink optical networks with
   time-delays}},
Journal = {{AUTOMATICA}},
Year = {{2013}},
Volume = {{49}},
Number = {{7}},
Pages = {{2261-2266}},
Month = {{JUL}},
Note = {{48th IEEE Conference on Decision and Control (CDC)/28th Chinese Control
   Conference (CCC), Shanghai, PEOPLES R CHINA, DEC 16-18, 2009}},
Organization = {{IEEE}},
Abstract = {{Optical networks provide high data transmission rates which require
   optimal power control algorithms to ensure the proper delivery of the
   signals. Time-delays may destabilize the control algorithms. We study
   the stability of game-theoretic based primal-dual control algorithms
   applied to multi-link single-sink optical networks. We present a
   perturbed optical signal-to-noise ratio (OSNR) model with time-delays.
   By optimizing the OSNR values, we reduce the bit error rates of the
   channels. We modify singular perturbation theory to include
   Lyapunov-Krasovskii stability theory with multiple time-delays and
   uncertainties. Simulations show the stabilization of perturbed systems
   at the expense of transient convergence time. (C) 2013 Elsevier Ltd. All
   rights reserved.}},
