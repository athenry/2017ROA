@article{ ISI:000320163600001,
Author = {Cai, X. -D. and Weedbrook, C. and Su, Z. -E. and Chen, M. -C. and Gu,
   Mile and Zhu, M. -J. and Li, Li and Liu, Nai-Le and Lu, Chao-Yang and
   Pan, Jian-Wei},
Title = {{Experimental Quantum Computing to Solve Systems of Linear Equations}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2013}},
Volume = {{110}},
Number = {{23}},
Month = {{JUN 6}},
Abstract = {{Solving linear systems of equations is ubiquitous in all areas of
   science and engineering. With rapidly growing data sets, such a task can
   be intractable for classical computers, as the best known classical
   algorithms require a time proportional to the number of variables N. A
   recently proposed quantum algorithm shows that quantum computers could
   solve linear systems in a time scale of order log(N), giving an
   exponential speedup over classical computers. Here we realize the
   simplest instance of this algorithm, solving 2 x 2 linear equations for
   various input vectors on a quantum computer. We use four quantum bits
   and four controlled logic gates to implement every subroutine required,
   demonstrating the working principle of this algorithm.}},
