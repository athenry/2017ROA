@article{ ISI:000319016400010,
Author = {Li, Shuai and Wang, Zheng and Li, Yangming},
Title = {{Using Laplacian Eigenmap as Heuristic Information to Solve Nonlinear
   Constraints Defined on a Graph and Its Application in Distributed
   Range-Free Localization of Wireless Sensor Networks}},
