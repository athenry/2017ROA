@article{ ISI:000317441400014,
Author = {Sheridan, Patricia Kristine and Gluck, Erich and Guan, Qi and Pickles,
   Thomas and Balcioglu, Baris and Benhabib, Beno},
Title = {{The dynamic nearest neighbor policy for the multi-vehicle pick-up and
   delivery problem}},
Journal = {{TRANSPORTATION RESEARCH PART A-POLICY AND PRACTICE}},
Year = {{2013}},
Volume = {{49}},
Pages = {{178-194}},
Month = {{MAR}},
Abstract = {{In this paper, a dynamic nearest neighbor (DNN) policy is proposed for
   operating a fleet of vehicles to serve customers, who place calls in a
   Euclidean service area according to a Poisson process. Each vehicle
   serves one customer at a time, who has a distinct origin and destination
   independently and uniformly distributed within the service area. The new
   DNN policy is a refined version of the nearest neighbor (NN) policy that
   is well known to perform sub-optimally when the frequency of customer
   requests is high. The DNN policy maintains geographically closest
   customer-to-vehicle assignments, due to its ability to divert/re-assign
   vehicles that may be already en-route to pick up other customers, when
   another vehicle becomes available or a new customer call arrives. Two
   other pertinent issues addressed include: the pro-active deployment of
   the vehicles by anticipating in which regions of the service area future
   calls are more likely to arise; and, imposition of limits to avoid
   prohibitively long customer wait times. The paper also presents accurate
   approximations for all the policies compared. Extensive simulations,
   some of which are included herein, clearly show the DNN policy to be
   tangibly superior to the first-come-first-served (FCFS) and NN policies.
   Crown Copyright (C) 2013 Published by Elsevier Ltd. All rights reserved.}},
