@article{ ISI:000320387200003,
Author = {Sinclair, Sarina K. and Bell, Spencer and Epperson, Richard Tyler and
   Bloebaum, Roy D.},
Title = {{The Significance of Calcified Fibrocartilage on the Cortical Endplate of
   the Translational Sheep Spine Model}},
Journal = {{ANATOMICAL RECORD-ADVANCES IN INTEGRATIVE ANATOMY AND EVOLUTIONARY
   BIOLOGY}},
Year = {{2013}},
Volume = {{296}},
Number = {{5}},
Pages = {{736-744}},
Month = {{MAY}},
Abstract = {{To gain an understanding of the vertebral cortical endplate and factors
   that may affect the ability to achieve skeletal attachment to
   intervertebral implants and fusion, this study aimed to characterize the
   hypermineralized tissue on the cortical endplate of the vertebral body
   on a commonly used animal model. Skeletally mature sheep were injected
   with tetracycline prior to euthanasia and the C2-C3, T5-T6, and L2-L3
   spinal motion segments were excised and prepared. Vertebral tissues were
   imaged using backscatter electron (BSE) imaging, histology, and
   tetracycline labeling was used to assess bone remodeling within
   different tissue layers. It was determined that the hypermineralized
   tissue layer was calcified fibrocartilage (CFC). No tetracycline labels
   were identified in the CFC layer, in contrast to single and double
   labels that were present in the underlying bone, indicating the CFC
   present on the cortical endplate was not being actively remodeled. The
   average thickness of the CFC layer was 146.3 +/- 70.53 mu m in the
   cervical region, 98.2 +/- 40.29 mu m in the thoracic region, and 150.89
   +/- 69.25 mu m in the lumbar region. This difference in thickness may be
   attributed to the regional biomechanical properties of the spine.
   Results from this investigation indicate the presence of a nonremodeling
   tissue on the cortical endplate of the vertebral body in sheep spines,
   which attaches the intervertebral disc to the vertebrae. This tissue, if
   not removed, would likely prevent successful bony attachment to an
   intervertebral device in spinal fusion studies and total disc
   replacement surgeries. (C) 2013 Wiley Periodicals, Inc.}},
