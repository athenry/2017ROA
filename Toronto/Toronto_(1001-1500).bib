Author = {Kim, Justin Y. and Colaco, Tyler and Kashino, Zendai and Nejat, Goldie
   and Benhabib, Beno},
Book-Group-Author = {{IEEE}},
Title = {{mROBerTO: A Modular Millirobot for Swarm-Behavior Studies}},
Booktitle = {{2016 IEEE/RSJ INTERNATIONAL CONFERENCE ON INTELLIGENT ROBOTS AND SYSTEMS
   (IROS 2016)}},
