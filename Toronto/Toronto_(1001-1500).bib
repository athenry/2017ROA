@inproceedings{ ISI:000387117700169,
Author = {Preibisch, Jan B. and Triverio, Piero and Schuster, Christian},
Book-Group-Author = {{IEEE}},
Title = {{Design Space Exploration for Printed Circuit Board Vias Using Polynomial
   Chaos Expansion}},
