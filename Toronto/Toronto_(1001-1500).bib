@inproceedings{ ISI:000378437000051,
Author = {Lashkari, Bahman and Choi, Sung Soo Sean and Dovlo, Edem and Mandelis,
   Andreas},
Editor = {{Oraevsky, AA and Wang, LV}},
Title = {{Photoacoustic Cross-Correlation High-Frame-Rate and Phase Spectroscopy:
   Two New Biomedical Imaging Modalities}},
