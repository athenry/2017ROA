@inproceedings{ ISI:000390094701247,
Author = {Salam, M. Tariqus and Kassiri, Hossein and Soltani, Nima and He, Haoyu
   and Velazquez, Jose Luis Perez and Genov, Roman},
Book-Group-Author = {{IEEE}},
Title = {{Tradeoffs Between Wireless Communication and Computation in Closed-loop
   Implantable Devices}},
Booktitle = {{2016 IEEE INTERNATIONAL SYMPOSIUM ON CIRCUITS AND SYSTEMS (ISCAS)}},
Series = {{IEEE International Symposium on Circuits and Systems}},
