@inproceedings{ ISI:000388377101021,
Author = {Zhang, Xingqi and Sarris, Costas D.},
Book-Group-Author = {{IEEE}},
Title = {{A Gaussian Beam Approach for Embedding Antennas into Vector Parabolic
   Equation Based Propagation Models}},
Booktitle = {{2016 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
