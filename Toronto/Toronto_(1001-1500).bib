@inproceedings{ ISI:000380588800016,
Author = {Sun, Yu-Chen and Shi, HaoTian H. and Naguib, Hani E.},
Editor = {{Goulbourne, NC}},
Title = {{Fabrication and characterization of beta-poly(vinylidene
   fluoride)/silane-treated titanium dioxide dielectric nano-composites}},
Booktitle = {{BEHAVIOR AND MECHANICS OF MULTIFUNCTIONAL MATERIALS AND COMPOSITES 2016}},
