@inproceedings{ ISI:000400784902015,
Author = {Cameron, Trevor R. and Eleftheriades, George V.},
Book-Group-Author = {{IEEE}},
Title = {{Design Considerations for Slotted Substrate Integrated Waveguide
   Leaky-Wave Antennas}},
Booktitle = {{2016 10TH EUROPEAN CONFERENCE ON ANTENNAS AND PROPAGATION (EUCAP)}},
Series = {{Proceedings of the European Conference on Antennas and Propagation}},
Year = {{2016}},
Note = {{10th European Conference on Antennas and Propagation (EuCAP), Davos,
   SWITZERLAND, APR 10-15, 2016}},
Abstract = {{Substrate integrated waveguides (SIWs) are a popular choice for
   implementing compact slotted waveguide leakywave antennas (LWAs).
   However, certain design parameters that may not seem important can have
   a significant impact on the radiation performance of such structures.
   The junction between the end of the SIW and the microstrip line
   typically used to terminate such structures can result in spurious
   radiation if the extent of the substrate on which the antenna is printed
   is not judiciously chosen. Here, it is shown that the extent of the
   substrate beyond the via walls, as well as the length of the microstrip
   line used to terminate SIWs, can have a non-trivial impact on the gain
   and scanning performance of SIW LWAs. In many cases, this spurious
   radiation can easily be missed since it does not occur in the principle
   cuts of the radiation pattern. An increased extension of substrate as
   well as a shorter terminating microstrip line effectively eliminates the
   spurious beam.}},
