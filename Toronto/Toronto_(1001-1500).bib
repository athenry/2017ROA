@inproceedings{ ISI:000382143300067,
Author = {Zefreh, Mohammad Sheikh and Tizghadam, Ali and Leon-Garcia, Alberto and
   Elbiaze, Halima and Miron, Walter},
Book-Group-Author = {{IEEE}},
Title = {{CAPEX Optimization in Multi-Chassis Multi-Rate IP/Optical Networks}},
Booktitle = {{2016 INTERNATIONAL CONFERENCE ON COMPUTING, NETWORKING AND
   COMMUNICATIONS (ICNC)}},
Year = {{2016}},
Note = {{International Conference on Computing, Networking and Communications
   (ICNC), Kauai, HI, FEB 15-18, 2016}},
Organization = {{IEEE; IEEE Comp Soc}},
Abstract = {{The ever-increasing Internet traffic has challenged the business model
   of many Internet Service Providers (ISPs) in their core network as the
   revenue cannot not grow up as expected anymore. Therefore, ISPs should
   redesign their network to minimize their cost per bit using a careful
   provisioning and traffic routing in the network. In this paper, we
   consider multi-chassis routers with multi-rate line cards in IP/optical
   networks and develop a Mixed Linear Integer Program (MILP) to minimize
   the CAPital EXpenditure (CAPEX) cost. Placeholder for network topology,
   demand traffic matrix and network elements prices are the inputs to the
   problem, and the optimum network design and traffic routing with a
   detailed list of network elements are its outputs. Moreover, a heuristic
   provisioning algorithm is developed which reduces the complexity of the
   problem by separating routing from provisioning. Simulation results
   verify the performance of the proposed heuristic algorithm.(1)}},
