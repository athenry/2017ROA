@inproceedings{ ISI:000391310800001,
Author = {Kashino, Zendai and Vilela, Julio and Kim, Justin Y. and Nejat, Goldie
   and Benhabib, Beno},
Editor = {{Melo, K}},
Title = {{An Adaptive Static-Sensor Network Deployment Strategy for Detecting
   Mobile Targets}},
Booktitle = {{2016 IEEE INTERNATIONAL SYMPOSIUM ON SAFETY, SECURITY, AND RESCUE
   ROBOTICS (SSRR)}},
Series = {{IEEE International Symposium on Safety Security and Rescue Robots}},
