@inproceedings{ ISI:000389775900052,
Author = {Mirian, Vincent and Chow, Paul},
Book-Group-Author = {{IEEE}},
Title = {{Extracting Designs of Secure IPs Using FPGA CAD Tools}},
Booktitle = {{2016 INTERNATIONAL GREAT LAKES SYMPOSIUM ON VLSI (GLSVLSI)}},
