@article{ ISI:000366756500042,
Author = {Sun, X. and Alam, M. Z. and Aitchison, J. S. and Mojahedi, M.},
Title = {{Compact and broadband polarization beam splitter based on a silicon
   nitride augmented low-index guiding structure}},
Journal = {{OPTICS LETTERS}},
Year = {{2016}},
Volume = {{41}},
Number = {{1}},
Pages = {{163-166}},
Month = {{JAN 1}},
Abstract = {{We propose a compact polarization beam splitter (PBS) based on a silicon
   nitride enhanced silicon-on-insulator platform using an augmented
   low-index guiding (ALIG) waveguide structure. In the ALIG structure, the
   TM mode is mostly confined in the low-index silicon nitride, while the
   TE mode is confined in the high-index silicon. Since the two modes are
   confined in two separate layers, their properties can be controlled
   independently. The PBS is formed using an asymmetric multimode
   interference (MMI) section. The TM mode is directed to an output port by
   the ALIG waveguide, while the TE mode is coupled to the other output
   port via the multimode interferometer. Such a PBS has a very small
   footprint, low insertion loss, high polarization extinction ratio, and
   broadband response. (C) 2015 Optical Society of America}},
