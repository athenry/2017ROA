@inproceedings{ ISI:000390779200068,
Author = {Ezeme, Mellitus and Hammad, Eman and Kundur, Deepa},
Book-Group-Author = {{IEEE}},
Title = {{Control Verification via Off-Line Co-Simulation}},
Booktitle = {{2016 IEEE CANADIAN CONFERENCE ON ELECTRICAL AND COMPUTER ENGINEERING
   (CCECE)}},
Series = {{Canadian Conference on Electrical and Computer Engineering}},
Year = {{2016}},
Note = {{IEEE Canadian Conference on Electrical and Computer Engineering (CCECE),
   Vancouver, CANADA, MAY 14-18, 2016}},
Organization = {{IEEE; IEEE Vancouver Sect}},
Abstract = {{Power systems are evolving to accommodate more renewable sources of
   energy, and sophisticated control systems. A characteristic of the
   evolution is the integration of a communication network overlay that
   controls the bilateral flow of energy and information. We develop an
   off-line smart grid co-simulator test-bed from off-the-shelf
   communication, power and control simulators. The test-bed is targeted as
   a tool to help researchers verify various communication enabled control
   schemes designed for distribution system. The benefits of the test-bed
   are illustrated on a low voltage microgrid system. A test case of an
   energy management system is evaluated using the off-line co-simulation
