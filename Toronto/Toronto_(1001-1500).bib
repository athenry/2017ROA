@inproceedings{ ISI:000380588800011,
Author = {Kshad, Mohamed Ali Emhmed and Naguib, Hani E.},
Editor = {{Goulbourne, NC}},
Title = {{Characterization of Origami Shape Memory Metamaterials (SMMM) Made of
   Bio-Polymer Blends}},
Booktitle = {{BEHAVIOR AND MECHANICS OF MULTIFUNCTIONAL MATERIALS AND COMPOSITES 2016}},
Series = {{Proceedings of SPIE}},
