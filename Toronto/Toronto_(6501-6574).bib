@incollection{ ISI:000362273900018,
Author = {Riahi, Azadeh and Curran, John H. and Hammah, Reginald E.},
Editor = {{Sousa, LRE and Vargas, E and Fernandes, MD and Azevedo, R}},
Title = {{Application of continuum-interface computational methods in analysis of
   jointed rock problems}},
Booktitle = {{INNOVATIVE NUMERICAL MODELLING IN GEOMECHANICS}},
Year = {{2012}},
Pages = {{317-332}},
Abstract = {{This chapter focuses on continuum-based numerical techniques (such as
   finite element or finite difference methods) that use interface elements
   to represents problems with discrete fractures. Such problems are common
   to rock masses with pre-existing fracture networks.
   The governing equations, kinematics, and fundamental solutions of both
   continuum and discrete methods are discussed. The similarities and
   differences between the formulations of combined continuum-interface
   techniques and those of discrete-based approaches are outlined. It is
   shown that mathematically, for the case of an edge-to-edge contact,
   penalty enforcement of contact constraints in discrete element methods
   is identical to the finite element method with interface elements.
   Finally, guidelines for choosing between continuum and discontinuous
   techniques (namely, the distinct element method and discontinuous
   deformation analysis) are provided.}},
