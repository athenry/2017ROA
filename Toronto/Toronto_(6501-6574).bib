@inproceedings{ ISI:000398126700018,
Author = {Huang, Jun and Wang, Zhirui and Xue, Kemin},
Editor = {{Zhou, L and Chang, H and Lu, Y and Xu, D}},
Title = {{Cyclic Deformation Response of Ti-5Al-5V-5Mo-3Cr (Ti-5553) Alloy under
   the Solutionized-Aged Condition}},
Booktitle = {{TI-2011: PROCEEDINGS OF THE 12TH WORLD CONFERENCE ON TITANIUM, VOL II}},
Year = {{2012}},
Pages = {{914-918}},
Note = {{12th World Conference on Titanium (Ti-2011), Nonferrous Met Soc China,
   Beijing, PEOPLES R CHINA, JUN 19-24, 2011}},
Organization = {{NW Inst Nonferrous Met Res; NW Polytechn Univ; Baoti Grp Ltd; Beijing
   Inst Aeronaut Mat; Chinese Acad Sci, Inst Met Res}},
Abstract = {{The cyclic deformation behavior of two phase metastable beta
   Ti-5A1-5V-5Mo-3Cr alloy was systematically investigated under the
   pull-push low cycle fatigue (LCF) tests with a total strain control
   testing mode at room temperature. The beta-alpha bimodal microstructure
   demonstrated the micromechanical heterogeneity upon strain cycling,
   leading to special macroscopic behavior, i.e. early cyclic hardening at
   low strain amplitude; cyclic softening when the strain amplitude was
   high; and initial hardening followed by softening in the intermediate
   strain range. The role of primary alpha(p) particles is found to be
   particularly important in accommodating the cyclic plastic strain. The
   participation of the transformed beta phase including the lenticular
   alpha(s) precipitates embedded in the beta matrix was detected at high
   strain amplitude. The activation of slip activities in a, mainly gave
   rise to initially moderate hardening response. Dislocation annihilation
   upon cycling both in the alpha(p)/beta and alpha(s)/beta interfaces on
   the side of alpha(p) and alpha(s) particles was found to be the main
   reason for the softening behavior.}},
Publisher = {{SCIENCE PRESS BEIJING}},
Address = {{16 DONGHUANGCHENGGEN NORTH ST, BEIJING 100707, PEOPLES R CHINA}},
Type = {{Proceedings Paper}},
Language = {{English}},
