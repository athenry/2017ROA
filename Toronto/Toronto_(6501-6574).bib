@inproceedings{ ISI:000398126700019,
Author = {Huang, Jun and Wang, Zhirui and Zhou, Jie},
Editor = {{Zhou, L and Chang, H and Lu, Y and Xu, D}},
Title = {{Strain Localization and Planar-slip Behavior in Compressive Cyclic
   Deformation of the Beta -annealed Ti-5Al-5V-5Mo-3Cr Alloy}},
Booktitle = {{TI-2011: PROCEEDINGS OF THE 12TH WORLD CONFERENCE ON TITANIUM, VOL II}},
Year = {{2012}},
Pages = {{919-923}},
Note = {{12th World Conference on Titanium (Ti-2011), Nonferrous Met Soc China,
   Beijing, PEOPLES R CHINA, JUN 19-24, 2011}},
Organization = {{NW Inst Nonferrous Met Res; NW Polytechn Univ; Baoti Grp Ltd; Beijing
   Inst Aeronaut Mat; Chinese Acad Sci, Inst Met Res}},
Abstract = {{In the present study, the cyclic deformation behavior of the
   beta-annealed metastable Ti-5A1-5V-5Mo-3Cr alloy in pure compressive
   fatigue tests is thoroughly investigated. The material illustrates the
   initial cyclic softening followed by saturation irrespective of the
   employed stress levels. The progressive observation of
   surface-microstructure evolution reveals typical planar slip behavior
   and early formation of strain localization-induced microcracks. Upon
   cycling, the annihilation, i.e. reduced dislocation density and
   simplified dislocation configurations, as well as the detwinning and
   changed twin boundary structures have been well illustrated by the
   transmission electron microscopy (TEM) investigation. These activities
   in combination with the omega-penetration process are considered to
   introduce the initial softening behaviour; and the dislocation flip-flop
   is attributed to the following cyclic saturation response. In
   consideration of the stacking fault energy (SFE), the
   free-electron-to-atom (e/a) ratio, and the as received microstructure,
   the strain localized planar slip behavior is discussed. The shearing
   process of nano-scaled omega precipitates is related to the strain
   localization phenomenon as well.}},
