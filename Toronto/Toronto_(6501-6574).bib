@inproceedings{ ISI:000320654000179,
Author = {Taylor, Joshua A. and Mathieu, Johanna L. and Callaway, Duncan S. and
   Poolla, Kameshwar},
Book-Group-Author = {{IEEE}},
Title = {{Price and capacity competition in zero-mean storage and demand response
   markets}},
Booktitle = {{2012 50TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2012}},
Pages = {{1316-1323}},
