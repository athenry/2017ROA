@article{ ISI:000313603400007,
Author = {Gauthier, Cindy and Gagnon, Dany and Grangeon, Murielle and Jacquemin,
   Geraldine and Nadeau, Sylvie and Masani, Kei and Popovic, Milos R.},
Title = {{COMPARISON OF MULTIDIRECTIONAL SEATED POSTURAL STABILITY BETWEEN
   INDIVIDUALS WITH SPINAL CORD INJURY AND ABLE-BODIED INDIVIDUALS}},
Journal = {{JOURNAL OF REHABILITATION MEDICINE}},
Year = {{2013}},
Volume = {{45}},
Number = {{1}},
Pages = {{47-54}},
Month = {{JAN}},
Abstract = {{Objectives: To compare multidirectional seated postural stability
   between individuals with spinal cord injury and able-bodied individuals
   and to evaluate the impact of abdominal and low back muscle paralysis on
   multidirectional seated stability.
   Design: Case-control study.
   Participants: Fifteen individuals with complete or incomplete spinal
   cord injury affecting various vertebral levels participated in this
   study and were gender-matched with 15 able-bodied individuals.
   Methods: Participants were instructed to lean as far as possible in 8
   directions, set apart by 45 intervals, while seated on an instrumented
   chair with their feet placed on force plates. Eight direction-specific
   stability indices and a global stability index were calculated.
   Results: The global stability index and all direction-specific indices,
   except in the anterior and posterior directions, were lower in
   individuals with spinal cord injury than in able-bodied individuals.
   However, the individuals with spinal cord injury who had partial or full
   control of their abdominal and lower trunk muscles obtained a similar
   global stability index and similar direction-specific indices compared
   to the able-bodied individuals.
   Conclusion: Multidirectional seated postural stability is reduced in
   individuals with SCI who have paralysis of the abdominal and lower back
   muscles in comparison to able-bodied individuals.}},
