@inproceedings{ ISI:000346481201017,
Author = {Soliman, Hazem M. and Nasr, Omar A. and Khairy, Mohamed M.},
Book-Group-Author = {{IEEE}},
Title = {{Analysis and Optimization of Backhaul Sharing in CoMP}},
Booktitle = {{2013 IEEE 24TH INTERNATIONAL SYMPOSIUM ON PERSONAL, INDOOR, AND MOBILE
   RADIO COMMUNICATIONS (PIMRC)}},
Series = {{IEEE International Symposium on Personal Indoor and Mobile Radio
   Communications Workshops-PIMRC}},
Year = {{2013}},
Pages = {{1013-1018}},
Note = {{IEEE 24th International Symposium on Personal, Indoor and Mobile Radio
   Communications (PIMRC), London, ENGLAND, SEP 08-11, 2013}},
Organization = {{IEEE}},
Abstract = {{The great potential of multi-cell processing in Coordinated Multi-point
   (CoMP) transmission is hindered by the limited bandwidth of the backhaul
   network. In this paper, we tackle this problem from two perspectives.
   First, we provide a thorough analytical study of the expected capacity
   when employing zero forcing (ZF) beamforming based on quantized channel
   state information (CSI). We derive simple yet accurate expression for
   the average capacity per user when Zero-forcing precoders are employed.
   Then, we propose a low-complexity algorithm to share the backhaul
   bandwidth between the different users in a fair manner. The algorithm
   does water-filling for the backhaul bandwidth among users based on their
   signal-to-interference ratio (SIR), with a target of Equal SIR for all
   users. We analytically prove that the proposed algorithm is more fair
   than the standard techniques for backhaul sharing. Simulation results
   are provided to asses the analytical findings about the superiority of
   our algorithm in terms of both fairness and sum rate.}},
