@inproceedings{ ISI:000326335203045,
Author = {Sun, Sun and Dong, Min and Liang, Ben},
Book-Group-Author = {{IEEE}},
Title = {{Real-Time Welfare-Maximizing Regulation Allocation in Aggregator-EVs
   Systems}},
Booktitle = {{2013 PROCEEDINGS IEEE INFOCOM}},
