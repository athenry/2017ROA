@article{ ISI:000309617100007,
Author = {Liu, Yiying and Lin, Zhiping and Molteni, Giuseppe and Zhang, Dongye},
Title = {{Eigenvalues and equivalent transformation of a trigonometric matrix
   associated with filter design}},
Journal = {{LINEAR ALGEBRA AND ITS APPLICATIONS}},
Year = {{2012}},
Volume = {{437}},
Number = {{12}},
Pages = {{2961-2972}},
Month = {{DEC 15}},
Abstract = {{The N x N trigonometric matrix P(omega) whose entries are P(omega)(i, j)
   = 1/2 (i+j-2) cos (i-j)omega appears in connection with the design of
   finite impulse response (FIR) digital filters with real coefficients. We
   prove several results about its eigenvalues; in particular, assuming N
   >= 4 we prove that P(omega) has one positive and one negative eigenvalue
   when omega/pi is an integer, while it has two positive and two negative
   eigenvalues when omega/pi is not an integer. We also show that for
   omega/pi not being an integer and a sufficiently large N. the two
   positive eigenvalues converge to alpha N-+(2) and the two negative
   eigenvalues to alpha N--(2), where alpha(+/-) = (1 +/- 2/root 3/)/8.
   Furthermore, an equivalent transformation diagonalizing P(omega) is
   described. (C) 2012 Elsevier Inc. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE INC}},
