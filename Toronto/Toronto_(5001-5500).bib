Author = {Mahanta, Mohammad Shahin and Aghaei, Amirhossein S. and Plataniotis,
   Konstantinos N.},
Book-Group-Author = {{IEEE}},
Title = {{REGULARIZED LDA BASED ON SEPARABLE SCATTER MATRICES FOR CLASSIFICATION
   OF SPATIO-SPECTRAL EEG PATTERNS}},
Booktitle = {{2013 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
