Title = {{Dynamic Gate Resistance Control for Current Balancing in Parallel
   Connected IGBTs}},
Booktitle = {{2013 TWENTY-EIGHTH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC 2013)}},
Series = {{Annual IEEE Applied Power Electronics Conference and Exposition (APEC)}},
Year = {{2013}},
