Title = {{Self-aligned Single-mask Fabrication Process for Electro-thermal
   Microactutors using ICP-RIE}},
Booktitle = {{SMART SENSORS, ACTUATORS, AND MEMS VI}},
Series = {{Proceedings of SPIE}},
