Title = {{ROOT CRACK SIZING USING PHASED ARRAY INSPECTIONS AND AUTOREGRESSIVE
   SPECTRAL EXTRAPOLATION SIGNAL PROCESSING}},
Booktitle = {{REVIEW OF PROGRESS IN QUANTITATIVE NONDESTRUCTIVE EVALUATION, VOLS 32A
   AND 32B}},
Series = {{AIP Conference Proceedings}},
Year = {{2013}},
Volume = {{1511}},
