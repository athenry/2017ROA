@inproceedings{ ISI:000332766801134,
Author = {Milford, Gregory N. and Eleftheriades, George V.},
Book-Group-Author = {{IEEE Antennas \& Propagat Soc}},
Title = {{2D Multiplier with Left-Handed Focusing Lens for Terahertz Signal
   Generation}},
Booktitle = {{2013 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM
   (APSURSI)}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2013}},
Pages = {{1182+}},
Note = {{International Symposium of the IEEE-Antennas-and-Propagation-Society,
   Orlando, FL, JUL 07-13, 2013}},
Organization = {{Inst Elect \& Elect Engineers; IEEE Antennas \& Propagat Soc; APS}},
Abstract = {{In this paper we propose a method for generating terahertz signals using
   a 2D array of nonlinear right-handed unit cells for harmonic generation,
   combined with a matched array of left-handed unit cells for both
   filtering and focusing the desired harmonic frequency component. The
   nonlinear array can be driven by a single sinusoidal source, or by
   multiple coherent sources, in which case the array topology acts as an
   input power combiner. The proposed structure is suited to monolithic
   fabrication and integration with other system components. Performance
   characterisation of the proposed structure is demonstrated using
