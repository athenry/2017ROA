@article{ ISI:000325683100009,
Author = {Bajwa, Gurvinder and Genovese, Matthew and Lian, Keryn},
Title = {{Multilayer Polyoxometalates-Carbon Nanotube Composites for
   Electrochemical Capacitors}},
Journal = {{ECS JOURNAL OF SOLID STATE SCIENCE AND TECHNOLOGY}},
Year = {{2013}},
Volume = {{2}},
Number = {{10}},
Pages = {{M3046-M3050}},
Abstract = {{Composites of multi-walled carbon nanotubes (MWCNT) and polyoxometalates
   (POM) were developed for electrochemical capacitors (EC). Two types of
   POM clusters, PMo12O403- (PMo12) and PMo10V2O405- (PMo10V2), were
   deposited onto MWCNT in single or double layers via layer-by-layer
   deposition. Charge storage capacity increased by a factor of 5 with one
   layer of PMo12 coating. PMo10V2 showed similar charge storage but a
   wider potential window. A dual-layer coating superimposing PMo12 and
   PMo10V2 on MWCNT showed a collective contribution of both active layers,
   increasing not only charge storage capacity but also the voltage window.
   X-ray Photoelectron Spectroscopy (XPS) and Scanning Electron Microscopy
   (SEM) demonstrated successful coating of POM on MWCNT with high coverage
   and evenly covered morphologies. (C) 2013 The Electrochemical Society.
   All rights reserved.}},
