@inproceedings{ ISI:000331367405008,
Author = {Ornan, Oni and Degani, Amir},
Editor = {{Amato, N}},
Title = {{Toward Autonomous Disassembling of Randomly Piled Objects with Minimal
   Perturbation}},
Booktitle = {{2013 IEEE/RSJ INTERNATIONAL CONFERENCE ON INTELLIGENT ROBOTS AND SYSTEMS
   (IROS)}},
