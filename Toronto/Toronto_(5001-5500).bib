@inproceedings{ ISI:000332186700129,
Author = {Alsohaily, Ahmed and Sousa, Elvino S.},
Book-Group-Author = {{IEEE}},
Title = {{Spectrum Sharing LTE-Advanced Small Cell Systems}},
