@article{ ISI:000319527400020,
Author = {Kamino, Brett A. and Bender, Timothy P.},
Title = {{The use of siloxanes, silsesquioxanes, and silicones in organic
   semiconducting materials}},
Journal = {{CHEMICAL SOCIETY REVIEWS}},
Year = {{2013}},
Volume = {{42}},
Number = {{12}},
Pages = {{5119-5130}},
Abstract = {{Optimization of the physical and electronic properties of organic
   semiconductors is a key step in improving the performance of organic
   light emitting diodes, organic photovoltaics, organic field effect
   transistors, and other electronic devices. Separate tuning of the
   physical and electronic properties of these organic semiconductors can
