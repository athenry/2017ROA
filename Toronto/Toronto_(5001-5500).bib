Editor = {{Kollias, N and Choi, B and Zeng, H and Kang, HW and Knudsen, BE and Wong, BJF and Ilgner, JF and Suter, MJ and Lam, S and Brenner, M and Gregory, KW and Tearney, GJ and Marcu, L and Hirschberg, H and Madsen, SJ and MahadevanJansen, A and Jansen, ED and Mandelis, A}},
Title = {{Blood flow velocity measurement by endovascular Doppler optical
   coherence tomography}},
