@inproceedings{ ISI:000329611501194,
Author = {Marsousi, Mahdi and Abhari, Kaveh and Babyn, Paul and Alirezaie, Javad},
Book-Group-Author = {{IEEE}},
Title = {{MULTI-STAGE OMP Sparse Coding Using Local Matching Pursuit Atoms
   Selection}},
Booktitle = {{2013 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
