@article{ ISI:000312072300003,
Author = {Kim, J. and Mandelis, A. and Matvienko, A. and Abrams, S. and Amaechi,
   B. T.},
Title = {{Detection of Dental Secondary Caries Using Frequency-Domain Infrared
   Photothermal Radiometry (PTR) and Modulated Luminescence (LUM)}},
Journal = {{INTERNATIONAL JOURNAL OF THERMOPHYSICS}},
Year = {{2012}},
Volume = {{33}},
Number = {{10-11, SI}},
Pages = {{1778-1786}},
Month = {{NOV}},
Note = {{16th International Conference on Photothermal and Photoacoustic
   Phenomena (ICPPP), Merida, MEXICO, NOV 27-DEC 01, 2011}},
Organization = {{Centro Investigacion Estudios Avanzados Instituto Politecnico Nacl
   (CINVESTAV-IPN); Centro Investigaciones en Optica (CIO)}},
Abstract = {{The ability of frequency-domain photothermal radiometry (PTR) and
   modulated luminescence (LUM) to detect secondary caries is presented.
   Signal behavior upon sequential demineralization and remineralization of
   a spot (diameter similar to 1 mm) on a vertical wall of sectioned tooth
   samples was investigated experimentally. From these studies, it was
   found that PTR-LUM signals change, showing a certain pattern upon
   progressive demineralization and remineralization. PTR amplitudes
   slightly decreased upon progressive demineralization and slightly
   increased upon subsequent remineralization. The PTR phase increased
   during both demineralization and remineralization. LUM amplitudes
   exhibit a decreasing trend at excitation/probe distances larger than 200
   mu m away from the edge for both demineralization and remineralization;
   however, at locations close to the edge (up to similar to 200 mu m), LUM
   signals slightly decrease upon demineralization and slightly increase
   during subsequent remineralization.}},
