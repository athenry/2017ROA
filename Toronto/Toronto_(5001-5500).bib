@inproceedings{ ISI:000331259200098,
Author = {Cotic, Marija and Chinvarun, Yotin and Carlen, Peter L. and Bardakjian,
   Berj L.},
Book-Group-Author = {{IEEE}},
Title = {{Spatial Coherence Patterns of Fast Ictal Activities Coincide with Those
   of Slow Interictal Oscillations in the Human Brain}},
Booktitle = {{2013 6TH INTERNATIONAL IEEE/EMBS CONFERENCE ON NEURAL ENGINEERING (NER)}},
Series = {{International IEEE EMBS Conference on Neural Engineering}},
Year = {{2013}},
