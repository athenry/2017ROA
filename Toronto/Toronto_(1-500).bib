@article{ ISI:000382801300007,
Author = {Fathi, Sepehr and Nayak, Chitra R. and Feld, Jordan J. and Zilman, Anton
   G.},
Title = {{Absolute Ligand Discrimination by Dimeric Signaling Receptors}},
Journal = {{BIOPHYSICAL JOURNAL}},
Year = {{2016}},
Volume = {{111}},
Number = {{5}},
Pages = {{917-920}},
Month = {{SEP 6}},
Abstract = {{Many signaling pathways act through shared components, where different
   ligand molecules bind the same receptors or activate overlapping sets of
   response regulators downstream. Nevertheless, different ligands acting
   through cross-wired pathways often lead to different outcomes in terms
   of the target cell behavior and function. Although a number of
   mechanisms have been proposed, it still largely remains unclear how
   cells can reliably discriminate different molecular ligands under such
   circumstances. Here we show that signaling via ligand-induced receptor
   dimerization a very common motif in cellular signaling naturally
   incorporates a mechanism for the discrimination of ligands acting
   through the same receptor.}},
