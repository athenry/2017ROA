@article{ ISI:000391739500027,
Author = {Sun, Yunhao and Duan, Ruchen and Liang, Yingbin and Khisti, Ashish and
   Shamai (Shitz), Shlomo},
Title = {{Capacity Characterization for State-Dependent Gaussian Channel With a
   Helper}},
Journal = {{IEEE TRANSACTIONS ON INFORMATION THEORY}},
Year = {{2016}},
Volume = {{62}},
Number = {{12}},
