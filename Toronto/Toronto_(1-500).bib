@article{ ISI:000384710500083,
Author = {Zhuang, Lin-Lan and Azimi, Yaldah and Yu, Dawei and Wang, Wen-Long and
   Wu, Yin-Hu and Dao, Guo-Hua and Hu, Hong-Ying},
Title = {{Enhanced attached growth of microalgae Scenedesmus. LX1 through ambient
   bacterial pre-coating of cotton fiber carriers}},
Journal = {{BIORESOURCE TECHNOLOGY}},
Year = {{2016}},
Volume = {{218}},
Pages = {{643-649}},
Month = {{OCT}},
Abstract = {{The role of bacteria/extracellular polymeric substances (EPS) coated
   carriers on attached microalgae growth in suspended-solid phase
   photobioreactor (sspBR) was assessed in this study. The results showed
   that pre-coating cotton with ambient bacteria and their EPS improved the
   attached microalgal growth by as much as 230\% in terms of attached
   microalgae density. Additionally, the single cell dry weight, chemical
   composition and oxygen evolving activity of attached microalgae were
   significantly affected by the presence of bacteria/EPS coating on the
   cotton carriers. The protein content of microalgae cells cultivated in
   the ssPBRs with carriers coated by bacteria and sterilized bacteria were
   on average 26\% and 15\% more than uncoated carriers, respectively.
   Through absorbing and immobilizing nutrients from the bulk medium, the
   bacteria/EPS coating provided the attached microalgae with
   nitrogen/phosphorus for protein synthesis, especially during the late
   stages of batch cultivation. (C) 2016 Elsevier Ltd. All rights reserved.}},
