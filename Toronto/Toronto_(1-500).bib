@article{ ISI:000379696800008,
Author = {Farraj, Abdallah and Hammad, Eman and Al Daoud, Ashraf and Kundur, Deepa},
Title = {{A Game-Theoretic Analysis of Cyber Switching Attacks and Mitigation in
   Smart Grid Systems}},
Journal = {{IEEE TRANSACTIONS ON SMART GRID}},
Year = {{2016}},
Volume = {{7}},
Number = {{4}},
Pages = {{1846-1855}},
Month = {{JUL}},
Abstract = {{We propose a framework for the analysis of cyber switching attacks and
   control-based mitigation in cyber-enabled power systems. Our model of
   the switching attack is simple, only requiring knowledge of the sign of
   the local relative rotor speed, which may be estimated. The controller
   is modeled to be resource constrained, choosing to act only during
   select intervals of time. We make use of an iterated game-theoretic
   formulation to describe the interactions of the parties and its effect
   on system stability. Analytic results indicate the potential of the
   constrained controller to achieve transient stabilization over time
   using zero-determinant strategies. Numerical results of the New England
   39-bus power system demonstrate the potential for such a controller to
   increase system resilience during cyber-attacks.}},
