@article{ ISI:000384033600053,
Author = {Castrucci, Jeffrey S. and Garner, Richard K. and Dang, Jeremy D. and
   Thibau, Emmanuel and Lu, Zheng-Hong and Bender, Timothy P.},
Title = {{Characterization of mu-oxo-(BsubPc)(2) in Multiple Organic Photovoltaic
   Device Architectures: Comparing against and Combining with Cl-BsubPc}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2016}},
Volume = {{8}},
Number = {{37}},
Pages = {{24712-24721}},
Month = {{SEP 21}},
Abstract = {{We demonstrate the first application of a unique boron subphthalocyanine
   (BsubPc) derivative, the oxygen bridged dimer mu-oxo(BsubPc)(2), as a
   multifunctional material within planar heterojunction organic
   photovoltaic (OPV) devices. We first explored the pairing of
   mu-oxo-(BsubPc)(2) with well-known electron accepting and electron
   donating materials to explore its basic functionality. These preliminary
   device structures and metrics indicated that mu-oxo-(BsubPc)(2) is best
   applied as an electron donating material when used in simple bilayer
   structures, as it yielded comparable OPV device efficiencies to that of
   the more. well established and highly optimized chloro-boron
   subphthalocyanine. (Cl-BsubPc) OPV device structures. Thereafter we
   established that the HOMO/LUMO energy levels of mu-oxo-(BsubPc)(2) are
   well-placed to apply it as a bifunctional donor/acceptor interlayer
   material in both energy and charge cascade OPV device architectures.
   Within this context, we found that mu-oxo-(BsubPc)(2) was particularly
   effective in a charge cascade device as an interlayer between Cl-BsubPc
   and C-70. We finally found evidence of an alloying-like effect for
   devices with mixed electron donor layers of (Cl-BsubPc) and
   mu-oxo-(BsubPc)(2), achieved through co-deposition. The overarching
   conclusion is therefore that mu-oxo-(BsubPc)(2) has the ability to
   improve the performance of Cl-BsubPc OPV devices and is a
   multifunctional material worthy of further study.}},
