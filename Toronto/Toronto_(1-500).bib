@article{ ISI:000385618000011,
Author = {Zamboni-Rached, Michel and Besieris, Ioannis M.},
Title = {{Modeling of space-time focusing of localized nondiffracting pulses}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2016}},
Volume = {{94}},
Number = {{4}},
Month = {{OCT 12}},
Abstract = {{In this paper we develop amethod capable of modeling the space-time
   focusing of nondiffracting pulses. These pulses can possess arbitrary
   peak velocities and, in addition to being resistant to diffraction, can
   have their peak intensities and focusing positions chosen a priori. More
