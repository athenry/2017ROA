@article{ ISI:000377503100001,
Author = {Cao, Changhong and Howe, Jane Y. and Perovic, Doug and Filleter, Tobin
   and Sun, Yu},
Title = {{In situ TEM tensile testing of carbon-linked graphene oxide nanosheets
   using a MEMS device}},
Journal = {{NANOTECHNOLOGY}},
Year = {{2016}},
Volume = {{27}},
Number = {{28}},
Month = {{JUL 15}},
Abstract = {{This paper reports in situ transmission electron microscopy (TEM)
   tensile testing of carbon-linked graphene oxide nanosheets using a
   monolithic TEM compatible microelectromechanical system device. The
   set-up allows direct on-chip nanosheet thickness mapping, high
