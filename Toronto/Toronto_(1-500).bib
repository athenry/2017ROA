@article{ ISI:000379930000004,
Author = {Li, Chuanwei and Liscidini, Antonio},
Title = {{Class-C PA-VCO Cell for FSK and GFSK Transmitters}},
Journal = {{IEEE JOURNAL OF SOLID-STATE CIRCUITS}},
Year = {{2016}},
Volume = {{51}},
Number = {{7}},
Pages = {{1537-1546}},
