@article{ ISI:000383392100003,
Author = {Persaud, S. Y. and Newman, R. C.},
Title = {{A Review of Oxidation Phenomena in Ni Alloys Exposed to Hydrogenated
   Steam Below 500 degrees C}},
Journal = {{CORROSION}},
Year = {{2016}},
Volume = {{72}},
Number = {{7}},
Pages = {{881-896}},
Month = {{JUL}},
Abstract = {{The following is a review of the accumulated literature over the past 20
   years for oxidation in Ni-Fe-Cr alloys exposed to 400 degrees C to 500
   degrees C hydrogenated steam. Hydrogenated steam at temperatures of 500
   degrees C and below is considered to simulate 300 degrees C to 360
   degrees C deaerated water often used in the primary circuit of nuclear
   power plants. Alloy 600 (Ni-16Cr-9Fe) is highly susceptible to stress
   corrosion cracking (SCC) in primary water and the peak crack growth rate
   is observed near the Ni/NiO equilibrium potential. It is believed that
   the oxidation tendency, internal and/or external, is directly related to
   the mechanism of SCC in Alloy 600. An initial review will be conducted
   exploring inter-and intragranular oxidation in Alloy 600 and the
   relationship to embrittlement and SCC. Following this, investigations
   into oxidation phenomena, in particular the possibility of internal
   oxidation, in alloys with high reactive element contents, such as Alloy
   690 and Alloy 800, will be analyzed. Finally, the practical
   applicability and validity of a high-temperature hydrogenated steam
   environment as a simulant for accelerated primary water conditions will
   be reviewed through comparisons to oxidation phenomena commonly reported
   in 300 degrees C to 360 degrees C deaerated water. In general,
   evaluation of the literature suggests that intergranular oxidation
   phenomena and the mechanism of SCC/embrittlement in Alloy 600 are likely
   to be similar, ranging from 300 degrees C deaerated water to 500 degrees
   C hydrogenated steam.}},
Publisher = {{NATL ASSOC CORROSION ENG}},
Address = {{1440 SOUTH CREEK DRIVE, HOUSTON, TX 77084-4906 USA}},
Type = {{Review}},
Language = {{English}},
Affiliation = {{Persaud, SY; Newman, RC (Reprint Author), Univ Toronto, Dept Chem Engn \& Appl Chem, 200 Coll St, Toronto, ON M5S 3E5, Canada.
   Persaud, S. Y.; Newman, R. C., Univ Toronto, Dept Chem Engn \& Appl Chem, 200 Coll St, Toronto, ON M5S 3E5, Canada.}},
DOI = {{10.5006/1957}},
ISSN = {{0010-9312}},
EISSN = {{1938-159X}},
Keywords = {{embrittlement; internal oxidation; nickel alloys; primary water;
