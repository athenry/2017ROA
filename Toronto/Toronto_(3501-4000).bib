@inproceedings{ ISI:000361076200013,
Author = {Tadavani, Pooyan Khajehpour and Alipanahi, Babak and Ghodsi, Ali},
Editor = {{Ahmed, SE}},
Title = {{Manifold unfolding by Isometric Patch Alignment with an application in
   protein structure determination}},
Booktitle = {{PERSPECTIVES ON BIG DATA ANALYSIS: METHODOLOGIES AND APPLICATIONS}},
Series = {{Contemporary Mathematics}},
Year = {{2014}},
Volume = {{622}},
Pages = {{177-191}},
Note = {{2nd International Workshop on Perspectives on High-Dimensional Data
   Analysis, Univ Montreal, Ctr Rech Math, Montreal, CANADA, MAY 30-JUN 01,
   2012}},
Organization = {{Univ Montreal, Ctr Rech Math}},
Abstract = {{We propose Isometric Patch Alignment (IPA), a novel dimensionality
   reduction method which has a low computational cost. This method is
   inspired by the observation that, reasonably large chunks of a
   high-dimensional dataset can be approximated by low-dimensional patches
   over its underlying manifold. Thus, in our proposed approach, first the
   input data is clustered, such that the neighboring clusters have some
   overlap. Then, each cluster is modeled by a low-dimensional patch on the
   underlying manifold of the data. Conceptually, the input is now reduced
   to a set of patches. IPA rearranges these patches such that the shared
   points between the neighboring ones match accordingly. We show that this
   rearrangement can be computed by solving a relatively small
   semi-definite program. In addition, we prove that attaching two
   neighboring patches on their overlapping parts aligns them together.
   Based on this, the manifold resulted from the convex optimization of IPA
   will be unfolded and thus, can be represented in a low-dimensional
   subspace. The proposed method is stable, fast, and scalable; our
   experimental results demonstrate its capability for dimensionality
   reduction and data visualization, particularly on input datasets with
   complex structures, for example where the underlying manifold is not
   convex. Moreover, as a distinct application, IPA is employed for protein
   structure determination.}},
