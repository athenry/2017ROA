@article{ ISI:000332207400003,
Author = {Chai, L. and White, R. T. and Greiner, M. T. and Lu, Z. H.},
Title = {{Experimental demonstration of the universal energy level alignment rule
   at oxide/organic semiconductor interfaces}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2014}},
Volume = {{89}},
Number = {{3}},
Month = {{JAN 10}},
Abstract = {{In this work, our experimental demonstration of the universal energy
   level alignment rule at oxide/organic semiconductor interfaces is
   reported. Photoemission spectroscopy is used to show the three different
   regimes of the energy level alignment: the lowest unoccupied molecular
   orbital (LUMO) is pinned to the substrate Fermi level at the extreme low
   end of work functions; the energy offset of the highest occupied
   molecular orbital (HOMO) follows the Schottky-Mott rule; the HOMO is
   pinned to the substrate Fermi level at the extreme high end of work
   functions. To demonstrate this, fullerene C-60 was deposited on eight
   different types of transition-metal oxides, ZrO2, TiO2, NiO, Co3O4, CuO,
   V2O5, MoO3, and WO3, followed by in situ ultraviolet photoemission
   spectroscopy.}},
