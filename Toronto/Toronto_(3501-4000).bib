@inproceedings{ ISI:000359716000009,
Author = {Botelho, S. J. and Bazylak, A.},
Editor = {{Nguyen, T and Atanassov, P and Kostecki, R}},
Title = {{Development of a High Resolution Thermal Model of the Microporous Layer
   found in PEM Fuel Cells}},
Booktitle = {{CHARACTERIZATION OF INTERFACES AND INTERPHASES}},
Series = {{ECS Transactions}},
Year = {{2014}},
Volume = {{61}},
