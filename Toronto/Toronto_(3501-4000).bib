@article{ ISI:000360167500007,
Author = {Zhao, Liming and Honghi Tran},
Title = {{COMBUSTION BEHAVIOURS OF SPENT PULPING LIQUORS}},
Journal = {{J-FOR-JOURNAL OF SCIENCE \& TECHNOLOGY FOR FOREST PRODUCTS AND PROCESSES}},
Year = {{2014}},
Volume = {{4}},
Number = {{6}},
Pages = {{50-57}},
Abstract = {{This study investigated the combustion behaviours of spent pulping
   liquors from 8 kraft mills, a soda mill, a dissolving pulp mill, a
   neutral sulfite semi-chemical mill and an NH4-based sulfite mill. We
   performed over 300 combustion tests using a thermogravimetric (TG)
   combustor that continuously recorded the liquor sample mass and
   documented the appearance of the sample during the combustion process.
   The results confirmed key findings reported in the literature and
   provided further insight into the combustion behaviours of the liquors.
   As the liquor sample was introduced into the TG combustor controlled at
   830 degrees C, it went through 3 main stages: drying,
   devolatilization/volatile burning, and char burning/smelt (or ash)
   formation. Drying and devolatilization were rapid, while char burning
   was slow with a rate less than one-tenth of the volatile burning rate.
   During devolatilization, all black liquors tested swelled enormously,
   reaching a maximum swollen volume of 20 to 40 times larger than their
   initial liquor volume, while the NSSC liquor and the NH4-based spent
   sulfite liquor did not swell much, only 2 to 3 times larger. The total
   combustion time was mainly governed by the char burning time, which
   varied significantly from liquor to liquor. In general, liquors that
   swelled more burned more quickly than liquors that swelled less. For
   liquors that swelled more than 25 times of their initial liquor volume,
   no further decrease in total combustion time was observed.}},
