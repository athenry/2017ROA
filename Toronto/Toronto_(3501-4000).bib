@inproceedings{ ISI:000351998300026,
Author = {Karamad, Ehsan and Adve, Raviraj S.},
Book-Group-Author = {{IEEE}},
Title = {{Distributed Power Control Subject to Channel and Interference Estimation
   Errors}},
Booktitle = {{2014 48TH ANNUAL CONFERENCE ON INFORMATION SCIENCES AND SYSTEMS (CISS)}},
Year = {{2014}},
Note = {{48th Annual Conference on Information Sciences and Systems (CISS),
   Princeton Univ, Princeton, NJ, MAR 19-21, 2014}},
Organization = {{IEEE Informat Theory Soc; Princeton Univ, Dept Elect Engn}},
Abstract = {{Perfect estimation of receiver interference power is a fundamental
   assumption in most distributed power control algorithms. Newer
   techniques in managing interference through transmit power control rely
   on local observations of channel information where the obtained
   information is assumed to be error-free. In this paper, we investigate
   the performance degradation in distributed power control algorithms due
   to estimation errors. We present some bounds on the resulted transmit
   power and signal to interference and noise ratio (SINR) errors due to
   imperfect estimations. Specifically, we find that for
   interference-limited scenarios, there is a potential SINR loss of up to
   3-dB even for tiny errors in channel state information.}},
