@article{ ISI:000334657500018,
Author = {Huynh, Elizabeth and Jin, Cheng S. and Wilson, Brian C. and Zheng, Gang},
Title = {{Aggregate Enhanced Trimodal Porphyrin Shell Microbubbles for Ultrasound,
   Photoacoustic, and Fluorescence Imaging}},
Journal = {{BIOCONJUGATE CHEMISTRY}},
Year = {{2014}},
Volume = {{25}},
Number = {{4}},
Pages = {{796-801}},
Month = {{APR}},
Abstract = {{Microbubbles (MBs) are currently used as ultrasound (US) contrast agents
   and as delivery vehicles for site-specific US-triggered drug and gene
   delivery. Multimodal US-based imaging methods have been applied
   preclinically to assess and validate the effectiveness and fate of MBs
   in imaging and therapy. Here we present the first intrinsically trimodal
   MBs by incorporating a dense concentration of porphyrin molecules within
   a MB shell, enabled by the use of a single porphyrin-lipid component.
   These MBs possess US, photoacoustic, and fluorescence properties that
   are demonstrated in solution and in a mouse tumor xenograft model. They
   also have potential to be extended to other imaging modalities such as
   magnetic resonance imaging and nuclear imaging.}},
