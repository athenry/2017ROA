Author = {Marsousi, Mahdi and Plataniotis, Konstantinos N. and Stergiopoulos,
   Stergios},
Book-Group-Author = {{IEEE}},
Title = {{Shape-Based Kidney Detection and Segmentation in Three-Dimensional
   Abdominal Ultrasound Images}},
Booktitle = {{2014 36TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
   MEDICINE AND BIOLOGY SOCIETY (EMBC)}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
Year = {{2014}},
