@inproceedings{ ISI:000361017900224,
Author = {Ghali, Bassma and Mamun, Khondaker A. and Chau, Tom},
Book-Group-Author = {{IEEE}},
Title = {{Variation of grip force profile during signature writing}},
Booktitle = {{2014 IEEE 27TH CANADIAN CONFERENCE ON ELECTRICAL AND COMPUTER
   ENGINEERING (CCECE)}},
Series = {{Canadian Conference on Electrical and Computer Engineering}},
