Title = {{Treatise on water hammer in hydropower standards and guidelines}},
Booktitle = {{27TH IAHR SYMPOSIUM ON HYDRAULIC MACHINERY AND SYSTEMS (IAHR 2014), PTS
   1-7}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
Year = {{2014}},
