@inproceedings{ ISI:000356049700009,
Author = {Dahrouj, Hayssam and Tang, Taiwen and Cheng, Ho Ting and Selea, Radu},
Book-Group-Author = {{IEEE}},
Title = {{Fairness and Throughput Balancing Via Power Control in Wireless Backhaul
   Networks}},
Booktitle = {{2014 27TH BIENNIAL SYMPOSIUM ON COMMUNICATIONS (QBSC)}},
Year = {{2014}},
Pages = {{38-41}},
Note = {{27th Queen's Biennial Symposium on Communications (QBSC), Kingston,
   CANADA, JUN 01-04, 2014}},
Organization = {{Queens Univ, Dept Elect \& Comp Engn}},
Abstract = {{Balancing between fairness and throughput remains a challenging problem
   in the design of future wireless networks. This paper addresses this
   challenge and proposes a practical power optimization method, which has
   the potential of balancing between fairness and data throughput. The
   paper assumes a non-line of sight (NLOS) wireless backhaul network,
   especially introduced to serve areas with dense data traffic. The
   network comprises several transmitters, known as access-nodes (AN), each
   serving several remote terminals (RT) via wireless links. To reach a
   feasible, yet a simple, proportional fairness solution, this paper
   maximizes an approximated network-wide utility function, where the
   individual terms are based on long-term channel gains, utilized to
   compute the geometry carrier to interference and noise ratio (CINR). The
   paper then solves the power optimization problem by introducing a scalar
   bias to balance fairness with throughput. Through Monte Carlo
   simulations, performance improvement of the proposed method is
   illustrated and compared against existing classical methods.}},
