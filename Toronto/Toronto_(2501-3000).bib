@article{ ISI:000346190100008,
Author = {Le, Adam Y. and Mills, James K. and Benhabib, Beno},
Title = {{Actuation of Linear Ultrasonic Motors with a Square-wave Based
   Excitation Voltage}},
Journal = {{FERROELECTRICS}},
Year = {{2014}},
Volume = {{473}},
Number = {{1}},
Pages = {{75-93}},
Month = {{DEC 10}},
Abstract = {{In this paper, a novel actuation approach is proposed for linear
   ultrasonic motors by applying a square-wave excitation voltage across
   the piezoceramic elements. Using this approach, the output force and
   power of the ultrasonic motor can be tangibly increased, without
   increasing its stator volume, thereby leading to improvement in the
   actuator power density. Added normal force is exerted on the moving
   stage by exciting the higher longitudinal resonant modes, the
   frequencies of which coincide with the harmonics of the square-wave
   voltage. The proposed approach is illustrated herein via the simulation
   of a suitably designed bimodal linear ultrasonic motor using
   finite-element analysis.}},
Publisher = {{TAYLOR \& FRANCIS LTD}},
Address = {{2-4 PARK SQUARE, MILTON PARK, ABINGDON OR14 4RN, OXON, ENGLAND}},
