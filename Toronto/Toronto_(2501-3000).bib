@inproceedings{ ISI:000371401400423,
Author = {Epstein, Ariel and Eleftheriades, George V.},
Book-Group-Author = {{IEEE}},
Title = {{Coupling Localized Sources to Controlled Polarized Broadside Radiation
   using Huygens Metasurfaces}},
Booktitle = {{2015 IEEE INTERNATIONAL SYMPOSIUM ON ANTENNAS AND PROPAGATION \&
   USNC/URSI NATIONAL RADIO SCIENCE MEETING}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
