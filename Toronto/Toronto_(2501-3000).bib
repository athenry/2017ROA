@inproceedings{ ISI:000392737000175,
Author = {Hessabi, R. Mirza and Mercan, O.},
Editor = {{Ingraffea, N and Libby, M}},
Title = {{Application of Gyro-Mass Dampers to Mitigate the Seismic Failure in Soft
   First Story Buildings}},
Booktitle = {{Structures Congress 2015}},
Year = {{2015}},
Pages = {{2032-2043}},
Note = {{Structures Congress, Portland, OR, APR 23-25, 2015}},
Organization = {{Amer Soc Civil Engineers, Struct Engn Inst}},
Abstract = {{Several studies have shown the vulnerability of buildings to sudden
   changes in lateral stiffness and strength, as in the case of a
   soft-story mechanism, which can cause damage or even collapse during
   strong earthquakes. This paper investigates the possibility of improving
   the seismic performance of soft-story structures by incorporating
   gyro-mass dampers (GMDs) as a novel alternative solution. GMDs are newly
   developed passive control devices. They generate a force that is
   proportional to the relative acceleration applied at their end nodes.
   The introduction of GMD elements increases the lateral capacity of the
   first story and the fundamental period of the entire structure. It also
   reduces the input energy of the earthquake ground motion. To evaluate
   the effectiveness of these devices, different configurations are
   proposed and time history analyses are performed.}},
