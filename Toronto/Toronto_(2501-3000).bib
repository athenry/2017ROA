Title = {{Electrical and Dielectric Properties of Foam Injection-Molded
   Polypropylene/Multiwalled Carbon Nanotube Composites}},
Booktitle = {{PROCEEDINGS OF PPS-30: THE 30TH INTERNATIONAL CONFERENCE OF THE POLYMER
   PROCESSING SOCIETY}},
Series = {{AIP Conference Proceedings}},
Year = {{2015}},
Volume = {{1664}},
