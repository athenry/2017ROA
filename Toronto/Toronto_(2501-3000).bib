@article{ ISI:000360120400013,
Author = {Comin, Riccardo and Walters, Grant and Thibau, Emmanuel Sol and Voznyy,
   Oleksandr and Lu, Zheng-Hong and Sargent, Edward H.},
Title = {{Structural, optical, and electronic studies of wide-bandgap lead halide
   perovskites}},
Journal = {{JOURNAL OF MATERIALS CHEMISTRY C}},
Year = {{2015}},
Volume = {{3}},
Number = {{34}},
Pages = {{8839-8843}},
Abstract = {{We investigate the family of mixed Br/Cl organolead halide perovskites
   which enable light emission in the blue-violet region of the visible
   spectrum. We report the structural, optical and electronic properties of
   this air-stable family of perovskites, demonstrating full bandgap
   tunability in the 400-550 nm range and enhanced exciton strength upon Cl
