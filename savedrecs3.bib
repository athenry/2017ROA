@inproceedings{ ISI:000390094700066,
Author = {Li, Yan and Hu, Jianhao and Lu, Hao and Chen, Jie},
Book-Group-Author = {{IEEE}},
Title = {{Area-Efficient Partial-Clique-Energy MRF Pair Design with Ultra-Low
   Supply Voltage}},
Booktitle = {{2016 IEEE INTERNATIONAL SYMPOSIUM ON CIRCUITS AND SYSTEMS (ISCAS)}},
Series = {{IEEE International Symposium on Circuits and Systems}},
Year = {{2016}},
Pages = {{261-264}},
