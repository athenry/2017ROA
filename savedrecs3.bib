
@article{ ISI:000366662700042,
Author = {Tong, Huiyan and Zhao, Peng and Huang, Chunkai and Zhang, Hongwei and
   Tian, Yimei and Li, Zhengkun},
Title = {{Development of iron release, turbidity, and dissolved silica integrated
   models for desalinated water in drinking water distribution systems}},
