@article{ ISI:000394518800004,
Author = {Wang, Gai-Ge and Deb, Suash and Gao, Xiao-Zhi and Coelho, Leandro dos
   Santos},
Title = {{A new metaheuristic optimisation algorithm motivated by elephant herding
   behaviour}},
Journal = {{INTERNATIONAL JOURNAL OF BIO-INSPIRED COMPUTATION}},
Year = {{2016}},
Volume = {{8}},
Number = {{6}},
Pages = {{394-409}},
Abstract = {{In this paper, a new swarm-based metaheuristic algorithm, called
   elephant herding optimisation (EHO), is proposed for solving global
   optimisation tasks, which is inspired by the herding behaviour of the
   elephant groups. In nature, the elephants belonging to different clans
   live together under the leadership of a matriarch, and the male
   elephants will leave their family group when growing up. These two
   behaviours can be modelled into two following operators: clan updating
   operator and separating operator. In EHO, the elephants are updated
   using its current position and matriarch through clan updating operator,
   and the separating operator is then implemented. Moreover, EHO has been
   benchmarked by 20 standard benchmarks, and two engineering cases in
   comparison with BBO, DE and GA. The results clearly establish the
   supremacy of EHO in finding the better function values on most test
   problems than those three algorithms.}},
