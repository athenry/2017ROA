@inproceedings{ ISI:000388377100323,
Author = {Baladi, Elham and Iyer, Ashwin K.},
Book-Group-Author = {{IEEE}},
Title = {{Far-field High-resolution Imaging of Conducting Obstacles using
   Metamaterial-lined Aperture Arrays}},
Booktitle = {{2016 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2016}},
Pages = {{667-668}},
