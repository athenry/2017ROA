Title = {{Predictive and Diagnostic Analysis of Shield Cutter Head Failures in
   Tunnel Construction}},
Booktitle = {{CONSTRUCTION RESEARCH CONGRESS 2016: OLD AND NEW CONSTRUCTION
   TECHNOLOGIES CONVERGE IN HISTORIC SAN JUAN}},
Year = {{2016}},
Pages = {{2553-2562}},
