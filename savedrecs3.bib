@inproceedings{ ISI:000399152503028,
Author = {Ji, Wenying and AbouRizk, Simaan},
Book-Group-Author = {{IEEE}},
Title = {{A BAYESIAN INFERENCE BASED SIMULATION APPROACH FOR ESTIMATING FRACTION
   NONCONFORMING OF PIPE SPOOL WELDING PROCESSES}},
Booktitle = {{2016 WINTER SIMULATION CONFERENCE (WSC)}},
Series = {{Winter Simulation Conference Proceedings}},
