@article{ ISI:000367757800013,
Author = {Jain, Siddharth and Jain, Shivani and Wolf, Ingo Tim and Lee, Jonathan
   and Tong, Yen Wah},
Title = {{A comprehensive review on operating parameters and different
   pretreatment methodologies for anaerobic digestion of municipal solid
   waste}},
Journal = {{RENEWABLE \& SUSTAINABLE ENERGY REVIEWS}},
Year = {{2015}},
Volume = {{52}},
Pages = {{142-154}},
Month = {{DEC}},
Abstract = {{Municipal solid waste (MSW) management is becoming a serious issue in
   all over the world. Anaerobic digestion (AD) is one of the technologies
   to convert that waste into useful form of energy. But megacities like
   Singapore having limited resources, cannot fulfill the cow dung or other
   animal manure requirements in AD. Therefore there is a need to study
   critically the operating parameters and also the pretreatment
   technologies available for treating the substrate so that one can get
