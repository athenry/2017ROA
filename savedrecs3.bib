@article{ ISI:000373948700021,
Author = {Li, P. and Xu, W. Y. and Song, L. W.},
Title = {{A Novel Compensation Strategy for the Radiation Characteristics of Large
   Dielectric Radomes Based on Phase Modification}},
Journal = {{IEEE ANTENNAS AND WIRELESS PROPAGATION LETTERS}},
Year = {{2016}},
Volume = {{15}},
Pages = {{1044-1047}},
Abstract = {{To mitigate the degradation of radiation characteristics for large
   ground-reflector antennas due to dielectric radomes, a simple
   compensation strategy based on phase modification has been proposed. A
   modification of the phase distribution in the original aperture of an
   enclosed reflector antenna via moving the feed or subreflector along
   axial direction opportunely could compensate the phase shift caused by
   dielectric radome, minimize the phase difference in transmission
   aperture, and reduce the distortion of the antenna's far-field pattern.
   The movement of the subreflector could be calculated by a formula, which
   has been provided in this work based on geometrical optics (GO). Good
   compensated results are obtained in a 9.14-m one-piece dielectric
   sandwich radome. The proposed compensation strategy could also be
   applied to metal-space frame radome; a 20-m radome is presented to
   verify the results.}},
