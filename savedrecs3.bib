@inproceedings{ ISI:000400048101065,
Author = {Wu, Ouyang and Hariprasad, K. and Huang, Biao and Forbes, J. Fraser},
Book-Group-Author = {{IEEE}},
Title = {{Identification of Linear Dynamic Errors-In-Variables Systems with a
   Dynamic Uncertain Input Using the EM Algorithm}},
Booktitle = {{2016 IEEE 55TH CONFERENCE ON DECISION AND CONTROL (CDC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2016}},
Pages = {{1229-1234}},
Note = {{55th IEEE Conference on Decision and Control (CDC), Las Vegas, NV, DEC
   12-14, 2016}},
Organization = {{IEEE; Soc Ind \& Appl Math; Inst Operat Res \& Management Sci; Japanese
   Soc Instrument Control Engineers; European Control Assoc}},
Abstract = {{A dynamic system with both input and output measurement errors is termed
   as an errors-in-variables (EIV) system. Employment of traditional
   identification methods for EIV systems will result in biased parameter
   estimates. The ex sting methods of EIV identification like Bias
