@article{ ISI:000367739000003,
Author = {Khosravi, Farhad and Moghadas, Hamid and Mousavi, Pedram},
Title = {{A GNSS Antenna With a Polarization Selective Surface for the Mitigation
   of Low-Angle Multipath Interference}},
Journal = {{IEEE TRANSACTIONS ON ANTENNAS AND PROPAGATION}},
Year = {{2015}},
Volume = {{63}},
Number = {{12}},
Pages = {{5287-5295}},
Month = {{DEC}},
Abstract = {{This paper presents a nonresonant polarization selective surface (PSS)
   to address the problem of multipath distortion in global navigation
   satellite system (GNSS) antennas. The study of the radiated fields of a
   circular patch antenna reveals that surface wave (SW) radiation on the
   edges is not the major source of a high axial ratio (AR) at grazing
   angles, which opposes presently accepted theory. It is shown that the AR
   problem originates in the different theta dependence of the intrinsic
   field components E-theta and E-phi. The use of a three-layer-tapered FSS
   in close proximity to the patch modifies E-theta and results in an
   acceptable AR for angles close to 90 degrees. An AR better than 2 dB is
   achieved at angles beyond the horizon (theta= 90 degrees) and in all
   planes at the GPS L1 frequency band (f = 1.575 GHz). This solution
   outperforms the techniques in literature in terms of profile, diameter,
   performance, and simplicity.}},
