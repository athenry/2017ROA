@article{ ISI:000368100800010,
Author = {Yang, Chao and Wang, Fajun and Li, Wen and Ou, Junfei and Li, Changquan
   and Amirfazli, Alidad},
Title = {{Anti-icing properties of superhydrophobic ZnO/PDMS composite coating}},
Journal = {{APPLIED PHYSICS A-MATERIALS SCIENCE \& PROCESSING}},
Year = {{2016}},
Volume = {{122}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{We present the excellent anti-icing performance for a superhydrophobic
   coating surface based on ZnO/polydimethylsiloxane (ZnO/PDMS) composite.
   The superhydrophobic ZnO/PDMS coating surface was prepared by a facile
   solution mixing, drop coating, room-temperature curing and surface
   abrading procedure. The superhydrophobic ZnO/PDMS composite coating
   possesses a water contact angle of 159.5 degrees and a water sliding
   angle of 8.3 degrees at room temperature (5 degrees C). The anti-icing
   properties of the superhydrophobic coating were investigated by
   continuously dropping cold-water droplets (about 0 degrees C) onto the
   pre-cooled surface using a home-made apparatus. The sample was placed at
   different tilting angle (0 degrees and 10 degrees) and pre-cooled to
   various temperatures (-5, -10 and -15 degrees C) prior to measure. The
   pure Al surface was also studied for comparison. It was found that icing
   accretion on the surface could be reduced apparently because the water
   droplets merged together and slid away from the superhydrophobic surface
   at all of the measuring temperatures when the surface is horizontally
   placed. In addition, water droplet slid away completely from the
   superhydrophobic surface at -5 and -10 degrees C when the surface is
   tilted at 10 degrees, which demonstrates its excellent anti-icing
   properties at these temperatures. When the temperature decreased to -15
   degrees C, though ice accretion on the tilted superhydrophobic coating
   surface could not be avoided absolutely, the amount of ice formed on the
   surface is very small, which indicated that the coating surface with
   superhydrophobicity could significantly reduce ice accumulation on the
   surface at very low temperature (-15 degrees C). Importantly, the sample
   is also stable against repeated icing/deicing cycles. More meaningfully,
   once the superhydrophobic surface is damaged, it can be repaired easily
   and rapidly.}},
