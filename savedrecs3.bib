@article{ ISI:000382068000004,
Author = {Etayash, H. and McGee, A. R. and Kaur, K. and Thundat, T.},
Title = {{Nanomechanical sandwich assay for multiple cancer biomarkers in breast
   cancer cell-derived exosomes}},
Journal = {{NANOSCALE}},
Year = {{2016}},
Volume = {{8}},
Number = {{33}},
Pages = {{15137-15141}},
Abstract = {{The use of exosomes as cancer diagnostic biomarkers is technically
   limited by their size, heterogeneity and the need for extensive
   purification and labelling. We report the use of cantilever arrays for
   simultaneous detection of multiple exosomal surface-antigens with high
   sensitivity and selectivity. Exosomes from breast cancer were
   selectively identified by detecting over-expressed membrane-proteins
   CD24, CD63, and EGFR. Excellent selectivity however, was achieved when
   targeting the cell-surface proteoglycan, Glypican-1 at extraordinary
   limits (similar to 200 exosomes per mL, similar to 0.1 pg mL(-1).}},
