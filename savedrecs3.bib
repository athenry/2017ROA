@article{ ISI:000375587500001,
Author = {Konecny, Jaromir and Prauzek, Michal and Kromer, Pavel and Musilek, Petr},
Title = {{Novel Point-to-Point Scan Matching Algorithm Based on Cross-Correlation}},
Journal = {{MOBILE INFORMATION SYSTEMS}},
Year = {{2016}},
Abstract = {{The localization of mobile robots in outdoor and indoor environments is
   a complex issue. Many sophisticated approaches, based on various types
   of sensory inputs and different computational concepts, are used to
   accomplish this task. However, many of the most efficient methods for
   mobile robot localization suffer from high computational costs and/or
   the need for high resolution sensory inputs. Scan cross-correlation is a
   traditional approach that can be, in special cases, used to match
   temporally aligned scans of robot environment. This work proposes a set
   of novel modifications to the cross-correlation method that extend its
   capability beyond these special cases to general scan matching and
   mitigate its computational costs so that it is usable in practical
   settings. The properties and validity of the proposed approach are in
   this study illustrated on a number of computational experiments.}},
