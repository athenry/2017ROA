@article{ ISI:000369680400111,
Author = {Yazdanbakhsh, Farzad and Alizadehgiashi, Moien and Blaesing, Marc and
   Mueller, Michael and Sawada, James A. and Kuznicki, Steven M.},
Title = {{Cu-Cr-O Functionalized ETS-2 Nanoparticles for Hot Gas Desulfurization}},
Journal = {{JOURNAL OF NANOSCIENCE AND NANOTECHNOLOGY}},
Year = {{2016}},
Volume = {{16}},
Number = {{1}},
Pages = {{878-884}},
Month = {{JAN}},
Abstract = {{Engelhard Titanium Silicate-2 (ETS-2), a sodium nanotitanate, was
   surface functionalized by ion exchanging the solid with copper and
   chromium ions. The ability of this bi-metallic adsorbent to remove H2S
   at elevated temperatures was assessed using a dynamic breakthrough
   system and contrasted against an analogous mixed metal oxide, Cu-Cr-O.
   Unlike Cu-Cr-O, the H2S capacity for CuCr-ETS-2 remains unchanged from
   350 degrees C up to 950 degrees C. Using ETS-2 as a support for the
   metals increased the adsorbents surface area and improved its sulfur
   capacity from 35 mg H2S/g for Cu-Cr-O to 61 mg H2S/g adsorbent for
   CuCr-ETS-2. The consistent presence of Cu9S5 on the sulfided adsorbents
   suggests that chromium effectively stabilizes the copper against
   reduction to metallic copper up to temperatures as high as 950 degrees
   C.}},
Publisher = {{AMER SCIENTIFIC PUBLISHERS}},
Address = {{26650 THE OLD RD, STE 208, VALENCIA, CA 91381-0751 USA}},
