@inproceedings{ ISI:000392150700216,
Author = {D'Aniello, Giuseppe and Gaeta, Angelo and Gaeta, Matteo and Loia,
   Vincenzo and Reformat, Marek Z.},
Book-Group-Author = {{IEEE}},
Title = {{Collective Awareness in Smart City with Fuzzy Cognitive Maps and Fuzzy
   sets}},
Booktitle = {{2016 IEEE INTERNATIONAL CONFERENCE ON FUZZY SYSTEMS (FUZZ-IEEE)}},
