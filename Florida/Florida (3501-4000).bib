@inproceedings{ ISI:000380478700068,
Author = {Nieh, Chien-Ming and Huang, Tien-Yu and Lin, Jenshan},
Book-Group-Author = {{IEEE}},
Title = {{Antenna Radiation Pattern Effects on a Short-Range Vibration-Detection
   Radar System}},
Booktitle = {{2014 INTERNATIONAL SYMPOSIUM ON ANTENNAS AND PROPAGATION (ISAP)}},
Year = {{2014}},
Pages = {{135-136}},
Note = {{International Symposium on Antennas and Propagation (ISAP), Kaohsiung,
   TAIWAN, DEC 02-04, 2014}},
Abstract = {{This paper presents a 5.8 GHz Doppler noncontact vibration-detection
   radar system with 2X2 patch array antennas and single patch antennas.
   Two different types of antennas are used on both transmitter (Tx) and
   receiver (Rx) sides, alternately. An actuator is connected with a 16 cm
   x 14 cm metal plate and generates a 1 Hz sinusoidal movement with 1 mm
   peak-to-peak displacement. The system can successfully detect a target's
   vibration when it is vertically misaligned with the target by 31 degrees
