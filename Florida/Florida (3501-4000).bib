@inproceedings{ ISI:000369860500070,
Author = {Adegbija, Tosiron and Gordon-Ross, Ann and Rawlins, Marisha},
Book-Group-Author = {{IEEE}},
Title = {{Analysis of Cache Tuner Architectural Layouts for Multicore Embedded
   Systems}},
Booktitle = {{2014 IEEE INTERNATIONAL PERFORMANCE COMPUTING AND COMMUNICATIONS
   CONFERENCE (IPCCC)}},
Series = {{IEEE International Performance Computing and Communications Conference
   (IPCCC)}},
