@article{ ISI:000323680200003,
Author = {Yun, YoungSang and Xia, Ye and Behdani, Behnam and Smith, J. Cole},
Title = {{Distributed Algorithm for Lifetime Maximization in a Delay-Tolerant
   Wireless Sensor Network with a Mobile Sink}},
Journal = {{IEEE TRANSACTIONS ON MOBILE COMPUTING}},
Year = {{2013}},
Volume = {{12}},
Number = {{10}},
Pages = {{1920-1930}},
Month = {{OCT}},
Abstract = {{We propose an algorithm for maximizing the lifetime of a wireless sensor
   network when there is a mobile sink and the underlying application can
   tolerate some amount of delay in delivering the data to the sink. The
   algorithm is distributed, and in addition, mostly uses local
   information. Such an algorithm can be implemented by parallel and/or
   distributed execution and the overhead of message passing is low. It is
   also possible to embed the algorithm into a network protocol so that the
   sensor nodes and the sink can run it directly as part of the network
   operation. We give a proof of the algorithm's optimality and the
   boundedness of the queue sizes, both in the long-run average sense. The
   proof is based on analyzing a Lyapunov drift.}},
