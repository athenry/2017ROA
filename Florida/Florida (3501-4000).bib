@article{ ISI:000326640800036,
Author = {Cheng, Shuo and Arnold, David P.},
Title = {{Defining the coupling coefficient for electrodynamic transducers}},
Journal = {{JOURNAL OF THE ACOUSTICAL SOCIETY OF AMERICA}},
Year = {{2013}},
Volume = {{134}},
Number = {{5, 1}},
Pages = {{3561-3572}},
Month = {{NOV}},
Abstract = {{This paper provides a simple, practical definition of the coupling
   coefficient for electrodynamic transducers. Comparing to efforts made in
   previous works that assumed a lossless spring-inductor model, the
   definition presented here is based on a lossy mass-inductor model.
   Time-harmonic analysis is used to model the energy flow in the
   transducer. Both energy storage and energy dissipation are included in
   the electrodynamic coupling coefficient definition. An in-depth
   discussion is provided to explain and justify the derivation and overall
   methodology. This definition is expected to provide a useful and
   practical measure of the electromechanical energy conversion performance
   of electrodynamic transducers, both actuators and generators. (C) 2013
   Acoustical Society of America.}},
