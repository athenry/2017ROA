@inproceedings{ ISI:000370963702039,
Author = {Dani, Ashwin and McCourt, Michael and Curtis, J. Willard and Mehta,
   Siddhartha},
Book-Group-Author = {{IEEE}},
Title = {{Information Fusion in Human-Robot Collaboration using Neural Network
   Representation}},
Booktitle = {{2014 IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN AND CYBERNETICS (SMC)}},
