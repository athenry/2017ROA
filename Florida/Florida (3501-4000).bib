Title = {{Lattice kinetic Monte Carlo modeling of germanium solid phase epitaxial
   growth}},
Booktitle = {{PHYSICA STATUS SOLIDI C: CURRENT TOPICS IN SOLID STATE PHYSICS, VOL 11,
   NO 1}},
