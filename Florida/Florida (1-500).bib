@article{ ISI:000386419100034,
Author = {Yu, Fang and Zhou, Yanmei and Gao, Bin and Qiao, Han and Li, Yonghong
   and Wang, Enze and Pang, Lanfang and Bao, Chong},
Title = {{Effective removal of ionic liquid using modified biochar and its
   biological effects}},
Journal = {{JOURNAL OF THE TAIWAN INSTITUTE OF CHEMICAL ENGINEERS}},
Year = {{2016}},
Volume = {{67}},
Pages = {{318-324}},
Month = {{OCT}},
Abstract = {{Bamboo was used as feedstock to prepare low-cost and effective
   biosorbents to remove a hydrophilic ionic liquid,
   1-butyl-3-methyl-imidazolium chloride ({[}BMIM]{[}Cl]), from aqueous
   solutions. The results showed that modified biochar (BB-K-N) had better
   adsorption performance than both pristine biochar (BB) and intermediate
   biochar (BB-K) toward {[}BMIM]{[}0]. Characterization of `the
   biosorbents showed that BB-K-N had high surface areas, large pore
   volume, large micropore volume, and abundant oxygen containing
   functional groups. Microporous structure provided appropriate pore
   space, polar groups on adsorbent surface displayed strong interactions
   with the adsorbate, which facilitated adsorption of {[}BMIM{[}Cl]. The
   sorption experimental data of BB-K-N were described the best by the
   pseudo-second order model and the Freundlich model. In addition to their
   good sorptive properties, the exhausted adsorbents were regenerated
   effectively by 0.1 mol/l HCl. Furthermore, adsorption of {[}BMIM]{[}Cl]
   on the modified biochar greatly reduced its biological toxicity towards
   seedlings growth. The seedings growth of {[}BMIM]{[}Cl]-laden BB-K-N
   group were similar to that of control groups. (C) 2016 Taiwan Institute
   of Chemical Engineers. Published by Elsevier B.V. All rights reserved.}},
