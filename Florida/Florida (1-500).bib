@article{ ISI:000385470000012,
Author = {Welte, Michael and Barhoumi, Rafik and Zbinden, Adrian and Scheffe,
   Jonathan R. and Steinfeld, Aldo},
Title = {{Experimental Demonstration of the Thermochemical Reduction of Ceria in a
   Solar Aerosol Reactor}},
Journal = {{INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH}},
Year = {{2016}},
Volume = {{55}},
Number = {{40}},
Pages = {{10618-10625}},
Month = {{OCT 12}},
Abstract = {{We report on the experimental demonstration of an aerosol solar reactor
   for the thermal reduction of ceria, as part of a thermochemical redox
   cycle for splitting H2O and CO2. The concept utilizes a cavityreceiver
   enclosing an array of alumina tubes, each containing a downward
   gravity-driven aerosol flow of ceria particles countercurrent to an
   inert sweep gas flow for intrinsic separation of reduced ceria and
   oxygen. A 2 kWth lab-scale prototype with a single tube was tested under
   radiative fluxes approaching 4000 suns, yielding reaction extents of up
   to 53\% of the thermodynamic equilibrium at 1919 K within residence
   times below 1 s. Upon thermal redox cycling, fresh primary particles of
   2.44 mu m mean size initially formed large agglomerates of 1000 mu m
   mean size, then sintered into stable particles of 150 mu m mean size.
   The reaction extent was primarily limited by heat transfer for large
   particles/agglomerates (mean size > 200 mu m) and by the gas phase
   advection of product O-2 for smaller particles.}},
