@article{ ISI:000385601500005,
Author = {Ache, Barry W. and Hein, Andrew M. and Bobkov, Yuriy V. and Principe,
   Jose C.},
Title = {{Smelling Time: A Neural Basis for Olfactory Scene Analysis}},
Journal = {{TRENDS IN NEUROSCIENCES}},
Year = {{2016}},
Volume = {{39}},
Number = {{10}},
Pages = {{649-655}},
Month = {{OCT}},
Abstract = {{Behavioral evidence from phylogenetically diverse animals and from
   humans suggests that, by extracting temporal information inherent in the
   olfactory signal, olfaction is more involved in interpreting space and
   time than heretofore imagined. If this is the case, the olfactory system
   must have neural mechanisms capable of encoding time at intervals
   relevant to the turbulent odor world in which many animals live. Here,
   we review evidence that animals can use populations of rhythmically
   active or `bursting' olfactory receptor neurons (bORNs) to extract and
   encode temporal information inherent in natural olfactory signals. We
   postulate that bORNs represent an unsuspected neural mechanism through
   which time can be accurately measured, and that `smelling time'
   completes the requirements for true olfactory scene analysis.}},
