@article{ ISI:000390825800013,
Author = {Wen, Xianfei and Yang, Haori},
Title = {{Simulation of delayed gamma-ray emission following photofission
   reactions induced by pulsed bremsstrahlung x-rays using MCNPX and
   experimental validation}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
Year = {{2016}},
Volume = {{840}},
Pages = {{102-106}},
Month = {{DEC 21}},
Abstract = {{There is a great demand to develop non-destructive techniques to
   identify and quantify Special Nuclear Materials (SNM) in homeland
   security and nuclear safeguards applications. Passive assay could be
   extremely challenging in some scenarios. Active interrogation technique
   based on photofission has been identified as one of the promising
   approaches. In radiation detection system design based on such
   technique, it is highly desired to have abilities to accurately and
   efficiently simulate delayed gamma-rays emitted from photofission
   reactions. In this work, simulation results were compared with
   measurement outcomes to demonstrate the capabilities and limitations of
   the code MCNPX 2.7.0 in the simulation of delayed gamma-rays from
   photofission of uranium and plutonium samples. First, high-energy
   delayed gamma-rays (Ey-2.7-4.5 MeV) from photofission of U-238 were
   simulated and validated against the energy spectra measured in between
   linac pulses. Second, low-energy delayed gamma-ray spectra (Ey-0.6-2.7
   MeV) measured with a list-mode system after irradiation of Pu-239 were
   used in the validation.}},
