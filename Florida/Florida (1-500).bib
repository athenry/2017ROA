@article{ ISI:000380414600010,
Author = {Kleiser, Geremy and Revil-Baudard, Benoit and Pasiliao, Crystal L.},
Title = {{High strain-rate plastic deformation of molybdenum: Experimental
   investigation, constitutive modeling and validation using impact tests}},
Journal = {{INTERNATIONAL JOURNAL OF IMPACT ENGINEERING}},
Year = {{2016}},
Volume = {{96}},
Pages = {{116-128}},
Month = {{OCT}},
Abstract = {{In this paper, an experimental study on the quasi-static behavior and
   dynamic behavior of a polycrystalline molybdenum material is presented.
   Due to the material's limited tensile ductility, successfully acquiring
   data for impact conditions is very challenging. For the first time,
   Taylor impact tests were successfully conducted on this material for
   impact velocities in the range of 140-165 m/s. For impact velocities
   beyond this range, the very high tensile pressures generated in the
   specimen immediately after impact lead to failure. A constitutive model
   accounting for the key features of the Mo plastic behavior, i.e. its
   tension-compression asymmetry and plastic anisotropy was developed. An
   implicit solver was used to simulate the impact deformation. A good
   agreement was obtained between predictions and experimental outlines of
   the specimens. Furthermore, it was shown that the model can be used to
   gain understanding of the dynamic deformation process in terms of time
   evolution of the pressure, the extent of the plastically deformed zone,
   distribution of the local plastic strain rates, and when the transition
   to quasi stable deformation occurs. (C) 2016 Elsevier Ltd. All rights
   reserved.}},
