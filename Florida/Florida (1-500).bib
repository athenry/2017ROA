@article{ ISI:000391457500023,
Author = {Ahn, Shihyun and Ren, F. and Patrick, Erin and Law, Mark E. and Pearton,
   S. J. and Kuramata, Akito},
Title = {{Deuterium incorporation and diffusivity in plasma-exposed bulk Ga2O3}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2016}},
Volume = {{109}},
Number = {{24}},
Month = {{DEC 12}},
Abstract = {{Deuterium incorporation depths of similar to 0.13-0.65 mu m were
   obtained in bulk, single-crystal Ga2O3 during exposure to H-2 plasmas
   for 0.5 h at 100-270 degrees C. The data were fit using the Florida
   Object Oriented Process Simulator simulation code. The estimated
   diffusivities were in the range of 2.3 x 10(-14)-6 x 10(-13) cm(2)/V.s
   over this temperature range. The activation energy for diffusion was
   0.30 +/- 0.08 eV, suggesting that the diffusing deuterium migrates as an
   interstitial. The solubility of the deuterium exhibited an activation
   energy of 0.35 eV. Subsequent annealing at 500 degrees C removed similar
   to 85\% of the deuterium out of the Ga2O3, with an activation energy of
   1.35 eV. This indicates that the outdiffusion is mediated through
   defects or molecule formation. The thermal stability of deuterium
   retention is lower (similar to 150 degrees C shift to lower
   temperatures) than when the deuterium is incorporated in the Ga2O3 by
   direct ion implantation, due to trapping at residual damage in the
   latter case. The incorporation depths of deuterium in the Ga2O3 are
   significantly less than in bulk ZnO under the same conditions, where we
   observed incorporation to similar to 25 mu m. Published by AIP
   Publishing.}},
