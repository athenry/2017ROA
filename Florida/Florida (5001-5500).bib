@article{ ISI:000308328900022,
Author = {Park, Zivin and Cho, Chun-Hyung},
Title = {{Size reduction and multi-resonance effects of slotted single-layer
   edge-fed patch antennas}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2012}},
Volume = {{61}},
Number = {{4}},
Pages = {{623-625}},
Month = {{AUG}},
Abstract = {{In this work, easy to fabricate edge-fed, single-substrate, slotted,
   square-patch antennas were designed, fabricated, and measured. Effects
   of slots on patch antenna, such as the size reduction effect and the
   multi-resonance effect, are demonstrated by examples. Measurements show
   that up to four additional resonant frequencies below the original patch
   antenna's TM020 frequency are obtainable and that the size of the
   antenna can be reduced to less than approximately 40\% its original
   size. The size reduction property of the proposed antenna may be
   suitable for miniaturized planar and conformal antennas that are needed
   for personal communication devices, and the multifrequency property of
