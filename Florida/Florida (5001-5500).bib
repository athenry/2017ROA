@article{ ISI:000303876300012,
Author = {Argibay, N. and Sawyer, W. G.},
Title = {{Frictional Voltammetry with Copper}},
Journal = {{TRIBOLOGY LETTERS}},
Year = {{2012}},
Volume = {{46}},
Number = {{3}},
Pages = {{337-342}},
Month = {{JUN}},
Abstract = {{This paper presents an experimental study correlating frictional
   behavior with in situ voltammetry for a unidirectional sliding contact
   between a hemispherical tipped alumina probe and a flat rotating copper
   counterface (maximum Hertzian contact pressure of 68 MPa and sliding
   speed of 10 mm/s). The contact was immersed in an aqueous 0.1 M Na2CO3
   solution (pH similar to 11) where the copper counterface acted as the
   working electrode in a potentiostat controlled three-electrode cell; a
   coiled Pt wire was used as the counter electrode and a saturated calomel
   electrode (SCE) as the reference. Clear and reproducible trends were
   found between friction coefficient and published data suggesting the
   onset of particular redox reactions, graphically presented in a
   frictional voltammetry plot. At anodic potentials primarily associated
   with the formation of copper(I) oxide (Cu2O) (V vs SCE similar to
   a'0.25), the measured friction coefficient was in the range mu similar
   to 0.4-0.5. At cathodic potentials primarily associated with the
   formation of CuO, Cu(OH)(2), and CuCO3 (V vs SCE similar to a'0.10), the
   friction coefficient transitions to the range mu similar to 0.7-1.0. At
   sustained cathodic potentials associated with reduction of the native
   copper oxide, Cu2O, (V vs SCE similar to a'0.65), the friction
   coefficient is observed to fluctuate between mu similar to 0.2 and 0.5,
   arguably a result of exposure of bare copper due to non-uniform
   reduction (fractional coverage) of Cu2O.}},
