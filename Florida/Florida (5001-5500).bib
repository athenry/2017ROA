@article{ ISI:000306017700011,
Author = {Szymczak, Piotr and Ladd, Anthony J. C.},
Title = {{Reactive-infiltration instabilities in rocks. Fracture dissolution}},
Journal = {{JOURNAL OF FLUID MECHANICS}},
Year = {{2012}},
Volume = {{702}},
Pages = {{239-264}},
Month = {{JUL 10}},
Abstract = {{A reactive fluid dissolving the surface of a uniform fracture will
   trigger an instability in the dissolution front, leading to spontaneous
   formation of pronounced well-spaced channels in the surrounding rock
   matrix. Although the underlying mechanism is similar to the wormhole
   instability in porous rocks there are significant differences in the
   physics, due to the absence of a steadily propagating reaction front. In
   previous work we have described the geophysical implications of this
   instability in regard to the formation of long conduits in soluble
   rocks. Here we describe a more general linear stability analysis,
   including axial diffusion, transport-limited dissolution, nonlinear
   kinetics, and a finite-length system.}},
