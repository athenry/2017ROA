@article{ ISI:000308511400001,
Author = {Chun, So Yeon and Shapiro, Alexander and Uryasev, Stan},
Title = {{Conditional Value-at-Risk and Average Value-at-Risk: Estimation and
   Asymptotics}},
Journal = {{OPERATIONS RESEARCH}},
Year = {{2012}},
Volume = {{60}},
Number = {{4}},
Pages = {{739-756}},
Month = {{JUL-AUG}},
Abstract = {{We discuss linear regression approaches to the estimation of
   law-invariant conditional risk measures. Two estimation procedures are
   considered and compared; one is based on residual analysis of the
   standard least-squares method, and the other is in the spirit of the
   M-estimation approach used in robust statistics. In particular,
   value-at-risk and average value-at-risk measures are discussed in
   detail. Large sample statistical inference of the estimators is derived.
   Furthermore, finite sample properties of the proposed estimators are
   investigated and compared with theoretical derivations in an extensive
   Monte Carlo study. Empirical results on the real data (different
   financial asset classes) are also provided to illustrate the performance
   of the estimators.}},
