@inproceedings{ ISI:000346496101096,
Author = {Graves, Eric and Wong, Tan F.},
Book-Group-Author = {{IEEE}},
Title = {{Equating the achievable exponent region to the achievable entropy region
   by partitioning the source}},
Booktitle = {{2014 IEEE INTERNATIONAL SYMPOSIUM ON INFORMATION THEORY (ISIT)}},
Series = {{IEEE International Symposium on Information Theory}},
Year = {{2014}},
Pages = {{1346-1350}},
Note = {{IEEE International Symposium on Information Theory (ISIT), Honolulu, HI,
   JUN 29-JUL 04, 2014}},
Organization = {{IEEE; IEEE Informat Theory Soc; NSF; Qualcomm; IBM Res; LG}},
Abstract = {{In this paper we investigate the image size characterization problem. We
   show that any arbitrary source set may be decomposed into sets whose
   image size characterization is the same as its entropy characterization.
   We also show that the number of these sets required is small enough that
   one may consider that from a coding perspective the achievable entropy
   region and achievable exponent region are equal. This has an impact on
   many source networks and network problems whose solution heretofore
