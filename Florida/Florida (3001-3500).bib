@article{ ISI:000335643700092,
Author = {Oniku, Ololade D. and Ryiz, Paul V. and Garraud, Alexandra and Arnold,
   David P.},
Title = {{Imprinting of fine-scale magnetic patterns in electroplated hard
   magnetic films using magnetic foil masks}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2014}},
Volume = {{115}},
Number = {{17}},
Month = {{MAY 7}},
Note = {{58th Annual Conference on Magnetism and Magnetic Materials, Denver, CO,
   NOV 04-08, 2013}},
Abstract = {{This paper presents a batch-fabrication approach to imprint complex
   magnetic pole patterns (perpendicular north/south poles) into hard
   magnetic films using fine-scale, laser-machined iron foil ``masks.{''}
   In this approach, pulsed magnetic fields varying between 0.1 and 1.0 T
   are used in conjunction with the field-concentrating effect of the iron
   foil masks to selectively reverse the magnetization of selected regions
   of 10-mu m-thick Co80Pt20 films (Br 1 T, Hci 340 kA/m). Simple stripe
   array patterns are systematically studied both experimentally and via
   simulation, achieving patterned feature sizes (poles) down to 30 mu m.
   Double exposures are also explored to create checkerboard patterns from
   simple stripe-like masks. Finally, masks with more complex patterns are
   used to demonstrate the viability of the approach to imprint arbitrary
   features. (C) 2014 AIP Publishing LLC.}},
