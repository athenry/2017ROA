@article{ ISI:000331461600038,
Author = {Zeng, Jijiao and Tong, Zhaohui and Wang, Letian and Zhu, J. Y. and
   Ingram, Lonnie},
Title = {{Isolation and structural characterization of sugarcane bagasse lignin
   after dilute phosphoric acid plus steam explosion pretreatment and its
   effect on cellulose hydrolysis}},
Journal = {{BIORESOURCE TECHNOLOGY}},
Year = {{2014}},
Volume = {{154}},
Pages = {{274-281}},
Month = {{FEB}},
Abstract = {{The structure of lignin after dilute phosphoric acid plus steam
   explosion pretreatment process of sugarcane bagasse in a pilot scale and
   the effect of the lignin extracted by ethanol on subsequent cellulose
   hydrolysis were investigated. The lignin structural changes caused by
   pretreatment were identified using advanced nondestructive techniques
   such as gel permeation chromatography (GPC), quantitative C-13, and 2-D
   nuclear magnetic resonance (NMR). The structural analysis revealed that
   ethanol extractable lignin preserved basic lignin structure, but had
   relatively lower amount of beta-O-4 linkages, syringyl/guaiacyl units
   ratio (S/G), p-coumarate/ferulate ratio, and other ending structures.
   The results also indicated that approximately 8\% of mass weight was
   extracted by pure ethanol. The bagasse after ethanol extraction had an
   approximate 22\% higher glucose yield after enzyme hydrolysis compared
   to pretreated bagasse without extraction. (C) 2013 Elsevier Ltd. All
   rights reserved.}},
