@article{ ISI:000337183500003,
Author = {Boginski, Vladimir and Butenko, Sergiy and Shirokikh, Oleg and
   Trukhanov, Svyatoslav and Gil Lafuente, Jaime},
Title = {{A network-based data mining approach to portfolio selection via weighted
   clique relaxations}},
Journal = {{ANNALS OF OPERATIONS RESEARCH}},
Year = {{2014}},
Volume = {{216}},
Number = {{1}},
Pages = {{23-34}},
Month = {{MAY}},
Abstract = {{We introduce a new network-based data mining approach to selecting
   diversified portfolios by modeling the stock market as a network and
   utilizing combinatorial optimization techniques to find maximum-weight
   s-plexes in the obtained networks. The considered approach is based on
   the weighted market graph model, which is used for identifying clusters
   of stocks according to a correlation-based criterion. The proposed
   techniques provide a new framework for selecting profitable diversified
   portfolios, which is verified by computational experiments on historical
   data over the past decade. In addition, the proposed approach can be
   used as a complementary tool for narrowing down a set of ``candidate{''}
   stocks for a diversified portfolio, which can potentially be analyzed
   using other known portfolio selection techniques.}},
