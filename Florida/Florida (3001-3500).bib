@article{ ISI:000347162400009,
Author = {Jones, C. D. and Morgan, K. T. and Porter, C. H. and Wu, J. Z. and
   Fraisse, C. W. and Zotarelli, L.},
Title = {{ADDITION OF A TWO-DIMENSIONAL WATER BALANCE MODEL TO THE DSSAT-CSM}},
Journal = {{TRANSACTIONS OF THE ASABE}},
Year = {{2014}},
Volume = {{57}},
Number = {{6}},
Pages = {{1643-1656}},
Abstract = {{There is increasing pressure for more efficient production of
   agricultural commodities. To this end, cropping systems models are
   useful tools for identifying efficient production practices and
   understanding complex relationships between management, system drivers,
   production, and environmental consequences. In this research, the
   Decision Support System for Agrotechnology Transfer Cropping Systems
   Model (DSSAT-CSM) was amended to allow for water-limited simulation
   under cultural practices common for intensive vegetable production,
   including drip irrigation under plastic-mulched raised beds. A
   two-dimensional water balance model was developed with an emphasis on
   creating a practical model with flexible input requirements and
   reasonable computation time. Model outputs were evaluated against lab
   and field measurements to assess the model's ability to simulate soil
   water dynamics under drip irrigation regimens. Evaluation of a parameter
   estimation method designed to utilize the standard DSSAT soil inputs
   indicated that the method provided unbiased characterization of soil
   water retention curves ( SWRC), with an overall root mean square error (
   RMSE) of 0.0458 cm(3) cm(-3). However, SWRCs were more accurately
   characterized when estimated from soil water measurements from a wider
   range of soil water potentials, with an overall RMSE of 0.0277 cm(3)
   cm(-3). Thus, detailed measurement of soil hydraulic properties was
   recommended to improve parameter estimation and simulation of soil water
   dynamics. Evaluation of soil water simulations against field-measured
   values demonstrated the model's ability to adequately simulate soil
   water flow patterns under drip irrigation for a fairly uniform soil
   under typical vegetable production management practices, with overall
   RMSE values of 0.0257 to 0.0378 cm(3) cm(-3) and 0.0245 to 0.0254 cm(3)
   cm(-3) using the simplified and the more detailed parameter estimation
   methods, respectively.}},
