@article{ ISI:000342612600008,
Author = {Ross, Brandon E. and Willis, Michael D. and Hamilton, H. R. and
   Consolazio, Gary R.},
Title = {{Comparison of details for controlling end-region cracks in precast,
   pretensioned concrete I-girders}},
Journal = {{PCI JOURNAL}},
Year = {{2014}},
Pages = {{96-108}},
Month = {{SPR}},
Abstract = {{This paper reports on a study comparing four detailing schemes for
   controlling end-region web-splitting cracks: end reinforcement per
   AASHTO LRFD specifications, large-diameter end reinforcement, vertical
   end-region posttensioning, and partial debonding of 45\% of prestressing
   strands. Crack locations, lengths, and widths in 63 in. (1600 mm) deep
   Florida I-beams (FIB-63) were monitored during transfer and in the
   months following prestress transfer. Crack data were quantified and
   compared to determine the relative effectiveness of each scheme in
   controlling web cracking. Specimens were then load tested to determine
   the effect of each scheme on ultimate capacity. All three modified
   details resulted in reduced web cracking compared with the standard
   detail.}},
