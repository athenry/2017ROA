@inproceedings{ ISI:000358572100065,
Author = {Baba, Yoshihiro and Rakov, Vladmir A.},
Book-Group-Author = {{IEEE}},
Title = {{Applications of the FDTD Method to Lightning Electromagnetic Pulse and
   Surge Simulations}},
