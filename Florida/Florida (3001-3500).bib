@article{ ISI:000330598600075,
Author = {McClain, K. Randall and O'Donohue, Christopher and Koley, Arijit and
   Bonsu, Richard O. and Abboud, Khalil A. and Revelli, Joseph C. and
   Anderson, Timothy J. and McElwee-White, Lisa},
Title = {{Tungsten Nitrido Complexes as Precursors for Low Temperature Chemical
   Vapor Deposition of WNxCy Films as Diffusion Barriers for Cu
   Metallization}},
Journal = {{JOURNAL OF THE AMERICAN CHEMICAL SOCIETY}},
Year = {{2014}},
Volume = {{136}},
Number = {{4}},
Pages = {{1650-1662}},
Month = {{JAN 29}},
Abstract = {{Tungsten nitrido complexes of the form WN(NR2)(3) {[}R = combinations of
   Me, Et, Pr-i, `' Pr] have been synthesized as precursors for the
   chemical vapor deposition of WNxCy, a material of interest for diffusion
   barriers in Cu-metallized integrated circuits. These precursors bear a
   fully nitrogen coordinated ligand environment and a nitrido moiety (W 
   N) designed to minimize the temperature required for film deposition.
   Mass spectrometry and solid state thermolysis of the precursors
   generated common fragments by loss of free dialkylamines from monomeric
   and dimeric tungsten species. DFT calculations on WN(NMe2)(3) indicated
   the lowest gas phase energy pathway for loss of HNMe2 to be beta-H
   transfer following formation of a nitrido bridged dimer. Amorphous films
   of WNxCy were grown from WN(NMe2)(3) as a single source precursor at
   temperatures ranging from 125 to 650 degrees C using aerosol-assisted
   chemical vapor deposition (AACVD) with pyridine as the solvent. Films
   with stoichiometry approaching W2NC were grown between 150 and 450
   degrees C, and films grown at 150 degrees C were highly smooth, with a
   RMS roughness of 0.5 nm. In diffusion barrier tests, 30 nm of film
   withstood Cu penetration when annealed at 500 degrees C for 30 min.}},
