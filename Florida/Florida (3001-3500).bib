
@article{ ISI:000335643700542,
Author = {Oniku, Ololade D. and Qi, Bin and Arnold, David P.},
Title = {{Electroplated L1(0) CoPt thick-film permanent magnets}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2014}},
Volume = {{115}},
