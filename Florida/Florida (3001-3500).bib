@article{ ISI:000336410700009,
Author = {Liu, Jian and Tao, Mengxian and Ma, Chaoqun and Wen, Fenghua},
Title = {{Utility indiffierence pricing of convertible bonds}},
Journal = {{INTERNATIONAL JOURNAL OF INFORMATION TECHNOLOGY \& DECISION MAKING}},
Year = {{2014}},
Volume = {{13}},
Number = {{2}},
Pages = {{429-444}},
Month = {{MAR}},
Abstract = {{We propose a pricing model for convertible bonds based on the
   utility-indiffierence method and get access to the empirical results by
   use of Information Technology. By using the stochastic control theory,
   the general expression of utility indiffierence price on convertible
   bonds is obtained under the CIR interest rate model. Furthermore, using
   the proposed theoretical model, we present an empirical pricing study of
   China's market, using three convertible bonds and more than 70 months of
   daily market prices. The parameters value is estimated by the maximum
   likelihood method, and the prices of convertible bonds are simulated by
   the Monte Carlo approach. The empirical results indicate that the
   theoretical prices are higher than the actual market prices 0.24-4.58\%,
   and the utility indiffierence prices are better than the Black-Scholes
   (BS) prices.}},
