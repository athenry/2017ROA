@inproceedings{ ISI:000359818000109,
Author = {Jenzri, Hamdi and Frigui, Hichem and Gader, Paul},
Book-Group-Author = {{IEEE}},
Title = {{Robust Context Dependent Spectral Unmixing}},
Booktitle = {{2014 22ND INTERNATIONAL CONFERENCE ON PATTERN RECOGNITION (ICPR)}},
