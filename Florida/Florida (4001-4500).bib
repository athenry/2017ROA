@article{ ISI:000315361500001,
Author = {Safak, I. and Allison, M. A. and Sheremet, A.},
Title = {{Floc variability under changing turbulent stresses and sediment
   availability on a wave energetic muddy shelf}},
Journal = {{CONTINENTAL SHELF RESEARCH}},
Year = {{2013}},
Volume = {{53}},
Pages = {{1-10}},
Month = {{FEB 1}},
Abstract = {{The interplay between cohesive sediment flocculation, turbulence, and
   sediment availability is studied using field observations collected on
   the muddy Atchafalaya inner shelf, Louisiana, USA. Observations were
   made at two sites, characterized by surficial sediments of different
   grain size. At the coarse-sediment site, a negative correlation between
   floc size and suspended sediment concentration was observed for the
   first time in a field experiment under energetic wave conditions. This
   agrees with previous laboratory studies and field observations, and
   suggests that strong near-bed turbulence associated with significant
   sediment resuspension also results in the breaking of macro-flocs
   (230-mu m mode). At the fine-sediment site, the relative abundance of
   clay and fine silt available for flocculation appears to promote
   persistent macro-flocs of approximately 300-mu m. The observations
   support the assumption that sediment supply and surficial sediment
   properties, in addition to turbulence and suspended sediment
   concentration, play an important role in flocculation. (C) 2012 Elsevier
   Ltd. All rights reserved.}},
