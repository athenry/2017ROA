@article{ ISI:000321884100020,
Author = {Grumstrup, Erik M. and Chen, Zhuo and Vary, Ryan P. and Moran, Andrew M.
   and Schanze, Kirk S. and Papanikolas, John M.},
Title = {{Frequency Modulated Femtosecond Stimulated Raman Spectroscopy of
   Ultrafast Energy Transfer in a Donor-Acceptor Copolymer}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY B}},
Year = {{2013}},
Volume = {{117}},
Number = {{27}},
Pages = {{8245-8255}},
Month = {{JUL 11}},
Abstract = {{A Raman-pump frequency modulation scheme and an automated
   signal-processing algorithm are developed for improved collection of
   time-resolved femtosecond stimulated Raman spectra. Together, these two
   advancements remove the broad background signals endemic to FSRS
   measurements and retrieve,signals with high sensitivity. We apply this
   frequency-modulated femtosecond stimulated Raman spectroscopy (FM-FSRS)
   to the characterization of ultrafast energy transport in a copolymer
