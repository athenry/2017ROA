@article{ ISI:000318354500014,
Author = {Lee, Seoung Ho and Parthasarathy, Anand and Schanze, Kirk S.},
Title = {{A Sensitive and Selective Mercury(II) Sensor Based on Amplified
   Fluorescence Quenching in a Conjugated Polyelectrolyte/Spiro-Cyclic
   Rhodamine System}},
Journal = {{MACROMOLECULAR RAPID COMMUNICATIONS}},
Year = {{2013}},
Volume = {{34}},
Number = {{9, SI}},
Pages = {{791-795}},
Month = {{MAY 14}},
Abstract = {{A novel fluorescence-based sensor for mercury(II) ion is described
   consisting of a water-soluble, anionic-conjugated polyelectrolyte (CPE)
   and spiro-cyclic rhodamine derivative (1). A solution containing the CPE
   (0.1 x 106 M repeat unit concentration) and 1 (1 x 106 M concentration)
   in aqueous solution exhibits a sensitive and selective turn-off
   fluorescence response to Hg(II). The SternVolmer quenching constant for
   quenching of the fluorescence of the system by Hg(II) is approximate to
   1.5 x 107 M1, and Hg(II) can be sensed with a detection limit <10 x 109
   M. A mechanism for the sensor response is proposed and it consists of
   efficient and selective complex formation between 1 and Hg(II), along
   with a very high association between the 1-Hg(II) complex and the
   anionic CPE.}},
