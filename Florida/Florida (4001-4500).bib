@article{ ISI:000320944500045,
Author = {Tran Huu Thang and Baba, Yoshihiro and Nagaoka, Naoto and Ametani,
   Akihiro and Itamoto, Naoki and Rakov, Vladimir A.},
Title = {{FDTD Simulation of Insulator Voltages at a Lightning-Struck Tower
   Considering Ground-Wire Corona}},
Journal = {{IEEE TRANSACTIONS ON POWER DELIVERY}},
Year = {{2013}},
Volume = {{28}},
Number = {{3}},
Pages = {{1635-1642}},
Month = {{JUL}},
Abstract = {{In this paper, a simplified model of corona discharge for the
   finite-difference time-domain (FDTD) computations has been applied to
   the analysis of transient voltages across insulators of a transmission
   line struck by lightning. In the simulation, three 60-m towers,
   separated by 200 m, with one overhead ground wire and three-phase
   conductors are employed. The progression of corona streamers from the
   ground wire is represented as the radial expansion of cylindrical
   conducting region around the wire. On the basis of the computed results,
   the effect of corona discharge at the ground wire on transient insulator
   voltages is examined. The insulator voltages are reduced by corona
   discharge on the ground wire. The reduction of insulator-voltage peak
   due to the ground-wire corona is not very significant: the upper-,
   middle-, and lower-phase-voltage peaks are reduced by 15, 14, and 13\%
   for a positive stroke with 50-kA-peak and 3-mu s-risetime current, and
   those for the negative stroke with the same current waveform parameters
   are reduced by 10, 9, and 8\%, respectively.}},
