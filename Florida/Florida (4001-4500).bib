@article{ ISI:000314629600032,
Author = {Lee, Jennifer A. and Kozikowski, Raymond T. and Sorg, Brian S.},
Title = {{Combination of spectral and fluorescence imaging microscopy for
   wide-field in vivo analysis of microvessel blood supply and oxygenation}},
