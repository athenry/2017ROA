@article{ ISI:000322153700001,
Author = {Arce, Stephen Hugo and Wu, Pei-Hsun and Tseng, Yiider},
Title = {{Fast and accurate automated cell boundary determination for fluorescence
   microscopy}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2013}},
Volume = {{3}},
Month = {{JUL 24}},
Abstract = {{Detailed measurement of cell phenotype information from digital
   fluorescence images has the potential to greatly advance biomedicine in
   various disciplines such as patient diagnostics or drug screening. Yet,
   the complexity of cell conformations presents a major barrier preventing
   effective determination of cell boundaries, and introduces measurement
   error that propagates throughout subsequent assessment of cellular
   parameters and statistical analysis. State-of-the-art image segmentation
   techniques that require user-interaction, prolonged computation time and
   specialized training cannot adequately provide the support for high
   content platforms, which often sacrifice resolution to foster the speedy
   collection of massive amounts of cellular data. This work introduces a
   strategy that allows us to rapidly obtain accurate cell boundaries from
   digital fluorescent images in an automated format. Hence, this new
   method has broad applicability to promote biotechnology.}},
