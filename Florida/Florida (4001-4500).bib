@article{ ISI:000325511600010,
Author = {Schiavone, D. F. and Teixeira, A. A. and Bucklin, R. A. and Sargent, S.
   A.},
Title = {{DESIGN AND PERFORMANCE EVALUATION OF A SOLAR CONVECTION DRYER FOR DRYING
   TROPICAL FRUIT}},
Journal = {{APPLIED ENGINEERING IN AGRICULTURE}},
Year = {{2013}},
Volume = {{29}},
Number = {{3}},
Pages = {{391-401}},
Month = {{MAY}},
Abstract = {{This article describes the design, construction, and performance
   evaluation of a natural-convection, solar dryer capable of producing
   dried mango slices in rural Haitian communities. A sorption isotherm was
   developed for the Tommy Atkins mango variety to determine target levels
   of moisture content needed to achieve required levels of water activity.
   The solar dryer was constructed and operated in Gainesville, Florida,
   during the summer of 2011. Performance evaluation revealed cabinet
   temperatures between 37.0 degrees C and 43.7 degrees C during sunlight
   hours depending on airflow and loading conditions. Damper restriction
   was shown to effectively control flow rates to adjust temperatures
   within the dryer Operating capacity of the dryer was 9.5 kg of fresh
   mango slices per 36 h, lowering the moisture content from 84.5\% (w.b.)
   to 10.3\% (w.b.) under a continuous mode of operation. The collector
   efficiency, pick-up efficiency, and system efficiency were 26.7\%,
   20.4\%, and 25.9\% respectively.}},
