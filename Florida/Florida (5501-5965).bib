@inproceedings{ ISI:000324163200018,
Author = {Landy, Aaron and Stitt, Greg},
Book-Group-Author = {{ACM}},
Title = {{A Low-Overhead Interconnect Architecture for Virtual Reconfigurable
   Fabrics}},
Booktitle = {{CASES'12: PROCEEDINGS OF THE 2012 ACM INTERNATIONAL CONFERENCE ON
   COMPILERS, ARCHITECTURES AND SYNTHESIS FOR EMBEDDED SYSTEMS}},
Year = {{2012}},
Pages = {{111-119}},
