@article{ ISI:000299367500003,
Author = {Beaujuge, Pierre M. and Vasilyeva, Svetlana V. and Liu, David Y. and
   Ellinger, Stefan and McCarley, Tracy D. and Reynolds, John R.},
Title = {{Structure-Performance Correlations in Spray-Processable Green
   Dioxythiophene-Benzothiadiazole Donor-Acceptor Polymer Electrochromes}},
Journal = {{CHEMISTRY OF MATERIALS}},
