@inproceedings{ ISI:000309229600023,
Author = {Guo, Linke and Zhang, Chi and Sun, Jinyuan and Fang, Yuguang},
Book-Group-Author = {{IEEE}},
Title = {{PAAS: A Privacy-Preserving Attribute-based Authentication System for
   eHealth Networks}},
Booktitle = {{2012 IEEE 32ND INTERNATIONAL CONFERENCE ON DISTRIBUTED COMPUTING SYSTEMS
   (ICDCS)}},
Series = {{IEEE International Conference on Distributed Computing Systems}},
Year = {{2012}},
