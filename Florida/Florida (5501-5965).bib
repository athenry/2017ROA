Author = {Ling, Yue and Haselbacher, Andreas and Balachandar, S.},
Editor = {{Elert, ML and Buttler, WT and Borg, JP and Jordan, JL and Vogler, TJ}},
Title = {{IMPORTANCE OF UNSTEADY FORCE AND HEATING TO PARTICLE INTERACTION WITH
   SHOCK/DETONATION WAVES}},
Booktitle = {{SHOCK COMPRESSION OF CONDENSED MATTER - 2011, PTS 1 AND 2}},
Series = {{AIP Conference Proceedings}},
Year = {{2012}},
Volume = {{1426}},
