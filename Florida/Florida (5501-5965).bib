@article{ ISI:000307648700022,
Author = {Antony, Abbin and Asthagiri, Aravind and Weaver, Jason F.},
Title = {{Pathways for C-H bond cleavage of propane sigma-complexes on PdO(101)}},
Journal = {{PHYSICAL CHEMISTRY CHEMICAL PHYSICS}},
