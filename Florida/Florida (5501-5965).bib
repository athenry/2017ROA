@inproceedings{ ISI:000304243100003,
Author = {Tang, Haixiong and Lin, Yirong and Sodano, Henry A.},
Editor = {{Goulbourne, NC and Ounaies, Z}},
Title = {{Ultra High Energy Density Nanocomposite Capacitors Using
   Surface-functionalized BaTiO3 Nanowires and PVDF-TrFE-CFE}},
Booktitle = {{BEHAVIOR AND MECHANICS OF MULTIFUNCTIONAL MATERIALS AND COMPOSITES 2012}},
