@inproceedings{ ISI:000324654500039,
Author = {Krieg, Michael and Mohseni, Kamran},
Book-Group-Author = {{ASME}},
Title = {{THRUST ENHANCEMENT FROM RADIAL VELOCITY IN SQUID INSPIRED THRUSTERS}},
Booktitle = {{PROCEEDINGS OF THE ASME 31ST INTERNATIONAL CONFERENCE ON OCEAN, OFFSHORE
   AND ARCTIC ENGINEERING, 2012, VOL 5}},
Year = {{2012}},
