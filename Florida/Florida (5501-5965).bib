@inproceedings{ ISI:000318702900184,
Author = {Nguyen, Nam P. and Thai, My T.},
Book-Group-Author = {{IEEE}},
Title = {{Finding Overlapped Communities in Online Social Networks with
   Nonnegative Matrix Factorization}},
Booktitle = {{2012 IEEE MILITARY COMMUNICATIONS CONFERENCE (MILCOM 2012)}},
Series = {{IEEE Military Communications Conference}},
Year = {{2012}},
Note = {{IEEE Military Communications Conference (MILCOM), Orlando, FL, OCT
   29-NOV 01, 2012}},
Organization = {{IEEE; AFCEA Int; IEEE Commun Soc}},
Abstract = {{In this work, we introduce two approaches, namely iSNMF and iANMF, for
   effectively identifying social communities using Nonnegative Matrix
   Factorization (NMF) with I-divergence as the cost function. Our
   approaches work by iteratively factorizing the nonnegative input matrix
   through derived multiplicative update rules. By doing so, we can not
   only extract meaningful overlapping communities via soft community
   assignments produced by NW, but also nicely handle all directed and
   undirected networks with or without weights. To validate the performance
   of our approaches, we extensively conduct experiments on both
   synthesized networks and real-world datasets in comparison with other
   NMF methods. Experimental results show that iSNMF is among the best
   efficient detection methods on reciprocity networks while iANMF
   outperforms current available methods on directed networks, especially
   in terms of detection quality.}},
