@article{ ISI:000315142200004,
Author = {Overman, A. R. and Howard, J. C.},
Title = {{MODEL ANALYSIS OF RESPONSE TO APPLIED NITROGEN BY COTTON}},
Journal = {{JOURNAL OF PLANT NUTRITION}},
Year = {{2012}},
Volume = {{35}},
Number = {{14}},
Pages = {{2118-2123}},
Abstract = {{The logistic model can be used to describe crop response to applied
   nutrients, such as nitrogen. It represents a three parameter model which
   is easy to use in practice. The model has been applied to numerous crops
   on different types of soils and under a variety of environmental
   conditions. In this article the model is applied to field data for
   cotton (Gossypium hirsutum L.) in Arkansas for the period 1988-1990.
   Average yields for the three year period are first used to estimate the
   exponential parameters of the model. Then, individual values for the
   linear parameter are estimated for each of the three years. Analysis of
   variance shows that the linear parameter is significantly different
   among years. The analysis shows that applied nitrogen to reach 50\% and
   90\% of maximum yield is 15 and 110 kg ha(-1), respectively.}},
