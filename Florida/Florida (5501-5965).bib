@article{ ISI:000299506300007,
Author = {Rebennack, Steffen and Flach, Bruno and Pereira, Mario V. F. and
   Pardalos, Panos M.},
Title = {{Stochastic Hydro-Thermal Scheduling Under CO2 Emissions Constraints}},
Journal = {{IEEE TRANSACTIONS ON POWER SYSTEMS}},
Year = {{2012}},
Volume = {{27}},
Number = {{1}},
Pages = {{58-68}},
Month = {{FEB}},
Abstract = {{Despite the uncertainty surrounding the design of a mechanism which is
   ultimately accepted by nations worldwide, the necessity to implement
   regulations to curb emissions of greenhouse gases on a global scale is
   consensual. The electricity sector plays a fundamental role in this
   puzzle and countries may soon have to revise their operating policy
   directives in order to make them compatible with additional constraints
   imposed by such regulations. We present a modeling approach for
   greenhouse gas emissions quotas which can be incorporated into a
   stochastic dual dynamic programming algorithm, commonly used to solve
   the hydro-thermal scheduling problem. Our approach is flexible and
   capable of accommodating a detailed representation of emissions and
   related constraints. A case study based on the Guatemalan power system
   exemplifies the potential effects of considering these restrictions.}},
