Author = {Rice, Jennifer A. and Gu, Changzhan and Li, Changzhi and Guan, Shanyue},
Editor = {{Tomizuka, M and Yun, CB and Lynch, JP}},
Title = {{A radar-based sensor network for bridge displacement measurements}},
Booktitle = {{SENSORS AND SMART STRUCTURES TECHNOLOGIES FOR CIVIL, MECHANICAL, AND
   AEROSPACE SYSTEMS 2012, PTS 1 AND 2}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
