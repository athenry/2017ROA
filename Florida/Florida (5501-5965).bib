@inproceedings{ ISI:000309162000334,
Author = {Cheng, Xiaoyu and Senior, David E. and Yoon, Yong-Kyu},
Book-Group-Author = {{IEEE}},
Title = {{A Compact 100 MHz to 7 GHz Frequency Equalizer Based on Distributed
   Passive Circuits}},
Booktitle = {{2012 IEEE 62ND ELECTRONIC COMPONENTS AND TECHNOLOGY CONFERENCE (ECTC)}},
