@inproceedings{ ISI:000324221200136,
Author = {Coppe, Alexandra and Pais, Matthew J. and Haftka, Raphael T. and Kim,
   Nam H.},
Book-Group-Author = {{ASME}},
Title = {{EQUIVALENT DAMAGE GROWTH PARAMETERS USING A SIMPLIFIED MODEL}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
   CONFERENCES AND COMPUTERS AND INFORMATION IN ENGINEERING CONFERENCE,
   2011, VOL 2, PTS A AND B}},
Year = {{2012}},
