@inproceedings{ ISI:000309406704127,
Author = {Zhang, Kai and Collins, Jr., Emmanuel G. and Barbu, Adrian},
Book-Group-Author = {{IEEE}},
Title = {{An Efficient Stochastic Clustering Auction for Heterogeneous Robot Teams}},
Booktitle = {{2012 IEEE INTERNATIONAL CONFERENCE ON ROBOTICS AND AUTOMATION (ICRA)}},
