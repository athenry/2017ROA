Editor = {{Waterland, J and Bouzid, HA}},
Title = {{NUMERICAL SIMULATION OF THE TRANSIENT PHASE CHANGE HEAT TRANSFER IN A
   PRESSURIZED WATER REACTOR CORE DURING A LOSS-OF-COOLANT ACCIDENT}},
Booktitle = {{PROCEEDINGS OF THE ASME PRESSURE VESSELS AND PIPING CONFERENCE, VOL 2:
