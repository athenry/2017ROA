@article{ ISI:000300919600001,
Author = {Yamamoto, Yutaka and Nagahara, Masaaki and Khargonekar, Pramod P.},
Title = {{A BRIEF OVERVIEW OF SIGNAL RECONSTRUCTION VIA SAMPLED-DATA H-infinity
