@article{ ISI:000299434300006,
Author = {Yamamoto, Yutaka and Nagahara, Masaaki and Khargonekar, Pramod P.},
Title = {{Signal Reconstruction via H-infinity Sampled-Data Control Theory-Beyond
   the Shannon Paradigm}},
Journal = {{IEEE TRANSACTIONS ON SIGNAL PROCESSING}},
Year = {{2012}},
Volume = {{60}},
Number = {{2}},
Pages = {{613-625}},
Month = {{FEB}},
Abstract = {{This paper presents a new method for signal reconstruction by leveraging
   sampled-data control theory. We formulate the signal reconstruction
   problem in terms of an analog performance optimization problem using a
   stable discrete-time filter. The proposed H-infinity performance
   criterion naturally takes intersample behavior into account, reflecting
   the energy distributions of the signal. We present methods for computing
   optimal solutions which are guaranteed to be stable and causal. Detailed
