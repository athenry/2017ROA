Title = {{Plastic deformation of high-purity alpha-titanium: model development and
   validation using the Taylor cylinder impact test}},
Booktitle = {{NUMISHEET 2016: 10TH INTERNATIONAL CONFERENCE AND WORKSHOP ON NUMERICAL
   SIMULATION OF 3D SHEET METAL FORMING PROCESSES, PTS A AND B}},
Series = {{Journal of Physics Conference Series}},
Year = {{2016}},
