Address = {{ONE ROGERS ST, CAMBRIDGE, MA 02142-1209 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Banerjee, A (Reprint Author), Univ Florida, Comp \& Informat Sci \& Engn Dept, Gainesville, FL 32611 USA.
   Banerjee, Arunava, Univ Florida, Comp \& Informat Sci \& Engn Dept, Gainesville, FL 32611 USA.}},
DOI = {{10.1162/NECO\_a\_00829}},
