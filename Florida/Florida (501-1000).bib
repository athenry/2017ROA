@inproceedings{ ISI:000400540800107,
Author = {Asadizanjani, Navid and Forte, Domenic and Tehranipoor, Mark},
Book-Group-Author = {{ASM International}},
Title = {{Non-Destructive Bond Pull and Ball Shear Failure Analysis Based on Real
   Structural Properties}},
