@article{ ISI:000378528100003,
Author = {Chen, Cheng and Zhang, Fengchao and Barras, Jamie and Althoefer, Kaspar
   and Bhunia, Swarup and Mandal, Soumyajit},
Title = {{Authentication of Medicines Using Nuclear Quadrupole Resonance
   Spectroscopy}},
Journal = {{IEEE-ACM TRANSACTIONS ON COMPUTATIONAL BIOLOGY AND BIOINFORMATICS}},
Year = {{2016}},
Volume = {{13}},
Number = {{3}},
Pages = {{417-430}},
Month = {{MAY-JUN}},
Abstract = {{The production and sale of counterfeit and substandard pharmaceutical
   products, such as essential medicines, is an important global public
   health problem. We describe a chemometric passport-based approach to
   improve the security of the pharmaceutical supply chain. Our method is
   based on applying nuclear quadrupole resonance (NQR) spectroscopy to
   authenticate the contents of medicine packets. NQR is a non-invasive,
   non-destructive, and quantitative radio frequency (RF) spectroscopic
   technique. It is sensitive to subtle features of the solid-state
   chemical environment and thus generates unique chemical fingerprints
   that are intrinsically difficult to replicate. We describe several
   advanced NQR techniques, including two-dimensional measurements,
   polarization enhancement, and spin density imaging, that further improve
   the security of our authentication approach. We also present
   experimental results that confirm the specificity and sensitivity of NQR
   and its ability to detect counterfeit medicines.}},
