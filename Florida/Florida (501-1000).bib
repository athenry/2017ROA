@article{ ISI:000376052500005,
Author = {Morrow, Wayne K. and Lee, Changmin and DenBaars, Steven P. and Ren, Fan
   and Pearton, Stephen J.},
Title = {{Role of graphene interlayers in mitigating degradation of Ni/Au ohmic
   contact morphology on p-type GaN}},
Journal = {{VACUUM}},
Year = {{2016}},
Volume = {{128}},
Pages = {{34-38}},
Month = {{JUN}},
Abstract = {{We report an investigation of the effect of graphene interlayers in
   maintaining good surface morphology in low resistance Ni/Au Ohmic
   contacts to p-GaN (similar to 10(18) cm(-3)) annealed at 600 degrees C.
   Two different thin-film contact metallizations were compared, namely 20
   nm Ni/200 nm Au with and without a graphene layer diffusion barrier
   placed between the metals. Raman spectroscopy measured at several spots
   indicated single layer graphene, but subsequent transmission electron
   microscopy indicated that more generally the graphene was several nm
   thick. The unannealed contacts showed specific contact resistances of
   2.1 x 10(-5) Omega-cm(2) and 3.1 x 10(-4) Omega-cm(2) for Ni/Au and
   Ni/Graphene/Au, respectively. After rapid thermal annealing at 600
   degrees C for 60 s in a flowing N-2 ambient, X-Ray Photoelectron
   Spectroscopy and cross-sectional Transmission Electron Microscopy showed
   the usual Ni/Au interchange occurred in samples without graphene. By
   sharp contrast, insertion of graphene interlayers prevented the nickel
   and gold interchange up to 600 degrees C and dramatically improved the
   morphological stability of the metallization stack. The results may be
   beneficial to optoelectronic devices in which reflectivity is critical.
   (C) 2016 Elsevier Ltd. All rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Pearton, SJ (Reprint Author), Univ Florida, Dept Mat Sci \& Engn, Gainesville, FL 32611 USA.
