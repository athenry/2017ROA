@article{ ISI:000377433800016,
Author = {Han, C. and Maghari, N.},
Title = {{Passive quantisation error extraction in continuous-time delta-sigma
   modulators}},
Journal = {{ELECTRONICS LETTERS}},
Year = {{2016}},
Volume = {{52}},
Number = {{11}},
Pages = {{917-918}},
Month = {{MAY 26}},
Abstract = {{A new method for quantisation error extraction in continuous-time
   delta-sigma modulators (CTDSMs) is proposed. The proposed method avoids
   the use of additional active components such as trans-conductors and
   opamps and uses only passive components. Instead, the virtual ground of
   the second stage integrator is used as a summing node and is utilised to
   extract the quantisation error. Thus, multi-stage CTDSM can be
   implemented in a power and area efficient way.}},
