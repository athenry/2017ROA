@inproceedings{ ISI:000401795900132,
Author = {Karam, Robert and Majerus, Steve and Bourbeau, Dennis and Damaser,
   Margot S. and Bhunia, Swarup},
Book-Group-Author = {{IEEE}},
Title = {{Ultralow-power Data Compression for Implantable Bladder Pressure
   Monitor: Algorithm and Hardware Implementation}},
Booktitle = {{PROCEEDINGS OF 2016 IEEE BIOMEDICAL CIRCUITS AND SYSTEMS CONFERENCE
   (BIOCAS)}},
Series = {{Biomedical Circuits and Systems Conference}},
Year = {{2016}},
Pages = {{500-503}},
Note = {{12th IEEE Biomedical Circuits and Systems Conference (BioCAS), Shanghai,
   PEOPLES R CHINA, OCT 17-19, 2016}},
Organization = {{IEEE; IEEE Circuits \& Syst Soc; IEEE EMB; IEEE SSC; Shanghai Jiaotong
   Univ}},
Abstract = {{Urinary incontinence, overactive bladder, and other dysfunctions of the
   lower urinary tract are conditions which affect millions worldwide,
   imposing a high financial burden and greatly affecting quality of life.
   Diagnosis of these conditions can be facilitated by monitoring bladder
   activity over time. Recent work has demonstrated the feasibility of
   device implantation for chronic bladder pressure monitoring, which can
   help to improve existing diagnostic techniques. For wireless implants, a
   significant portion of the implant power consumption results not from
   pressure sensing, but rather from data transmission. In this paper, we
   present a novel algorithm which is designed to perform efficient,
   on-chip compression of bladder pressure data with tunable quality,
   resulting in significant overall power savings. We validate our approach
   by applying the algorithm to prerecorded bladder pressure data from 14
   human subjects, and demonstrate high average compression ratios (similar
   to 5x), leading to similar reductions in transmission power draw, with
   low reconstruction error (RMSE approximate to 1.09). An ultralow-power
   hardware implementation of the proposed algorithm is obtained by
   synthesizing the design with TSMC 0 : 1 8 mu m technology, yielding an
   area of 1 : 3 4 mm(2) and average power of 2 : 3 n W using low power
   design techniques. To our knowledge, this is the first example of
   dedicated on-chip compression for bladder pressure data designed for an
   ultralow-power biomedical implant.}},
