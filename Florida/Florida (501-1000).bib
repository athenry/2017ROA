@inproceedings{ ISI:000401963301050,
Author = {Wang, Jie and Feng, Xueyan and Gao, Qinghua and Yue, Hao and Fang,
   Yuguang},
Book-Group-Author = {{IEEE}},
Title = {{Context Awareness with Ambient FM Signal Using Multi-domain Features}},
Booktitle = {{2016 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
