@article{ ISI:000355388100035,
Author = {Buxy, Samriddhi and Pullamrpanappallil, Pratap},
Title = {{Effects of different types of storage and size reduction on biomethane
   potential of sugarbeets}},
Journal = {{INTERNATIONAL SUGAR JOURNAL}},
Year = {{2015}},
Volume = {{117}},
Number = {{1397}},
Pages = {{362-369}},
Month = {{MAY}},
Abstract = {{Due to its high sugar content, sugarbeets are excellent feedstock for
   biogas production. Its limited annual harvests necessitate appropriate
   yet economic storage prior to anaerobic digestion. In present study,
   biomethane potential of frozen and freshly harvested sugarbeets was
   compared with low cost airtight ambient temperature storage. For
   airtight stored sugarbeets, the maximum rate of methane production was
   12.4 L CH4 STP/kg/d, which was the highest amongst the three types of
   storage studied, and methane yield was 360.8 LCH4 STP/kg VS which was
   lower than frozen sugarbeets 423.4 L CH4 STP/kg VS but higher than fresh
   sugarbeets 220.2 L CH4 STP/kg VS. It was also observed that size
   reduction increases the rate of production of biogas but not biomethane
   yield. Duration of digestion of frozen beets was decreased by 40\% when
   the beets were coarsely shredded to 3 cm in size. Rate of methane
   production from airtight stored sugarbeets was higher than that from
   size-reduced frozen sugarbeets, consequently the duration of digestion
   of airtight-stored beets was only about 75\% of the duration required
   for digesting shredded sugarbeets. Simple airtight ambient temperature
   storage may be a viable option for uninterrupted utilization of sugar
   beets as feedstock for anaerobic digestion between annual harvests of
   the crop.}},
Publisher = {{INT SUGAR JOURNAL LTD}},
Address = {{80 CALVERLEY, TUNBRIDGE WELLS, KENT TN1 2UN, WALES}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Pullamrpanappallil, P (Reprint Author), Univ Florida, Agr \& Biol Engn Dept, Gainesville, FL 32611 USA.
   Buxy, Samriddhi; Pullamrpanappallil, Pratap, Univ Florida, Agr \& Biol Engn Dept, Gainesville, FL 32611 USA.}},
