@article{ ISI:000355937100017,
Author = {Wang, Wei and Samuelson, Sean R. and Chen, Jiapin and Xie, Huikai},
Title = {{Miniaturizing Fourier Transform Spectrometer With an Electrothermal
   Micromirror}},
Journal = {{IEEE PHOTONICS TECHNOLOGY LETTERS}},
Year = {{2015}},
Volume = {{27}},
Number = {{13}},
Pages = {{1418-1421}},
Month = {{JUL 1}},
Abstract = {{An amplitude-division Fourier transform spectroscopy system has been
   constructed. The system design hinges on an electrothermally actuated
   micromirror with large piston motion. The micromirror is composed of
   electrothermal mesh actuators and can generate up to 95 mu m usable
   linear optical path difference of the system at only 0.8 V-dc. A custom
