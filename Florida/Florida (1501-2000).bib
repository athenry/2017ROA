@article{ ISI:000364205800034,
Author = {Wang, Zhiyong and Yang, Hao and Ding, Hao and Lu, Guijin and Jiang,
   Huabei and Bi, Xiaohong},
Title = {{Area-detection fibre-optic system for spatially offset Raman
   spectroscopy and Raman tomography in reflection mode}},
Journal = {{ELECTRONICS LETTERS}},
Year = {{2015}},
Volume = {{51}},
Number = {{21}},
Pages = {{1684-1685}},
Month = {{OCT 8}},
Abstract = {{A new design of a fibre-optic probe system is presented which can be
   used to simultaneously detect scattering signals from multiple channels
   for spatially offset Raman spectroscopy and Raman tomography in
   reflection mode. The fibre setup contained 32 source fibres with an
   auto-controlled optical switch and 40 detector fibres, which were
   deployed in a large-area planar interface. Phantom experiments were
   conducted using a prototype of the Raman instrument, and have
   demonstrated the practical applicability of the system design. The
