Address = {{2000 FLORIDA AVE NW, WASHINGTON, DC 20009 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Kotovsky, DA (Reprint Author), Univ Florida, Dept Elect \& Comp Engn, Gainesville, FL 32611 USA.
   Kotovsky, D. A.; Moore, R. C., Univ Florida, Dept Elect \& Comp Engn, Gainesville, FL 32611 USA.}},
DOI = {{10.1002/2015JA021370}},
