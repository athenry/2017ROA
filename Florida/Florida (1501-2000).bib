@article{ ISI:000358675600058,
Author = {Zhao, Pengfei and Portugal, Sherlie and Roy, Subrata},
Title = {{Efficient needle plasma actuators for flow control and surface cooling}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{107}},
Number = {{3}},
Month = {{JUL 20}},
Abstract = {{We introduce a milliwatt class needle actuator suitable for plasma
   channels, vortex generation, and surface cooling. Electrode
   configurations tested for a channel configuration show 1400\% and 300\%
   increase in energy conversion efficiency as compared to conventional
   surface and channel corona actuators, respectively, generating up to 3.4
   m/s air jet across the channel outlet. The positive polarity of the
   needle is shown to have a beneficial effect on actuator efficiency.
   Needle-plate configuration is demonstrated for improving cooling of a
   flat surface with a 57\% increase in convective heat transfer
   coefficient. Vortex generation by selective input signal manipulation is
   also demonstrated. (C) 2015 AIP Publishing LLC.}},
