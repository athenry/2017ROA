@article{ ISI:000359958800040,
Author = {Sanad, M. M. S. and Rashad, M. M. and Abdel-Aal, E. A. and Powers, K.},
Title = {{Novel cordierite nanopowders of new crystallization aspects and its
   cordierite-based glass ceramics of improved mechanical and electrical
   properties for optimal use in multidisciplinary scopes}},
Journal = {{MATERIALS CHEMISTRY AND PHYSICS}},
Year = {{2015}},
Volume = {{162}},
Pages = {{299-307}},
Month = {{JUL 15}},
Abstract = {{Nanocrystalline a-cordierite Mg2Al4Si5O18 powders have been synthesized
   via a novel and facile co-precipitation method using MgCl2 center dot
   6H(2)O, NaAlO2, Na2SiO3 center dot 5H(2)O as starting materials. XRD,
   SEM, HR-TEM and EDS techniques were employed to investigate the phase
   structure and crystal morphology of the synthesized powders at various
