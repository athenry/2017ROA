@article{ ISI:000361468800015,
Author = {Sharma, Ashesh and Sankar, Bhavani V.},
Title = {{Micromechanics of Composites Using a Two-Parameter Model and Response
   Surface Predictions}},
Journal = {{AIAA JOURNAL}},
Year = {{2015}},
Volume = {{53}},
Number = {{10}},
Pages = {{2988-2994}},
Month = {{OCT}},
Abstract = {{A novel method involving the use of micromechanical analysis to predict
   elastic constants of a unidirectional fiber-reinforced composite is
   proposed. The method revolves around a model called the two-parameter
   model. The two parameters are obtained by performing one micromechanical
   analysis of the composite unit cell along one of its transverse
   directions. The method involves construction of response surfaces that
   act as surrogate models, for the prediction of the two parameters,
   corresponding to any fiber-reinforced composite design that uses a
   transversely isotropic fiber and an isotropic matrix. Using the two
   parameters, four of the five elastic constants can be computed. The
   fifth elastic constant, longitudinal shear modulus, is predicted using a
   separate response surface. The results indicate that the two-parameter
   model in conjunction with the response surface makes accurate prediction
   of composite elastic constants.}},
