@article{ ISI:000356733400020,
Author = {Liu, Wei and Yang, Hai and Yin, Yafeng},
Title = {{Efficiency of a highway use reservation system for morning commute}},
Journal = {{TRANSPORTATION RESEARCH PART C-EMERGING TECHNOLOGIES}},
Year = {{2015}},
Volume = {{56}},
Pages = {{293-308}},
Month = {{JUL}},
Abstract = {{This paper examines the design and efficiency of a highway use
   reservation system where commuters need reservations to access a highway
   facility at specific times. We show that, by accommodating reservation
   requests to the level that the highway capacity allows, traffic
   congestion can be relieved. Generally, a more differentiated design of
   the reservation system yields a higher reduction of travel cost and thus
   achieves a higher efficiency. The efficiency bound of the system is
   established. We also show that braking or tactical waiting behaviors of
   drivers would cause a loss of efficiency, which thus need be proactively
   accommodated. Given that user heterogeneity cause further loss of
   efficiency, we explore how two specific types of user heterogeneity
   affect the system efficiency. Auction-based reservation is then proposed
   to mitigate the efficiency loss. (C) 2015 Elsevier Ltd. All rights
   reserved.}},
