@article{ ISI:000358719400016,
Author = {Davis, III, Calvin and Nino, Juan C.},
Title = {{Microwave Processing for Improved Ionic Conductivity in
   Li2O-Al2O3-TiO2-P2O5 Glass-Ceramics}},
Journal = {{JOURNAL OF THE AMERICAN CERAMIC SOCIETY}},
Year = {{2015}},
Volume = {{98}},
Number = {{8}},
Pages = {{2422-2427}},
Month = {{AUG}},
Abstract = {{Li1.4Al0.4Ti1.6(PO4)(3) (LATP) was synthesized using a glass-ceramics
   approach through crystallization in a conventional box furnace and a
   modified microwave furnace. The microstructure of samples that were
   microwave processed at 1000 degrees C showed a larger average grain size
   (0.87m) when compared with the grain size of conventionally processed
   samples (0.30m) at the same temperature. Microwave processing led to
   significant enhancement of the conductivity when compared with
   conventional processing for all crystallization temperatures
   investigated. The highest total conductivity achieved was of glass
   microwave processed at 1000 degrees C, with a conductivity of
   5.33x10(-4)S/cm. This conductivity was five times higher than that of
   LATP crystallized conventionally at the same temperature.}},
