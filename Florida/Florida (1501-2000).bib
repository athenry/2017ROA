@article{ ISI:000363915300044,
Author = {Blonsky, Michael N. and Zhuang, Houlong L. and Singh, Arunima K. and
   Hennig, Richard G.},
Title = {{Ab Initio Prediction of Piezoelectricity in Two-Dimensional Materials}},
Journal = {{ACS NANO}},
