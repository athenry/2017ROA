@article{ ISI:000343929400038,
Author = {Lewis, Jason M. and Raetz, Dominik and Murer, David and Jordan, Kelly A.},
Title = {{Analysis of In-Situ Fission Rate Measurements Using He-4 Gas
   Scintillation Detectors}},
Journal = {{IEEE TRANSACTIONS ON NUCLEAR SCIENCE}},
Year = {{2014}},
Volume = {{61}},
Number = {{4, 2}},
Pages = {{2217-2221}},
Month = {{AUG}},
Abstract = {{Active neutron interrogation is a powerful non-destructive assay
   technique that relies on detecting and analyzing fission neutrons
   produced in a fissionable sample by an interrogating high neutron flux.
   He-4 gas scintillation fast neutron detectors are investigated in this
   paper for use in a novel fission rate measurement technique. The He-4
   detectors have excellent gamma rejection, a fast response time, and give
   significant information on incident neutron energy, allowing for energy
   cuts to be applied to the detected signal. These features are shown in
   this work to allow for the detection of prompt fission neutrons in-situ
   during active neutron interrogation of a natural uranium sample. The
   detected neutron energy spectra from Cf-252 and (d,d) generator neutron
   sources are measured with the He-4 detection system and compared against
   incident and simulated spectra. An partial response matrix for the
   detector is determined using the kinematic interaction properties of the
   elastic scattering with the He-4.}},
