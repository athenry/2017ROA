@article{ ISI:000342245400012,
Author = {Erol, Salim and Orazem, Mark E. and Muller, Richard P.},
Title = {{Influence of overcharge and over-discharge on the impedance response of
   LiCoO2 vertical bar C batteries}},
Journal = {{JOURNAL OF POWER SOURCES}},
Year = {{2014}},
Volume = {{270}},
Pages = {{92-100}},
Month = {{DEC 15}},
Abstract = {{This paper provides the results of impedance measurements on
   commercially available LiCoO2 vertical bar C coin-type cells. The
   impedance response was shown to be extremely sensitive to
   state-of-charge, overcharge, and over-discharge. Interestingly, the
   impedance showed a persistent change to the electrochemical
   characteristics of a coin cell subject to overcharge; whereas, the
   electrochemical characteristics returned to normal for a coin cell
   subject to over-discharge. A measurement model analysis was used to show
   the reversibility of the impedance behavior of an over-discharged cell
   and the irreversibility of the impedance response of an overcharged
   cell. (C) 2014 Elsevier B.V. All rights reserved.}},
