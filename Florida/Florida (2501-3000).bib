@article{ ISI:000340408000023,
Author = {Biagi, C. J. and Uman, M. A. and Hill, J. D. and Jordan, D. M.},
Title = {{Negative leader step mechanisms observed in altitude triggered lightning}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES}},
Year = {{2014}},
Volume = {{119}},
Number = {{13}},
Month = {{JUL 16}},
Abstract = {{We present 63 high-speed video frames (108 kilo-frames per second
   (kfps), 9.26 s per frame) showing the development of the downward
   negative stepped leader in the initial stage of an altitude-triggered
   flash. The downward negative stepped leader initiated from the bottom of
   the triggering wire at a height of about 128 m above ground and, 553 s
   later, it struck a lightning rod located at a distance of about 50 m
   from the launch tower. During the leader's development, electric field
   derivative pulses were detected associated with leader stepping. The
   interpulse intervals ranged from 3 to 27 mu s with a mean value of 13 mu
   s. Distinct segments of luminosity were observed ahead of the main
   leader channel that appear similar to space leaders were observed in the
   high-speed video frames. A total of eight luminous segments were
   observed that were 1 m to 6 m in length and were centered at distances
   from the main leader channel ranging from 3 m to 8 m. The new leader
   steps that appeared in the frames following the luminous segments were 5
   m to 8 m in length. Two of the observed segments apparently never
   connected to the leader channel and thus failed to produce a new leader
   step.}},
