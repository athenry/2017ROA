@article{ ISI:000339692700049,
Author = {Vernon, Julianne D. and Bonzongo, Jean-Claude J.},
Title = {{Volatilization and sorption of dissolved mercury by metallic iron of
   different particle sizes: Implications for treatment of mercury
   contaminated water effluents}},
Journal = {{JOURNAL OF HAZARDOUS MATERIALS}},
Year = {{2014}},
Volume = {{276}},
Pages = {{408-414}},
Month = {{JUL 15}},
Abstract = {{Batch experiments were conducted to investigate the interactions between
   metallic iron particles and mercury (Hg) dissolved in aqueous solutions.
   The effect of bulk zero valent iron (ZVI) particles was tested by use of
   (i) granular iron and (ii) iron particles with diameters in the
   nano-size range and referred to herein as nZVI. The results show that
   the interactions between Hgn+ and Fe are dominated by Hg volatilization
   and Hg adsorption; with Hg adsorption being the main pathway for Hg
   removal from solution. Hg adsorption kinetic studies using ZVI and nZVI
   resulted in higher rate constants (k) for nZVI when k values were
   expressed as a function of mass of iron used (day(-1) g(-1)). In
   contrast, ZVI showed higher rates of Hg removal from solution when k
   values were expressed as a function iron particles' specific surface
   area (gm(-2) day(-1)). Overall, nZVI particles had a higher maximum
   sorption capacity for Hg than ZVI, and appeared to be an efficient
   adsorbent for Hg dissolved in aqueous solutions. (C) 2014 Elsevier B.V.
   All rights reserved.}},
