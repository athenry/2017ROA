@article{ ISI:000346321700053,
Author = {Turner, Christopher and Johns, Paul M. and Thatcher, Evan M. and Tanner,
   D. B. and Nino, Juan C.},
Title = {{Atomic Displacive Disorder in Bi2Ti2O7}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2014}},
Volume = {{118}},
Number = {{49}},
Pages = {{28797-28803}},
Month = {{DEC 11}},
Abstract = {{The recently synthesized cubic pyrochlore Bi2Ti2O7 has been shown to
   possess displacive disorder in both the anion and cation sublattices.
   Here, the nature and characteristics of the displacive disorder are
   further investigated via vibrational spectroscopy. The infrared
   reflectance was measured over 30-3300 cm(-1) at temperatures between 20
   and 300 K, while the Raman spectra were collected from 50 to 3500 cm(-1)
   at room temperature. It is found that Bi2Ti2O7 exhibits more than the
   six modes expected in the Raman spectrum for the ideal pyrochlore
   structure. In addition, infrared-active modes are also present in the
   Raman spectra. These two results suggest displacements in the atomic
   positions of bismuth and oxygen away from their higher symmetry
   conventional pyrochlore Wyckoff positions and are strong and surprising
   evidence of disorder at the titanium site. Infrared-active phonon modes
   have been assigned to specific bending and stretching vibrational modes.
   The effect of the Bi ion on the OBiO and O'BiO' phonon modes is
   discussed.}},
