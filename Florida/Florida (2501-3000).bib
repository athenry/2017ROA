@article{ ISI:000344544200041,
Author = {Cheung, Chi Hang and Kim, Do Young and Subbiah, Jegadesan and Amb, Chad
   M. and Reynolds, John R. and So, Franky},
Title = {{Solution Processed Polymer Near-Infrared Photodiode With Electron and
   Hole Blockers}},
Journal = {{IEEE TRANSACTIONS ON ELECTRON DEVICES}},
Year = {{2014}},
Volume = {{61}},
Number = {{11}},
Pages = {{3852-3857}},
Month = {{NOV}},
Abstract = {{A solution-processed polymer near-infrared photodetector with charge
   blocking layers is reported using a low-bandgap polymer, poly{[}(4,4
   `-bis(2-ethylhexyl) dithieno {[}3,2-b: 2 `,3 `-d]
   silole)-2,6-diyl-alt-(2,1,3-benzothiadiazole) 4,7-diyl]. Zinc oxide
   nanoparticles and poly{[}(9,9 `-dioctylfluorenyl-2,7-diyl)co-(4,4
   `-(N-(4-sec-butyl)) diphenylamine)] are used as a hole blocking layer
   and an electron blocking layer, respectively. A maximum detectivity of 2
   x 10(13) Jones is achieved with a 330-nm-thick photoactive layer that
