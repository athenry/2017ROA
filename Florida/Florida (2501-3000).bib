@article{ ISI:000334136900008,
Author = {Ding, Wenchuan and Dong, Xiaoling and Ime, Inyang Mandu and Gao, Bin and
   Ma, Lena Q.},
Title = {{Pyrolytic temperatures impact lead sorption mechanisms by bagasse
   biochars}},
Journal = {{CHEMOSPHERE}},
Year = {{2014}},
Volume = {{105}},
Pages = {{68-74}},
Month = {{JUN}},
Abstract = {{The characteristics and mechanisms of Pb sorption by biochars produced
   from sugarcane bagasse at 250, 400, 500, and 600 degrees C were
   examined. The Pb sorption isotherms, kinetics and desorption were
   investigated. All biochars were effective in Pb sorption and were well
   described by Langmuir isotherm model and pseudo-second-order kinetic
   model. The maximum sorption capacity decreased from 21 to 6.1 mg g(-1)
   as temperature increased from 250 to 600 degrees C. The Pb sorption was
   rapid initially, probably controlled by cation exchange and complexation
   and then slowed down, which might be due to intraparticle diffusions.
   FTIR data and kinetic models suggested that oxygen functional groups
   were probably responsible for the high Pb sorption onto low temperature
   biochars (250 and 400 degrees C) whereas intraparticle diffusion was
   mainly responsible for low Pb sorption onto high temperature biochars
   (500 and 600 degrees C). Decreased phosphorus concentration indicated
   that P-induced Pb precipitation was also responsible for Pb sorption.
   Pyrolysis temperature significantly affected biochar properties and
   played an important role in Pb sorption capacity and mechanisms by
   biochars. (C) 2013 Elsevier Ltd. All rights reserved.}},
