@article{ ISI:000343003600027,
Author = {Liu, Lu and Xi, Yuyin and Ahn, Shihyun and Ren, Fan and Gila, Brent P.
   and Pearton, Stephen J. and Kravohenko, Ivan I.},
Title = {{Characteristics of gate leakage current and breakdown voltage of
   AlGaN/GaN high electron mobility transistors after postprocess annealing}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY B}},
Year = {{2014}},
Volume = {{32}},
Number = {{5}},
Month = {{SEP}},
Abstract = {{The effects of postprocess annealing on the gate leakage current and
   breakdown voltage characteristics of AlGaN/GaN high electron mobility
   transistors (HEMTs) was investigated. The fabricated AlGaN/GaN HEMTs
   were postannealed at 250, 300, 350, 400, or 450 degrees C under a
   nitrogen (N-2) atmosphere by using rapid thermal annealing, and both
   direct current (dc) and pulsed measurements were performed to
   characterize the changes in device performance. The reverse gate leakage
   current (I-G) at V-G = -10V was reduced by one order of magnitude and
