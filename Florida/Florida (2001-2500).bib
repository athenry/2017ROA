Title = {{Beyond Classification: Structured Regression for Robust Cell Detection
   Using Convolutional Neural Network}},
Booktitle = {{MEDICAL IMAGE COMPUTING AND COMPUTER-ASSISTED INTERVENTION, PT III}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2015}},
Volume = {{9351}},
