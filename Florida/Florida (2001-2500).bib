@inproceedings{ ISI:000370259201091,
Author = {Sun, Yifei and Kumar, Mrinal},
Book-Group-Author = {{IEEE}},
Title = {{Solution of High Dimensional Transient Fokker-Planck Equations by Tensor
   Decomposition}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2015}},
Pages = {{1475-1480}},
Note = {{American Control Conference, Chicago, IL, JUL 01-03, 2015}},
