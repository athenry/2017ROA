@inproceedings{ ISI:000380440800151,
Author = {Xie, H. and Lan, S. and Wang, D. and Wang, W. and Sun, J. and Liu, H.
   and Cheng, J. and Ding, J. and Qin, Z. and Chen, Q. and Kang, H. and
   Tian, Z.},
Book-Group-Author = {{IEEE}},
Title = {{Miniature Fourier transform spectrometers based on electrothermal MEMS
   mirrors with large piston scan range}},
Booktitle = {{2015 IEEE SENSORS}},
Series = {{IEEE Sensors}},
Year = {{2015}},
Pages = {{556-559}},
Note = {{2015 IEEE SENSORS, Busan, SOUTH KOREA, NOV 01-04, 2015}},
Organization = {{IEEE; Sensors Council; Korean Sensors Soc}},
Abstract = {{This WeThis paper reports a MEMS-based Fourier transform spectrometer
   (FTS) with small size, high resolution, fast sampling and low cost. The
   MEMS scanning mirror can generate over 300 mu m piston motion at 10 Hz
   with only 7Vpp and maintain a very small tilting less than 0.03 degrees.
   This high scanning performance is achieved by an innovative three-level
