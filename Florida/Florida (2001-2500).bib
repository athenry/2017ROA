@inproceedings{ ISI:000355651100024,
Author = {Malakooti, Mohammad H. and Hwang, Hyun-Sik and Sodano, Henry A.},
Editor = {{Goulbourne, NC}},
Title = {{Power generation from base excitation of a Kevlar composite beam with
   ZnO nanowires}},
