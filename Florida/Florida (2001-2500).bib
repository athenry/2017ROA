@inproceedings{ ISI:000380479800019,
Author = {Kumar, Rohit and Gordon-Ross, Ann},
Book-Group-Author = {{IEEE}},
Title = {{An Automated High-level Design Framework for Partially Reconfigurable
   FPGAs}},
