Title = {{A Switched Systems Approach to Vision-Based Localization of a Target
   with Intermittent Measurements}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
