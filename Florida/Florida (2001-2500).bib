@inproceedings{ ISI:000370259203017,
Author = {Lin, Yashen and Barooah, Prahir and Mathieu, Johanna L.},
Book-Group-Author = {{IEEE}},
Title = {{Ancillary Services to the Grid from Commercial Buildings through Demand
   Scheduling and Control}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2015}},
Pages = {{3007-3012}},
Note = {{American Control Conference, Chicago, IL, JUL 01-03, 2015}},
Organization = {{Amer Automat Control Council; IFAC; Adaptics Inc; Altair; dSPACE; Eaton
