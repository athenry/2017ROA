Title = {{The Cognitive Driving Framework: Joint Inference for Collision
   Prediction and Avoidance in Autonomous Vehicles}},
Booktitle = {{2015 15TH INTERNATIONAL CONFERENCE ON CONTROL, AUTOMATION AND SYSTEMS
   (ICCAS)}},
Series = {{International Conference on Control Automation and Systems}},
