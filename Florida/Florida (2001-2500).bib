@article{ ISI:000347494200057,
Author = {Oehmig, Wesley N. and Roessler, Justin G. and Zhang, Jianye and
   Townsend, Timothy G.},
Title = {{Effect of ferrous metal presence on lead leaching in municipal waste
   incineration bottom ashes}},
Journal = {{JOURNAL OF HAZARDOUS MATERIALS}},
Year = {{2015}},
Volume = {{283}},
Pages = {{500-506}},
Month = {{FEB 11}},
Abstract = {{The recovery of ferrous and non-ferrous metals from waste to energy
   (WTE) ash continues to advance as the sale of removed metals improves
   the economics of waste combustion. Published literature suggests that Fe
   and Fe oxides play a role in suppressing Pb leaching in the Toxicity
   Characteristic Leaching Procedure (TCLP); further removal of ferrous
   metals from WTE ashes may facilitate higher Pb leaching under the TCLP.
   Eight WTE bottom ash size-fractions, from three facilities, were
   evaluated to assess the effect of metallic Fe addition and ferrous metal
   removal on TCLP leaching. Metallic Fe addition was demonstrated to
   reduce Pb leaching; the removal of ferrous metals by magnet resulted in
   a decrease in total available Pb (mg/kg) in most ash samples, yet Pb
   leachability increased in 5 of 6 ash samples. The research points to two
   chemical mechanisms to explain these results: redox interactions between
   Pb and Fe and the sorption of soluble Pb onto Fe oxide surfaces, as well
   as the effect of the leachate pH before and after metals recovery. The
   findings presented here indicate that generators, processors, and
   regulators of ash should be aware of the impact ferrous metal removal
   may have on Pb leaching, as a substantial increase in leaching may have
   significant implications regarding the management of WTE ashes. (C) 2014
   Elsevier B.V. All rights reserved.}},
