@inproceedings{ ISI:000387732700016,
Author = {Burrs, S. L. and Sidhu, R. and Bhargava, M. and Kiernan-Lewis, J. and
   Schwalb, N. and Rong, Y. and Gomes, C. and Claussen, J. and Vanegas, D.
