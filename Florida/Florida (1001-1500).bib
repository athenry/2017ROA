@inproceedings{ ISI:000389019300022,
Author = {Smock, Brandon and Wilson, Joseph and Milner, Martin},
Editor = {{Kadar, I}},
Title = {{Improved landmine detection through context-dependent score calibration}},
Booktitle = {{SIGNAL PROCESSING, SENSOR/INFORMATION FUSION, AND TARGET RECOGNITION XXV}},
