@article{ ISI:000366077500005,
Author = {Chen, Zhichao and Subhash, Ghatu and Tulenko, James S.},
Title = {{Spark plasma sintering of diamond-reinforced uranium dioxide composite
   fuel pellets}},
Journal = {{NUCLEAR ENGINEERING AND DESIGN}},
Year = {{2015}},
Volume = {{294}},
Pages = {{52-59}},
Month = {{DEC 1}},
Abstract = {{In an effort to increase the thermal conductivity of UO2 fuel pellets,
   high density UO2-5 vol\% diamond composite pellets were fabricated using
   the spark plasma sintering (SPS) technique. Diamond particles with
   nano-size (0.25 mu m) and several micro-sizes (3 mu m, 12 mu m and 25 mu
   m) were mixed with UO2 powder and sintered using SPS at 1300-1600
   degrees C with a hold time of 5 min. The resultant density, chemical
   reaction, microstructure, thermal conductivity and Young's modulus of
   the sintered pellets were investigated. The pellets with 3 mu m diamond
   particles had a uniform distribution of particles as well as better
   thermal and mechanical properties compared to others. An increase in
   thermal conductivity of up to 41.6\%, 38.3\% and 34.2\% at 100 degrees
   C, 500 degrees C and 900 degrees C, respectively, was measured in the
   UO2-diamond composite pellets compared to the pure UO2 fuel pellets. (C)
   2015 Elsevier B.V. All rights reserved.}},
