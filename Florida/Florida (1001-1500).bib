@article{ ISI:000366055600055,
Author = {Kang, Tsung-Sheng and Ren, Fan and Gila, Brent P. and Pearton, Steve J.
   and Patrick, Erin and Cheney, David J. and Law, Mark and Zhang, Ming-Lan},
Title = {{Investigation of traps in AlGaN/GaN high electron mobility transistors
   by sub-bandgap optical pumping}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY B}},
Year = {{2015}},
Volume = {{33}},
Number = {{6}},
Month = {{NOV}},
Abstract = {{Sub-bandgap optical pumping with wavelengths of 671, 532, or 447 nm was
   employed to study traps in AlGaN/GaN high electron mobility transistors.
   The trap energies were determined from the Arrhenius plots of transient
   drain current at different temperatures. Prominent states were located
   around 0.7 eV below the conduction band, and these are commonly reported
   to be nonradiative traps due to defects trapped on dislocations or
   possibly Ga interstitials. In addition, traps located at 1.9 and 2.35 eV
   below the conduction band were found, which have been reported as N-Ga
   antisite and V-Ga-O-N complexes, respectively. The postillumination
   drain current decays were analyzed with a persistent photoconductivity
   method, and time constants were extracted and associated with the
   recapture process in the AlGaN barrier and GaN channel layers. (C) 2015
   American Vacuum Society.}},
