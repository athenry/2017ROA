@article{ ISI:000378395900003,
Author = {Bachler, Patricia R. and Forry, Kaitlyn E. and Sparks, Chelsea A. and
   Schulz, Michael D. and Wagener, Kenneth B. and Sumerlin, Brent S.},
Title = {{Modular segmented hyperbranched copolymers}},
Journal = {{POLYMER CHEMISTRY}},
Year = {{2016}},
Volume = {{7}},
Number = {{25}},
Pages = {{4155-4159}},
Abstract = {{Modular segmented hyperbranched polymers, amenable to facile
   post-polymerization functionalization, were created via two distinct
   approaches. Self-condensing vinyl polymerization via reversible
