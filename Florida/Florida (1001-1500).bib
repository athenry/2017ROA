@article{ ISI:000379619500002,
Author = {Moosaei, Hossein and Ketabchi, Saeed and Pardalos, Panos M.},
Title = {{Tikhonov regularization for infeasible absolute value equations}},
Journal = {{OPTIMIZATION}},
Year = {{2016}},
Volume = {{65}},
Number = {{8}},
Pages = {{1531-1537}},
Abstract = {{We investigate the optimum correction of an absolute value equation by
   minimally changing the coefficient matrix and right-hand side vector
   using Tikhonov regularization. Solving this problem is equivalent to
   minimizing the sum of fractional quadratic and quadratic functions. The
   primary difficulty with this problem is its nonconvexity. Nonetheless,
   we show that a global optimal solution to this problem can be found by
   solving an equation on a closed interval using the subgradient method.
   Some examples are provided to illustrate the efficiency and validity of
   the proposed method.}},
