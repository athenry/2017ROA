@inproceedings{ ISI:000389086100007,
Author = {Feng, Bing and Zhang, Chi and Wei, Lingbo and Fang, Yuguang},
Editor = {{Yang, Q and Yu, W and Challal, Y}},
Title = {{Piggybacking Lightweight Control Messages on Physical Layer for
   Multicarrier Wireless LANs}},
Booktitle = {{WIRELESS ALGORITHMS, SYSTEMS, AND APPLICATIONS, WASA 2016}},
Series = {{Lecture Notes in Computer Science}},
