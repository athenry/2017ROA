@inproceedings{ ISI:000383223900011,
Author = {Pittaluga, Francesco and Zivkovic, Aleksandar and Koppal, Sanjeev J.},
Book-Group-Author = {{IEEE}},
Title = {{Sensor-level Privacy for Thermal Cameras}},
Booktitle = {{2016 IEEE INTERNATIONAL CONFERENCE ON COMPUTATIONAL PHOTOGRAPHY (ICCP)}},
Series = {{IEEE International Conference on Computational Photography}},
Year = {{2016}},
Pages = {{103-114}},
Note = {{IEEE International Conference on Computational Photography (ICCP), NW
   Univ, Evanston, IL, MAY 13-15, 2016}},
Abstract = {{As cameras turn ubiquitous, balancing privacy and utility becomes
   crucial. To achieve both, we enforce privacy at the sensor level, as
   incident photons are converted into an electrical signal and then
   digitized into image measurements. We present sensor protocols and
   accompanying algorithms that degrade facial information for thermal
   sensors, where there is usually a clear distinction between humans and
   the scene. By manipulating the sensor processes of gain, digitization,
   exposure time, and bias voltage, we are able to provide privacy during
   the actual image formation process and the original face data is never
   directly captured or stored. We show privacy-preserving thermal imaging
   applications such as temperature segmentation, night vision, gesture
   recognition and HDR imaging.}},
