@inproceedings{ ISI:000369724800001,
Author = {Subhash, S. G. and Jannotti, P. and Subhash, G.},
Editor = {{Song, B and Lamberson, L and Casem, D and Kimberley, J}},
Title = {{Impact Response of Coquina}},
Booktitle = {{DYNAMIC BEHAVIOR OF MATERIALS, VOL 1}},
