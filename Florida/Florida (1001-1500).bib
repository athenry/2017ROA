@inproceedings{ ISI:000382679200249,
Author = {Farahmandi, Farimah and Mishra, Prabhat},
Book-Group-Author = {{IEEE}},
Title = {{Automated Test Generation for Debugging Arithmetic Circuits}},
Booktitle = {{PROCEEDINGS OF THE 2016 DESIGN, AUTOMATION \& TEST IN EUROPE CONFERENCE
   \& EXHIBITION (DATE)}},
Series = {{Design, Automation, and Test in Europe Conference and Exhibition}},
Year = {{2016}},
