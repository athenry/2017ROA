@article{ ISI:000309096500002,
Author = {Xiong, Liming and McDowell, David L. and Chen, Youping},
Title = {{Nucleation and growth of dislocation loops in Cu, Al and Si by a
   concurrent atomistic-continuum method}},
Journal = {{SCRIPTA MATERIALIA}},
Year = {{2012}},
Volume = {{67}},
Number = {{7-8}},
Pages = {{633-636}},
Month = {{OCT}},
Abstract = {{Submicron-sized samples with 42,000 finite elements containing up to
   similar to 86 million atoms have been simulated using a concurrent
   atomistic-continuum method. The simulations reproduce not only
   nucleation and growth of semicircular dislocation loops in Cu and Al,
   but also hexagonal shuffle dislocation loops in Si, with the loop radius
   approaching similar to 75 nm. Details of leading and trailing partial
