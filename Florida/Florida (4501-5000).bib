@article{ ISI:000313352500002,
Author = {Motz, Louis H. and Sedighi, Ali},
Title = {{Saltwater Intrusion and Recirculation of Seawater at a Coastal Boundary}},
Journal = {{JOURNAL OF HYDROLOGIC ENGINEERING}},
