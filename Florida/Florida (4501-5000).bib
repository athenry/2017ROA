@article{ ISI:000310836900004,
Author = {Miao, Xiaochang and Tongay, Sefaattin and Hebard, Arthur F.},
Title = {{Strain-induced suppression of weak localization in CVD-grown graphene}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{2012}},
Volume = {{24}},
Number = {{47}},
Month = {{NOV 28}},
Abstract = {{We investigate the magnetic-field-and temperature-dependent transport
   properties of CVD-grown graphene transferred to a flexible substrate
   (Kapton) and subjected to externally applied strain. In zero magnetic
   field, a logarithmic temperature-dependent conductivity correction,
   resulting from strong electron-electron interaction, becomes weaker with
   the application of strains as large as 0.6\% because of an increased
   rate of chiral-symmetry-breaking scattering. With the application of a
   perpendicular magnetic field, we also observe positive
   magnetoconductance at low temperature (T = 5 K) due to weak
   localization. This magnetoconductance is suppressed with increasing
   strain, concomitant with a rapid decrease of the intervalley scattering
   rate (tau(-1)(i)). Our results are in good agreement with theoretical
   expectations and are consistent with a strain-induced decoupling between
   graphene and its underlying Kapton substrate.}},
