@article{ ISI:000315203100014,
Author = {Aung, Nyi Nyi and Ye Jihong and Masters, Forrest James},
Title = {{Simulation of non-Gaussian wind pressure fields on domed structures}},
Journal = {{JOURNAL OF THE CHINESE INSTITUTE OF ENGINEERS}},
Year = {{2013}},
Volume = {{36}},
Number = {{2}},
Pages = {{257-271}},
Abstract = {{Wind pressure fluctuations acting on structures are important for the
   prediction of peak pressure values and for design against fatigue.
   Collection of time histories of pressure fluctuations by traditional
   wind tunnel measurements is time consuming and expensive. To overcome
   this problem, a study on developing a new wind pressure simulation
   technique for domed structures is carried out. A simulation algorithm to
   generate non-Gaussian wind pressure fields using wavelet bases is
   proposed. It presents an efficient, flexible, and easily applied
   stochastic non-Gaussian simulation using a cumulative distribution
   function mapping technique that converges to a desired target power
   spectral density. This method first generates Gaussian sample fields
   using wavelet bases and then maps them into non-Gaussian sample fields
   with the aid of an iterative procedure. Results from this technique are
   presented and compared for domes of different rise-span ratios to
   demonstrate its efficacy. The advantages and limitations of this method
   are also discussed.}},
