@inproceedings{ ISI:000357299600106,
Author = {Guo, Linke and Zhu, Xiaoyan and Zhang, Chi and Fang, Yuguang},
Book-Group-Author = {{IEEE}},
Title = {{Privacy-preserving Attribute-based Friend Search in Geosocial Networks
   with Untrusted Servers}},
