@inproceedings{ ISI:000352223507015,
Author = {Kamalapurkar, Rushikesh and Walters, Patrick and Dixon, Warren},
Book-Group-Author = {{IEEE}},
Title = {{Concurrent learning-based approximate optimal regulation}},
