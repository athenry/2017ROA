@inproceedings{ ISI:000326373700018,
Author = {Liu, Yonggang and Figueiredo, Renato and Xu, Yiqi and Zhao, Ming},
Book-Group-Author = {{IEEE}},
Title = {{On the Design and Implementation of a Simulator for Parallel File System
   Research}},
Booktitle = {{2013 IEEE 29TH SYMPOSIUM ON MASS STORAGE SYSTEMS AND TECHNOLOGIES (MSST)}},
Series = {{IEEE Symposium on Mass Storage Systems and Technologies Proceedings-MSST}},
Year = {{2013}},
Note = {{29th IEEE Symposium on Massive Storage Systems and Technologies (MSST) -
   Research Track, Long Beach, CA, MAY 06-10, 2013}},
Organization = {{IEEE}},
Abstract = {{Due to the popularity and importance of Parallel File Systems (PFSs) in
   modern High Performance Computing (HPC) centers, PFS designs and I/O
   optimizations are active research topics. However, the research process
   is often time-consuming and faces cost and complexity challenges in
   deploying experiments in real HPC systems. This paper describes PFSsim,
   a trace-driven simulator of distributed storage systems that allows the
   evaluation of PFS designs, I/O schedulers, network structures, and
   workloads. PFSsim differentiates itself from related work in that it
   provides a powerful platform featuring a modular design with high
   flexibility in the modeling of subsystems including the network,
   clients, data servers and I/O schedulers. It does so by designing the
   simulator to capture abstractions found in common PFSs. PFSsim also
   exposes script-based interfaces for detailed configurations. Experiments
   and validation against real systems considering sub-modules and the
   entire simulator show that PFSsim is capable of simulating a
   representative PFS (PVFS2) and of modeling different I/O scheduler
   algorithms with good fidelity. In addition, the simulation speed is also
   shown to be acceptable.}},
