@article{ ISI:000309990300045,
Author = {Lee, Kang Taek and Jung, Doh Won and Yoon, Hee Sung and Lidie, Ashley A.
   and Camaratta, Matthew A. and Wachsman, Eric D.},
Title = {{Interfacial modification of La0.80Sr0.20MnO3-delta-Er0.4Bi0.6O3 cathodes
   for high performance lower temperature solid oxide fuel cells}},
Journal = {{JOURNAL OF POWER SOURCES}},
Year = {{2012}},
Volume = {{220}},
Pages = {{324-330}},
Month = {{DEC 15}},
Abstract = {{The performance of conventional La0.80Sr0.20MnO3-delta (LSM) cathodes
   was dramatically improved using a highly conductive Er-0.4Bi1.6O3 (ESB)
   phase. ESB was utilized not only as the ion-conducting phase in the
   LSM-ESB cathode, but also as an electrolyte coupled to the LSM-ESB
   cathode. The electrode area specific resistance (ASR) measured from a
   symmetric cell consisting of LSM-ESB electrodes on an ESB electrolyte
