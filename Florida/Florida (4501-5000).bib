@inproceedings{ ISI:000327210202021,
Author = {Kan, Z. and Klotz, J. and Pasiliao, Jr., E. L. and Dixon, W. E.},
Book-Group-Author = {{IEEE}},
Title = {{Containment Control for a Directed Social Network with State-Dependent
   Connectivity}},
Booktitle = {{2013 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
