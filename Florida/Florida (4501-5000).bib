@inproceedings{ ISI:000352223506145,
Author = {Xu, Yiming and Mohseni, Kamran},
Book-Group-Author = {{IEEE}},
Title = {{Fish Lateral Line Inspired Hydrodynamic Force Estimation for Autonomous
   Underwater Vehicle Control}},
