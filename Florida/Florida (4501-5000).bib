@article{ ISI:000311522600003,
Author = {Chen, Song and Manders, Jesse R. and Tsang, Sai-Wing and So, Franky},
Title = {{Metal oxides for interface engineering in polymer solar cells}},
Journal = {{JOURNAL OF MATERIALS CHEMISTRY}},
Year = {{2012}},
Volume = {{22}},
Number = {{46}},
Pages = {{24202-24212}},
Month = {{DEC 14}},
Abstract = {{Significant progress in power conversion efficiencies and stabilities of
   polymer solar cells has been achieved using semiconducting metal oxides
   as charge extraction interlayers. Both n- and p-type transition metal
   oxides with good transparency in the visible as well as infrared region
   make good Ohmic contacts to both donors and acceptors in polymer bulk
   heterojunction solar cells. Their compatibility with roll-to-roll
   processing makes them very attractive for low cost manufacturing of
   polymer solar cells. In this review, we will present the recent results
   on synthesis and characterization of these metal oxides along with the
   device performance of the solar cells using these metal oxides as
   interlayers.}},
