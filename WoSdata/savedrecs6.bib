@article{ ISI:000348904800003,
Author = {Rossa, Carlos and Lozada, Jose and Micaelli, Alain},
Title = {{Design and Control of a Dual Unidirectional Brake Hybrid Actuation
   System for Haptic Devices}},
Journal = {{IEEE TRANSACTIONS ON HAPTICS}},
Year = {{2014}},
Volume = {{7}},
Number = {{4}},
Pages = {{442-453}},
Month = {{OCT-DEC}},
Abstract = {{Hybrid actuators combining brakes and motors have emerged as an
   efficient solution to achieve high performance in haptic devices. In
   this paper, an actuation approach using two unidirectional brakes and a
   DC motor is proposed. The brakes are coupled to overrunning clutches and
   can apply a torque in only one rotational direction. The associated
   control laws, that are independent of the virtual environment model,
   calculate the control gains in real time in order limit the energy and
