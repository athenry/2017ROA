@inproceedings{ ISI:000370259205105,
Author = {Zeng, Jing and Liu, Jinfeng},
Book-Group-Author = {{IEEE}},
Title = {{Distributed Moving Horizon Estimation Subject to Communication Delays
   and Losses}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
