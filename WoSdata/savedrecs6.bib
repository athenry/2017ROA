@article{ ISI:000365218500002,
Author = {Jiang, Keren and Etayash, Hashem and Azmi, Sarfuddin and Naicker,
   Selvaraj and Hassanpourfard, Mahtab and Shaibani, Parmiss Mojir and
   Thakur, Garima and Kaur, Kamaljit and Thundat, Thomas},
Title = {{Rapid label-free detection of E. coli using antimicrobial peptide
   assisted impedance spectroscopy}},
Journal = {{ANALYTICAL METHODS}},
Year = {{2015}},
Volume = {{7}},
Number = {{23}},
Pages = {{9744-9748}},
Abstract = {{There is an increasing demand for rapid detection of waterborne
   pathogens to monitor drinking water safety. We demonstrate a compact,
   label-free sensor array for rapid detection of Escherichia coli (E.
   coli) in contaminated water samples using antimicrobial peptide assisted
   impedimetric sensor platform. Interdigitated electrode arrays
   immobilized with the antimicrobial peptide Colicin V (ColV) were used to
   screen the affinity towards different bacterial strains by monitoring
   impedance variations in real-time. This ColV assisted impedance
   biosensor exhibited high selectivity towards Gram-negative strains
   particularly towards E. coli strains. This selective detection of E.
   coli from other strains was observed at 10(2) cfu mL(-1), which is
   clinically relevant. The sensor can detect E. coli from 10(2) to 10(6)
   cfu mL(-1) in water sample at pH 7 to 9. These results show that the
   antimicrobial peptide ColV assisted impedimetric array is capable of
   rapid, specific detection of E. coli in contaminated water samples.}},
