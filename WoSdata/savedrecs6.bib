@article{ ISI:000341686200005,
Author = {Lavoie, Shawn R. and Tang, Tian},
Title = {{A discontinuity associated with the capillary deformation of
   microstructures}},
Journal = {{JOURNAL OF ENGINEERING MATHEMATICS}},
Year = {{2014}},
Volume = {{88}},
Number = {{1}},
Pages = {{79-97}},
Month = {{OCT}},
Abstract = {{Capillary forces are known to deform microstructures. In this work, we
   consider the deformation of a microstructure idealized as a double
   cantilever beam (DCB) caused by a liquid droplet at the free end. When
   the volume of the liquid is small, a discontinuity may appear in the
   deflection of the DCB. As the volume of the small droplet is gradually
   decreased in a quasi-static fashion, a discontinuous decrease in the
   beam deflection is observed, accompanied by an abrupt movement of one of
   the menisci. For small droplets, a discontinuity from low to high
   deflection may be observed when the ratio of the capillary force to the
   elastic restoring force increases. The discontinuity, attributed to the
   nonlinear nature of the equations governing the beam deformation, is
   important for determining whether the DCB will collapse as the capillary
   loading increases. An analysis using energy and branching diagrams is
   performed to help understand the occurrence of the discontinuity.}},
