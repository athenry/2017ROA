@article{ ISI:000343320100006,
Author = {Wang, Xu and Schiavone, Peter},
Title = {{Finite deformation of harmonic solids with cusp cracks}},
Journal = {{IMA JOURNAL OF APPLIED MATHEMATICS}},
Year = {{2014}},
Volume = {{79}},
Number = {{5, SI}},
Pages = {{790-803}},
Month = {{OCT}},
Abstract = {{We consider the finite deformation of a particular class of compressible
   hyperelastic solids of harmonic type containing a cusp crack. Two types
   of the cusp crack are considered: one is a symmetric airfoil cusp crack
   and the other one is a symmetric lip cusp crack. Our analytical result
   demonstrates that it is possible to have two, one or even no permissible
   solution for given loading, geometric and material parameters.
   Furthermore the singular near-tip stress field is discussed with an
   emphasis on the derivation of the stress intensity factors and the
   bounded T-stress. Also investigated is a harmonic solid containing a
   pore having an n-fold axis of symmetry under a remote hydrostatic
   loading.}},
