@article{ ISI:000347898500008,
Author = {Cheng, Edwin and Mabee, Myles and Swami, Vimarsha G. and Pi, Yeli and
   Thompson, Richard and Dulai, Sukhdeep and Jaremko, Jacob L.},
Title = {{ULTRASOUND QUANTIFICATION OF ACETABULAR ROUNDING IN HIP DYSPLASIA:
   RELIABILITY AND CORRELATION TO TREATMENT DECISIONS IN A RETROSPECTIVE
   STUDY}},
Journal = {{ULTRASOUND IN MEDICINE AND BIOLOGY}},
Year = {{2015}},
Volume = {{41}},
Number = {{1}},
Pages = {{56-63}},
Month = {{JAN}},
Abstract = {{Currently, acetabular rounding is only subjectively assessed on
   ultrasound for developmental dysplasia of the hip. We tested whether
   acetabular rounding can be quantified reliably and can distinguish
   between hips requiring and not requiring treatment. Consecutive infants
   (n = 90) suspected of having dysplasia of the hip, seen at a pediatric
   orthopedic clinic, were separated into four diagnostic categories
   (normal, borderline but resolved, treated by brace, treated surgically).
   Acetabular rounding was assessed by semi-quantitative grade (0 = nil, 1
   = mild, 2 = moderate, 3 = severe) by three observers and by direct
   measurement of acetabular radius of curvature (AROC) by two observers.
   Inter-observer reliability of rounding grade was poor (k = 0.30-0.37).
   AROC had an inter-observer intra-class correlation coefficient of 0.84
   and coefficient of variation of 29\%-34\%. Mean AROC was significantly
   higher for hips requiring treatment than for those not requiring
   treatment (3.3 mm vs. 1.6 mm, p = 0.007). AROC reliably quantifies an
   observation currently being made subjectively by radiologists and
   surgeons, and may be useful as a supplementary ultrasound index of
   dysplasia of the hip in future prospective studies. (C) 2015 World
   Federation for Ultrasound in Medicine \& Biology.}},
