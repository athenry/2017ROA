@article{ ISI:000363091600033,
Author = {Mao, Haiyan and Zhou, Dingguo and Chen, Heng and Wang, Haiyan (Helena)},
Title = {{EFFECTIVE DEGRADATION OF PHARMACEUTICAL WASTEWATER BY ADVANCED OXIDATION
   PROCESS}},
Journal = {{OXIDATION COMMUNICATIONS}},
Year = {{2015}},
Volume = {{38}},
Number = {{3}},
Pages = {{1452-1457}},
Abstract = {{The pre-treatment of a pharmaceutical wastewater (PWW) by wet hydrogen
   peroxide oxidation with Fe2+ was investigated to improve the degradation
   of the complex-mixture of organic compounds present in the wastewater.
   The influence of different crucial parameters such as temperature,
   reaction time, Fe concentration, hydrogen peroxide excess (HE) and
   initial chemical oxygen demand (COD) have been evaluated. The optimum
   conditions for degradation led to COD reductions of up to 80\% in only 1
   h of treatment. The results showed that WHPO is an effective process for
   treating pharmaceutical wastewater. Fe2+ addition improved COD removal.
   When reaction temperature is 440 degrees C, reaction time is 4 h,
   hydrogen peroxide excess is 0.8 and initial COD is 60 000 mg/l, and COD
   removal increased 11\%.}},
