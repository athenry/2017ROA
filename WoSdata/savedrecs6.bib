Address = {{1155 16TH ST, NW, WASHINGTON, DC 20036 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Elliott, JAW (Reprint Author), Univ Alberta, Dept Chem \& Mat Engn, Edmonton, AB T6G 2V4, Canada.
   Eslami, Fatemeh; Elliott, Janet A. W., Univ Alberta, Dept Chem \& Mat Engn, Edmonton, AB T6G 2V4, Canada.}},
DOI = {{10.1021/jp5063786}},
