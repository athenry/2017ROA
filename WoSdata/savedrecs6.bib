@article{ ISI:000346154200009,
Author = {Barnes, N. and Borle, S. and Dewar, M. and Andreiuk, J. and Mendez, P.
   F.},
Title = {{3D microstructure reconstruction of chrome carbide weld overlays}},
Journal = {{SCIENCE AND TECHNOLOGY OF WELDING AND JOINING}},
Year = {{2014}},
Volume = {{19}},
Number = {{8}},
Pages = {{696-702}},
Month = {{NOV}},
Abstract = {{This paper addresses a current debate about the morphology of M7C3 phase
   in the chrome carbide overlay (CCO) and white iron community. This
   morphology was investigated using a serial sectioning technique in
   combination with computer post-processing software. A 600 x 6006 x 300
   mu m volume of CCO was reconstructed in three dimensions (3D) to
   determine the difference between blade and rod-like carbide
   morphologies. The 3D analysis revealed that the two carbides
   morphologies correspond to the same hexagonal rod-like structures, but
