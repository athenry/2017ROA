@article{ ISI:000339963800048,
Author = {Wang, Yiyu and Luo, Guoqiang and Li, Leijun and Shen, Qiang and Zhang,
   Lianmeng},
Title = {{Formation of intermetallic compounds in Mg-Ag-Al joints during diffusion
   bonding}},
Journal = {{JOURNAL OF MATERIALS SCIENCE}},
Year = {{2014}},
Volume = {{49}},
Number = {{20}},
Pages = {{7298-7308}},
Month = {{OCT}},
Abstract = {{In this study, we conducted the diffusion bonding of Mg and Al alloys
   using a 30-mu m-thick pure silver foil interlayer at median temperatures
   between 390 and 490 A degrees C. We obtained a multilayered structure
   across the Mg-Ag-Al joint: Mg/Mg(ss, Ag)/Mg3Ag/MgAg/Ag/Ag(ss,
   Al)/Ag2Al/Al. The silver diffusion barrier prevented the formation of
   brittle intermetallics between Mg and Al. Intermetallics identified at
   the joint interface include the more ductile types between Mg and Ag,
   epsilon-Mg3Ag and beta'-MgAg, and Ag and Al, delta-Ag2Al. As the bonding
   temperature increased, Ag2Al, followed by MgAg, favored the growth of
   Mg3Ag IMC layer. The shear strength of the joints increased with the
   rising bonding temperature to a maximum value of 11.8 MPa at 470 A
   degrees C. Fracture failure in the joints mainly occurred in the Ag2Al
   layer. The formation mechanism for interfacial layers in the joints is
   believed to consist of four stages: (1) solid-solution formation, (2)
   Mg-Ag IMC formation, (3) Ag-Al IMC formation, and (4) growth of Mg-Ag
   and Ag-Al IMCs.}},
