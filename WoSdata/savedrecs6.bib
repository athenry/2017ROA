@inproceedings{ ISI:000357778100040,
Author = {Vo, Q. N. and Lou, E. and Le, Lawrence H. and Huynh, L. Q.},
Editor = {{VanToi, V and Phuong, THL}},
Title = {{Investigation of the Optimal Freehand Three-Dimensional Ultrasound
   Configuration to Image Scoliosis: An In-vitro Study}},
Booktitle = {{5TH INTERNATIONAL CONFERENCE ON BIOMEDICAL ENGINEERING IN VIETNAM}},
Series = {{IFMBE Proceedings}},
