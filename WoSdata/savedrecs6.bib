@inproceedings{ ISI:000370728700073,
Author = {Wang, Ran and Han, Jie and Cockburn, Bruce and Elliott, Duncan},
Book-Group-Author = {{IEEE}},
Title = {{Design and Evaluation of Stochastic FIR Filters}},
Booktitle = {{2015 IEEE PACIFIC RIM CONFERENCE ON COMMUNICATIONS, COMPUTERS AND SIGNAL
   PROCESSING (PACRIM)}},
