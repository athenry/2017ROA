@article{ ISI:000347139400031,
Author = {Soltannia, Babak and Sameoto, Dan},
Title = {{Strong, Reversible Underwater Adhesion via Gecko-Inspired Hydrophobic
   Fibers}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2014}},
Volume = {{6}},
Number = {{24}},
Pages = {{21995-22003}},
Month = {{DEC 24}},
Abstract = {{Strong, reversible underwater adhesion using gecko-inspired surfaces is
   achievable through the use of a hydrophobic structural material and does
   not require surface modification or suction cup effects for this
   adhesion to be effective. Increased surface energy can aid in dry
   adhesion in an air environment but strongly degrades wet adhesion via
   reduction of interfacial energy underwater. A direct comparison of
   structurally identical but chemically different mushroom shaped fibers
   shows that strong, reversible adhesion, even in a fully wetted, stable
   state, is feasible underwater if the structural material of the fibers
   is hydrophobic and the mating surface is not strongly hydrophilic. The
   exact adhesion strength will be a function of the underwater interfacial
   energy between surfaces and the specific failure modes of individual
   fibers. This underwater adhesion has been calculated to be potentially
   greater than the dry adhesion for specific combinations of hydrophobic
   surfaces.}},
