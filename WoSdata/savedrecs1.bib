@article{ ISI:000381436500072,
Author = {Gao, Panpan and Wu, Quan and Li, Xi and Ma, Hongxin and Zhang, Hao and
   Volinsky, Alex A. and Qiao, Lijie and Su, Yanjing},
Title = {{Size-dependent concentrations of thermal vacancies in solid films}},
Journal = {{PHYSICAL CHEMISTRY CHEMICAL PHYSICS}},
Year = {{2016}},
Volume = {{18}},
Number = {{32}},
Pages = {{22661-22667}},
Month = {{AUG 28}},
Abstract = {{Solid films are considered as typical model systems to study size
   effects on thermal vacancy concentration in nanomaterials. By combining
   the generalized Young-Laplace equation with the chemical potential of
   vacancies, a strict size-dependent thermodynamic model of vacancies,
   which includes the surface intrinsic elastic parameters of the
   eigenstress, Young's modulus and the geometric size of the solid films,
   was established. The vacancy concentration changes in the film with
   respect to the bulk value, depending on the geometric size and surface
   stress sign of the solid films. Atomistic simulations of Au and Pt films
   verified the developed thermodynamic model. These results provide
   physical insights into the size-dependent thermal vacancy concentration
   in nanomaterials.}},
