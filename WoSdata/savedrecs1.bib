DOI = {{10.1021/acs.jpcc.6b03850}},
ISSN = {{1932-7447}},
Keywords-Plus = {{IN-OIL EMULSIONS; HEPTANE-TOLUENE MIXTURES; SIDE-CHAIN LENGTH;
