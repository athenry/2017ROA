@article{ ISI:000391897600028,
Author = {Sandhu, Navjot Kaur and Hanifi, Amir Reza and Woldnik, Andrew and Amiri,
   Taghi and Etsell, Thomas H. and Luo, Jingli and Sarkar, Partha},
Title = {{Electrochemical performance of a short tubular solid oxide fuel cell
   stack at intermediate temperatures}},
Journal = {{APPLIED ENERGY}},
Year = {{2016}},
Volume = {{183}},
Pages = {{358-368}},
Month = {{DEC 1}},
Abstract = {{A short stack composed of six micro-tubular fuel cells was fabricated in
   order to study its electrochemical performance under different
   electrical connection configurations (parallel, series and
   parallel-series) at intermediate temperatures. Two cells were in the
   Ni-YSZ/YSZ/Pr2NiO4-YSZ system and four were in the
   Ni-YSZ/YSZ/Nd2NiO4-YSZ configuration, of which one failed following
   testing at 600 degrees C. Overall, individual cells had similar
   performance delivering 202, 302 and 340 mW/cm(2) at 600, 650 and 700
   degrees C, respectively. The stack delivered a maximum of 7.40, 1032 and
   11.56 W at these temperatures. No significant differences were found
   among different arrangements. However, as expected, the stack
   performance was most affected by the malfunctioning cell under a series
   arrangement at 600 degrees C. Since the parallel-series configuration
   delivers an intermediate voltage and current and similar power to the
   parallel or series connections, it can be more suitable for stack
   assembly. Such an arrangement also offers the possibility of replacement
   of a stack module in case a cell fails in that module during operation.
   A repairable fuel cell stack eliminates the negative economic impacts
   caused by a malfunctioning cell which, in extreme cases, can lead to the
   complete loss of the valuable energy conversion device. (C) 2016
   Elsevier Ltd. All rights reserved.}},
