@article{ ISI:000390433000010,
Author = {Saghlatoon, Hossein and Boroujeni, Rashid Mirzavand and Honari, Mohammad
   Mahdi and Mousavi, Pedram},
Title = {{Low-Cost Inkjet Printed Passive Booster for Increasing the Magnetic
   Coupling in Proximity of Metal Object for NFC Systems}},
Journal = {{IEEE MICROWAVE AND WIRELESS COMPONENTS LETTERS}},
Year = {{2016}},
Volume = {{26}},
Number = {{12}},
Pages = {{996-998}},
Month = {{DEC}},
Abstract = {{A passive fully inkjet-printed circuit on a plastic based flexible
   substrate for increasing the communication read range of an NFC based
   system is proposed. Two parallel coils resonating at 13.56 MHz are used
   for the improvement of coupling between the transmitter and receiver.
   The proposed system behaves like a lens by concentrating the absorbed
   magnetic energy from a wide area in a small focal region. Being a
   passive circuit, fully inkjet-printed and having only one discrete
   component made this circuit a very low-cost and tempting solution for
   improving the magnetic link between transmitter and receiver. Another
