@article{ ISI:000385161800002,
Author = {Nguyen, Kim-Cuong T. and Le, Lawrence H. and Kaipatur, Neelambar R. and
   Zheng, Rui and Lou, Edmond H. and Major, Paul W.},
Title = {{High-Resolution Ultrasonic Imaging of Dento-Periodontal Tissues Using a
   Multi-Element Phased Array System}},
Journal = {{ANNALS OF BIOMEDICAL ENGINEERING}},
