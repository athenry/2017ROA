@article{ ISI:000386418500001,
Author = {Liang, Xihui and Zhang, Hongsheng and Liu, Libin and Zuo, Ming J.},
Title = {{The influence of tooth pitting on the mesh stiffness of a pair of
   external spur gears}},
Journal = {{MECHANISM AND MACHINE THEORY}},
Year = {{2016}},
Volume = {{106}},
Pages = {{1-15}},
Month = {{DEC}},
Abstract = {{Time-varying mesh stiffness is one of the main internal excitations of
   gear dynamics. With the growth of gear tooth fault, like pitting,
   cracking and spalling, the mesh stiffness amplitude changes and
   consequently the dynamic properties of the gear system change. This
   study is devoted to deriving equations of the mesh stiffness of a pair
   of external spur gears with tooth pitting. Different pitting severity
   levels are modeled. The influence of tooth pitting on the gear mesh
   stiffness is investigated. The relationship between pitting severity and
   mesh stiffness is established, The proposed method is validated to be
   effective in mesh stiffness evaluation by comparing with a finite
   element model. (C) 2016 Published by Elsevier Ltd.on behalf of
   International Federation for the Promotion of Mechanism and Machine
   Science}},
