@article{ ISI:000385968400012,
Author = {Ng, A. and Sterne, P. and Hansen, S. and Recoules, V. and Chen, Z. and
   Tsui, Y. Y. and Wilson, B.},
Title = {{dc conductivity of two-temperature warm dense gold}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2016}},
Volume = {{94}},
Number = {{3}},
Month = {{SEP 30}},
Abstract = {{Using recently obtained ac conductivity data we have derived dc
   conductivity together with free electron density and electron momentum
   relaxation time in two-temperature warm dense gold with energy density
   up to 4.1 MJ/kg (0.8 x 10(11) J/m(3)). The derivation is based on a
   Drude interpretation of the dielectric function that takes into account
   contributions of intraband and interband transitions as well as atomic
   polarizability. The results provide valuable benchmarks for assessing
   the extended Ziman formula for electrical resistivity and an
   accompanying average atom model.}},
