Title = {{Analysis of Wind Ramp Events Using Their Conditional Probabilities from
   PCA 3D Tables}},
Booktitle = {{PROCEEDINGS OF THE 2015 16TH INTERNATIONAL SCIENTIFIC CONFERENCE ON
   ELECTRIC POWER ENGINEERING (EPE)}},
Series = {{International Scientific Conference on Electric Power Engineering}},
Year = {{2015}},
