@article{ ISI:000352651600007,
Author = {Chen, H. and Tang, T. and Amirfazli, A.},
Title = {{Effect of contact angle hysteresis on breakage of a liquid bridge}},
Journal = {{EUROPEAN PHYSICAL JOURNAL-SPECIAL TOPICS}},
Year = {{2015}},
Volume = {{224}},
Number = {{2}},
Pages = {{277-288}},
Month = {{MAR}},
Abstract = {{In this paper, the importance of considering contact angle hysteresis
   (CAH) during the process of stretching and breaking a liquid bridge
   between two solid surfaces is addressed. We clearly show that due to the
   pinning of contact line at the end of the stretching stage, the contact
   angle between liquid bridge and surfaces cannot be simply assumed to
   have a constant value (e.g. receding contact angle, theta (r) ).
   Simulation results for stretching a liquid bridge with and without CAH,
   showed that the contact line pinning can lead to breakage at a larger
   surface separation and smaller value of pull-off force (F (p) ). A
   systematic study about the effect of CAH and contact line pinning on the
   value of F (p) is provided. It is found that when one of the surfaces
   has a theta (r) larger than 90(a similar to), F (p) decreases with the
   increase of theta (r) on either surface delimiting the bridge. For the
   cases where theta (r) of both surfaces are smaller than 90(a similar
   to), significantly smaller F (p) is seen when contact line pinning
   occurs on both surfaces, as compared to F (p) when contact line pinning
   occurs only on one surface. This smaller F (p) is caused by more curved
   profile and later breakage of liquid bridge.}},
