@article{ ISI:000347687800021,
Author = {Zhang, Yan and Liu, Hongyuan and Liu, Runsheng},
Title = {{Kinetic model of the ozone oxidation by-product bromate removal by
   nanoparticle zero iron}},
Journal = {{DESALINATION AND WATER TREATMENT}},
Year = {{2015}},
Volume = {{53}},
Number = {{2}},
Pages = {{469-474}},
Month = {{JAN 9}},
Abstract = {{Bromate formation during ozone oxidation process is becoming a
   particular concern because it is a carcinogenic compound. Reduction by
   zerovalent iron shows a potential technique for the removal of bromate
   from water. A series of batch experiments were carried out using
   nanoparticle zero iron. A tentative reaction mechanism and the kinetic
   model for bromate reduction were discussed. It was found that the
   efficiency of bromate removal was affected by the amount of iron,
   initial concentration of bromate, pH, and reaction temperature. Bromate
   removal was based on pseudo-first-order reaction kinetics, assuming an
   irreversible surface reaction to be the rate-controlling step. The
   apparent activation energy for bromate reduction, in the temperature
   range of 283-303 K-,K- was found to be 23.2 kJ/mol.}},
