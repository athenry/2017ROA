@article{ ISI:000353177700054,
Author = {Laguna-Bercero, M. A. and Hanifi, A. R. and Etsell, T. H. and Sarkar, P.
   and Orera, V. M.},
Title = {{Microtubular solid oxide fuel cells with lanthanum strontium manganite
   infiltrated cathodes}},
