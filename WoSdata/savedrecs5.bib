@article{ ISI:000363334600003,
Author = {Nassiri, Somayeh and Shafiee, Mohammad Hossein and Khan, Md. Rokibul
   Hossain and Bayat, Alireza},
Title = {{Evaluation of variation in unbound layers' moduli and its effect on
   pavement design}},
Journal = {{INTERNATIONAL JOURNAL OF GEOTECHNICAL ENGINEERING}},
Year = {{2015}},
Volume = {{9}},
Number = {{2}},
Pages = {{140-149}},
Month = {{MAR}},
Abstract = {{This study evaluates the uniformity of the pavement unbound layers'
   moduli along a section of a test road in Edmonton, Alberta using falling
   weight deflectometer (FWD) test data. The FWD tests were performed on
   the finished subgrade and base surfaces during construction along the
   outer wheel path (OWP) and centerline (CL). The subgrade modulus along
   the OWP and CL showed a coefficient of variance (COV) of 0.31 and 0.59,
   correspondingly. Similar variation was observed in the subgrade modulus
