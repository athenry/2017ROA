@article{ ISI:000363355700018,
Author = {Huang, Chunkai and Shi, Yijing and El-Din, Mohamed Gamal and Liu, Yang},
Title = {{Treatment of oil sands process-affected water (OSPW) using ozonation
   combined with integrated fixed-film activated sludge (IFAS)}},
Journal = {{WATER RESEARCH}},
Year = {{2015}},
Volume = {{85}},
Pages = {{167-176}},
Month = {{NOV 15}},
Abstract = {{Two integrated fixed-film activated sludge (IFAS) reactors were operated
   continuously to treat raw (untreated) and ozonated (30 mg/L) oil sands
   process-affected water (OSPW). After 11 months, 12.1\% of the acid
   extractable fraction (AEF) and 43.1\% of the parent naphthenic acids
   (NAs) were removed in the raw OSPW IFAS, while 42.0\% AEF and 80.2\% of
   parent NM were removed in the ozonated OSPW IFAS. UPLC/HRMS analysis
   showed that NA biodegradation significantly decreased as the NA
   cyclization number increased. Confocal laser scanning microscopy (CLSM)
   results showed that the biofilm in the ozonated OSPW IFAS was
   significantly thicker (94 +/- 1.6 mu m) than the biofilm in the raw OSPW
   IFAS (72 +/- 2.8 mu m) after 283 days of cultivation. The quantitative
   polymerase chain reaction (q-PCR) revealed that the abundance
   proportions of both nitrifier genes (AomA, NSR and Nitro) and
   denitrifier genes (narG, nirS, nirK and nosZ) within total bacteria were
   significantly higher in biofilms than in flocs in the raw OSPW IFAS
   system, but a different trend was observed in the ozonated OSPW IFAS
   system. (C) 2015 Elsevier Ltd. All rights reserved.}},
