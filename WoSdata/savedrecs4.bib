@article{ ISI:000363326600004,
Author = {Boulter, Ashley and Hall, Robert},
Title = {{Wireless network requirements for the successful implementation of
   automation and other innovative technologies in open-pit mining}},
Journal = {{INTERNATIONAL JOURNAL OF MINING RECLAMATION AND ENVIRONMENT}},
Year = {{2015}},
Volume = {{29}},
Number = {{5, SI}},
Pages = {{368-379}},
Month = {{SEP 3}},
Abstract = {{Automation and other innovative technologies are being implemented into
   the mining industry to improve production efficiency, reduce maintenance
   costs and improve reliability. This article highlights the wireless
   networking requirements for the successful implementation of these
   technologies and addresses some of the issues with existing networks.
   Alternative wireless technologies are considered and compared before
   investigating the impact of automation on the requirements of the
   networks. Future technologies are considered and predicted using
   Nielsen's Law and Moore's Law. Recommendations are given for how the
   mining industry must adapt to successfully implement innovative
   technologies as well as giving predictions of the consequence of failing
   to do so.}},
