@article{ ISI:000364150200010,
Author = {Popli, Khushaal and Sekhavat, Masih and Afacan, Artin and Dubljevic,
   Stevan and Liu, Qi and Prasad, Vinay},
Title = {{Dynamic Modeling and Real-Time Monitoring of Froth Flotation}},
