@article{ ISI:000359641900017,
Author = {Cui, Yi and Chen, Zengtao},
Title = {{Molecular dynamics simulation of the influence of elliptical void
   interaction on the tensile behavior of aluminum}},
Journal = {{COMPUTATIONAL MATERIALS SCIENCE}},
Year = {{2015}},
Volume = {{108}},
Number = {{A}},
Pages = {{103-113}},
Month = {{OCT}},
Abstract = {{The effect of void interaction on the damage progression in aluminum
   under uniaxial tension is studied via molecular dynamics (MD) method. MD
   geometries containing two adjacent elliptical voids are created with
   different void shape combination and intervoid ligament distance (ILD).
   The dislocation emission and development, stress-strain relation,
   porosity accumulation, void shape evolution and ILD shrinkage are
   monitored during loading process. The critical stress required by the
   voids to trigger the dislocation emission is in line with the prediction
   of the Lubarda model under the uniaxial tension case. Our results
   indicate that the onset strain of dislocation emission is mainly subject
   to the void shape. The void shape combination is found more influential
   than the ILD on the stress response and the porosity accumulation. (C)
   2015 Elsevier B.V. All rights reserved.}},
