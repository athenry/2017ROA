@article{ ISI:000356808600013,
Author = {Wang, Xu and Schiavone, Peter},
Title = {{Harmonic three-phase circular inclusions in finite elasticity}},
Journal = {{CONTINUUM MECHANICS AND THERMODYNAMICS}},
Year = {{2015}},
Volume = {{27}},
Number = {{4-5, SI}},
Pages = {{739-747}},
Month = {{SEP}},
Abstract = {{We study the exterior stress field in a three-phase circular inclusion
   which is bonded to the surrounding matrix through an intermediate
   interphase layer. All three phases belong to a particular class of
   compressible hyperelastic materials of harmonic type. We focus on the
   design of a harmonic elastic inclusion which by definition, does not
   disturb the sum of the normal stresses in the surrounding matrix. We
   show that in order to make the coated inclusion harmonic, certain
   inequalities concerning the material and geometric parameters of the
   three-phase composite must first be satisfied. The corresponding remote
   loading parameters can then be uniquely determined while keeping the
   associated phase angles arbitrary. Our results allow for both uniform
   and non-uniform remote loading. We show that the stress field inside the
   inclusion is uniform when the remote loading is uniform.}},
