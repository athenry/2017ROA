@article{ ISI:000356980700016,
Author = {Dong, Liang and Zhao, Yuemin and Peng, Liping and Zhao, Jie and Luo,
   Zhenfu and Liu, Qingxia and Duan, Chenlong},
Title = {{Characteristics of pressure fluctuations and fine coal preparation in
   gas-vibro fluidized bed}},
Journal = {{PARTICUOLOGY}},
Year = {{2015}},
Volume = {{21}},
Pages = {{146-153}},
Month = {{AUG}},
Abstract = {{To improve the separation efficiency of air dense medium fluidized beds
   for dry coal preparation, a gas-vibro fluidized bed has been proposed in
   which magnetic powder is used as the heavy medium. Pressure fluctuations
   in the gas-vibro fluidized bed were investigated using time- and
   frequency-domain analysis methods. The relationship between pressure
   fluctuations, bubble behavior, and separation efficiency was
   established. The low amplitude of the standard deviation, the power
   spectral density (PSD), the incoherent-output PSD, and the high
   amplitude of the coherent-output PSD, which corresponds to the bubble
   behavior in the bed, were improved for coal preparation. The coal ash
   content was reduced from 42.55\% to 16.54\% by using the gas-vibro
   fluidized bed. (C) 2014 Chinese Society of Particuology and Institute of
   Process Engineering, Chinese Academy of Sciences. Published by Elsevier
   B.V. All rights reserved.}},
