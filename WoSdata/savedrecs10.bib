Author = {Hoai An Le Thi and Quang Thuan Nguyen and Khoa Tran Phan and Tao Pham
   Dinh},
Editor = {{Selamat, A and Nguyen, NT and Haron, H}},
Title = {{DC Programming and DCA Based Cross-Layer Optimization in Multi-hop TDMA
   Networks}},
Booktitle = {{INTELLIGENT INFORMATION AND DATABASE SYSTEMS (ACIIDS 2013), PT II}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2013}},
