Language = {{English}},
Affiliation = {{Wang, W (Reprint Author), Univ Toronto, Dept Elect \& Comp Engn, Toronto, ON M5S 1A1, Canada.
   Wang, Wei; Li, Baochun; Liang, Ben, Univ Toronto, Dept Elect \& Comp Engn, Toronto, ON M5S 1A1, Canada.
   Niu, Di, Univ Alberta, Dept Elect \& Comp Engn, Edmonton, AB T6G 2M7, Canada.}},
DOI = {{10.1109/ICDCS.2013.20}},
