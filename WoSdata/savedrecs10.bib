Title = {{A New Granular Particle Swarm Optimization Variant for Granular
   Optimization Problems}},
Booktitle = {{PROCEEDINGS OF THE 2013 JOINT IFSA WORLD CONGRESS AND NAFIPS ANNUAL
   MEETING (IFSA/NAFIPS)}},
Year = {{2013}},
Pages = {{24-28}},
Note = {{Joint World Congress of the International-Fuzzy-Systems-Association
   (IFSA) / Annual Meeting of the
