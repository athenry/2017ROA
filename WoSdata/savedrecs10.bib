Author = {Jazayeri, Ali and Dyck, Matthew and Tavakoli, Mahdi},
Editor = {{Colgate, JE}},
Title = {{Stability Analysis of Teleoperation Systems under Strictly Passive and
   Non-passive Operator}},
Booktitle = {{2013 WORLD HAPTICS CONFERENCE (WHC)}},
Year = {{2013}},
Pages = {{695-700}},
Note = {{IEEE World Haptics Conference (WHC), Daejeon, SOUTH KOREA, APR 14-18,
