@inproceedings{ ISI:000329611505030,
Author = {Khabbazibasmenj, Arash and Vorobyov, Sergiy A.},
Book-Group-Author = {{IEEE}},
Title = {{TWO-WAY RELAY BEAMFORMING DESIGN: PROPORTIONAL FAIR AND MAX-MIN RATE
   FAIR APPROACHES USING POTDC}},
Booktitle = {{2013 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
