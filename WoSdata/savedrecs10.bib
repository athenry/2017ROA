Author = {Chalmers, Eric and Pedrycz, Witold and Lou, Edmond},
Editor = {{Pedrycz, W and Reformat, MZ}},
Title = {{Predicting the Outcome of Brace Treatment for Scoliosis Using
   Conditional Fuzzy Clustering}},
Booktitle = {{PROCEEDINGS OF THE 2013 JOINT IFSA WORLD CONGRESS AND NAFIPS ANNUAL
