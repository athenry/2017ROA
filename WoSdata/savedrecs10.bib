Address = {{1155 16TH ST, NW, WASHINGTON, DC 20036 USA}},
Type = {{Article; Proceedings Paper}},
Language = {{English}},
Affiliation = {{Sinton, D (Reprint Author), Univ Toronto, Dept Mech \& Ind Engn, Toronto, ON, Canada.
   Fadaei, Hossein; Sinton, David, Univ Toronto, Dept Mech \& Ind Engn, Toronto, ON, Canada.
   Fadaei, Hossein; Sinton, David, Univ Toronto, Inst Sustainable Energy, Toronto, ON, Canada.
   Shaw, John M., Univ Alberta, Dept Chem \& Mat Engn, Edmonton, AB, Canada.}},
