Editor = {{Loster, T and Pavelka, T}},
Title = {{THE COUPLING SPECTRUM: A NEW METHOD FOR DETECTING TEMPORAL NONLINEAR
   CAUSALITY IN FINANCIAL TIME SERIES}},
