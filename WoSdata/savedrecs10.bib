@inproceedings{ ISI:000320594100008,
Author = {He, A. and Ivey, D. G.},
Book-Group-Author = {{IEEE}},
Title = {{Electrodeposition of Au-Sn Alloys for Lead-free Solders: Au-rich
   Eutectic and Sn-rich Eutectic Compositions}},
Booktitle = {{2013 IEEE INTERNATIONAL SYMPOSIUM ON ADVANCED PACKAGING MATERIALS (APM)}},
Series = {{International Symposium on Advanced Packaging Materials-Processes,
   Properties, and Interfaces}},
Year = {{2013}},
Note = {{IEEE International Symposium on Advanced Packaging Materials (APM),
   Irvine, CA, FEB 27-MAR 01, 2013}},
Organization = {{IEEE}},
Abstract = {{The Au-Sn binary phase diagram shows that there are two eutectic alloy
   compositions. The first one is at similar to 30 at\% Sn (Au-rich) with a
   melting point of 280 degrees C and the second composition is at similar
   to 94 at\% Sn (Sn-rich) with an eutectic temperature at 217 degrees C.
   This paper summarizes work that demonstrates that both eutectic alloys
   can be electrodeposited from electrolytes that are non-toxic and
   environmentally friendly. The Au-rich eutectic alloy is electrodeposited
   as alternating layers of Au5Sn and AuSn, while the Sn-rich eutectic
   alloy is electrodeposited as sequential Au5Sn and Sn layers. In both
   cases, the entire electrodeposition process is achieved using a single
   solution, for each eutectic composition, without exposing the sample to
   air during electrodeposition. Each phase composition is attained by
   varying the current density only. The microstructure of as-deposited and
   reflowed solders is characterized and confirmed using X-ray diffraction
   and scanning electron microscopy.}},
