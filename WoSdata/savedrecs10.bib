@inproceedings{ ISI:000324988600063,
Author = {Shang, Jian and Li, Yun Wei},
Book-Group-Author = {{IEEE}},
Title = {{A Reduced Common-Mode Voltage Space Vector Modulation Method for Current
   Source Converters}},
Booktitle = {{2013 TWENTY-EIGHTH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC 2013)}},
