@article{ ISI:000342230200005,
Author = {Han, Sizhe and Zhao, Yuhui and Yu, Tong and Craik, Stephen},
Title = {{Challenge Studies of the Hach GuardianBlue Early Warning System}},
Journal = {{JOURNAL OF ENVIRONMENTAL ENGINEERING}},
Year = {{2014}},
Volume = {{140}},
Number = {{9, SI}},
Month = {{SEP}},
Abstract = {{The drinking water industry is paying more attention to water quality
   monitoring systems that provide real-time detection of contaminants in
   drinking water distribution systems. This thesis presents the results of
   an experimental challenge study on one such system, the Hach
   GuardianBlue Early Warning System. Contaminants of interest included
   organic and inorganic chemicals, and sewage. Experimental results showed
   that the system responded to all of the injected contaminants, except
   dichloromethane, but failed to correctly identify any of the
   contaminants. A mathematical model was used to determine the detection
   limit. Several system design and operational limitations were also
   identified. The GuardianBlue system could not identify a contaminant
   when it detected the changes in water quality parameters. A few
   improvements on the system design need to be done before the system can
   be deployed in the distribution network. At this moment, it can be
   merely used as an online water quality monitor. (C) 2014 American
   Society of Civil Engineers.}},
