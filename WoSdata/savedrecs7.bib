@article{ ISI:000333059200011,
Author = {Hoseini, Mohammad R. and Wang, Xiaodong and Zuo, Ming J.},
Title = {{Modified relative arrival time technique for sizing inclined cracks}},
Journal = {{MEASUREMENT}},
Year = {{2014}},
Volume = {{50}},
Pages = {{86-92}},
Month = {{APR}},
Abstract = {{In this paper, a modified relative time technique is proposed for
   estimating the length and the orientation of inclined cracks using
   ultrasonic B-scan signals. The proposed technique utilizes the relative
   position of the tip diffraction echo to the corner reflection echo in an
   ultrasonic B-scan image to estimate the crack length and the crack
   inclination angle. Both finite element simulated signals and
   experimental signals, which are obtained from a specimen with a 3 mm
   crack with different inclination angles, are used to evaluate the
   estimation error of the proposed technique. The results show that the
   proposed technique overcomes the shortcoming of the conventional
   relative arrival time technique in sizing inclined cracks. (C) 2014
   Elsevier Ltd. All rights reserved.}},
