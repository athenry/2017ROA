@article{ ISI:000336190500018,
Author = {Lin, Feng and Esmaeili, Payman and Long, Jun and Bara, Barry and Yeung,
   Anthony},
Title = {{Random charge variations at the bitumen-water interface:
   Characterization by autocorrelation}},
Journal = {{COLLOIDS AND SURFACES A-PHYSICOCHEMICAL AND ENGINEERING ASPECTS}},
Year = {{2014}},
Volume = {{451}},
Pages = {{144-150}},
Month = {{JUN 1}},
Abstract = {{Atomic force microscopy was used to measure local surface potentials (or
   equivalently, surface charges) at the bitumen-water interface. When
   plotted as a function of position along the bitumen surface, the
   potential appeared as a random ``noise.{''} Autocorrelation of the
   random noise revealed that the surface charges were correlated over
   distances much larger than molecular dimensions. Further, this
   correlation length appeared to be pH-dependent, exhibiting an almost
   threefold decrease as the pH was increased from 6 to 10. The correlation
   length, which is a measure of surface charge heterogeneity, is of
   practical importance as it affects strongly the probability of
   coalescence between emulsified bitumen droplets. (C) 2014 Elsevier B.V.
   All rights reserved.}},
