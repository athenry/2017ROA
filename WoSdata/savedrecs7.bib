@article{ ISI:000336478600008,
Author = {Deutsch, J. L. and Szymanski, J. and Deutsch, C. V.},
Title = {{Checks and measures of performance for kriging estimates}},
Journal = {{JOURNAL OF THE SOUTHERN AFRICAN INSTITUTE OF MINING AND METALLURGY}},
Year = {{2014}},
Volume = {{114}},
Number = {{3}},
Pages = {{223-230}},
Month = {{MAR}},
Abstract = {{Block model estimates are commonly calculated by the well-established
   technique of kriging. The mathematics are well established, but
   implementation details require site-specific considerations. Checking
   and measuring the performance of the estimates is important to ensure
   the block model is suitable for its intended purpose. There are
   different criteria for long-term planning and short-term planning. The
   implementation of kriging should be checked with cross-validation and
   assessed for conditional bias and departure from theoretical optimality
   with the calculation of kriging efficiency. A new expression of kriging
   efficiency, which compares the kriging variance with the theoretically
   optimal kriging variance, is developed to aid in this assessment. The
   assessment tools presented here can be applied in a number of
   situations; however, ordinary kriging with a reasonably large search
   performs well in most cases.}},
