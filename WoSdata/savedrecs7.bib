@article{ ISI:000343874900014,
Author = {Harrison, Tyler and Shao, Peng and Zemp, Roger J.},
Title = {{S-sequence patterned illumination iterative photoacoustic tomography}},
Journal = {{JOURNAL OF BIOMEDICAL OPTICS}},
Year = {{2014}},
Volume = {{19}},
Number = {{9}},
Month = {{SEP}},
Abstract = {{Quantitatively reconstructing optical absorption using photoacoustic
   imaging is nontrivial. Theoretical hurdles, such as nonuniqueness and
   numerical instability, can be mitigated by using multiple illuminations.
   However, even with multiple illuminations, using ANSI-safety-limited
   fluence for practical imaging may result in poor performance owing to
   limited signal-to-noise ratio (SNR). We demonstrate the use of
   S-sequence coded patterned illumination to boost SNR while preserving
   the enhanced stability of multiple-illumination iterative techniques.
   (C) 2014 Society of Photo-Optical Instrumentation Engineers (SPIE)}},
