@article{ ISI:000338610500022,
Author = {Hashemi, S. A. and Wilson, K. C. and Sanders, R. S.},
Title = {{Specific energy consumption and optimum operating condition for
   coarse-particle slurries}},
Journal = {{POWDER TECHNOLOGY}},
Year = {{2014}},
Volume = {{262}},
Pages = {{183-187}},
Month = {{AUG}},
Abstract = {{This paper deals with settling solid-liquid (slurry) flows of coarse
   particles with particle sizes of 150 pm or larger. The specific energy
   consumption (SEC), which is a measure of transport efficiency, was used
   to find the optimum operating condition. The effect of different
   parameters on SEC is determined by using the near-wall lift model. The
   analysis shows that the minimum SEC occurs at a solids concentration of
   approximately 30\% by volume. The accuracy of the near-wall lift model
   is investigated by comparing the model predictions with the results of a
   number of experimental works. The comparisons show good agreement
   between model prediction and experimental data in the literature. (C)
   2014 Elsevier B.V. All rights reserved.}},
