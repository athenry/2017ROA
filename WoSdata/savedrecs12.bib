Title = {{Mosaicing for Fast Wide-Field-of-View Optical-Resolution Photoacoustic
   Microscopy}},
Booktitle = {{PHOTONS PLUS ULTRASOUND: IMAGING AND SENSING 2012}},
Series = {{Proceedings of SPIE}},
