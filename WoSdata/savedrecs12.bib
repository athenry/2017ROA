@inproceedings{ ISI:000310776206161,
Author = {Adnan, Naseeb Ahmed and Cheng, Yue and Izadi, Iman and Chen, Tongwen},
Book-Group-Author = {{IEEE}},
Title = {{A Generalized Delay-timer for Alarm Triggering}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
