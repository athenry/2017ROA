@inproceedings{ ISI:000320286500032,
Author = {Pant, Lalit M. and Mitra, Sushanta K. and Secanell, Marc},
Book-Group-Author = {{ASME}},
Title = {{MASS TRANSPORT MEASUREMENTS IN POROUS TRANSPORT LAYERS OF A PEM FUEL
   CELL}},
Booktitle = {{PROCEEDINGS IF THE ASME 9TH INTERNATIONAL CONFERENCE ON NANOCHANNELS,
   MICROCHANNELS AND MINICHANNELS 2011, VOL 1}},
