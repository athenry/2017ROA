@inproceedings{ ISI:000325405800051,
Author = {Sameoto, D.},
Editor = {{Misra, D and Chen, Z and Iwai, H and Datta, S and Bauza, D and Chikyow, T and Obeng, Y}},
Title = {{Dry Adhesives for MEMS Assembly, Manipulation and Integration: Progress
   and Challenges}},
Booktitle = {{DIELECTRICS FOR NANOSYSTEMS 5: MATERIALS SCIENCE, PROCESSING,
   RELIABILITY, AND MANUFACTURING -AND-TUTORIALS IN NANOTECHNOLOGY: MORE
   THAN MOORE - BEYOND CMOS EMERGING MATERIALS AND DEVICES}},
Series = {{ECS Transactions}},
Year = {{2012}},
Volume = {{45}},
Number = {{3}},
Pages = {{515-528}},
Note = {{5th International Symposium on Dielectrics for Nanosystems - Materials
   Science, Processing, Reliability and Manufacturing as Part of the 221st
   Meeting of the Electrochemical-Society (ECS), Seattle, WA, MAY 06-10,
   2012}},
Organization = {{Electrochem Soc; Electrochem Soc, Dielectr Sci \& Technol Div; IEEE
   Electron Devices Soc; Elect \& Photon; Sensor; New Technol Subcommittee}},
Abstract = {{Bioinspired synthetic dry adhesives offer unique potential in the field
   of micro assembly and manipulation. These adhesives, which take their
   basic operating principle from animals like geckos, use van der Waals
   adhesion forces and mechanical fiber optimization to produce significant
   forces. For several years, these materials have been of interest for
   climbing robots and adhesive surfaces due to potential characteristics
   like self-cleaning or anti-fouling behavior, anisotropic adhesion
   strengths and non-transferring materials. These same strengths would
   make these materials an ideal mechanism for handling delicate parts or
   manipulating structures in the field of micro-electro-mechanical systems
   (MEMS) packaging. In this paper, I review the specific fabrication
   processes developed by our group and examine some of the challenges in
   their integration with MEMS assembly and packaging processes. Improving
