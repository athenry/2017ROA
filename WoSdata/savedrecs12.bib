@inproceedings{ ISI:000310776203132,
Author = {Duan, Ping and Yang, Fan and Chen, Tongwen and Shah, Sirish L.},
Book-Group-Author = {{IEEE}},
Title = {{Detection of Direct Causality Based on Process Data}},
