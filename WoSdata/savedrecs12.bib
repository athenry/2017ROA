@inproceedings{ ISI:000312381402141,
Author = {Zhang, Jianshu and Roemer, Florian and Haardt, Martin and
   Khabbazibasmenj, Arash and Vorobyov, Sergiy A.},
Book-Group-Author = {{IEEE}},
Title = {{SUM RATE MAXIMIZATION FOR MULTI-PAIR TWO-WAY RELAYING WITH
   SINGLE-ANTENNA AMPLIFY AND FORWARD RELAYS}},
Booktitle = {{2012 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
