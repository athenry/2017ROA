@article{ ISI:000300426000003,
Author = {Amarasuriya, Gayan and Tellambura, Chintha and Ardakani, Masoud},
Title = {{Joint Relay and Antenna Selection for Dual-Hop Amplify-and-Forward MIMO
   Relay Networks}},
Journal = {{IEEE TRANSACTIONS ON WIRELESS COMMUNICATIONS}},
Year = {{2012}},
Volume = {{11}},
Number = {{2}},
Pages = {{493-499}},
Month = {{FEB}},
Abstract = {{Four joint relay and antenna selection strategies for dual-hop
   amplify-and-forward (AF) multiple-input multiple-output relay networks
   are studied. Two of them require full channel state information (CSI)
   whereas the other two require only partial CSI. The relays are either
   channel-assisted AF or fixed-gain AF type. The first joint selection
   strategy involves choosing the best relay and the best single transmit
   antennas at the source and the relay. The second strategy jointly
   involves choosing the best relay and the best single transmit/receive
   antenna pairs at the source-to-relay and relay-to-destination channels.
   Moreover, two partial selection strategies, which can be used when the
   global CSI is not available, are also proposed and analyzed. In order to
   quantify the system performance analytically, the exact outage
   probability of all selection strategies is derived in closed-form.
   Direct insights into the system-design are obtained by deriving the
   asymptotic outage probability, asymptotic average symbol error rate,
   diversity order and array gain.}},
