@inproceedings{ ISI:000313296505141,
Author = {Lu, Cheng and Mandal, Mrinal},
Book-Group-Author = {{IEEE}},
Title = {{Automated Segmentation and Analysis of the Epidermis Area in Skin
   Histopathological Images}},
Booktitle = {{2012 ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN MEDICINE
   AND BIOLOGY SOCIETY (EMBC)}},
