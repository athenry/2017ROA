@inproceedings{ ISI:000320768400357,
Author = {Fang, Hao and Vorobyov, Sergiy A. and Jiang, Hai and Taheri, Omid},
Editor = {{Matthews, MB}},
Title = {{2D Signal Compression via Parallel Compressed Sensing with Permutations}},
Booktitle = {{2012 CONFERENCE RECORD OF THE FORTY SIXTH ASILOMAR CONFERENCE ON
   SIGNALS, SYSTEMS AND COMPUTERS (ASILOMAR)}},
Series = {{Conference Record of the Asilomar Conference on Signals Systems and
   Computers}},
Year = {{2012}},
Pages = {{1925-1929}},
Note = {{46th Asilomar Conference on Signals, Systems and Computers, Pacific
   Grove, CA, NOV 04-07, 2012}},
Organization = {{Naval Postgraduate Sch; ATK Miss Res; IEEE Signal Proc Soc}},
Abstract = {{In this paper, we propose a new scheme to compress 2D signals using
   parallel compressed sensing. According to this seheme, the
   reeonstruction at the decoder can be performed in parallel. By
   performing certain permutation on a 2D signal, all columns are insured
   to have approximately the same density level and can be sampled using
   the same measurement matrix. In this way, vectorization of a 2D signal
   can be avoided, and thus the size of the measurement matrix can be
   dramatically reduced. We prove that with a good permutation, we can have
   a tighter upper bound on reconstruction mean square error. To illustrate
   this scheme, we apply it to video compression and use two kinds of
   permutations for different frames: the zigzag-scan-based permutation for
   reference frames and the block-test-based permutation for non-reference
   frames. Simulation results show that under the same compression ratio,
   the peak signal-to-noise ratio can be improved by approximately 3-7 dB
   compared to the case without permutation.}},
