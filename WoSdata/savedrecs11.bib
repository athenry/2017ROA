@article{ ISI:000307416500006,
Author = {Feng, Zhipeng and Zuo, Ming J.},
Title = {{Vibration signal models for fault diagnosis of planetary gearboxes}},
Journal = {{JOURNAL OF SOUND AND VIBRATION}},
Year = {{2012}},
Volume = {{331}},
Number = {{22}},
Pages = {{4919-4939}},
Month = {{OCT 22}},
Abstract = {{A thorough understanding of the spectral structure of planetary gear
   system vibration signals is helpful to fault diagnosis of planetary
   gearboxes. Considering both the amplitude modulation and the frequency
   modulation effects due to gear damage and periodically time variant
   working condition, as well as the effect of vibration transfer path,
   signal models of gear damage for fault diagnosis of planetary gearboxes
   are given and the spectral characteristics are summarized in closed
   form. Meanwhile, explicit equations for calculating the characteristic
   frequency of local and distributed gear fault are deduced. The
   theoretical derivations are validated using both experimental and
   industrial signals. According to the theoretical basis derived, manually
   created local gear damage of different levels and naturally developed
   gear damage in a planetary gearbox can be detected and located. (c) 2012
   Elsevier Ltd. All rights reserved.}},
