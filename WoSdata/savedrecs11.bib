@article{ ISI:000307789500005,
Author = {Tsay, Alan and Vien Van},
Title = {{Field Coupling Method for the Direct Synthesis of 2-D Microring
   Resonator Networks}},
Journal = {{IEEE JOURNAL OF QUANTUM ELECTRONICS}},
Year = {{2012}},
Volume = {{48}},
Number = {{10}},
Pages = {{1314-1321}},
Month = {{OCT}},
Abstract = {{We present an analytical method for designing coupled microring
   resonator (CMR) networks of general 2-D coupling topologies. The method
   is based on the ``field coupling in space{''} formalism of microring
   resonators, which gives a more rigorous and accurate description of CMRs
   than formulations based on ``energy coupling in time,{''} especially
   under strong coupling conditions. Given the discrete-time transfer
   functions of the optical filter, this method enables the field coupling
   matrix of the CMR network to be extracted, so that an optimum coupling
