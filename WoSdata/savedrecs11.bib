@article{ ISI:000308577900026,
Author = {Gimbun, J. and Rielly, C. D. and Nagy, Z. K. and Derksen, J. J.},
Title = {{Detached eddy simulation on the turbulent flow in a stirred tank}},
Journal = {{AICHE JOURNAL}},
