@article{ ISI:000307415800035,
Author = {Gagnon, K. and Wilson, J. S. and Holt, C. M. B. and Abrams, D. N. and
   McEwan, A. J. B. and Mitlin, D. and McQuarrie, S. A.},
Title = {{Cyclotron production of Tc-99m: Recycling of enriched Mo-100 metal
   targets}},
Journal = {{APPLIED RADIATION AND ISOTOPES}},
Year = {{2012}},
Volume = {{70}},
Number = {{8}},
Pages = {{1685-1690}},
Month = {{AUG}},
Abstract = {{There is growing interest in the large scale cyclotron production of
   Tc-99m via the Mo-100(p,2n)Tc-99m reaction. While the use and recycling
   of cyclotron-irradiated enriched molybdenum targets has been reported
   previously in the context of Tc-94m production, to the best of our
   knowledge, previous recycling studies have been limited to the use of
   oxide targets. To facilitate reuse of high-power enriched Mo-100
   targets, this work presents and evaluates a strategy for recycling of
   enriched metallic molybdenum. For the irradiated Mo-100 targets in this
   study, an overall metal to metal recovery of 87\% is reported.
   Evaluation of ``new{''} and ``recycled{''} Mo-100 revealed no changes in
   the molybdenum isotopic composition (as measured via ICP-MS). For
   similar irradiation conditions of ``new{''} and ``recycled{''} Mo-100,
   (i.e. target thicknesses, irradiation time, and energy), comparable
   levels of Tc-94g, Tc-95g, and Tc-96g contaminants were observed.
   Comparable QC specifications (i.e. aluminum ion concentration, pH, and
   radiochemical purity) were also reported. We finally note that
   {[}Tc-99m]-MDP images obtained by comparing MDP labelled with
   generator-based Tc-99m vs. Tc-99m obtained following the irradiation of
   recycled Mo-100 demonstrated comparable biodistribution. With the goal
   of producing large quantities of Tc-99m, the proposed methodology
   demonstrates that efficient recycling of enriched metallic Mo-100
   targets is feasible and effective. (C) 2012 Elsevier Ltd. All rights
   reserved.}},
