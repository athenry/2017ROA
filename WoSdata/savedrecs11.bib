@inproceedings{ ISI:000369918000323,
Author = {Phillips, Seth W. and Fair, Ivan J.},
Book-Group-Author = {{IEEE}},
Title = {{Binary-to-Ternary Sparsity Encoding for Holographic Data Storage}},
Booktitle = {{2013 IEEE PHOTONICS CONFERENCE (IPC)}},
Series = {{IEEE Photonics Conference}},
Year = {{2013}},
Pages = {{663-664}},
Note = {{26th IEEE Photonics Conference (IPC), Bellevue, WA, SEP 08-12, 2013}},
Organization = {{IEEE; Amer Chem Soc; Multidisciplinary Digital Publishing Inst; IEEE
   Cloud Comp; LioniX}},
Abstract = {{We consider holographic data storage in which data is represented by an
   array of pixels with modulation in amplitude and phase, and the Fourier
   transform of this array is created optically and recorded in a
   holographic medium. However, if the stored image contains high intensity
   peaks, the material can saturate, and on readout, the reconstructed data
   array will contain errors. Therefore, in this paper we present a coding
   technique to reduce the severity of Fourier domain peaks in the recorded
   images by enforcing a sparsity constraint on the data array prior to
   creating the Fourier transform image. We limit our consideration to
   hybrid-ternary modulated (HTM) data arrays.}},
