@article{ ISI:000377315500007,
Author = {Xu, Puhao and Wang, Fajun and Yang, Chao and Ou, Junfei and Li, Wen and
   Amirfazli, Alidad},
Title = {{Reversible transition between superhydrophobicity and
   superhydrophilicity of a silver surface}},
Journal = {{SURFACE \& COATINGS TECHNOLOGY}},
Year = {{2016}},
Volume = {{294}},
Pages = {{47-53}},
Month = {{MAY 25}},
Abstract = {{In the present work, superhydrophobic silver surface was prepared by
   immersing a copper plate in an aqueous solution of AgNO3 for 20 min. The
   sample exhibits superhydrophilicity after dried at 150 degrees C for 30
   min in an oven. The as-prepared silver surfaces exhibit contact angle
   (CA) larger than 160 degrees and sliding angle (SA) lower than 5
   degrees. Neither low surface free energy (SFE) materials nor organic
   compounds were used during the producing process. It was suggested that
   the spontaneously adsorbed carbon contaminant with low SFE was very
   critical for the superhydrophobicity of silver surface during heat
   treatment. Additionally, the silver surface could be switched between
   superhydrophobicity and superhydrophilicity by alternating UV
   irradiation and heat treatment for many times. Our findings in this work
   might provide a simple route to rational control the spontaneously
   adsorbed carbon contaminant on solid surface to endow the metal or other
   solid surfaces with superhydrophobicity or superhydrophilicity. (C) 2016
   Elsevier B.V. All rights reserved.}},
