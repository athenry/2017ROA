@article{ ISI:000371828600005,
Author = {Goudarzi, Nabi and Korany, Yasser and Adeeb, Samer and Cheng, Roger},
Title = {{Characterization of the shear behavior of Z-shaped steel plate
   connectors used in insulated concrete panels}},
Journal = {{PCI JOURNAL}},
Year = {{2016}},
Pages = {{23-37}},
Month = {{MAR-APR}},
Abstract = {{Insulated concrete panels are composed of two concrete layers with
   thermal insulation between them. The out-of-plane behavior of these
   panels depends on the shear behavior of their interlayer mechanical
   connectors. A new connector system using Z-shaped steel plates is under
   development. The shear strength and stiffness of the Z-shaped steel
   plate connectors should be determined to approximate the out-of-plane
   flexural response of insulated concrete panels. This paper presents the
   results of 11 push-off shear tests performed on Z-shaped steel plate
   connectors to study the effect of their width and thickness on their
   shear strength and stiffness. A simplified plate model and a tension tie
   model were adopted to estimate the shear behavior of the Z-shaped steel
   plate connectors. The experimental results indicate that the shear
   behavior of the tested Z-shaped steel plate connectors is affected by
   connector-concrete debonding and concrete cracking around the
   connector-concrete connection and that these effects increase with the
   thickness-to-width ratio of the connectors. The plastic shear strength
   of the simplified plate model using the Tresca yield criterion and the
   lateral stiffness of the tension tie model estimate the shear strength
   and stiffness of the tested connectors, respectively, with reasonable
   accuracy.}},
