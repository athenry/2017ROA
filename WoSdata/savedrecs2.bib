@article{ ISI:000371240500010,
Author = {White, C. W. and Ambrose, E. and Mueller, A. and Li, Y. and Le, H. and
   Thliveris, J. and Arora, R. C. and Lee, T. W. and Dixon, I. M. C. and
   Tian, G. and Nagendran, J. and Hryshko, L. V. and Freed, D. H.},
Title = {{Avoidance of Profound Hypothermia During Initial Reperfusion Improves
   the Functional Recovery of Hearts Donated After Circulatory Death}},
Journal = {{AMERICAN JOURNAL OF TRANSPLANTATION}},
Year = {{2016}},
Volume = {{16}},
Number = {{3}},
Pages = {{773-782}},
Month = {{MAR}},
Abstract = {{The resuscitation of hearts donated after circulatory death (DCD) is
   gaining widespread interest; however, the method of initial reperfusion
   (IR) that optimizes functional recovery has not been elucidated. We
   sought to determine the impact of IR temperature on the recovery of
   myocardial function during ex vivo heart perfusion (EVHP). Eighteen pigs
   were anesthetized, mechanical ventilation was discontinued, and cardiac
   arrest ensued. A 15-min standoff period was observed and then hearts
   were reperfused for 3min at three different temperatures (5 degrees C;
   N=6, 25 degrees C; N=5, and 35 degrees C; N=7) with a normokalemic
   adenosine-lidocaine crystalloid cardioplegia. Hearts then underwent
   normothermic EVHP for 6h during which time myocardial function was
   assessed in a working mode. We found that IR coronary blood flow
   differed among treatment groups (5 degrees C=48353, 25 degrees C=722 +/-
   60, 35 degrees C=906 +/- 36mL/min, p<0.01). During subsequent EVHP, less
   myocardial injury (troponin I: 5 degrees C=91 +/- 6, 25 degrees C=64 +/-
   16, 35 degrees C=57 +/- 7pg/mL/g, p=0.04) and greater preservation of
   endothelial cell integrity (electron microscopy injury score: 5 degrees
   C=3.2 +/- 0.5, 25 degrees C=1.8 +/- 0.2, 35 degrees C=1.7 +/- 0.3,
   p=0.01) were evident in hearts initially reperfused at warmer
   temperatures. IR under profoundly hypothermic conditions impaired the
   recovery of myocardial function (cardiac index: 5 degrees C=3.9 +/- 0.8,
   25 degrees C=6.2 +/- 0.4, 35 degrees C=6.5 +/- 0.6mL/minute/g, p=0.03)
   during EVHP. We conclude that the avoidance of profound hypothermia
   during IR minimizes injury and improves the functional recovery of DCD
   hearts.}},
