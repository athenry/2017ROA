@article{ ISI:000369196500020,
Author = {Oosterhof, Steven A. and Driver, Robert G.},
Title = {{Shear connection modelling for column removal analysis}},
Journal = {{JOURNAL OF CONSTRUCTIONAL STEEL RESEARCH}},
Year = {{2016}},
Volume = {{117}},
Pages = {{227-242}},
Month = {{FEB}},
Abstract = {{Although methods for modelling steel shear connections under column
   removal scenarios are available in widely-used design guidelines, they
   tend to be based on earthquake engineering research rather than research
   examining scenarios relevant to progressive collapse. Even where these
   have been adapted explicitly for this new purpose, they take a form that
   does not account for the true connection behaviour under this unique
   loading regime. This paper describes an accurate mechanical model that
   has been developed based on observations from a comprehensive testing
   programme designed specifically to study the behaviour of steel shear
   connections under column removal scenarios. The model is used to assess
   the influences of key parameters and develop a simple single-spring
   model practical for use in full-building collapse analyses. (C) 2015
   Elsevier Ltd. All rights reserved.}},
