@article{ ISI:000373147800026,
Author = {Bagheri, Pooya and Xu, Wilsun and Ding, Tianyu},
Title = {{A Distributed Filtering Scheme to Mitigate Harmonics in Residential
   Distribution Systems}},
Journal = {{IEEE TRANSACTIONS ON POWER DELIVERY}},
Year = {{2016}},
Volume = {{31}},
Number = {{2}},
Pages = {{648-656}},
Month = {{APR}},
Abstract = {{This paper presents a novel scheme to mitigate harmonic distortions in
   modern residential distribution systems. The scheme deploys multiple
   low-voltage filters in randomly selected locations of a distribution
   system. The main challenges of such a distributed filtering scheme, cost
   of installation and hardware, are solved through an innovative filter
   configuration and installation concept, namely, the filter is a 240 V
   device that can be easily plugged into the sockets of utility revenue
   meters. Analytical, simulation, and economic studies have been conducted
   to understand the filter design parameters, performance characteristics,
   and cost implications. As an example application, the scheme is
   evaluated for mitigating an actual harmonic-caused telephone
   interference problem. The results show that the proposed scheme can be a
   promising option to reduce the harmonic distortions in selected areas of
   a distribution system.}},
