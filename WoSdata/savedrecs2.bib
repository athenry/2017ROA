@article{ ISI:000372379700071,
Author = {Schulze, S. and Richter, A. and Vascellari, M. and Gupta, A. and Meyer,
   B. and Nikrityuk, P. A.},
Title = {{Novel intrinsic-based submodel for char particle gasification in
   entrained-flow gasifiers: Model development, validation and illustration}},
Journal = {{APPLIED ENERGY}},
Year = {{2016}},
Volume = {{164}},
Pages = {{805-814}},
Month = {{FEB 15}},
Abstract = {{The final carbon conversion rate is of critical importance in the
   efficiency of gasifiers. Therefore, comprehensive modeling of char
   particle conversion is of primary interest for designing new gasifiers.
   This work presents a novel intrinsic-based submodel for the gasification
   of a char particle moving in a hot flue gas environment considering CO2
   and H2O as inlet species. The first part of the manuscript describes the
   model and its derivation. Validations against experiments carried out in
   this work for German lignite char are reported in the second part. The
   comparison between submodel predictions and experimental data shows good
   agreement. The importance of char porosity change during gasification is
   demonstrated. The third part presents the results of CFD simulations
   using the new submodel and a surface-based sub model for a generic
   endothermic gasifier. The focus of CFD simulations is to demonstrate the
   crucial role of intrinsic based heterogeneous reactions in the adequate
   prediction of carbon conversion rates. (C) 2015 Elsevier Ltd. All rights
   reserved.}},
