@article{ ISI:000376652400009,
Author = {Chen, Jiao and Pan, Tianhong and Pu, Tianqing and Huang, Biao and Huang,
   Dorothy Yu and Zhang, Weiping and Gabos, Stephan and Jin, Can},
Title = {{Predicting GHS toxicity using RTCA and discrete-time Fourier transform}},
Journal = {{JOURNAL OF BIOINFORMATICS AND COMPUTATIONAL BIOLOGY}},
Year = {{2016}},
