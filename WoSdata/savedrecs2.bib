@article{ ISI:000375557900004,
Author = {Wang, Xu and Schiavone, Peter},
Title = {{Torsion of an arbitrarily shaped nanosized bar}},
Journal = {{ARCHIVE OF APPLIED MECHANICS}},
Year = {{2016}},
Volume = {{86}},
Number = {{6}},
Pages = {{1037-1048}},
Month = {{JUN}},
Abstract = {{We investigate the Saint-Venant torsion problem of a nanosized bar with
   arbitrary smooth cross section by enhancing the classical
   continuum-based model with a theory of surface elasticity based on a
   version of the Gurtin-Murdoch surface/interface model. An elegant
   analytical solution is derived by means of conformal mapping, Faber
   series, Fourier series, and simple matrix algebra. Our solution leads to
   the establishment of a complex torsion function which, in turn, allows
   for a concise expression for the torsional rigidity. Our results clearly
   demonstrate that both the stress field and the normalized torsional
   rigidity are size dependent. An approximate formula for the
   size-dependent torsional rigidity is also proposed.}},
