@article{ ISI:000370148900014,
Author = {He, Chuan and Zeng, Hongbo and Chen, Jie},
Title = {{Modeling of the Effect of Cell Deformation Associated with Microbubble
   Collision in Centrifugation Field}},
Journal = {{CELLULAR AND MOLECULAR BIOENGINEERING}},
Year = {{2016}},
Volume = {{9}},
Number = {{1}},
Pages = {{162-174}},
Month = {{MAR}},
Abstract = {{Cell membranes are highly selective structures and prevent foreign
   objects, such as biomolecules and genes, from entering cells. Different
   carrier-based delivery methods have been developed and each method has
   its advantages and disadvantages. In previous article, a
   microbubble-based gene delivery method was developed to increase cell
   membrane permeability. In a high centrifugation field, cells deform and
   collide with microbubbles which induces intracellular pathways through
   cell membranes. This delivery method significantly increases delivery
   efficiency especially when using hard-to-deliver cells, such as stem
   cells and plant cells. To understand the mechanism of this delivery
   method, the structural behaviour of cell models under hydrodynamic
   collision pressure was analyzed by finite element analysis. Simulation
   results show that cells with more deformed structures experience higher
   strain on the cell membrane than cells with less deformed structures
   while the cells' mechanical properties and the collision conditions were
   kept the same.}},
