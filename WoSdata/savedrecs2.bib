@article{ ISI:000378925400030,
Author = {Cui, Yi and Chen, Zengtao},
Title = {{Material transport via the emission of shear loops during void growth: A
   molecular dynamics study}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2016}},
Volume = {{119}},
Number = {{22}},
Month = {{JUN 14}},
Abstract = {{The growth of a nanovoid in single-crystal copper has been studied via
   molecular dynamics ( MD) method. The objective is to build the
   correlation between material transport pattern and dislocation
   structures. MD results are examined by characterizing the material
   transport via the ``relative displacement{''} of atoms, where the
   homogenous elastic deformation has been excluded. Through this novel
   approach, we are able to illustrate the feasibility of void growth
   induced by shear loops/curves. At a smaller scale, the formation and
   emission of shear loops/curves contribute to the local mass transport.
   At a larger scale, a new mechanism of void growth via frustum-like
   dislocation structure is revealed. A phenomenological description of
   void growth via frustum-like dislocation structure is also proposed.
   Published by AIP Publishing.}},
