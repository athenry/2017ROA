@article{ ISI:000299111000001,
Author = {Agharazi, Alireza and Tannant, DwayneD. and Martin, C. Derek},
Title = {{Characterizing rock mass deformation mechanisms during plate load tests
   at the Bakhtiary dam project}},
Journal = {{INTERNATIONAL JOURNAL OF ROCK MECHANICS AND MINING SCIENCES}},
Year = {{2012}},
Volume = {{49}},
Pages = {{1-11}},
Month = {{JAN}},
Abstract = {{The results of 26 plate load tests carried out at the Bakhtiary dam site
   were analyzed to define and characterise the mechanisms governing the
   rock mass deformation during the tests. Tests were categorized into
   groups based on the relative direction of loading to the orientation of
   major discontinuity sets running through the rock mass. Five mechanisms
   were distinguished as the main modes controlling deformations of the
   rock mass during the tests. These mechanisms were based on analysis of
   the shape of stress-deformation curves, total and plastic deformation,
   anisotropy and deformation hysteresis. This study shows that part of the
   scatter commonly observed in the moduli calculated from plate load tests
   can be attributed to the different deformation mechanisms that are
   mobilized during the tests. Incompatibility of the theoretical
   assumptions, made for the interpretation of the test results, with
   actual deformation mechanisms is shown to be another factor causing
   scatter in the calculated moduli. (C) 2011 Elsevier Ltd. All rights
   reserved.}},
