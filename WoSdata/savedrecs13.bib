@article{ ISI:000300462300044,
Author = {Dong, Ziqiang and Chen, Weixing and Zheng, Wenyue and Guzonas, Dave},
Title = {{Corrosion behavior of porous chromium carbide in supercritical water}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2012}},
Volume = {{420}},
Number = {{1-3}},
Pages = {{320-327}},
Month = {{JAN}},
Abstract = {{The corrosion behavior of highly porous chromium carbide (Cr3C2)
   prepared by a reactive sintering process was characterized at
   temperatures ranging from 375 degrees C to 625 degrees C in a
   supercritical water environment with a pressure of 25-30 MPa. The test
   results show that porous chromium carbide is stable in SCW environments
   at temperatures under 425 degrees C, above which disintegration
   occurred. The porous carbide was also tested under hydrothermal
   conditions of pressures between 12 MPa and 50 MPa at constant
   temperatures of 400 degrees C and 415 degrees C, respectively. The
   pressure showed little effect on the stability of chromium carbide in
   the tests at those temperatures. The mechanism of disintegration of
   chromium carbide in SCW environments is discussed. (C) 2011 Elsevier
   B.V. All rights reserved.}},
