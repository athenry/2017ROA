@inproceedings{ ISI:000331660100013,
Author = {Kandakkathara, A. and Utkin, I. and Fedosejevs, R.},
Editor = {{Kieffer, JC}},
Title = {{High sensitivity Surface Enhanced Raman Scattering Detection of Organic
   Molecules and Amino Acids}},
Booktitle = {{PHOTONICS NORTH 2012}},
