@article{ ISI:000361549900004,
Author = {Ong, Florian R. and Cui, Zheng and Yurtalan, Muhammet A. and Vojvodin,
   Cameron and Papaj, Michal and Orgiazzi, Jean-Luc F. X. and Deng,
   Chunqing and Bal, Mustafa and Lupascu, Adrian},
Title = {{Suspended graphene devices with local gate control on an insulating
   substrate}},
Journal = {{NANOTECHNOLOGY}},
Year = {{2015}},
Volume = {{26}},
Number = {{40}},
Month = {{OCT 9}},
Abstract = {{We present a fabrication process for graphene-based devices where a
   graphene monolayer is suspended above a local metallic gate placed in a
   trench. As an example we detail the fabrication steps of a graphene
   field-effect transistor. The devices are built on a bare
   high-resistivity silicon substrate. At temperatures of 77 K and below,
   we observe the field-effect modulation of the graphene resistivity by a
   voltage applied to the gate. This fabrication approach enables new
   experiments involving graphene-based superconducting qubits and
   nano-electromechanical resonators. The method is applicable to other
   two-dimensional materials.}},
