@inproceedings{ ISI:000387871100035,
Author = {Harris, John and Hancock, Mark and Scott, Stacey D.},
Book-Group-Author = {{ACM}},
Title = {{Leveraging Asymmetries in Multiplayer Games: Investigating Design
   Elements of Interdependent Play}},
