@article{ ISI:000364014000009,
Author = {Al Mashagbeh, Mohammad and Khamesee, Mir Behrad},
Title = {{Force control for position interface industrial manipulator working in
   unknown environment}},
Journal = {{MICROSYSTEM TECHNOLOGIES-MICRO-AND NANOSYSTEMS-INFORMATION STORAGE AND
   PROCESSING SYSTEMS}},
Year = {{2015}},
Volume = {{21}},
Number = {{12}},
