@article{ ISI:000380366000009,
Author = {Wen, Chao and Huang, Ping and Li, Jianguang and Peng, Qiuan and Jiang,
   Chaozhe},
Title = {{Identifying key trains and their reasonable ratios in high-speed rail
   timetables}},
Journal = {{JOURNAL OF TRANSPORTATION SAFETY \& SECURITY}},
Year = {{2016}},
Volume = {{8}},
Number = {{1, SI}},
Pages = {{118-135}},
Abstract = {{A key train plays an important role in the rail timetable. The
   unreasonable key trains' layout greatly influences the normal
   train-running order, thus affecting the quality of the high-speed rail
   (HSR) timetable. This article studies the probability produced by train
   paths for overcoming bottlenecks and identifies key trains in the HSR
   timetable by means of predicting the conflicts using triangular fuzzy
   numbered workflow nets, that is, key trains have greater effect on other
   trains and need to pay close attention to them. In addition,
   stimulations results that were performed with the HSR dispatching system
   illustrate the conclusion. Moreover, under certain capacity
   requirements, this article calculates reasonable key trains' ratio for
   different combinations of speed in a HSR timetable. This research
   demonstrates that (1) low-speed (Class B) trains are key trains in the
   HSR timetable and (2) when trains of 350 km/h and 250 km/h run on the
   same line, the key trains (250 km/h) ratio of the Beijing-Shanghai HSR
   should be less than 10\%. Currently, for trains running on the
   Beijing-Shanghai HSR, whose speeds are 300 km/h and 250 km/h, the key
   trains (250 km/h) ratio should be no more than 20\%.}},
