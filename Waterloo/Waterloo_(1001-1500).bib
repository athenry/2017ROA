@article{ ISI:000363422100082,
Author = {Pathirane, M. and Iheanacho, B. and Tamang, A. and Lee, C-H. and Lujan,
   R. and Knipp, D. and Wong, W. S.},
Title = {{Hybrid ZnO nanowire/a-Si: H thin-film radial junction solar cells using
   nanoparticle front contacts}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{107}},
Number = {{14}},
Month = {{OCT 5}},
Abstract = {{Hydrothermally synthesized disordered ZnO nanowires were conformally
   coated with a-Si: H thin-films to fabricate three dimensional hybrid
   nanowire/thin-film structures. The a-Si: H layer formed a radial
   junction p-i-n diode solar cell around the ZnO nanowire. The cylindrical
   hybrid solar cells enhanced light scattering throughout the
   UV-visible-NIR spectrum (300 nm-800 nm) resulting in a 22\% increase in
   short-circuit current density compared to the reference planar p-i-n
   device. A fill factor of 69\% and a total power conversion efficiency of
   6.5\% were achieved with the hybrid nanowire solar cells using a spin-on
   indium tin oxide nanoparticle suspension as the top contact. (C) 2015
   AIP Publishing LLC.}},
Publisher = {{AMER INST PHYSICS}},
Address = {{1305 WALT WHITMAN RD, STE 300, MELVILLE, NY 11747-4501 USA}},
