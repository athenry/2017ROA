@inproceedings{ ISI:000378539900018,
Author = {Olesen, Jacob Bjerring and Villagomez-Hoyos, Carlos Armando and Traberg,
   Marie Sand and Chee, Adrian J. Y. and Yiu, Billy Y. S. and Ho, Chung Kit
   and Yu, Alfred C. H. and Jensen, Jorgen Arendt},
Editor = {{Duric, N and Heyde, B}},
Title = {{Preliminary investigation of an ultrasound method for estimating
   pressure changes in deep-positioned vessels}},
Booktitle = {{MEDICAL IMAGING 2016: ULTRASONIC IMAGING AND TOMOGRAPHY}},
