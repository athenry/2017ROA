@article{ ISI:000364245500001,
Author = {Huang, Yifeng and Du, Jennifer R. and Zhang, Yufeng and Lawless, Darren
   and Feng, Xianshe},
Title = {{Removal of mercury (II) from wastewater by polyvinylamine-enhanced
   ultrafiltration}},
Journal = {{SEPARATION AND PURIFICATION TECHNOLOGY}},
Year = {{2015}},
Volume = {{154}},
Pages = {{1-10}},
Month = {{NOV 5}},
Abstract = {{This study deals with the concentration and removal of mercury (II) from
   wastewater by polymer-enhanced ultrafiltration (PEUF) using
   polyvinylamine as the mercury-binding polymer. A mercury removal as high
   as 99\% was obtained, which was otherwise impossible to achieve with
   conventional ultrafiltration. Over the feed mercury concentration range
   tested (0-50 ppm), the PVAm dosage used did not affect the mercury
   rejection considerably, while water flux was reduced significantly at a
   higher dosage of PVAm. A flux vs pressure relationship typical of
   ultrafiltration of macromolecular solutes was observed because of the
   amine polymer used to bind mercury, and the limiting flux appeared to
