@article{ ISI:000365688700003,
Author = {Kwon, Junyeon and Hong, Seongin and Hong, Young Ki and Lee, Sungho and
   Yoo, Geonwook and Yoon, Youngki and Kim, Sunkook},
Title = {{Photosensitivity enhancement in hydrogenated amorphous silicon thin-film
   phototransistors with gate underlap}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{107}},
Number = {{20}},
Month = {{NOV 16}},
Abstract = {{Conventional alpha-Si:H phototransistors exhibit poor photosensitivity
   due to low photo-conversion efficiency. To overcome this intrinsic
   limit, we introduce gate underlap in alpha-Si:H phototransistors and
   demonstrate photosensitivity enhancement. We show that photocurrent can
   be significantly larger than dark current by 4 orders of magnitude,
   using 1-mu m gate underlap at incident optical power density (P-inc) of
   3.2 W/cm(2). Our 1-mu m gate-underlap phototransistor exhibits higher
   photosensitivity than the device without gate underlap by 64 times with
   P-inc = 0.2 W/cm(2) and a wavelength of 785 nm. Our gate-underlapped
   phototransistors also show excellent time-resolved photoswitching
   behaviors, demonstrating the great potential for highly sensitive
   photodetectors. (C) 2015 AIP Publishing LLC.}},
