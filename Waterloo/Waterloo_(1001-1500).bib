Title = {{Effect of Column Type on Polyolefin Fractionation by High-Temperature
   Thermal Gradient Interaction Chromatography}},
Journal = {{MACROMOLECULAR SYMPOSIA}},
Year = {{2015}},
Volume = {{356}},
Number = {{1, SI}},
Pages = {{10-18}},
