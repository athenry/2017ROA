@inproceedings{ ISI:000387258800022,
Author = {Hazra, Budhaditya and Narasimhan, Sriram},
Editor = {{Dwivedy, SK and Tiwari, R}},
Title = {{Gearbox Fault Detection using Synchro-squeezing Transform}},
Booktitle = {{INTERNATIONAL CONFERENCE ON VIBRATION PROBLEMS 2015}},
Series = {{Procedia Engineering}},
