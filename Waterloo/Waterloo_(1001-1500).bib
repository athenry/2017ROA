@article{ ISI:000373015700016,
Author = {Abdallah, Mona and Wang, Ying and Abdel-Wahab, Wael M. and
   Safavi-Naeini, Safieddin},
Title = {{A Tunable Circuit Model for the Modeling of Dielectric Resonator Antenna
   Array}},
Journal = {{IEEE ANTENNAS AND WIRELESS PROPAGATION LETTERS}},
Year = {{2016}},
Volume = {{15}},
Pages = {{830-833}},
Abstract = {{An efficient, accurate, and tunable circuit model is presented for
   modeling of the substrate integrated waveguide (SIW) series-fed
   dielectric resonator antenna (DRA) array. For much improved model
   flexibility, the mutual coupling between antenna elements is taken into
   account and represented by a fully adjustable model, allowing the design
   parameters to be varied. Development of the circuit model is
   straightforward. Only one or two antenna elements from the array need to
   be simulated with a full-wave solver. Furthermore, the model is
   applicable for antenna arrays with different numbers of elements. The
   circuit model enables efficient iterative design and optimization of
   antenna arrays. Millimeter-wave SIW series-fed DRA array examples are
   used to validate the model. The simulation results using the proposed
   circuit model agree well with the full-wave solver and measurement
   results.}},
