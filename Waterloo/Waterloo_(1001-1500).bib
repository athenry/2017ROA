@article{ ISI:000366671200005,
Author = {Hadadzadeh, A. and Wells, M. A.},
Title = {{Inverse and centreline segregation formation in twin roll cast AZ31
   magnesium alloy}},
Journal = {{MATERIALS SCIENCE AND TECHNOLOGY}},
Year = {{2015}},
Volume = {{31}},
Number = {{14}},
Pages = {{1715-1726}},
Month = {{NOV}},
Abstract = {{Two microstructure defects formed in the twin roll cast AZ31 strips were
   investigated: inverse and centreline segregations. A two-dimensional
   finite element thermal-fluid-stress model was employed to study the
   thermomechanical response of the AZ31 strip during twin roll casting
   process. The results showed that the key parameter for centreline
   segregation is the mushy zone thickness at centreline. For inverse
   segregation, the interaction between the yy peak stress at the
   centreline in the mushy zone and the solidified shell on the roll
   surface is the determinant parameter. The modelling results suggested
   increasing the setback distance decreases the risk of both defects.
   Moreover, scaling up the caster reduces the propensity to inverse
   segregation but appears to have a minor effect for centreline
   segregation formation.}},
