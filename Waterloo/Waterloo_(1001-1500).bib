@article{ ISI:000359641900027,
Author = {Riahifar, Reza and Raissi, Babak and Zamani, Cyrus and Marzbanrad, Ehsan},
Title = {{Simulation of TiO2 particle trajectory in AC electric field}},
Journal = {{COMPUTATIONAL MATERIALS SCIENCE}},
Year = {{2015}},
Volume = {{108}},
Number = {{A}},
Pages = {{183-191}},
Month = {{OCT}},
Abstract = {{In this work, trajectory of ceramic particles is simulated to explain
   different patterns formed during deposition process under AC electric
   fields. For this purpose, Navier-Stokes and Laplace's equations were
   numerically solved to obtain fluid velocity and electric field in the
   domain. All dominant forces such as electrical, Buoyancy, drag, and
   Brownian forces are considered in the equation of particles' motion
   which is solved by 4th order Runge-Kutta method. The present simulation
   was able to predict which of the particles would move to high electric
   field regions and which ones would remain in the medium according to
   system parameters. Using the results of the present simulation,
   different deposition patterns of TiO2 particles on coplanar electrodes
   under applied AC electric field were explained. Two deposition
   mechanisms were suggested for TiO2 particles dispersed in liquid medium
   depending on the applied frequency. (C) 2015 Elsevier B.V. All rights
   reserved.}},
