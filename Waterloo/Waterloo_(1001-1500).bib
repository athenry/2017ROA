@inproceedings{ ISI:000387497400051,
Author = {Qin, Peng and Jin, Lifang and Li, Fan and Han, Tao and Du, Lianfang and
   Yu, Alfred C. H.},
Book-Group-Author = {{IEEE}},
Title = {{The relationship Between Microbubble Size and Heterogeneous Sonoporation
   at the Single-Cell Level}},
Booktitle = {{2016 IEEE INTERNATIONAL ULTRASONICS SYMPOSIUM (IUS)}},
Series = {{IEEE International Ultrasonics Symposium}},
Year = {{2016}},
Note = {{IEEE International Ultrasonics Symposium (IUS), Tours, FRANCE, SEP
   18-21, 2016}},
Organization = {{IEEE}},
Abstract = {{The aim of this investigation is to determine if the heterogeneous
   sonoporation depends on the microbubble size. Using a customized
   ultrasound exposure and observation setup, we observed sonoporation
   dynamics at the single-cell level and quantified exogenous molecular
   uptake levels to evaluate the degree of sonoporation. The non-uniform
   size distribution of SonoVue microbubbles themselves allowed us to
   analyze the influence of bubble size on sonoporation. 1 MHz ultrasound
   with 10-mu s duration and 0.6 MPa peak rarefactional pressure was used
   to induce the inertial collapse of microbubbles. Our data revealed the
   relationship between the microbubble size (diameter: D) and the
   heterogeneity of sonoporation when the bubble-to-cell distance was
   fixed. 1) The localized collapse of relatively small bubbles (D < 5.5 mu
   m) led to predictable sonoporation, the degree of which correlated with
   the bubble-to-cell distance (d). No sonoporation was observed when dID >
   1, whereas reversible sonoporation occurred when d/D < 1. 2) Large
   bubbles (D > 5.5 mu m) exhibited translational movement over large
   distances, resulting in unpredictable sonoporation. Translation towards
   the cell surface led to reversible or irreversible sonoporation, and
   translation away from the cell caused either no or reversible
   sonoporation. 3) Theoretical calculations indicated a nonlinear
   dependency of the primary radiation force on the bubble diameter, and
   the large bubbles were more strongly influenced by the radiation force.}},
