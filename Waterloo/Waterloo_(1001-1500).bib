@article{ ISI:000367067600029,
Author = {Diciano, Massimo and Caron, E. J. F. R. and Weckman, D. C. and Wells, M.
   A.},
Title = {{Interface Formation During Fusion (TM) Casting of AA3003/AA4045 Aluminum
   Alloy Ingots}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS B-PROCESS METALLURGY AND
   MATERIALS PROCESSING SCIENCE}},
Year = {{2015}},
Volume = {{46}},
Number = {{6}},
Pages = {{2674-2691}},
Month = {{DEC}},
Abstract = {{Fusion (TM) casting is a unique Direct Chill continuous casting process
   whereby two different alloys can be cast simultaneously, producing a
   laminated ingot for rolling into clad sheet metal such as AA3003/AA4045
   brazing sheet. Better understanding of the wetting and interface
   formation process during Fusion (TM) casting is required to further
   improve process yields and also explore use of other alloy systems for
   new applications. In this research, AA3003-core/AA4045-clad ingots were
   cast using a well-instrumented lab-scale Fusion (TM) casting system.
   As-cast Fusion (TM) interfaces were examined metallurgically and by
   mechanical testing. Computational fluid dynamic analyses of the Fusion
   (TM) casts were also performed. It was shown that the liquid AA4045-clad
   alloy was able to successfully wet and create an oxide-free,
   metallurgical, and mechanically sound interface with the lightly
   oxidized AA3003-core shell material. Based on the results of this study,
   it is proposed that the bond formation process at the alloys interface
   during casting is a result of discrete penetration of AA4045 liquid at
   defects in the preexisting AA3003 oxide, dissolution of underlying
   AA3003 by liquid AA4045, and subsequent bridging between penetration
   sites. Spot exudation on the AA3003 chill cast surface due to remelting
   and inverse segregation may also improve the wetting and bonding
   process. (C) The Minerals, Metals \& Materials Society and ASM
   International 2015}},
