@article{ ISI:000341897500026,
Author = {Pairojpiriyakul, Thirasak and Croiset, Eric and Kiatkittipong, Kunlanan
   and Kiatkittipong, Worapon and Arpornwichanop, Amornchai and
   Assabumrungrat, Suttichai},
Title = {{Catalytic reforming of glycerol in supercritical water with nickel-based
   catalysts}},
Journal = {{INTERNATIONAL JOURNAL OF HYDROGEN ENERGY}},
Year = {{2014}},
Volume = {{39}},
Number = {{27}},
Pages = {{14739-14750}},
Month = {{SEP 12}},
Abstract = {{The catalytic performance of nickel catalysts supported on La2O3,
   alpha-Al2O3, gamma-Al2O3, ZrO2, and YSZ for supercritical water
   reforming of glycerol was investigated. Experiments were conducted in a
   tubular reactor made of Inconel-625 with the temperature range of 723
   -848 K under a pressure of 25 MPa. Carbon formation causing operation
   failure was observed for alpha-Al2O3, gamma-Al2O3 and ZrO2 at
   temperatures higher than 748, 798 and 823 K, respectively. Ni/La2O3
   exhibited the highest H-2 yield where almost complete conversion was
   obtained at 798 K. Moderate space velocities (WHSV = 6.45 h(-1)) and
   glycerol feed concentration (5wt.\%) favor high hydrogen selectivity and
   yield. Methanation is favored at a low WHSV or high glycerol feed
   concentration, resulting in a lower H-2 yield. Increasing Ni loading on
   the Ni/La2O3 catalyst strongly promoted the reforming, water-gas shift,
   and methanation reactions, which contributed significantly to the
   product species distribution. Copyright (C) 2014, Hydrogen Energy
   Publications, LLC. Published by Elsevier Ltd. All rights reserved.}},
