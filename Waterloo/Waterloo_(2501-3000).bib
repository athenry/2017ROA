@article{ ISI:000343705300003,
Author = {Scott, Alison J. and Nabifar, Afsaneh and Penlidis, Alexander},
Title = {{Branched and Crosslinked Polymers Synthesized through NMRP: Quantitative
   Indicators for Network Homogeneity?}},
Journal = {{MACROMOLECULAR REACTION ENGINEERING}},
Year = {{2014}},
Volume = {{8}},
Number = {{9}},
Pages = {{639-657}},
Month = {{SEP}},
Abstract = {{Characterization of branched and crosslinked polystyrene (PS) samples,
   synthesized via nitroxide-mediated radical polymerization (NMRP) in the
   presence of a small amount of divinylbenzene (DVB), is presented.
   Results are compared with crosslinked PS samples synthesized through
   regular free radical polymerization (FRP). Characterization results of
   PS samples (before the gelation point) using size exclusion
   chromatography (SEC) and of crosslinked PS samples with
   thermo-mechanical analysis are discussed. Both techniques are used in an
   attempt to quantify the homogeneity of the polymer network.
   Characterization via thermo-mechanical analysis shows that no
   significant differences are detected between networks synthesized
   through NMRP and FRP. In fact, replicated results indicate that networks
   synthesized via NMRP might in fact be as heterogeneous as networks
   synthesized through FRP; this is not as expected!}},
