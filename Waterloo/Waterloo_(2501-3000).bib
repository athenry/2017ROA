@article{ ISI:000339664900083,
Author = {Zhang, Yingjie and Aziz, Hany},
Title = {{Very high efficiency phosphorescent organic light-emitting devices by
   using rough indium tin oxide}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2014}},
Volume = {{105}},
Number = {{1}},
Month = {{JUL 7}},
Abstract = {{The efficiency of organic light-emitting devices (OLEDs) is shown to
   significantly depend on the roughness of the indium tin oxide (ITO)
   anode. By using rougher ITO, light trapped in the ITO/organic
   wave-guided mode can be efficiently extracted, and a light outcoupling
   enhancement as high as 40\% is achieved. Moreover, contrary to
   expectations, the lifetime of OLEDs is not affected by ITO roughness.
   Finally, an OLED employing rough ITO anode that exhibits a current
   efficiency of 56 cd/A at the remarkably high brightness of 10(5) cd/m(2)
   is obtained. This represents the highest current efficiency at such high
   brightness to date for an OLED utilizing an ITO anode, without any
   external light outcoupling techniques. The results demonstrate the
   significant efficiency benefits of using ITO with higher roughness in
   OLEDs. (C) 2014 AIP Publishing LLC.}},
