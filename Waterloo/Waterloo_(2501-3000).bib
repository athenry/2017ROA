@article{ ISI:000342160400042,
Author = {Ismail, Muhammad and Zhuang, Weihua},
Title = {{Mobile Terminal Energy Management for Sustainable Multi-Homing Video
   Transmission}},
Journal = {{IEEE TRANSACTIONS ON WIRELESS COMMUNICATIONS}},
Year = {{2014}},
Volume = {{13}},
Number = {{8}},
Pages = {{4616-4627}},
Month = {{AUG}},
Abstract = {{In this paper, an energy management sub-system is proposed for mobile
   terminals (MTs) to support a sustainable multi-homing video
   transmission, over the call duration, in a heterogeneous wireless access
   medium. Through statistical video quality guarantee, the MT can
   determine a target video quality lower bound that can be supported for a
   target call duration. The target video quality lower bound captures the
   MT available energy at the beginning of the call, the time varying
   bandwidth availability and channel conditions at different radio
   interfaces, the target call duration, and the video packet
   characteristics in terms of distortion impact, delay deadlines, and
   video packet encoding statistics. The MT then adapts its energy
   consumption to support at least the target video quality lower bound
   during the call. Simulation results demonstrate the superior performance
   of the proposed framework over two benchmarks and some performance
   trade-offs.}},
