@article{ ISI:000340401400054,
Author = {Amintowlieh, Yasaman and Tzoganakis, Costas and Penlidis, Alexander},
Title = {{The Effect of Depth and Duration of UV Radiation on Polypropylene
   Modification via Photoinitiation}},
Journal = {{JOURNAL OF APPLIED POLYMER SCIENCE}},
Year = {{2014}},
Volume = {{131}},
Number = {{21}},
Month = {{NOV 5}},
Abstract = {{Linear polypropylene (PP) was modified using UV radiation in the
   presence of 0.5 wt \% of benzophenone photoinitiator to introduce long
   chain branching (LCB) to the PP backbone. Irradiation was carried out in
   the solid state and the temperature level was kept below 60 degrees C.
   The effects of radiation duration and sample thickness on the extent of
   these branching modification reactions were investigated. Viscoelastic
   properties, molecular weight, molecular weight distribution, and gel
   content were determined and compared for runs having different sample
   thicknesses, irradiated for different times. Comparisons were also
   conducted with the parent PP and the PP mixed with photoinitiator. It
   was found that LCB decreased by increasing the thickness of the samples.
   Conversely, an increase in radiation duration resulted in enhanced LCB
   but also led to larger gel content in the samples. Based on all these
   measurements and observations, a mechanism was suggested to explain
   formation of long chain branches (LCBs) in PP in the solid state via
   photoinitiation. (C) 2014 Wiley Periodicals, Inc.}},
