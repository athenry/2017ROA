@article{ ISI:000340239000063,
Author = {Riahinezhad, Marzieh and Kazemi, Niousha and McManus, Neil and Penlidis,
   Alexander},
Title = {{Effect of Ionic Strength on the Reactivity Ratios of Acrylamide/Acrylic
   Acid (sodium acrylate) Copolymerization}},
Journal = {{JOURNAL OF APPLIED POLYMER SCIENCE}},
Year = {{2014}},
Volume = {{131}},
Number = {{20}},
Month = {{OCT 15}},
Abstract = {{The ionic strength (IS) of polyelectrolyte solutions plays an important
   role in influencing reaction kinetics. The largely unstudied effect of
   IS on monomer reactivity ratios and copolymerization rates of acrylamide
   (AAm) and acrylic acid (AAc), in the form of sodium acrylate (NaAc), is
   investigated. Salt addition affects the nature of overall charges of the
   polyelectrolyte solution and diminishes the electrostatic repulsions
   between reacting chains. Therefore, changing the IS of the solution by
   incorporating salts affect not only the point estimates of the monomer
   reactivity ratios but also the overall behavior of the copolymerization
