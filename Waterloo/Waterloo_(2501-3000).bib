@article{ ISI:000343336400048,
Author = {Biro, Elliot and McDermid, Joseph R. and Vignier, Samuel and Zhou, Y.
   Norman},
Title = {{Decoupling of the softening processes during rapid tempering of a
   martensitic steel}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2014}},
Volume = {{615}},
Pages = {{395-404}},
Month = {{OCT 6}},
Abstract = {{The increased adoption of martensite-containing advanced high strength
   steels, such as martensitic and dual-phase steels, into automotive
   applications has led to concerns among practitioners with respect to
   softening during rapid tempering cycles such as those experienced during
   laser welding. Past studies on rapid tempering have successfully modeled
   the rapid tempering process; however, the activation energies and
   softening rates calculated did not match the classic literature values
   associated with martensite tempering. The present study examined rapid
   tempering data for a martensitic steel and separated the softening
   process into two stages: carbide nucleation and carbide coarsening or
   growth. The activation energies calculated for each process were found
   to be consistent with classic literature values for diffusion controlled
   nucleation and growth of carbides during martensite tempering. (C) 2014
   Elsevier B.V. All rights reserved.}},
