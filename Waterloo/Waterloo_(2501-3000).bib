@article{ ISI:000339691700022,
Author = {Evans, R. David and Ricardez-Sandoval, Luis A.},
Title = {{Multi-scenario modelling of uncertainty in stochastic chemical systems}},
Journal = {{JOURNAL OF COMPUTATIONAL PHYSICS}},
Year = {{2014}},
Volume = {{273}},
Pages = {{374-392}},
Month = {{SEP 15}},
Abstract = {{Uncertainty analysis has not been well studied at the molecular scale,
   despite extensive knowledge of uncertainty in macroscale systems. The
   ability to predict the effect of uncertainty allows for robust control
   of small scale systems such as nanoreactors, surface reactions, and gene
   toggle switches. However, it is difficult to model uncertainty in such
   chemical systems as they are stochastic in nature, and require a large
   computational cost. To address this issue, a new model of uncertainty
   propagation in stochastic chemical systems, based on the Chemical Master
   Equation, is proposed in the present study. The uncertain solution is
   approximated by a composite state comprised of the averaged effect of
   samples from the uncertain parameter distributions. This model is then
   used to study the effect of uncertainty on an isomerization system and a
   two gene regulation network called a repressilator. The results of this
   model show that uncertainty in stochastic systems is dependent on both
   the uncertain distribution, and the system under investigation. (C) 2014
   Elsevier Inc. All rights reserved.}},
