@article{ ISI:000337111700024,
Author = {Kazemzadeh, Farnoud and Haider, Shahid A. and Scharfenberger, Christian
   and Wong, Alexander and Clausi, David A.},
Title = {{Multispectral Stereoscopic Imaging Device: Simultaneous Multiview
   Imaging from the Visible to the Near-Infrared}},
Journal = {{IEEE TRANSACTIONS ON INSTRUMENTATION AND MEASUREMENT}},
Year = {{2014}},
Volume = {{63}},
Number = {{7}},
Pages = {{1871-1873}},
Month = {{JUL}},
Abstract = {{We have designed a hand-held imaging device which is capable of
   capturing images of multiple spectral bands, spanning the visible to the
   near-infrared, as well as imaging multiple perspective views of an
   object simultaneously on one detector. This concept utilizes a set of
   mirrors positioned obliquely to the principle axis of the imaging device
   that present different perspective views, of a target, and
   simultaneously bandpass filters those views before the light enters the
   device. In its current state, our instrument is capable of
   simultaneously imaging nine independent spectral bands and three
   different perspective views. The device allows for measurement of
   multispectral properties of different material enabling
   multispectroscopic science as demonstrated by studying the separability
   of four types of black ink. Exploiting the stereoscopic imaging
   capability of the device, we are able to construct 3-D surface features
   of a target.}},
