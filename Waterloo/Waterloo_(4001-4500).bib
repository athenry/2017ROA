@article{ ISI:000319601900015,
Author = {Hasan, M. Anwar and Negre, Christophe},
Title = {{Multiway Splitting Method for Toeplitz Matrix Vector Product}},
Journal = {{IEEE TRANSACTIONS ON COMPUTERS}},
Year = {{2013}},
Volume = {{62}},
Number = {{7}},
Pages = {{1467-1471}},
Month = {{JUL}},
Abstract = {{Computing the product of a Toeplitz matrix and a vector arises in
   various applications including cryptography. In this paper, we consider
   Toeplitz matrices and vectors with entries in IF2. For improved
   efficiency in such computations, large Toeplitz matrices and vectors are
   recursively split and special formulas with subquadratic arithmetic
   complexity are applied. To this end, we first present a formula for the
   five-way splitting and then provide a generalization for the k-way
   splitting, where k is an arbitrary integer. These formulas can be used
   to compute a Toeplitz matrix-vector product (TMVP) of size n with an
   arithmetic complexity of O(n(logk) ((k(k+1)/2))).}},
Publisher = {{IEEE COMPUTER SOC}},
Address = {{10662 LOS VAQUEROS CIRCLE, PO BOX 3014, LOS ALAMITOS, CA 90720-1314 USA}},
