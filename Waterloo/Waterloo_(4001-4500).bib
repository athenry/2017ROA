@article{ ISI:000316706400004,
Author = {Gong, Yanglin and Xue, Yusong and Xu, Lei},
Title = {{Optimal capacity design of eccentrically braced steel frameworks using
   nonlinear response history analysis}},
Journal = {{ENGINEERING STRUCTURES}},
Year = {{2013}},
Volume = {{48}},
Pages = {{28-36}},
Month = {{MAR}},
Abstract = {{Presented in this paper is an optimal capacity design method for steel
   building frameworks by adopting a nonlinear response history analysis
   procedure as the evolution tool for the structures under extreme
   earthquake loading. Minimum structural weight, minimum seismic input
   energy, and maximum hysteretic energy of fuse members are identified as
   design objectives. The design constraints include plastic deformation
   limit on every frame member and total deformation limit on each
   inter-story drift. The design optimization method employs a
   multi-objective genetic algorithm to search for optimal member section
   sizes from among commercially available steel section shapes. The design
   method is illustrated in details for a three-story eccentrically-braced
   steel frame. (C) 2012 Elsevier Ltd. All rights reserved.}},
