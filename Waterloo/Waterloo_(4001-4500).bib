@article{ ISI:000315063000018,
Author = {Xu, W. and Liu, L. and Zhou, Y. and Mori, H. and Chen, D. L.},
Title = {{Tensile and fatigue properties of weld-bonded and adhesive-bonded
   magnesium alloy joints}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2013}},
Volume = {{563}},
Pages = {{125-132}},
Month = {{FEB 15}},
Abstract = {{The microstructures, tensile and fatigue properties of weld-bonded (WB)
   AZ31B-H24 Mg/Mg joints with different sizes of bonding area were
   evaluated and compared with the adhesive-bonded (AB) Mg/Mg joints.
   Typical equiaxed dendritic structures containing divorced eutectic
   Mg17Al12 particles formed in the fusion zone of both WB-1 (with a
   bonding area of 35 mm x 35 mm) and WB-0.5 (with a bonding area of 17.5
   mm x 35 mm) joints. Less solidification shrinkage cracking was observed
   in the WB-0.5 joints than WB-1 joints. While the WB-0.5 joints exhibited
   a slightly lower maximum tensile shear stress than the AB-0.5 joints
   (with a bonding area of 17.5 mm x 35 mm), the energy absorption was
   equivalent. Although the AB-0.5 joints exhibited a higher fatigue
   resistance at higher cyclic stress levels, both the AB-0.5 and WB-0.5
   joints showed an equivalent fatigue resistance at lower cyclic stress
   levels. A higher fatigue limit was observed in the WB-0.5 joints than in
   the WB-1 joints owing to the presence of fewer shrinkage pores. Cohesive
   failure mode along the adhesive layer in conjunction with partial nugget
   pull-out from the weld was observed at the higher cyclic loads, and
   fatigue failure occurred in the base metal at the lower cyclic loads.
   (c) 2012 Elsevier B.V. All rights reserved.}},
