@article{ ISI:000314320800030,
Author = {Jiang, Changyun and Koh, Wei Lin and Leung, Man Yin and Hong, Wei and
   Li, Yuning and Zhang, Jie},
Title = {{Influences of alcoholic solvents on spray pyrolysis deposition of TiO2
   blocking layer films for solid-state dye-sensitized solar cells}},
Journal = {{JOURNAL OF SOLID STATE CHEMISTRY}},
Year = {{2013}},
Volume = {{198}},
Pages = {{197-202}},
Month = {{FEB}},
Abstract = {{Influences of alcoholic solvents for titanium diisopropoxide
   bis(acetylacetonate) (TPA) precursor solutions on the spray pyrolysis
   deposited TiO2 films and the photovoltaic performance of the solid-state
   dye-sensitized solar cells (SDSCs) using these TiO2 films as the
   blocking layers were investigated. Smooth TiO2 films were obtained by
   spray pyrolysis deposition of a TPA solution in isopropanol (IPA) at a
   relatively low temperature of 260 degrees C. On the other hand, when
   ethanol was used as solvent, the TiO2 films fabricated at the same
   temperature showed much rougher surfaces with many pinholes. Our results
   showed that ethanol reacts with TPA to form titanium diethoxide
   bis(acetylacetonate) (TEA), which requires a higher thermal
   decomposition temperature than that of TPA. SDSCs with TiO2 blocking
   layer films fabricated using a TPA solution in IPA showed higher power
   conversion efficiencies with smaller variations. (C) 2012 Elsevier Inc.
   All rights reserved.}},
