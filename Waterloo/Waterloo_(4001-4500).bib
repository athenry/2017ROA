@article{ ISI:000320462100031,
Author = {Niknejad, Seyed Tirdad and Liu, Lei and Tam Nguyen and Lee, Mok-Young
   and Esmaeili, Shahrzad and Zhou, Norman Y.},
Title = {{Effects of Heat Treatment on Grain-Boundary beta-Mg17Al12 and Fracture
   Properties of Resistance Spot-Welded AZ80 Mg Alloy}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS A-PHYSICAL METALLURGY AND
   MATERIALS SCIENCE}},
Year = {{2013}},
Volume = {{44A}},
Number = {{8}},
Pages = {{3747-3756}},
Month = {{AUG}},
Abstract = {{The distribution and morphology of beta-Mg17Al12 intermetallic phase in
   resistance spot-welded AZ80 Mg alloy were investigated by means of
   optical microscopy, scanning electron microscopy, and X-ray diffraction.
   The influence of intermetallic phase on mechanical strength was studied
   by tensile shear testing and fractography. The results showed that
   continuous networks of beta-Mg17Al12 formed along grain boundaries in
   both the nugget and heat-affected zone of the spot-welded AZ80 Mg alloy.
   Those continuous grain-boundary beta-Mg17Al12 networks acted as
   effective crack propagation paths, which had negative effects on the
   weld strength. Post-weld solution heat treatment effectively reduced the
   amount of beta-Mg17Al12 and broke the grain-boundary intermetallic
   networks in both the nugget and heat-affected zone. This significantly
   increased the weld strength of AZ80 Mg alloy and changed the fracture
   mode from nugget pull-out in the as-welded condition to
   through-thickness after heat treatment.}},
