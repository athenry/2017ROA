@article{ ISI:000318138300021,
Author = {Piya-areetham, Pornlert and Prasassarakich, Pattarapan and Rempel, Garry
   L.},
Title = {{Organic solvent-free hydrogenation of natural rubber latex and synthetic
   polyisoprene emulsion catalyzed by water-soluble rhodium complexes}},
Journal = {{JOURNAL OF MOLECULAR CATALYSIS A-CHEMICAL}},
Year = {{2013}},
Volume = {{372}},
Pages = {{151-159}},
Month = {{JUN}},
Abstract = {{A water-soluble rhodium catalyst has been found to be an efficient
   catalyst for hydrogenation of unsaturated polymers in the absence of any
   organic solvent. Natural rubber (NR) latex and synthetic polyisoprene
   (PIP) emulsion were hydrogenated using rhodium trichloride (RhCl3 center
   dot 3H(2)O) and different ligand types, triphenylphosphine (PPh3),
   trisulfonated triphenylphosphine (TPPTS) and monosulfonated
   triphenylphosphine (TPPMS) as a catalyst precursor. The hydrogenated NR
   and PIP were characterized by proton nuclear magnetic resonance (H-1
   NMR). A low degree of hydrogenation (HD) was observed when using
   RhCl3/PPh3 or RhCl3/TPPTS. In contrast, a high HD of 85.8\% was achieved
   by using RhCl3/TPPMS. The catalytic behavior could be well explained by
   the Hartley ionic spherical micelle model. The HD increased with
   increasing catalyst amount, reaction temperature and H-2 pressure. The
   catalyst activity of RhCl3/TPPMS for NR hydrogenation was found to be
   lower than that for PIP hydrogenation for all conditions due to
   impurities in the latex, including protein. The hydrogenated NR (86\%HD)
   has high thermal stability with a maximum decomposition temperature of
   466 degrees C and a glass transition temperature of -60 degrees C. From
   dynamic mechanical analysis, hydrogenated NR had a maximum storage
   modulus due to the saturated carbon domains of the ethylene propylene
   segments in the polymer chains. Crown Copyright (C) 2013 Published by
   Elsevier B.V. All rights reserved.}},
