Title = {{Explicit Rate based Transmission Control in Vehicle to Infrastructure
   Communications}},
Booktitle = {{2013 IEEE/CIC INTERNATIONAL CONFERENCE ON COMMUNICATIONS IN CHINA (ICCC)}},
