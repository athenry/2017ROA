@article{ ISI:000312936400042,
Author = {Kongsinlark, Anong and Rempel, Garry L. and Prasassarakich, Pattarapan},
Title = {{Synthesis of nanosized ethylene-propylene rubber latex via polyisoprene
   hydrogenation}},
Journal = {{JOURNAL OF APPLIED POLYMER SCIENCE}},
Year = {{2013}},
Volume = {{127}},
Number = {{5}},
Pages = {{3622-3632}},
Month = {{MAR 5}},
Abstract = {{Nanosized ethylenepropylene rubber (EPM) latex with a particle size of
   47 nm was synthesized via an alternative route consisting of isoprene
   (IP) polymerization followed by hydrogenation. First, the IP monomer was
   polymerized by differential microemulsion polymerization to obtain
   polyisoprene (PIP) rubber latex with a particle size of 42 nm. The
   structure of synthetic PIP was hydrogenated at the carboncarbon double
   bonds to produce an ethylenepropylene copolymer by diimide reduction in
   the presence of hydrazine and hydrogen peroxide using boric acid as
   promotor. The degree of hydrogenation was determined by proton nuclear
   magnetic resonance (1H-NMR) spectroscopy and the structure of the
   ethylenepropylene copolymer was identified by 13C-NMR spectroscopy. In
   nanosized PIP hydrogenation, the hydrogenation level was found to be
   increased by boric acid addition. An EPM yield of 94\% was achieved
   using a hydrogen peroxide : hydrazine ratio of 1.5 : 1. The EPM produced
   from PIP has high thermal stability with the maximum decomposition
   temperature of 510 degrees C and a glass transition temperature of -42.4
   degrees C close to commercial ethylenepropylene diene rubber. Dynamic
   mechanical analysis indicated that EPM had a maximum storage modulus due
   to the saturated carbons domains of the ethylene segments in the polymer
   chains. (c) 2012 Wiley Periodicals, Inc. J. Appl. Polym. Sci., 2013}},
