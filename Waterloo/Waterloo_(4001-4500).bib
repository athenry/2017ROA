@article{ ISI:000315079700004,
Author = {Meerasa, Ameena and Huang, Jasper G. and Gu, Frank X.},
Title = {{Human Serum Lipoproteins Influence Protein Deposition Patterns on
   Nanoparticle Surfaces}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2013}},
Volume = {{5}},
Number = {{3}},
Pages = {{489-493}},
Month = {{FEB 13}},
Abstract = {{We report the concentration-dependent adsorption of serum lipoproteins
   onto silica nanoparticles, wherein elevated lipid levels deter
   complement activation. Two clinically relevant serum lipid levels -
   corresponding to low and borderline high levels in normal, healthy
   adults - were used to examine the influence of lipoprotein concentration
   on nanoparticle complement activation. Human serum albumin was used to
   study protein adsorption in the presence of lipoproteins. Preferential
   adsorption of high affinity lipoproteins led to greater lipid fractions
   in the protein corona, shielding particles from complement activation.
   These findings have significant implications for the design of
   intravenously administered carriers with biocompatible surface
   chemistries.}},
