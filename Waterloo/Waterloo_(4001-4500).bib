@article{ ISI:000319969500012,
Author = {Zendehboudi, Sohrab and Ahmadi, Mohammad Ali and Bahadori, Alireza and
   Shafiei, Ali and Babadagli, Tayfun},
Title = {{A developed smart technique to predict minimum miscible pressureeor
   implications}},
