@article{ ISI:000315937600001,
Author = {Chabot, Victor and Kim, Brian and Sloper, Brent and Tzoganakis, Costas
   and Yu, Aiping},
Title = {{High yield production and purification of few layer graphene by Gum
   Arabic assisted physical sonication}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2013}},
Volume = {{3}},
Month = {{MAR 12}},
Abstract = {{Exploiting the emulsification properties of low cost, environmentally
   safe Gum Arabic we demonstrate a high yield process to produce a few
   layer graphene with a low defect ratio, maintaining the pristine
   graphite structure. In addition, we demonstrate the need for and
   efficacy of an acid hydrolysis treatment to remove the polymer residues
   to produce 100\% pure graphene. The scalable process gives yield of up
   to 5 wt\% graphene based on 10 g starting graphite. The graphene product
   is compared with reduced graphene oxide produced through Hummer's method
   using UV-visible spectroscopy, SEM, TEM, and Raman spectroscopy. The two
   graphene materials show significant difference in these
   characterizations. Further, the film fabricated from this graphene
   exhibits 20 times higher electrical conductivity than that of the
   reduced graphene oxide. Sonication processing of graphite with
   environmentally approved biopolymers such as Gum Arabic opens up a
   scalable avenue for production of cheap graphene.}},
