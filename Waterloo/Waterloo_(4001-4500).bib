@article{ ISI:000317190200015,
Author = {Shi, Yongrong and Tang, Wanchun and Liu, Sheng and Rao, Xin and Chow,
   Yung Leonard},
Title = {{Ultra-Wideband Suppression of Power/Ground Noise in High-Speed Circuits
   Using a Novel Electromagnetic Bandgap Power Plane}},
Journal = {{IEEE TRANSACTIONS ON COMPONENTS PACKAGING AND MANUFACTURING TECHNOLOGY}},
Year = {{2013}},
Volume = {{3}},
Number = {{4}},
Pages = {{653-660}},
Month = {{APR}},
Abstract = {{A power/ground plane pair is proposed using a novel planar
   electromagnetic bandgap (EBG) structure for isolating the
   power-to-ground noise (PGN) in high-speed circuits. Each unit cell of
   the novel EBG is designed by etching slots in a ``C{''} shape on the
   power plane while maintaining the ground plane solid. Without cascading
   hybrid periodic structures, it shows an efficient mitigation of PGN
   within an ultra-wideband frequency range of 0.25-2.18 GHz. In this
   paper, the equivalent circuit model and cavity mode analysis for the
   novel EBG unit cell are given to quickly predict the lower and upper
   bound cutoff frequency, respectively. The dispersion diagram of the
   proposed EBG structure is validated by insertion loss measurements with
   ports at different locations of the unit cell for different modes. The
   result shows that there is a good consistency between the simulated and
   measured results.}},
