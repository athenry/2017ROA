@inproceedings{ ISI:000333633500064,
Author = {Rathi, Yogesh and Gagoski, Borjan and Setsompop, Kawin and Michailovich,
   Oleg and Grant, P. Ellen and Westin, Carl-Fredrik},
Editor = {{Mori, K and Sakuma, I and Sato, Y and Barillot, C and Navab, N}},
Title = {{Diffusion Propagator Estimation from Sparse Measurements in a
   Tractography Framework}},
Booktitle = {{MEDICAL IMAGE COMPUTING AND COMPUTER-ASSISTED INTERVENTION (MICCAI
   2013), PT III}},
