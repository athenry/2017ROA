@inproceedings{ ISI:000332201904001,
Author = {Karg, Michelle and Seiberl, Wolfgang and Hoey, Jesse and Kulic, Dana},
Book-Group-Author = {{IEEE}},
Title = {{Human Movement Analysis: Extension of the F-Statistic to Time Series
   using HMM}},
