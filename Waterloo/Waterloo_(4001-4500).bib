@article{ ISI:000314192100051,
Author = {Baig, Aijaz and Paszti, Michael and Ng, Flora T. T.},
Title = {{A simple and green analytical method for acid number analysis of
   biodiesel and biodiesel blends based on potentiometric technique}},
