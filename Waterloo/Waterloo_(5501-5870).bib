Title = {{Optimization of a localized surface plasmon resonance biosensor for heat
   shock protein 70}},
Booktitle = {{PHOTONIC AND PHONONIC PROPERTIES OF ENGINEERED NANOSTRUCTURES II}},
Series = {{Proceedings of SPIE}},
