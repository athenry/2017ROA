@inproceedings{ ISI:000347871500099,
Author = {MacVicar, B. J. and Piegay, H.},
Editor = {{Munoz, RM}},
Title = {{Validation of video monitoring technique to measure wood transport in a
   river}},
Booktitle = {{RIVER FLOW 2012, VOLS 1 AND 2}},
Year = {{2012}},
Pages = {{735-740}},
Note = {{6th International Conference on Fluvial Hydraulics (River Flow), San
   Jose, COSTA RICA, SEP 05-07, 2012}},
Organization = {{Colegio Ingenieros Civiles Costa Rica; Int Assoc Hydro Environm Engn \&
   Res, Comm Fluvial Hydraul}},
Abstract = {{Quantitative data on the transport of wood in large rivers are needed to
   address management objectives and balance wood budgets. Streamside video
   cameras are suitable if image quality and wood visibility are
   sufficient. The objectives of this study are to verify a wood detection
   and measurement procedure and to quantify the relation between wood
   transport and discharge for a large piedmont river. Verification
   includes detection frequency, and pixel resolution tests. It was found
   that the camera location and resolution are sufficient for detecting
   floating wood. Wood transport is initiated at two thirds of the bankfull
   discharge. Wood transport increases above the threshold, but an
   inflexion point occurs near the bankfull discharge. Above the bankfull
   discharge, wood transport is insensitive to discharge and characterized
   by high variability. A three-stage model with hysteresis is proposed.
   Comparisons with other sources suggest that model estimates are high.
   Future work should address uncertainties.}},
