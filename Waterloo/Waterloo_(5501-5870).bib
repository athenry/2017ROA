Editor = {{Katkoori, S and Guthaus, M and Coskun, A and Burg, A and Reis, R}},
Title = {{Suppression of On-Chip Power Supply Noise Generated by a 64-Bit Static
   Logic ALU Block}},
Booktitle = {{2012 IEEE/IFIP 20TH INTERNATIONAL CONFERENCE ON VLSI AND SYSTEM-ON-CHIP
   (VLSI-SOC)}},
Series = {{IEEE-IFIP International Conference on VLSI and System-on-Chip}},
Year = {{2012}},
Pages = {{201-206}},
Note = {{20th IFIP/IEEE International conference on Very Large Scale Integration
   (VLSI-SoC), Santa Cruz, CA, OCT 07-10, 2012}},
Organization = {{IFIP; IEEE}},
Abstract = {{In modern VLSI and SoC circuits, the minimum power supply voltage is
   often constrained by data storage elements on the chip. Using an ALU, we
   show that redistributing the current draw pattern using a delay element
   within the evaluation period (after known clock skewing techniques have
   been implemented), can further improve the worst case voltage droop in
   the power supply by similar to 27\% at a minimal cost to area (similar
