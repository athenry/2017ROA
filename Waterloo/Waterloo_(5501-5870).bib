@inproceedings{ ISI:000331660100010,
Author = {Ma, Tianchi and Northrup, Victoria and Fung, Andrew O. and Glerum, D.
   Moira and Backhouse, Christopher J.},
Editor = {{Kieffer, JC}},
Title = {{Polymeric rapid prototyping for inexpensive and portable medical
   diagnostics}},
Booktitle = {{PHOTONICS NORTH 2012}},
Series = {{Proceedings of SPIE}},
