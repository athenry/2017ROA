@inproceedings{ ISI:000310776204057,
Author = {Davison, D. E. and Vanderwater, R. and Zhou, K.},
Book-Group-Author = {{IEEE}},
Title = {{A Control-Theory Reward-Based Approach to Behavior Modification in the
   Presence of Social-Norm Pressure and Conformity Pressure}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2012}},
Pages = {{4046-4052}},
Note = {{American Control Conference (ACC), Montreal, CANADA, JUN 27-29, 2012}},
