@inproceedings{ ISI:000310776206068,
Author = {Zibaeenejad, M. H. and Thistle, J. G.},
Book-Group-Author = {{IEEE}},
Title = {{Invariant Weak Simulation and Analysis of Parameterized Networks}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2012}},
