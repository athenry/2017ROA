@inproceedings{ ISI:000320286500006,
Author = {Zade, Azad Qazi and Renksizbulut, Metin and Friedman, Jacob},
Book-Group-Author = {{ASME}},
Title = {{HOMOGENEOUS OXIDATION OF HYDROGEN IN CATALYTIC MINI/MICROCHANNELS}},
Booktitle = {{PROCEEDINGS IF THE ASME 9TH INTERNATIONAL CONFERENCE ON NANOCHANNELS,
   MICROCHANNELS AND MINICHANNELS 2011, VOL 1}},
Year = {{2012}},
