@inproceedings{ ISI:000320009200061,
Author = {Farhad, Siamak and Hamdullahpur, Feridun},
Book-Group-Author = {{ASME}},
Title = {{EFFECT OF THE MICROSTRUCTURE OF POROUS COMPOSITE ELECTRODES ON THE
   ELECTRIC POWER DENSITY OF SOLID OXIDE FUEL CELLS}},
