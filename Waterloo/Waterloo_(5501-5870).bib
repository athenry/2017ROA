@inproceedings{ ISI:000313005700046,
Author = {Gangeh, Mehrdad J. and Ghodsi, Ali and Kamel, Mohamed S.},
Editor = {{Bolc, L and Tadeusiewicz, R and Chmielewski, LJ and Wojciechowski, K}},
Title = {{Supervised Texture Classification Using a Novel Compression-Based
   Similarity Measure}},
Booktitle = {{COMPUTER VISION AND GRAPHICS}},
