@inproceedings{ ISI:000319334901038,
Author = {Jain, Aanchal and Wong, Alexander and Fieguth, Paul},
Book-Group-Author = {{IEEE}},
Title = {{SALIENCY DETECTION VIA STATISTICAL NON-REDUNDANCY}},
Booktitle = {{2012 IEEE INTERNATIONAL CONFERENCE ON IMAGE PROCESSING (ICIP 2012)}},
Series = {{IEEE International Conference on Image Processing ICIP}},
Year = {{2012}},
