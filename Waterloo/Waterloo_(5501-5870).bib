@inproceedings{ ISI:000320654000039,
Author = {Yang, En-hui and Meng, Jin},
Book-Group-Author = {{IEEE}},
Title = {{Channel Capacity in the Non-asymptotic Regime: Taylor-type Expansion and
   Computable Benchmarks}},
Booktitle = {{2012 50TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2012}},
Pages = {{278-285}},
Note = {{50th Annual Allerton Conference on Communication, Control, and Computing
   (Allerton), Monticello, IL, OCT 01-05, 2012}},
Organization = {{Coordinated Sci Lab; Univ Illinois Urbana Champaign, Dept Elect \& Comp
   Engn; IEEE; IEEE Informat Theory Soc; IEEE Control Syst Soc (CSS); Univ
   Illinois, Coll Engn}},
Abstract = {{In this paper, the non-asymptotic counterpart of Shannon capacity is
   investigated for any discrete input memoryless channel with discrete or
   continuous output (DIMC). Given any block length n and word error
   probability epsilon, let R-n(epsilon) be the best channel coding rate
   achievable with the block length n subject to the error probability
   epsilon. Based on the non-asymptotic equipartition properties (NEP)
   established recently by Yang and Meng, a quantity delta(t,n)(epsilon) is
   first defined to measure the relative magnitude of error probability
   epsilon and block length n with respect to a given DIMC and an input
   distribution t. Then, by combining the non-asymptotic achievability and
   converse established recently by Yang and Meng via jar decoding, it is
   shown that, given n and epsilon < 1/ 2, R-n(epsilon) has a ``Taylor-type
   expansion{''} with respect to delta(t,n)(epsilon), with the first two
   terms of the expansion being max(t){[}I(t; P)-delta(t,n)(epsilon)] =
   I(t{*}; P)-delta(t{*},n)(epsilon) for some optimal distribution t{*},
   and the third order term being O(delta(2)(t{*},n)) + O(ln n/n). Finally,
   based on the Taylor-type expansion and the non-asymptotic converse, two
   easy to compute approximation formulas for R-n(epsilon) (dubbed ``SO{''}
   and ``NEP{''}) are provided. Numerical results show that both the SO and
   NEP approximation formulas provide reliable and accurate estimation, in
   contrast with the normal approximation which sometimes falls below
   achievable bounds and sometimes rises above converses. An important
   implication arising from the Taylor-type expansion of R-n(epsilon) is
   that in the practical non-asymptotic regime, the optimal marginal
   codeword symbol distribution is not necessarily a Shannon capacity
   achieving distribution.}},
