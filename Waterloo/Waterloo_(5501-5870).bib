@inproceedings{ ISI:000320654000087,
Author = {Xu, Duo and Meng, Jin and Yang, En-hui},
Book-Group-Author = {{IEEE}},
Title = {{Non-asymptotic Fixed-rate Slepian-Wolf Coding Theorem}},
Booktitle = {{2012 50TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2012}},
