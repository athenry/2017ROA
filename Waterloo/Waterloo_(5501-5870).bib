Title = {{Spatio-temporal Monte Carlo modeling of a-Se detectors for breast
   imaging: Energy-weighted Swank noise and detective quantum efficiency}},
Booktitle = {{MEDICAL IMAGING 2012: PHYSICS OF MEDICAL IMAGING}},
Series = {{Proceedings of SPIE}},
