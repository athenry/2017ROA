@inproceedings{ ISI:000319334901139,
Author = {Yeganeh, Hojatollah and Rostami, Mohammad and Wang, Zhou},
Book-Group-Author = {{IEEE}},
Title = {{OBJECTIVE QUALITY ASSESSMENT FOR IMAGE SUPER-RESOLUTION: A NATURAL SCENE
   STATISTICS APPROACH}},
