@inproceedings{ ISI:000320286600089,
Author = {Li, Dongqing},
Book-Group-Author = {{ASME}},
Title = {{ELECTROKINETIC MICROFLUIDICS AND BIOMEDICAL LAB-ON-A-CHIP DEVICES}},
Booktitle = {{PROCEEDINGS OF THE ASME 9TH INTERNATIONAL CONFERENCE ON NANOCHANNELS,
   MICROCHANNELS AND MINICHANNELS 2011, VOL 2}},
Year = {{2012}},
Pages = {{737-738}},
Note = {{9th International Conference on Nanochannels, Microchannels and
   Minichannels, Univ Alberta Edmonton, Edmonton, CANADA, JUN 19-22, 2011}},
Organization = {{Amer Soc Mech Engineers}},
Abstract = {{Lab-on-a-chip devices are miniaturized bio-medical laboratories on a
   small glass/plastic plate. These lab chips can duplicate the specialized
   functions of their room-sized counterparts such as clinical diagnoses
   and tests. The key microfluidic functions required in various
   lab-on-a-chip devices include pumping and mixing liquids, controlling
   bio-reactions, dispensing samples and reagents, and separating molecules
   and cells/particles. Using electrolcinetic microfluidics to realize
   these functions can make the devices fully automatic, independent of
   external support (e.g., tubing, valves and pump), and truly portable.
   Understanding, modeling and controlling of various electrokinetic
   microfluidic phenomena and the electrokinetic microfluidic processes are
   essential to systematic design and operation control of the
   lab-on-a-chip systems. This presentation will explain the principles of
   these electrokinetic microfluidic processes and how they are used in
   lab-on-a-chip devices. Some lab-on-a-chip devices such as real-time PCR
   chip, immunoassay chip and flow cytometer chip developed in Dr. Li's lab
   will be introduced..}},
