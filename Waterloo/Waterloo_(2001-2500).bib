@article{ ISI:000354457800015,
Author = {Guo, Chang and Quinn, Jesse and Sun, Bin and Li, Yuning},
Title = {{An indigo-based polymer bearing thermocleavable side chains for n-type
   organic thin film transistors}},
Journal = {{JOURNAL OF MATERIALS CHEMISTRY C}},
Year = {{2015}},
Volume = {{3}},
Number = {{20}},
Pages = {{5226-5232}},
Abstract = {{A new n-type semiconducting polymer based on indigo having
   thermocleavable tert-butoxycarbonyl (t-Boc) groups was synthesized and
   used as an active layer in organic thin film transistors (OTFTs).
   Twisting of the polymer main chain due to the presence of the bulky
   t-Boc groups renders this indigo-based polymer highly soluble. A
   post-deposition thermal treatment at a temperature above 170 degrees C
   could remove the t-Boc groups to retrieve the highly coplanar geometry
   of the unsubstituted indigo units. The thermally annealed polymer
