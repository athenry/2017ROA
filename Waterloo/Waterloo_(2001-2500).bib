@inproceedings{ ISI:000371977803078,
Author = {Shafiee, M. J. and Chung, A. G. and Wong, A. and Fieguth, P.},
Book-Group-Author = {{IEEE}},
Title = {{IMPROVED FINE STRUCTURE MODELING VIA GUIDED STOCHASTIC CLIQUE FORMATION
   IN FULLY CONNECTED CONDITIONAL RANDOM FIELDS}},
