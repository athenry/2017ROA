@inproceedings{ ISI:000383742200114,
Author = {Cheung, Victor and Scott, Stacey D.},
Book-Group-Author = {{ACM}},
Title = {{A Laboratory-based Study Methodology to Investigate Attraction Power of
   Large Public Interactive Displays}},
Booktitle = {{PROCEEDINGS OF THE 2015 ACM INTERNATIONAL JOINT CONFERENCE ON PERVASIVE
   AND UBIQUITOUS COMPUTING (UBICOMP 2015)}},
