@inproceedings{ ISI:000382389301009,
Author = {Zhang, Yani and Shan, Hangguan and Zhuang, Weihua and Huang, Aiping},
Book-Group-Author = {{IEEE}},
Title = {{Time-Frequency Resource Conversion based Scheduling for On-Demand Data
   Services}},
Booktitle = {{2015 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
