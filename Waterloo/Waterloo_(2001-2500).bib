@inproceedings{ ISI:000372587700033,
Author = {Rahmaan, Taamjeed and Butcher, Cliff and Abedini, Armin and Worswick,
   Michael},
Editor = {{Cadoni, E}},
Title = {{Effect of strain rate on shear properties and fracture characteristics
   of DP600 and AA5182-O sheet metal alloys}},
Booktitle = {{DYMAT 2015 - 11TH INTERNATIONAL CONFERENCE ON THE MECHANICAL AND
   PHYSICAL BEHAVIOUR OF MATERIALS UNDER DYNAMIC LOADING}},
