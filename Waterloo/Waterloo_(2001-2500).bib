@inproceedings{ ISI:000365757600005,
Author = {Kazemzadeh, Farnoud and Jin, Chao and Mei, Yu and Amelard, Robert and
   Haider, Shahid and Saini, Simarjeet and Emelko, Monica B. and Clausi,
   David A. and Wong, Alexander},
