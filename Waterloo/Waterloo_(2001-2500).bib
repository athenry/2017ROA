Title = {{Fatigue Strength Improvement of Aluminum and High Strength Steel Welded
   Structures using High Frequency Mechanical Impact Treatment}},
Booktitle = {{FATIGUE DESIGN 2015, INTERNATIONAL CONFERENCE PROCEEDINGS, 6TH EDITION}},
Series = {{Procedia Engineering}},
