@inproceedings{ ISI:000372587700048,
Author = {Imbert, Jose and Rahmaan, Taamjeed and Worswick, Michael},
Editor = {{Cadoni, E}},
Title = {{Interrupted pulse electromagnetic expanding ring test for sheet metal}},
