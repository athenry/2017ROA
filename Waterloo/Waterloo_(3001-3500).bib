@inproceedings{ ISI:000337900800073,
Author = {Kofman, J. and Borribanbunpotkat, K.},
Editor = {{Bartolo, PJD and DeLemos, ACS and Pereira, AMH and Mateus, AJD and Ramos, C and DosSantos, C and Oliveira, D and Pinto, E and Craveiro, F and Bartolo, HMCDTG and Almeia, HD and Sousa, I and Matias, JM and Durao, L and Gaspar, M and Alves, NMF and Carreira, P and Ferreira, T and Marques, T}},
Title = {{Hand-held 3D scanner without sensor pose tracking or surface markers}},
Booktitle = {{HIGH VALUE MANUFACTURING: ADVANCED RESEARCH IN VIRTUAL AND RAPID
   PROTOTYPING}},
Year = {{2014}},
Pages = {{429-434}},
Note = {{6th International Conference on Advanced Research in Virtual and
   Physical Prototyping (VRatP), Leiria, PORTUGAL, OCT 01-05, 2013}},
Organization = {{Polytechn Inst Leiria, Ctr Rapid \& Sustainable Prod Dev}},
Abstract = {{Hand-held scanners are required for the three-dimensional (3D) surface
   measurement of immobile objects and those difficult to access by
   conventional range-scanners. Existing hand-held scanners require
   secondary lighting, a tracking system, or surface markers to track the
   position and orientation of the range-sensor. Secondary lighting and
   tracking systems are bulky, limit the portability and ability to scan in
   confined spaces, and add cost to the system. This paper presents a
   hand-held 3D scanner that eliminates the above drawbacks. Multiple
   planes of light are simultaneously projected onto an object surface to
   provide sufficient surface information to enable surface fitting and
   fine interpolation between acquired data points. In scanning across a
   surface, the hand-held scanner acquires a sequence of spatiotemporally
   dense overlapping range-views from different viewpoints with enough
   overlap between range views and abundant information within a range view
   to permit registration of all acquired data into a single 3D object
   surface model.}},
