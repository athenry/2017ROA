@article{ ISI:000336437600011,
Author = {Newark, Daniel A. and Flynn, Francis J. and Bohns, Vanessa K.},
Title = {{Once Bitten, Twice Shy: The Effect of a Past Refusal on Expectations of
   Future Compliance}},
Journal = {{SOCIAL PSYCHOLOGICAL AND PERSONALITY SCIENCE}},
Year = {{2014}},
Volume = {{5}},
Number = {{2}},
Pages = {{218-225}},
Month = {{MAR}},
Abstract = {{Four studies examined help-seekers' beliefs about how past refusals
   affect future compliance. In Study 1, help-seekers were more likely than
   potential helpers to believe that a previous refusal would lead a
   potential helper to deny a subsequent request of similar size. Study 2
   replicated this effect and found that help-seekers underestimated the
   actual compliance rate of potential helpers who had previously refused
   to help. Studies 3 and 4 explain this asymmetry. Whereas potential
   helpers' willingness to comply with a subsequent request stems from the
   discomfort of rejecting others not once, but twice, help-seekers rely on
   dispositional attributions of helpfulness to estimate the likelihood of
   hearing ``yes'' from someone who has previously told them ``no.''}},
