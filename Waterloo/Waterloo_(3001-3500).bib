@inproceedings{ ISI:000337900800075,
Author = {Li, D. and Kofman, J.},
Editor = {{Bartolo, PJD and DeLemos, ACS and Pereira, AMH and Mateus, AJD and Ramos, C and DosSantos, C and Oliveira, D and Pinto, E and Craveiro, F and Bartolo, HMCDTG and Almeia, HD and Sousa, I and Matias, JM and Durao, L and Gaspar, M and Alves, NMF and Carreira, P and Ferreira, T and Marques, T}},
Title = {{Error compensated camera-projector calibration in shape measurement}},
Booktitle = {{HIGH VALUE MANUFACTURING: ADVANCED RESEARCH IN VIRTUAL AND RAPID
   PROTOTYPING}},
Year = {{2014}},
Pages = {{441-446}},
Note = {{6th International Conference on Advanced Research in Virtual and
   Physical Prototyping (VRatP), Leiria, PORTUGAL, OCT 01-05, 2013}},
Organization = {{Polytechn Inst Leiria, Ctr Rapid \& Sustainable Prod Dev}},
Abstract = {{In surface-shape measurement based on fringe projection,
   camera-projector stereovision, and the heterodyne temporal phase
   unwrapping, residual random phase errors in the wrapped phase maps can
   lead to spike-like errors near the beat frequency in the computed
   unwrapped phase map. If the spike-like errors correspond to control
   points in the camera image, the corresponding projector-image control
   point coordinates cannot be correctly determined and the projector
   calibration will fail. This paper presents a method of error
   compensation in camera-projector calibration using an improved
   heterodyne phase-unwrapping method that detects and compensates for the
   spike-like errors, and thereby preserves rather than remove phase values
   at control points to ensure useful fringe-projection camera-projector
   stereovision-system calibration. Phase-unwrapping simulations and
   real-system experiments demonstrated the advantage of the improved
   method in correcting errors at the phase-unwrapping stage of system
   calibration. The projector calibration accuracy was improved from 18.2
   to 0.2 pixels, enabling surface-shape measurement on various test
   objects.}},
