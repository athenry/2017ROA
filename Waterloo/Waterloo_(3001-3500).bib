@inproceedings{ ISI:000345576900031,
Author = {Sattar, F. and Campilho, Aurelio and Kamel, M.},
Editor = {{Campilho, A and Kamel, M}},
Title = {{Optic Disk Localization for Gray-Scale Retinal Images Based on Patch
   Filtering}},
Booktitle = {{IMAGE ANALYSIS AND RECOGNITION, ICIAR 2014, PT II}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2014}},
Volume = {{8815}},
Pages = {{277-284}},
Note = {{11th International Conference on Image Analysis and Recognition (ICIAR),
   PORTUGAL, OCT 22-24, 2014}},
Organization = {{Assoc Image \& Machine Intelligence}},
Abstract = {{In this paper, an optic disk (OD) localization method is proposed for
   the retinal images based on a novel patch filtering approach. The patch
   filtering has been performed sequentially based on clustering in two
   stages. In the first stage, the patches are selected exploiting an
   `isotropic' measure based on the ratio of maximum and minimum
   eigenvalues of the moment matrix representing the structure tensor. In
   the second stage, the patch filtering is based on the saliency measure.
   Finally, the optic disk is located from the centroids of the selected
   patches. Promising results are obtained for the low-contrast
   pathological retinal images using STARE database providing high
   localization accuracy.}},
