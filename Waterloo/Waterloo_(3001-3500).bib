@article{ ISI:000339727700003,
Author = {Lu, Ning and Shen, Xuemin (Sherman)},
Title = {{Scaling Laws for Throughput Capacity and Delay in Wireless Networks - A
   Survey}},
Journal = {{IEEE COMMUNICATIONS SURVEYS AND TUTORIALS}},
Year = {{2014}},
Volume = {{16}},
Number = {{2}},
Pages = {{642-657}},
Abstract = {{The capacity scaling law of wireless networks has been considered as one
   of the most fundamental issues. In this survey, we aim at providing a
   comprehensive overview of the development in the area of scaling laws
   for throughput capacity and delay in wireless networks. We begin with
   background information on the notion of throughput capacity of random
   networks. Based on the benchmark random network model, we then elaborate
   the advanced strategies adopted to improve the throughput capacity, and
   other factors that affect the scaling laws. We also present the
   fundamental tradeoffs between throughput capacity and delay under a
   variety of mobility models. In addition, the capacity and delay for
   hybrid wireless networks are surveyed, in which there are at least two
   types of nodes functioning differently, e.g., normal nodes and
   infrastructure nodes. Finally, recent studies on scaling law for
   throughput capacity and delay in emerging vehicular networks are
   introduced.}},
