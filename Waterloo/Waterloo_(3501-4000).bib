@article{ ISI:000327546400100,
Author = {Dong, Dexian and Zhang, Yongguang and Sutaria, Sanjana and Konarov,
   Aishuak and Chen, Pu},
Title = {{Binding Mechanism and Electrochemical Properties of M13 Phage-Sulfur
   Composite}},
Journal = {{PLOS ONE}},
Year = {{2013}},
Volume = {{8}},
Number = {{11}},
Month = {{NOV 26}},
Abstract = {{Self-assembly of nanostructured materials has been proven a powerful
   technique in material design and synthesis. By phage display screening,
   M13 phage was found to strongly bind sulfur particles. Fourier transform
   infrared and X-ray photoelectron spectroscopy measurements indicated
   that the strong sulfur-binding ability of M13 phage derives from newly
   generated S-O and C-S bonds. Using this phage assembled sulfur composite
   in a lithium battery, the first discharge capacity reached 1117 mAh
