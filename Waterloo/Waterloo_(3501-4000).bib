@article{ ISI:000323667600020,
Author = {Gumprecht, Jan D. J. and Lueth, Tim C. and Khamesee, Mir Behrad},
Title = {{Navigation of a robotic capsule endoscope with a novel ultrasound
   tracking system}},
Journal = {{MICROSYSTEM TECHNOLOGIES-MICRO-AND NANOSYSTEMS-INFORMATION STORAGE AND
   PROCESSING SYSTEMS}},
Year = {{2013}},
Volume = {{19}},
Number = {{9-10, SI}},
