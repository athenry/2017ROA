Title = {{Accommodating High Levels of Renewable Generation in Remote Microgrids
   under Uncertainty}},
Booktitle = {{2014 IEEE ELECTRICAL POWER AND ENERGY CONFERENCE (EPEC)}},
Series = {{IEEE Electrical Power and Energy Conference}},
Year = {{2014}},
