@inproceedings{ ISI:000369908602410,
Author = {Liang, Guozhen and Dupont, Emmanuel and Fathololoumi, Saeed and
   Wasilewski, Zbigniew R. and Ban, Dayan and Yu, Siu Fung and Li, Lianhe
   and Davies, Alexander Giles and Linfield, Edmund and Liu, Hui Chun and
   Wang, Qi Jie},
Book-Group-Author = {{IEEE}},
Title = {{In-plane surface plasmonics integrated with THz Quantum cascade lasers
   for high collimation}},
Booktitle = {{2014 CONFERENCE ON LASERS AND ELECTRO-OPTICS (CLEO)}},
Series = {{Conference on Lasers and Electro-Optics}},
Year = {{2014}},
Note = {{Conference on Lasers and Electro-Optics (CLEO), San Jose, CA, JUN 08-13,
   2014}},
Abstract = {{We report planar integration of tapered Terahertz (THz) quantum cascade
   lasers (QCLs) with spoof surface plasmon (SSP) structures. The SSP
   structure consists of one plasmonic coupler and periodically arranged
   scatters. The resulting surface-emitting THz beam is highly collimated
   with a beam divergence as narrow as 3.6 degrees x9.7 degrees. As the
   beam divergence is inverse proportional to the light emission area, this
   low divergence indicates a good waveguiding property of the SSP
   structure, while the low optical background of the beam implies a high
   coupling efficiency of the THz wave from the laser cavity to the SSPs.
   Since all the structures are in-plane, this scheme provides a promising
