@article{ ISI:000377487000020,
Author = {Abadi, Mehdi Naseri Ali and Golestaneh, Hamed and Sarbishaei, Hassan and
   Boumaiza, Slim},
Title = {{Doherty Power Amplifier With Extended Bandwidth and Improved
   Linearizability Under Carrier-Aggregated Signal Stimuli}},
Journal = {{IEEE MICROWAVE AND WIRELESS COMPONENTS LETTERS}},
Year = {{2016}},
Volume = {{26}},
Number = {{5}},
Pages = {{358-360}},
Month = {{MAY}},
Abstract = {{This letter proposes a novel output combining network devised to extend
   a Doherty power amplifier's (DPA's) radio frequency bandwidth (RFBW),
   while maintaining the proper load modulation, and to ensure its
   linearizability when driven with intra-and inter-band carrier aggregated
   (CA) communication signals. Based on the proposed topology, a wideband
   20-W DPA was designed and implemented using packaged GaN transistors.
   Drain efficiency in excess of 48\% was recorded at 6 dB back-off over
   the frequency range of 1.72-2.27 GHz. The DPA prototype was successfully
   linearized when driven with wideband (up to 160 MHz instantaneous
   bandwidth) and dual-band (up to 300 MHz carrier separation) modulated
   stimuli and achieved an average drain efficiency in excess of 45\%.}},
