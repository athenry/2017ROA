@inproceedings{ ISI:000401963304086,
Author = {Zhang, Deyu and Chen, Zhigang and Cai, Lin X. and Zhou, Haibo and Ren,
   Ju and Shen, Xuemin( Sherman)},
Book-Group-Author = {{IEEE}},
Title = {{Resource Allocation for Green Cloud Radio Access Networks Powered by
   Renewable Energy}},
Booktitle = {{2016 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
Series = {{IEEE Global Communications Conference}},
