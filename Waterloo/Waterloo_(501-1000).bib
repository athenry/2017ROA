@article{ ISI:000368688700010,
Author = {Shi, Kaibo and Liu, Xinzhi and Zhu, Hong and Zhong, Shouming and Liu,
   Yajuan and Yin, Chun},
Title = {{Novel integral inequality approach on master-slave synchronization of
   chaotic delayed Lur'e systems with sampled-data feedback control}},
Journal = {{NONLINEAR DYNAMICS}},
Year = {{2016}},
Volume = {{83}},
Number = {{3}},
Pages = {{1259-1274}},
Month = {{FEB}},
Abstract = {{This paper proposes a novel approach to study the problem of
   master-slave synchronization for chaotic delayed Lur'e systems with
   sampled-data feedback control. Specifically, first, it is assumed that
   the sampling intervals are randomly variable but bounded. By getting the
   utmost out of the usable information on the actual sampling pattern and
   the nonlinear part condition, a newly augmented Lyapunov-Krasovskii
   functional is constructed via a more general delay-partition approach.
   Second, in order to obtain less conservative synchronization criteria, a
   novel integral inequality is developed by the mean of the new adjustable
   parameters. Third, a longer sampling period is achieved by using a
   double integral form of Wirtinger-based integral inequality. Finally,
   three numerical examples with simulations of Chua's circuit are given to
   demonstrate the effectiveness and merits of the proposed methods.}},
