@article{ ISI:000380826000002,
Author = {de Melo, Joelma Ribeiro and Lima, Ed Pinheiro and Gimenes, Marcelino
   Luiz},
Title = {{Computacional study of the atmospheric emissions and dispersions from
   Kraft pulp production: air pollution assessment}},
