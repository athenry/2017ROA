@article{ ISI:000376695800006,
Author = {Walker, Sean B. and van Lanen, Daniel and Fowler, Michael and Mukherjee,
   Ushnik},
Title = {{Economic analysis with respect to Power-to-Gas energy storage with
   consideration of various market mechanisms}},
