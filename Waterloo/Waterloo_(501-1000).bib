@inproceedings{ ISI:000391421300117,
Author = {Eajal, A. A. and Abdelwahed, Mohamed A. and El-Saadany, E. F. and
   Ponnambalam, K.},
Book-Group-Author = {{IEEE}},
Title = {{Power Flow Analysis of AC/DC Hybrid Microgrids}},
Booktitle = {{2016 IEEE ELECTRICAL POWER AND ENERGY CONFERENCE (EPEC)}},
Series = {{IEEE Electrical Power and Energy Conference}},
Year = {{2016}},
Note = {{IEEE Electrical Power and Energy Conference (EPEC), Ottawa, CANADA, OCT
   12-14, 2016}},
Organization = {{IEEE}},
Abstract = {{The future smart grid can have an AC/DC hybrid structure that enables
   the integration of AC/DC energy supply and demand, thus permitting the
   formation of AC/DC hybrid microgrids (HMGs). The AC/DC HMG is a
   promising concept that provides the envisioned smart grid with the
   plug-and-play feature. Nevertheless, understanding such hybrid systems,
   especially during islanding, require an accurate and robust load flow
   program. To this end, this paper proposes a novel robust power flow
   algorithm for isolated AC/DC HMGs. The power flow problem is formulated
   as a least-squares minimization problem, and solved using the
   Levenberg-Marquardt algorithm. The accuracy of the proposed load flow
   algorithm is validated against time domain simulation results, while its
   robustness is demonstrated on a highly-resistive network. The proposed
   load flow algorithm is expected to reveal the light for further studies
   on AC/DC HMGs.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
