@article{ ISI:000376088100001,
Author = {Li, Hongwei and Yang, Yi and Luan, Tom H. and Liang, Xiaohui and Zhou,
   Liang and Shen, Xuemin (Sherman)},
Title = {{Enabling Fine-Grained Multi-Keyword Search Supporting Classified
   Sub-Dictionaries over Encrypted Cloud Data}},
