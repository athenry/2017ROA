@article{ ISI:000372559900045,
Author = {Mohee, Faizul M. and Al-Mayah, Adil and Plumtree, Alan},
Title = {{Anchors for CFRP plates: State-of-the-art review and future potential}},
Journal = {{COMPOSITES PART B-ENGINEERING}},
Year = {{2016}},
Volume = {{90}},
Pages = {{432-442}},
Month = {{APR 1}},
Abstract = {{In order to utilize the high tensile strength of the carbon
   fibre-reinforced-polymer (CFRP) plate, prestressing of the plate is
   recommended. End-anchors are required for the prestressing of the CFRP
   plate. Although a number of anchors have been developed, the literature
   lacks a review of the existing CFRP plate anchors. This article presents
   a state-of-the-art review of the available anchors for CFRP plates. The
   strength, performance, design parameters and failure modes of the
   anchors have been discussed. Barriers to the commercial use of the
   existing anchors include unconventional manufacturing, complicated
   installation steps, large size and high cost. An overall assessment of
   the existing CFRP plate anchors, considering industry requirements, has
   led to suggestions on research and development priorities. (C) 2016
   Elsevier Ltd. All rights reserved.}},
