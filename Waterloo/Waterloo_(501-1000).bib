@inproceedings{ ISI:000390098702054,
Author = {Haghi, Ali and Khandani, Amir K.},
Book-Group-Author = {{IEEE}},
Title = {{The Maximum Han-Kobayashi Sum-Rate for Gaussian Interference Channels}},
Booktitle = {{2016 IEEE INTERNATIONAL SYMPOSIUM ON INFORMATION THEORY}},
Series = {{IEEE International Symposium on Information Theory}},
Year = {{2016}},
Pages = {{2204-2208}},
Note = {{IEEE International Symposium on Information Theory (ISIT), Barcelona,
   SPAIN, JUL 10-15, 2016}},
Organization = {{IEEE; IEEE Informat Theory Soc; Univ Pompeu Fabra Barcelona; NSF;
   Qualcomm; Huawei; Google; IEEE BigData; Gobierno Espana, Minist Economia
   Compititividad}},
Abstract = {{The best known achievable rate region for the two-user Gaussian
   interference channel is due to the Han-Kobayashi (HK) scheme. The HK
   achievable region includes the regions achieved by all other known
   schemes. However, mathematical expressions that characterize the HK
   region are complicated and involve a time sharing variable and two
   arbitrary power splitting variables. Accordingly, the boundary points of
   the HK region, and in particular the maximum HK sum-rate, are not known
   in general. This paper studies the sum-rate of the HK scheme with
   Gaussian inputs. For the weak interference class, this study fully
   characterizes the maximum achievable sum-rate and shows that the weak
   interference class is partitioned into five regions. For each region,
   the optimal power splitting and the corresponding maximum achievable
   sum-rate are expressed in closed forms. Moreover, we show that the same
   approach can be adopted to characterize all boundary points.}},
