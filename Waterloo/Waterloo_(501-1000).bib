Title = {{RADON-GABOR BARCODES FOR MEDICAL IMAGE RETRIEVAL}},
Booktitle = {{2016 23RD INTERNATIONAL CONFERENCE ON PATTERN RECOGNITION (ICPR)}},
Series = {{International Conference on Pattern Recognition}},
Year = {{2016}},
