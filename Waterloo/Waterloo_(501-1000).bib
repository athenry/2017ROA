@inproceedings{ ISI:000393328700013,
Author = {Lin, Jonathan Feng-Shun and Samadani, Ali-Akbar and Kulic, Dana},
Editor = {{LeonGarcia, A and Lenort, R and Holman, D and Stas, D and Krutilova, V and Wicher, P and Caganova, D and Spirkova, D and Golej, J and Nguyen, K}},
Title = {{Segmentation by Data Point Classification Applied to Forearm Surface EMG}},
Booktitle = {{SMART CITY 360}},
Series = {{Lecture Notes of the Institute for Computer Sciences Social Informatics
   and Telecommunications Engineering}},
Year = {{2016}},
Volume = {{166}},
Pages = {{153-165}},
Note = {{1st International Conference on Smart Wearable Devices and IoT for
   Health and Wellbeing Applications (SWIT-Health), Toronto, CAMBODIA, OCT
   13-14, 2015}},
Abstract = {{Recent advances in wearable technologies have led to the development of
   new modalities for human-machine interaction such as gesture-based
   interaction via surface electromyograph (EMG). An important challenge
   when performing EMG gesture recognition is to temporally segment the
   individual gestures from continuously recorded time-series data. This
   paper proposes an approach for EMG data segmentation, by formulating the
   segmentation problem as a classification task, where a classifier is
   used to label each data point as either a segment point or a non-segment
