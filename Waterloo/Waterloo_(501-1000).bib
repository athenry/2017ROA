@inproceedings{ ISI:000392125600052,
Author = {Jeya, Logan and Zelek, John},
Book-Group-Author = {{IEEE}},
Title = {{KinectScenes: Robust Real-time RGB-D Fusion for Animal Behavioural
   Monitoring}},
Booktitle = {{2016 13TH CONFERENCE ON COMPUTER AND ROBOT VISION (CRV)}},
Year = {{2016}},
