@inproceedings{ ISI:000390782002092,
Author = {Zeng, Kai and Yeganeh, Hojatollah and Wang, Zhou},
Book-Group-Author = {{IEEE}},
Title = {{QUALITY-OF-EXPERIENCE OF STREAMING VIDEO: INTERACTIONS BETWEEN
   PRESENTATION QUALITY AND PLAYBACK STALLING}},
