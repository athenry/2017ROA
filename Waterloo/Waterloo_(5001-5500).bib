Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Epling, WS (Reprint Author), Univ Waterloo, Dept Chem Engn, Waterloo, ON N2L 3G1, Canada.
   Abedi, Ali; Epling, William S., Univ Waterloo, Dept Chem Engn, Waterloo, ON N2L 3G1, Canada.
   Hayes, Robert, Univ Alberta, Dept Chem \& Mat Engn, Edmonton, AB, Canada.
   Votsmeier, Martin, Umicore, Res \& Dev, Automot Catalysis Div, Hanau, Germany.}},
DOI = {{10.1007/s10562-012-0861-x}},
