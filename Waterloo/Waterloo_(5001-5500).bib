@article{ ISI:000304518600009,
Author = {Mikheevskiy, S. and Glinka, G. and Algera, D.},
Title = {{Analysis of fatigue crack growth in an attachment lug based on the
   weight function technique and the UniGrow fatigue crack growth model}},
Journal = {{INTERNATIONAL JOURNAL OF FATIGUE}},
Year = {{2012}},
Volume = {{42}},
Pages = {{88-94}},
Month = {{SEP}},
Abstract = {{A generalised step-by-step procedure for fatigue crack growth analysis
   of structural components subjected to variable amplitude loading spectra
   has been presented. The method has been illustrated by analysing fatigue
   growth of planar corner crack in an attachment lug made of Al7050-T7451
   alloy.
   Stress intensity factors required for the fatigue crack growth analysis
   were calculated using the weight function method. In addition, so-called
   ``load-shedding{''} effect was accounted for in order to determine
   appropriate magnitudes of the applied stress intensity factors. The rate
   of the load shedding was determined with the help of the finite element
   (FE) method by finding the amount of the load transferred through the
   cracked ligament. The UniGrow fatigue crack growth model, based on the
   material stress-strain behaviour near the crack tip, has been used to
   simulate the fatigue crack growth under two variable amplitude loading
   spectra. The comparison between theoretical predictions and experimental
   data proved the ability of the UniGrow model to correctly predict
   fatigue crack growth behaviour of two-dimensional planar cracks under
   complex stress field and subjected to arbitrary variable amplitude
   loading. (C) 2011 Elsevier Ltd. All rights reserved.}},
