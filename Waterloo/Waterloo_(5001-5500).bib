@article{ ISI:000301197400001,
Author = {Lee, Sang-Mae and Jeong, Myung-Yung and Saini, Simarjeet S.},
Title = {{Etched-Core Fiber Bragg Grating Sensors Integrated With Microfluidic
   Channels}},
Journal = {{JOURNAL OF LIGHTWAVE TECHNOLOGY}},
Year = {{2012}},
Volume = {{30}},
Number = {{8}},
Pages = {{1025-1031}},
Month = {{APR 15}},
Abstract = {{In this paper, we demonstrate a fiber Bragg grating sensor for
   bio-chemical detection. In the sensor area, the fiber has been
   chemically etched to the core with a diameter of 7 mu m. Due to
   increased index contrast, the etched-core fiber Bragg grating supports
   multiple guided modes which respond differently to the refractive index
   of the surrounding medium, temperature and the strain. An asymmetric
   adiabatic taper is created between the etched and the unetched parts of
   the fiber resulting in excitation of both the symmetrical and the
   asymmetrical modes which can be interrogated with an erbium doped fiber
   amplifier amplified spontaneous emission source. The sensor is
   integrated with a Poly-dimethylsiloxane (PDMS) microfluidic channel for
   introduction of chemicals to be tested. A refractive index sensitivity
   of 92 nm/riu was achieved using the 3rd order mode. Further the sensor
   can simultaneously measure the refractive index, temperature and strain
   to an accuracy of 1 x 10(-4), 0.32 degrees C and 10 mu epsilon assuming
   a wavelength resolution of 0.01 nm. By using signal processing, these
   resolutions can be improved by a factor of 10.}},
