@article{ ISI:000301758400033,
Author = {Liu, Li and Fieguth, Paul and Clausi, David and Kuang, Gangyao},
Title = {{Sorted random projections for robust rotation-invariant texture
   classification}},
Journal = {{PATTERN RECOGNITION}},
Year = {{2012}},
Volume = {{45}},
Number = {{6}},
Pages = {{2405-2418}},
Month = {{JUN}},
Abstract = {{This paper presents a simple, novel, yet very powerful approach for
   robust rotation-invariant texture classification based on random
   projection. The proposed sorted random projection maintains the
   strengths of random projection, in being computationally efficient and
   low-dimensional, with the addition of a straightforward sorting step to
   introduce rotation invariance. At the feature extraction stage, a small
   set of random measurements is extracted from sorted pixels or sorted
   pixel differences in local image patches. The rotation invariant random
   features are embedded into a bag-of-words model to perform texture
   classification, allowing us to achieve global rotation invariance. The
   proposed unconventional and novel random features are very robust, yet
   by leveraging the sparse nature of texture images, our approach
   outperforms traditional feature extraction methods which involve careful
   design and complex steps. We report extensive experiments comparing the
   proposed method to six state-of-the-art methods, RP, Patch, LBP, WMFS
   and the methods of Lazebnik et al. and Zhang et al., in texture
   classification on five databases: CUReT, Brodatz, UIUC, UMD and
   KTH-TIPS. Our approach leads to significant improvements in
   classification accuracy, producing consistently good results on each
   database, including what we believe to be the best reported results for
   Brodatz, UMD and KTH-TIPS. (C) 2011 Elsevier Ltd. All rights reserved.}},
