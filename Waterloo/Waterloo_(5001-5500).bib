Author = {Zeng, Anmin and Wang, Ye and Zhang, Qinyu and Cao, Bin and Lu, Rongxing},
Book-Group-Author = {{IEEE}},
Title = {{Network Selection Strategy for Cognitive Radios with Heterogeneous
   Primary Networks}},
Booktitle = {{2012 INTERNATIONAL CONFERENCE ON WIRELESS COMMUNICATIONS AND SIGNAL
   PROCESSING (WCSP 2012)}},
Series = {{International Conference on Wireless Communications and Signal
   Processing}},
