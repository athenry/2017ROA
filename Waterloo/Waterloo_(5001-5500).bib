@inproceedings{ ISI:000323559300038,
Author = {Yeganeh, Hojatollah and Wang, Zhou and Vrscay, Edward R.},
Editor = {{Campilho, A and Kamel, M}},
Title = {{Adaptive Windowing for Optimal Visualization of Medical Images Based on
   a Structural Fidelity Measure}},
Booktitle = {{IMAGE ANALYSIS AND RECOGNITION, PT II}},
