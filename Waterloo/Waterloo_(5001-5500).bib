@article{ ISI:000305834100001,
Author = {Hu, L. and Hesjedal, T.},
Title = {{Micromagnetic Investigation of the S-State Reconfigurable Logic Element}},
Journal = {{IEEE TRANSACTIONS ON MAGNETICS}},
