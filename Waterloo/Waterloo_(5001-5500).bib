@article{ ISI:000307998800023,
Author = {Mehrtash, Moein and Khamesee, Mir Behrad and Tarao, Susumu and Tsuda,
   Naoaki and Chang, Jen-Yuan},
Title = {{Human-assisted virtual reality for a magnetic-haptic micromanipulation
   platform}},
Journal = {{MICROSYSTEM TECHNOLOGIES-MICRO-AND NANOSYSTEMS-INFORMATION STORAGE AND
   PROCESSING SYSTEMS}},
