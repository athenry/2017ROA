@article{ ISI:000302836500013,
Author = {Alaefour, Ibrahim and Karimi, G. and Jiao, Kui and Li, X.},
Title = {{Measurement of current distribution in a proton exchange membrane fuel
   cell with various flow arrangements - A parametric study}},
Journal = {{APPLIED ENERGY}},
Year = {{2012}},
Volume = {{93}},
Number = {{SI}},
Pages = {{80-89}},
Month = {{MAY}},
Abstract = {{Understanding of current distributions in proton exchange membrane fuel
   cells (PEMFCs) is crucial for designing cell components such as the flow
   field plates and the membrane electrode assembly (MEA). In this study,
   the spatial current density distributions in a single PEMFC with three
   serpentine flow channels are measured using a segmented bipolar plate
   and printed circuit board technique. The effects of key operating
   conditions such as stoichiometry ratios, inlet humidity levels, cell
   pressure and temperature on the local current density distributions for
   co-, counter-, and cross-flow arrangements are examined. It is observed
   that the local current density distribution over the MEA is directly
   affected by the cell operating conditions along with the configuration
   of the flow arrangement. It is also found that among the different flow
   configurations tested under the various operating conditions, the
   counter flow arrangement provides the optimum average current density
   and the lowest variations in the local current densities along the flow
   channels. (C) 2011 Elsevier Ltd. All rights reserved.}},
