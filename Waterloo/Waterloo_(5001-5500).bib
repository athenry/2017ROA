@inproceedings{ ISI:000324153400053,
Author = {Pandey, M. D. and Lu, D. and Riznic, Jovica},
Book-Group-Author = {{ASME}},
Title = {{A PROBABILISTIC ASSESSMENT OF FLOW-ACCELERATED CORROSION RATE IN PIPE
   BENDS WITH UNKNOWN INITIAL THICKNESS}},
Booktitle = {{PROCEEDINGS OF THE 20TH INTERNATIONAL CONFERENCE ON NUCLEAR ENGINEERING
   AND THE ASME 2012 POWER CONFERENCE - 2012, VOL 5}},
Year = {{2012}},
Pages = {{435+}},
