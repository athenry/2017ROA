@article{ ISI:000305802000023,
Author = {Khorasaninejad, M. and Walia, J. and Saini, S. S.},
Title = {{Enhanced first-order Raman scattering from arrays of vertical silicon
   nanowires}},
Journal = {{NANOTECHNOLOGY}},
Year = {{2012}},
Volume = {{23}},
Number = {{27}},
Month = {{JUL 11}},
Abstract = {{Vertical ordered silicon nanowire arrays with diameters ranging from 30
   to 60 nm are fabricated and display enhanced Raman scattering. The
   first-order 520 cm(-1) phonon mode shows no significant shift or peak
   broadening with increasing laser power, suggesting that the excellent
   defect-free diamond crystalline structure and thermal properties of bulk
   silicon are maintained. The Raman enhancement per unit volume of the
   first-order phonon peak increases with increasing nanowire diameter, and
