@article{ ISI:000302022000017,
Author = {Liu, Xinzhi and Shen, Xuemin (Sherman) and Zhang, Hongtao},
Title = {{MULTI-SCROLL CHAOTIC AND HYPERCHAOTIC ATTRACTORS GENERATED FROM CHEN
   SYSTEM}},
Journal = {{INTERNATIONAL JOURNAL OF BIFURCATION AND CHAOS}},
Year = {{2012}},
Volume = {{22}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{In this paper, we create a multi-scroll chaotic attractor from Chen
   system by a nonlinear feedback control. The dynamic behavior of the new
   chaotic attractor is analyzed. Specially, the Lyapunov spectrum and
   Lyapunov dimension are calculated and the bifurcation diagram is
   sketched. Furthermore, via changing the value of the control parameters,
   we can increase the number of equilibrium points and obtain a family of
   more complex chaotic attractors with different topological structures.
   By introducing time delay to the feedback control, we then generalize
   the multi-scroll attractor to a set of hyperchaotic attractors. Computer
   simulations are given to illustrate the phase portraits with different
   system parameters.}},
