@article{ ISI:000351719000005,
Author = {Pan Su and Zhou Weiwei and Gu Qingqing and Ye Qiang},
Title = {{Network Selection Algorithm Based on Spectral Bandwidth Mapping and an
   Economic Model in WLAN\&LTE heterogeneous networks}},
Journal = {{KSII TRANSACTIONS ON INTERNET AND INFORMATION SYSTEMS}},
Year = {{2015}},
Volume = {{9}},
Number = {{1}},
Pages = {{68-86}},
Month = {{JAN 31}},
Abstract = {{Future wireless network aims to integrate different radio access
   networks (RANs) to provide a seamless access and service continuity. In
   this paper, a new resource denotation method is proposed in the WLAN and
   LTE heterogeneous networks based on a concept of spectral bandwidth
   mapping. This method simplifies the denotation of system resources and
   makes it possible to calculate system residual capacity, upon which an
   economic model-based network selection algorithm is designed in both
   under-loaded and over-loaded scenarios in the heterogeneous networks.
   The simulation results show that this algorithm achieves better
   performance than the utility function-based access selection (UFAS)
   method proposed in {[}12] in increasing system capacity and system
   revenue, achieving load balancing and reducing the new call blocking
   probability in the heterogeneous networks.}},
