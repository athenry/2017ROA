@article{ ISI:000363689200032,
Author = {Ohadi, Kaveh and Legge, Raymond L. and Budman, Hector M.},
Title = {{Intrinsic fluorescence-based at situ soft sensor for monitoring
   monoclonal antibody aggregation}},
