@article{ ISI:000351734400016,
Author = {Ren, Ju and Zhang, Yaoxue and Zhang, Kuan and Shen, Xuemin (Sherman)},
Title = {{Exploiting Mobile Crowdsourcing for Pervasive Cloud Services: Challenges
