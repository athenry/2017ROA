@article{ ISI:000350462900022,
Author = {Wei, Shih-Chung and Chuang, Tsung-Liang and Wang, Da-Shin and Lu,
   Hui-Hsin and Gu, Frank X. and Sung, Kung-Bin and Lin, Chii-Wann},
Title = {{Tip-enhanced fluorescence with radially polarized illumination for
   monitoring loop-mediated isothermal amplification on Hepatitis C virus
   cDNA}},
Journal = {{JOURNAL OF BIOMEDICAL OPTICS}},
Year = {{2015}},
Volume = {{20}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{A tip nanobiosensor for monitoring DNA replication was presented. The
   effects of excitation power and polarization on tip-enhanced
   fluorescence (TEF) were assessed with the tip immersed in fluorescein
   isothiocyanate solution first. The photon count rose on average fivefold
   with radially polarized illumination at 50 mW. We then used
   polymerase-functionalized tips for monitoring loop-mediated isothermal
   amplification on Hepatitis C virus cDNA. The amplicon-SYBR (R) Green I
