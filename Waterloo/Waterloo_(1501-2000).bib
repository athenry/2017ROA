@article{ ISI:000356457900045,
Author = {Ghaffari, Saeedeh and Abbaszadeh, Shiva and Ghanbarzadeh, Sina and
   Karim, Karim S.},
Title = {{Characterization of Optically Sensitive Amorphous Selenium Photodetector
   at High Electric Fields}},
Journal = {{IEEE TRANSACTIONS ON ELECTRON DEVICES}},
Year = {{2015}},
Volume = {{62}},
Number = {{7}},
Pages = {{2364-2366}},
Month = {{JUL}},
Abstract = {{We investigated the electrical conduction in indium tin oxide/polyimide
   (PI)/amorphous selenium/Au device structures, operated at high fields
   (up to 40 V/mu m). Devices having different PI thicknesses (0.4-2.5 mu
   m) with the same thickness of a-Se (16 mu m) were fabricated, and the
   dark and photocurrent transient behavior of each device under different
   biasing voltages was tested. The results indicate that an optimal
   thickness of PI (similar to 1 mu m) was necessary to limit the dark
   current to below 10 pA/mm(2) while achieving an ON/OFF ratio of 10(4), a
   result not achievable in devices without the PI layer. Furthermore,
   time-of-flight (TOF) measurements at low electric fields (similar to 10
   V/mu m) were carried out to measure the exact voltage drop within the PI
   and a-Se films for the same structure. Based on these TOF measurements,
   the electric field within the PI layer was determined to reach 138 V/mu
   m to provide efficient electrical conduction for photogenerated
   electrons.}},
