@article{ ISI:000359696900002,
Author = {Mozaffari, Ahmad and Emami, Mahdi and Azad, Nasser L. and Fathi, Alireza},
Title = {{On the efficacy of chaos-enhanced heuristic walks with nature-based
   controllers for robust and accurate intelligent search, part A: an
   experimental analysis}},
Journal = {{JOURNAL OF EXPERIMENTAL \& THEORETICAL ARTIFICIAL INTELLIGENCE}},
Year = {{2015}},
Volume = {{27}},
Number = {{4}},
Pages = {{389-422}},
Month = {{JUL 4}},
Abstract = {{In this investigation, the authors intend to find out whether
   chaos-embedded heuristic walks with nature-based controllers are capable
   of increasing the performance of the standard random walks of
   evolutionary algorithms. Indeed, the research motivation emanates in the
   pursuit of addressing an increasing interest in using chaos sequences
   for modifying the exploitation capability of evolutionary optimisers.
   Here, the authors propose a nature-based controller to dynamically
   balance the exploration/exploitation capability of the evolutionary
   optimiser. To test the validity of the proposed method, a number of
   well-known Gaussian distribution-based evolutionary operators are
   considered, and their random parameters are changed with discrete-time
   chaotic sequences. Besides, to obtain authentic and reliable results
   regarding the accuracy and robustness of the rival techniques, the
   authors went through the existing literature and extracted 23 scalable,
   multimodal, constraint and unconstraint numerical benchmark problems and
   one engineering problem, i.e. optimal control of shape memory alloy
   actuators. The convergence rate and computational time of the resulting
   self-controlled chaos-based evolutionary approaches are analysed to find
   out whether they are capable of stabilising the chromosomes within a
   logical time. The results of the numerical experiments indicate that the
   self-controlled chaos-enhanced heuristic walks significantly increase
   the global searching capability of the evolutionary optimisers and have
   an aptitude to show a robust performance.}},
