@article{ ISI:000360273900035,
Author = {Yin, Demin and Han, Gyuchull and Yoon, Youngki},
Title = {{Scaling Limit of Bilayer Phosphorene FETs}},
Journal = {{IEEE ELECTRON DEVICE LETTERS}},
Year = {{2015}},
Volume = {{36}},
Number = {{9}},
Pages = {{978-980}},
Month = {{SEP}},
Abstract = {{We investigate bilayer phosphorene field-effect transistors (FETs) by
   self-consistent atomistic quantum transport simulations. Despite a
   penalty in electrostatic control for multiple layers, 10-nm-channel
   bilayer phosphorene FETs can exhibit excellent device characteristics,
   such as I-on > 3 mA/mu m, large current ratio (> 10(7)), and small
   subthreshold swing (SS) of 66 mV/dec, with a double-gate device
   structure. While the scaling of gate dielectric monotonically enhances
   the overall performance of this device, channel length can only be
   scaled down to similar to 8 nm due to significant short-channel effects.
   We benchmark bilayer phosphorene FETs against bilayer MoS2 and WSe2 FETs
   along with a monolayer phosphorene device, which reveals that bilayer
   phosphorene FETs have favorable switching characteristics over other
   similar 2-D bilayer semiconductor devices, making both monolayer and
   bilayer phosphorene attractive for future switching applications. Our
   simulation results not only provide the performance and scaling limit of
   bilayer phosphorene FETs but also create irreplaceable insights into
   proper device design and parameter optimizations.}},
