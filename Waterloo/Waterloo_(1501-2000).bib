@article{ ISI:000349983600007,
Author = {Guana, C. Yong Liang and Elkamel, Ali and Wang, Kean},
Title = {{ENERGY GAS STORAGE IN TEMPLATE-SYNTHESIZED CARBONS WITH DIFFERENT POROUS
   STRUCTURES}},
Journal = {{CANADIAN JOURNAL OF CHEMICAL ENGINEERING}},
Year = {{2015}},
Volume = {{93}},
Number = {{3}},
Pages = {{527-531}},
Month = {{MAR}},
Abstract = {{Three activated carbon samples were synthesized via the template
   synthesis, in which a polymer was impregnated into the porous networks
   of zeolite Y and carbonized at three different temperatures. The carbon
   samples were structurally characterized and examined, respectively, for
   the application of energy gas (CH4 and H-2) storage. It was found that
   (1) at high pressure end (P > 35 bar) the methane storage capacity is
   approximately proportional to the surface area while the hydrogen
   storage capacity is predominated by the volume of micropores; (2) at low
   pressure end (P < 1bar), however, the adsorption capacity is a strong
   function of the size and volume fraction of micropores/small micropores.
   The role of mesopore, micropore, and small micropore was discussed in
   details with respect to the adsorption of H-2/CH4.}},
