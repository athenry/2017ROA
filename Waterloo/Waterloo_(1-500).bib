@article{ ISI:000379934100011,
Author = {Lee, Czang-Ho and Vardy, Nicholas and Wong, William S.},
Title = {{Multilayer MoS2 Thin-Film Transistors Employing Silicon Nitride and
   Silicon Oxide Dielectric Layers}},
Journal = {{IEEE ELECTRON DEVICE LETTERS}},
Year = {{2016}},
Volume = {{37}},
Number = {{6}},
Pages = {{731-734}},
Month = {{JUN}},
Abstract = {{Bottom-gate molybdenum disulfide (MoS2) TFTs, having plasma-enhanced
   chemical vapor deposition (PECVD) gate-dielectric thin films, were
   fabricated from 80-100 nm thick mechanically exfoliated MoS2
   multilayers. Three separate gate dielectric structures were investigated
   consisting of: 1) a single PECVD SiNx layer; 2) a dual PECVD SiOx/SiNx
   layer; and 3) a reference thermal SiO2 layer. The devices fabricated on
   the bi-layer dielectric had a field-effect mobility (mu(e)) of similar
   to 12 cm(2)/Vs and an ON/OFF ratio of similar to 10(5), while the TFTs
   on SiNx or thermal SiO2 dielectric had mu(e) similar to 4 to 5 cm(2)/Vs
   and an ON/OFF ratio similar to 10(4) to 10(5). The calculated interface
   trap density for the bi-layer dielectric devices was found to be 10x
   less than the SiNx devices and 2x less than the thermal SiO2 devices.
   The variation in the defect density was due to localized trap states in
   the dielectric/semiconductor interface.}},
