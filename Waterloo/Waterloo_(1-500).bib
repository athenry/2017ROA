@article{ ISI:000389344200023,
Author = {Bi, Yuanguo and Zhou, Haibo and Xu, Wenchao and Shen, Xuemin Sherman and
   Zhao, Hai},
Title = {{An Efficient PMIPv6-Based Handoff Scheme for Urban Vehicular Networks}},
Journal = {{IEEE TRANSACTIONS ON INTELLIGENT TRANSPORTATION SYSTEMS}},
Year = {{2016}},
Volume = {{17}},
Number = {{12}},
Pages = {{3613-3628}},
Month = {{DEC}},
Abstract = {{In urban vehicular networks, traveling users can enjoy Internet
   multimedia services through various mobile devices, such as smart phones
   and laptops. To maintain seamless and ubiquitous Internet connectivity,
   an efficient handoff scheme has to be employed when mobile users travel
   across different access networks. However, in the urban vehicular
   environment, the high velocity of vehicles and the random mobility of
   users impose great challenges to the design of an effective handoff
   scheme. In this paper, we propose an Efficient ProxyMobile IPv6
   (E-PMIPv6)-based handoff scheme that guarantees session continuity for
   urban mobile users. In the registration process, E-PMIPv6 enables mobile
   users to obtain seamless Internet connectivity either from fixed
   roadside units or mobile routers and improves cache utilization at the
   local mobility anchor by merging the binding cache entries of themobile
   users. In the handoff process, E-PMIPv6 comprehensively considers
   various handoff scenarios in the urban vehicular environment and
   provides transparent network-based mobility support to individual mobile
   users or a group of users in the same mobile network without disrupting
   ongoing sessions. In addition, E-PMIPv6 eliminates packet loss by either
   packet buffering or packet tunneling to improve handoff performance in
   each handoff scenario. Finally, a detailed analytical model is developed
   to study the performance of E-PMIPv6 in terms of handoff latency,
   signaling overhead, buffering cost, and tunneling cost. Analysis and
   simulation results demonstrate that the proposed E-PMIPv6 successfully
   extends the scalability of user mobility and greatly improves handoff
   efficiency in urban vehicular networks.}},
