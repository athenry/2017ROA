@article{ ISI:000386781000002,
Author = {Etminan, Jalil and Keshavarz, Hengameh and Mazumdar, Ravi R.},
Title = {{Effects of shadowing on the number of active users in random multi-user
   channels}},
Journal = {{WIRELESS NETWORKS}},
Year = {{2016}},
Volume = {{22}},
Number = {{8}},
Pages = {{2501-2509}},
Month = {{NOV}},
Abstract = {{Prior work has addressed the effects of multipath fading and path loss
   separately for broadcast and multiple-access channels. It has been shown
   that the number of simultaneously active users are of the order for
   random channel gains with exponentially-decaying tails, where n is the
   total number of users. Furthermore, it has been shown that assuming path
   loss is dominant and ignoring multipath fading, the user capacity (i.e.
   the maximum number of simultaneously active users in a wireless system)
   of multi-user channels is of the order when the users are spatially
   distributed on the plane with a Gaussian or Uniform distribution. In
   this paper, we study the shadowing effects on the number of active users
   for broadcast and multiple-access channels in which the users are
   randomly distributed on the plane. It is shown that as the total number
   of users in the multi-user channel goes to infinity, the number of
   active users is of the order Theta (root In(n)).}},
