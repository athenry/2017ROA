Title = {{Sensor Array for Volatile Organic Compounds Based on Doped Poly
   (2,5-dimethyl aniline)}},
Journal = {{MACROMOLECULAR SYMPOSIA}},
Year = {{2016}},
Volume = {{370}},
Number = {{1, SI}},
