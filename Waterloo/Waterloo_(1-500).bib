@article{ ISI:000389800800035,
Author = {Benoit, Michael J. and Whitney, Mark A. and Wells, Mary A. and Winkler,
   Sooky},
Title = {{Reduction of Liquid Clad Formation Due to Solid State Diffusion in Clad
   Brazing Sheet}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS B-PROCESS METALLURGY AND
   MATERIALS PROCESSING SCIENCE}},
