@article{ ISI:000386658500021,
Author = {Fang, Yong and Stankovic, Vladimir and Cheng, Samuel and Yang, En-Hui},
Title = {{Analysis on Tailed Distributed Arithmetic Codes for Uniform Binary
   Sources}},
