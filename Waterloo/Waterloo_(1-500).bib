@article{ ISI:000389800800015,
Author = {Shi, Cangji and Daun, Kyle J. and Wells, Mary A.},
Title = {{Evolution of the Spectral Emissivity and Phase Transformations of the
   Al-Si Coating on Usibor (R) 1500P Steel During Austenitization}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS B-PROCESS METALLURGY AND
   MATERIALS PROCESSING SCIENCE}},
Year = {{2016}},
Volume = {{47}},
Number = {{6}},
Pages = {{3301-3309}},
