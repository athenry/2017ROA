@article{ ISI:000380084100048,
Author = {Gonzalez, Marcelo and Tighe, Susan L. and Hui, Kathy and Rahman, Sonia
   and Lima, Arthur de Oliveira},
Title = {{Evaluation of freeze/thaw and scaling response of nanoconcrete for
   Portland Cement Concrete (PCC) pavements}},
Journal = {{CONSTRUCTION AND BUILDING MATERIALS}},
Year = {{2016}},
Volume = {{120}},
Pages = {{465-472}},
Month = {{SEP 1}},
Abstract = {{The durability of PCC refers to the ability of concrete to resist
   weathering action, chemical attack and abrasion. Freeze/thaw and scaling
   are extreme conditions that occur in cold climates and with the use of
