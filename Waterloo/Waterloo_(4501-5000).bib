@article{ ISI:000311506900024,
Author = {Li, Dezhi and Wang, Wilson and Ismail, Fathy},
Title = {{Enhanced fuzzy-filtered neural networks for material fatigue prognosis}},
Journal = {{APPLIED SOFT COMPUTING}},
Year = {{2013}},
Volume = {{13}},
Number = {{1}},
Pages = {{283-291}},
Month = {{JAN}},
Abstract = {{Although fuzzy-filtered neural networks (FFNN) have been used in pattern
   classification because of their unique characteristics in feature
   extraction, they usually have poor performance in forecasting
   applications due to their structure complexities especially in their
   consequent reasoning part. In this paper, an enhanced FFNN, EFFNN, is
   proposed for time series forecasting and material fatigue prognosis. A
   novel neural network scheme is developed to facilitate computation
   implementation. A new conjugate technique is proposed to improve
   training efficiency. The effectiveness of the developed EFFNN scheme and
   the related training technique is demonstrated by a series of simulation
   tests. The EFFNN is also implemented for material fatigue prognosis.
   Test results show that the developed EFFNN predictor is an effective
   forecasting tool; it can capture system dynamics effectively and track
   system characteristics accurately. (C) 2012 Elsevier B. V. All rights
