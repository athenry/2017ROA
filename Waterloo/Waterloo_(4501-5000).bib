@article{ ISI:000312181700008,
Author = {He, Wei and Ho, Pin-Han and Wu, Bin and Tapolcai, Janos},
Title = {{On identifying SRLG failures in all-optical networks}},
Journal = {{OPTICAL SWITCHING AND NETWORKING}},
Year = {{2013}},
Volume = {{10}},
Number = {{1, SI}},
Pages = {{77-88}},
Month = {{JAN}},
Abstract = {{Fast and unambiguous failure localization (UFL) is a desired feature in
   all-optical networks in order to provision high availability services.
   To save monitoring resources for signalling-free UFL, the paper studies
   a novel algorithm to localize any SRLG failure via monitoring-trails
   (m-trails) together with the working lightpaths. The feasibility
   conditions on the proposed m-trail allocation problem are discussed
   while different m-trail sharing strategies are introduced and compared
   via simulation. The problem of selecting an optimal set of working
   lightpaths is shown to be NP-complete and the simulation results verify
   the proposed algorithm with respect to monitoring resource consumption
   and running time. (c) 2012 Elsevier B.V. All rights reserved.}},
