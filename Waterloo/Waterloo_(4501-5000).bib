@article{ ISI:000312829400035,
Author = {Huang, H. and Liu, L. and Peng, P. and Hu, A. and Duley, W. W. and Zhou,
   Y.},
Title = {{Controlled joining of Ag nanoparticles with femtosecond laser radiation}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2012}},
Volume = {{112}},
Number = {{12}},
Month = {{DEC 15}},
Abstract = {{We show that it is possible to tailor the gap separation and interface
   geometry between adjacent Ag nanoparticles (NPs) by controlling fluence
   when irradiating with pulses from a fs laser. Unirradiated samples
   extracted from aqueous solution consist of networks of Ag NPs coated
   with polyvinylpyrrolidone (PVP). At low laser fluence, bonding between
   NPs occurs via the formation of an intervening hydrogenated amorphous
   carbon (alpha-C:H) layer resulting from the laser-induced decomposition
   of PVP. This occurs when electrons are emitted at hot-spots created by
   the trapping of plasmons. The thickness of the alpha-C:H layer
   determines the minimum separation between NPs. Ag NPs with different
   contact geometries can be produced by irradiation of the networks in
   solution at fluences exceeding the threshold for the formation of
   alpha-C:H. At fluences between 200 and 380 mu J/cm(2), the alpha-C:H
   interface layer is replaced with a metallic neck. Surface enhanced Raman
   scattering (SERS) has been used to quantify the electromagnetic field
   enhancement in joined NP samples. We find that Ag NPs bonded by
   alpha-C:H and exhibiting a narrow gap possess the highest SERS
   enhancement. (c) 2012 American Institute of Physics.
   {[}http://dx.doi.org/10.1063/1.4770476]}},
