@inproceedings{ ISI:000372859400007,
Author = {George, Ryan and Bardelcik, Alexander and Worswick, Michael},
Editor = {{Oldenburg, M and Prakash, B and Steinhoff, K}},
Title = {{Improving the Prediction of the Bainite and Ferrite Phase Transformation
   During Hot Stamping Simulations Using LS-DYNA}},
Booktitle = {{HOT SHEET METAL FORMING OF HIGH-PERFORMANCE STEEL CHS2 (4TH
   INTERNATIONAL CONFERENCE)}},
