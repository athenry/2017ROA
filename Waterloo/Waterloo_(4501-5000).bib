@inproceedings{ ISI:000372859400047,
Author = {Caron, Etienne J. F. R. and Daun, Kyle J. and Wells, Mary A.},
Editor = {{Oldenburg, M and Prakash, B and Steinhoff, K}},
Title = {{Heat Transfer Coefficient Characterization during Hot Forming Die
   Quenching of Boron Steel Blanks}},
Booktitle = {{HOT SHEET METAL FORMING OF HIGH-PERFORMANCE STEEL CHS2 (4TH
   INTERNATIONAL CONFERENCE)}},
Series = {{CHS2-Series}},
Year = {{2013}},
Number = {{4}},
Pages = {{407-414}},
Note = {{4th International Conference on Hot Sheet Metal Forming of
   High-Performance Steel (CHS2 2013), Lulea, SWEDEN, JUN 09-12, 2013}},
Organization = {{Lulea Univ Technol}},
Abstract = {{Precise knowledge of the heat transfer coefficient (HTC) at the blank /
   tool interface is required to locally control the blank cooling rate in
   order to obtain hot stamped parts with tailored mechanical properties.
   This paper describes hot forming die quenching (HFDQ) experiments
   conducted on Usibor (R) 1500P boron steel samples quenched with heated
   dies. An inverse heat conduction algorithm is used to convert measured
   blank and die temperatures into a HTC while taking into account the
   effect of thermocouple response time. The HTC during the approach phase
   of an HFDQ experiment is a function of the die temperature. In the
   stamping phase, the HTC increases exponentially as the temperature
   difference between the blank and die surfaces decreases. The HTC
   increase during the stamping phase depends on the pressure and die
   temperature, whereas the equilibrium HTC measured when the blank and die
   surface temperatures become equal increases with the stamping pressure.}},
