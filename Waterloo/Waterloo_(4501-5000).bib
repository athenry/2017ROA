@inproceedings{ ISI:000392796700013,
Author = {Fan, Xinxin and Gong, Guang},
Editor = {{Zheng, J and Mitton, N and Li, J and Lorenz, P}},
Title = {{LPKM: A Lightweight Polynomial-Based Key Management Protocol for
   Distributed Wireless Sensor Networks}},
Booktitle = {{Ad Hoc Networks, (ADHOCNETS 2012)}},
