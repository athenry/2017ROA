Author = {Malekshan, Kamal Rahimi and Zhuang, Weihua},
Book-Group-Author = {{IEEE}},
Title = {{An Energy Efficient MAC Protocol for Fully-Connected Wireless Networks}},
Booktitle = {{2013 IEEE INTERNATIONAL CONFERENCE ON COMMUNICATIONS (ICC)}},
