@inproceedings{ ISI:000322002700184,
Author = {Mann, Ryan and Cunningham, Ian A. and Karim, Karim S.},
Editor = {{Nishikawa, RM and Whiting, BR and Hoeschen, C}},
Title = {{Characterization of a Digital X-ray Detector for Region of Interest
   Tuberculosis Screening}},
