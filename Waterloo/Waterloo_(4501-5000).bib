@inproceedings{ ISI:000327210202022,
Author = {Ni, J. and Kulic, D. and Davison, D. E.},
Book-Group-Author = {{IEEE}},
Title = {{A Model-Based Feedback-Control Approach to Behavior Modification Through
   Reward-Induced Attitude Change}},
Booktitle = {{2013 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2013}},
Pages = {{1956-1963}},
