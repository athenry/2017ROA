@article{ ISI:000328248800007,
Author = {Hipel, Keith W. and Fang, Liping and Wang, Lizhong},
Title = {{Fair water resources allocation with application to the South
   Saskatchewan river basin}},
Journal = {{CANADIAN WATER RESOURCES JOURNAL}},
Year = {{2013}},
Volume = {{38}},
Number = {{1, SI}},
Pages = {{47-60}},
Abstract = {{A formal decision making methodology is presented for equitably
   allocating water among competing users in a river basin when taking into
   account both the societal and physical systems aspects of the allocation
   problem. More specifically, within the societal component of the
   decision problem, multiple participants, their multiple objectives,
   equity principles, and economic factors are taken into account, while
   the physical systems part reflects relevant hydrologic and environmental
   factors. The Cooperative Water Allocation Model (CWAM) incorporates
   these societal and physical systems concerns within the framework of a
   large-scale optimization program which is divided into two main steps.
   Firstly, water is allocated among users based on existing legal water
   rights regimes or agreements. In the second step, water and associated
   benefits are reallocated among stakeholders to maximize basin-wide
   welfare. The CWAM methodology is used to study the South Saskatchewan
   River Basin located in Alberta, Canada, to demonstrate how it can be
   conveniently applied to a complex water allocation problem.}},
Publisher = {{CANADIAN WATER RESOURCES ASSOC-CWRA}},
Address = {{280 ALBERT ST, STE 900, OTTAWA, ONTARIO K1P 5G8, CANADA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Hipel, KW (Reprint Author), Univ Waterloo, Dept Syst Design Engn, Waterloo, ON N2L 3G1, Canada.
   Hipel, Keith W., Univ Waterloo, Dept Syst Design Engn, Waterloo, ON N2L 3G1, Canada.
   Fang, Liping, Ryerson Univ, Dept Mech \& Ind Engn, Toronto, ON M5B 2K3, Canada.
   Wang, Lizhong, Golder Associates, Calgary, AB T2A 7W5, Canada.}},
DOI = {{10.1080/07011784.2013.773767}},
