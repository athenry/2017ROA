@inproceedings{ ISI:000372859400064,
Author = {ten Kortenaar, Lukas and Bardelcik, Alexander and Worswick, Michael and
   Detwiler, Duane},
Editor = {{Oldenburg, M and Prakash, B and Steinhoff, K}},
Title = {{The Effect of Stress Triaxiality on the Failure Response of Boron Steel
   Quenched to a Martensitic and Bainitic Material Condition}},
Booktitle = {{HOT SHEET METAL FORMING OF HIGH-PERFORMANCE STEEL CHS2 (4TH
   INTERNATIONAL CONFERENCE)}},
Series = {{CHS2-Series}},
Year = {{2013}},
Number = {{4}},
Pages = {{565-572}},
Note = {{4th International Conference on Hot Sheet Metal Forming of
   High-Performance Steel (CHS2 2013), Lulea, SWEDEN, JUN 09-12, 2013}},
Organization = {{Lulea Univ Technol}},
Abstract = {{Recently, a strain rate sensitive constitutive model has been developed
   to predict the crash response of hot stamped (and tailored) components
   with varying as-quenched volume fractions of martensite, bainite and
   ferrite. However, failure criteria are also required for tailored hot
   stamped components, which is the motivation for the current work. Here,
   boron steel blanks were quenched to produce fully martensitic and
   bainitic microstructures. Miniature tensile specimens were cut from the
   quenched blanks with varying notch geometries that imposed different
   stress triaxialities during tensile loading. Digital image correlation
   (DIC) analysis was used to measure the strain distribution during
   testing and identify the fracture strain. Finite element (Fh) models of
   the experiments were developed and successfully validated against the
   experiments. These models were then used to predict the stress
   triaxiality and Lode parameter for each notch geometry. The results
   showed that when the average triaxiality was increased from similar to
   0.33 to similar to 0.54, the strain at failure reduced from 1.1 to 0.6
   for the bainitic material and 0.8 to 0.3 for the martensitic material
   condition. The Lode parameter also indicated that the stress-state
   transitioned from uniaxial tension to a combined tensile/shear
   stress-state for the most severe specimen notch geometry.}},
