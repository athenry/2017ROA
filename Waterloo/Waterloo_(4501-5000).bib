@article{ ISI:000311790400023,
Author = {Reynolds, Matthew and Elias, Anastasia and Elliott, Duncan G. and
   Backhouse, Christopher and Sameoto, Dan},
Title = {{Variation of thermal and mechanical properties of KMPR due to processing
   parameters}},
Journal = {{JOURNAL OF MICROMECHANICS AND MICROENGINEERING}},
Year = {{2012}},
Volume = {{22}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{We present a study of the thermal and mechanical properties of the
   negative photoresist KMPR and the influence of processing conditions on
   those properties. The process parameters chosen all relate to the
   cross-linking level of the photoresist: the UV exposure dose, the baking
   temperature and the bake length. The stability of KMPR at high
   temperatures was also examined. The glass transition temperature was
   measured using dynamic mechanical analysis, with a maximum measured
   value of 128 degrees C achieved in our tests. Relating the glass
   transition temperature to the cross-linking level of the material,
   exposure doses higher than 2 J cm(-2) were shown to have a negligible
   effect on the cross-linking (for 80 mu m thick films). Using
   thermogravitmetric analysis, KMPR has been shown to lose significant
   mass when heated above 200 degrees C. Young's modulus of KMPR was
   measured to be between 2.0 GPa for samples hard-baked at 100 degrees C
   and 2.7 GPa for samples baked at 150 and 200 degrees C. Creep behavior
   for KMPR held under strain was studied for samples prepared under a
   range of processing temperatures. Finally the thermally-induced
   cross-linking of unexposed KMPR was studied, with samples post-exposure
   baked at 150 degrees C, or 120 degrees C for at least an hour,
   cross-linking sufficiently to prevent development.}},
