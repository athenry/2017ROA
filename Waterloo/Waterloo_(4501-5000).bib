Title = {{Hardware Implementations of the WG-5 Cipher for Passive RFID Tags}},
Booktitle = {{2013 IEEE INTERNATIONAL SYMPOSIUM ON HARDWARE-ORIENTED SECURITY AND
   TRUST (HOST)}},
Year = {{2013}},
