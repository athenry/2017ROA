@inproceedings{ ISI:000326335200084,
Author = {Tapolcai, Janos and Ho, Pin-Han and Babarczi, Peter and Ronyai, Lajos},
Book-Group-Author = {{IEEE}},
Title = {{On Achieving All-Optical Failure Restoration via Monitoring Trails}},
Booktitle = {{2013 PROCEEDINGS IEEE INFOCOM}},
Series = {{IEEE INFOCOM}},
Year = {{2013}},
Pages = {{380-384}},
Note = {{32nd IEEE INFOCOM Conference, Turin, ITALY, APR 14-19, 2013}},
Organization = {{IEEE}},
Abstract = {{The paper investigates a novel monitoring trail (m-trail) scenario that
   can enable any shared protection scheme for achieving all-optical and
   ultra-fast failure restoration. Given a set of working (W-LPs) and
   protection (P-LPs) lightpaths, we firstly define the neighborhood of a
   node, which is a set of links whose failure states should be known to
   the node in restoration of the corresponding W-LPs. A set of m-trails is
   routed such that each node can localize any failure in its neighborhood
   according to the ON-OFF status of the traversing m-trails. Bound
   analysis is performed on the minimum bandwidth required for the
   m-trails. Extensive simulation is conducted to verify the proposed
   scheme.}},
