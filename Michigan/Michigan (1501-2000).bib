@inproceedings{ ISI:000388376105144,
Author = {Dunham, William and Petersen, Christopher and Kolmanovsky, Ilya},
Book-Group-Author = {{IEEE}},
Title = {{Constrained Control for Soft Landing on an Asteroid with Gravity Model
   Uncertainty}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2016}},
Pages = {{5842-5847}},
Note = {{American Control Conference (ACC), Boston, MA, JUL 06-08, 2016}},
Organization = {{Amer Automat Control Council}},
Abstract = {{An autonomous control strategy is developed that enables soft landing of
   a spacecraft on a small rotating asteroid while exploiting a constant
   density ellipsoid gravity model. A disturbance canceling input observer
   is used to compensate for the error between the estimated and actual
   gravity force. An extended command governor is used to enforce
   constraints on control and landing speed while performing collision
   avoidance. The spacecraft landing mission is divided into two phases. In
   the first, circumnavigating phase an ellipsoidal constraint is placed
   around the asteroid that guarantees collision avoidance while bounding
   control. In the second, landing phase, the spacecraft trajectory is
   maintained within a pyramid that tapers near the landing point while
   spacecraft speed is reduced as it approaches the surface. Simulations
   performed on the full nonlinear model with actual gravity parameters
   demonstrate successful landing using the proposed controller scheme.}},
