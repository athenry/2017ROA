@inproceedings{ ISI:000388376106002,
Author = {Ansari, Ahmad and Bernstein, Dennis S.},
Book-Group-Author = {{IEEE}},
Title = {{Aircraft Sensor Fault Detection Using State and Input Estimation}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2016}},
Pages = {{5951-5956}},
Note = {{American Control Conference (ACC), Boston, MA, JUL 06-08, 2016}},
Organization = {{Amer Automat Control Council}},
Abstract = {{This paper presents a method for detecting aircraft sensor faults using
   state and input estimation. We formulate the kinematics as a nonlinear
   state space system, which requires no modeling information, and thus is
   applicable to all aircraft. To illustrate the method, we investigate
   three fault-detection scenarios, namely, faulty pitot tube,
