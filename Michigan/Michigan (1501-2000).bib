@inproceedings{ ISI:000399823503037,
Author = {Marshall, Shaun P. and Lin, Wei-Chen and Patel, Paras R. and Shih,
   Albert J. and Chestek, Cynthia A.},
Editor = {{Patton, J and Barbieri, R and Ji, J and Jabbari, E and Dokos, S and Mukkamala, R and Guiraud, D and Jovanov, E and Dhaher, Y and Panescu, D and Vangils, M and Wheeler, B and Dhawan, AP}},
Title = {{Effects of geometry and material on the insertion of very small neural
   electrode}},
Booktitle = {{2016 38TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
   MEDICINE AND BIOLOGY SOCIETY (EMBC)}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
Year = {{2016}},
Pages = {{2784-2788}},
Note = {{38th Annual International Conference of the
   IEEE-Engineering-in-Medicine-and-Biology-Society (EMBC), Orlando, FL,
   AUG 16-20, 2016}},
Organization = {{IEEE Engn Med \& Biol Soc}},
Abstract = {{For neural probes to be used chronically for years in the human body,
   they must provoke minimal scarring. Recently, a number of groups have
   reported substantially reduced scar tissue using cellular scale
   electrodes below 15 mu m in size. This size scale is accessible to
   manufacturing techniques, but can be very difficult to insert in the
   brain for most common electrode materials. In this study, we explore the
   design space available to cellular scale electrodes that will selfinsert
   into the brain. First a mathematical model is developed using beam
   buckling equations for different materials and geometries. Buckling mode
   was found to be one fixed and one hinged end resulting in a mode
   conditional constant of, n, 2.045. Model predicts insertion success
   between 90 - 100\% for a 6.8 mu m diameter electrode and was used to
   approximate applied force as 750 mu N which is close to reference data
   of 780 mu N {[}1]. Second, we developed a PVC phantom that mimics the
