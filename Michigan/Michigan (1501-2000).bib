@inproceedings{ ISI:000388376106119,
Author = {ElMoaqet, Hisham and Tilbury, Dawn M. and Ramachandran, Satya Krishna},
Book-Group-Author = {{IEEE}},
Title = {{A Probabilistic Approach for Evaluating Predictions of Critical Levels
   in Physiological Time Series}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2016}},
Pages = {{6664-6669}},
Note = {{American Control Conference (ACC), Boston, MA, JUL 06-08, 2016}},
Organization = {{Amer Automat Control Council}},
Abstract = {{Many challenges arise in evaluating predictions from dynamical models of
   physiological signals. A fundamental objective desired by clinicians is
   to predict regions of clinical interest rather than just match these
   signals. Although threshold based metrics provide much better insight by
   classifying predictions based on their ability to capture critical
   levels of abnormality, these metrics lack the ability to well
   characterize the prediction performance near decision boundaries. This
   paper presents a probabilistic approach for evaluating predictions by
   incorporating prediction uncertainty into the evaluation process. A case
   study is presented for evaluating 20s and 60s ahead predictions of
   critical oxygen levels in the blood using Gaussian Process (GP) models.
   Results show significant improvement in the ability to characterize the
   performance in predicting critical desaturation events.}},
