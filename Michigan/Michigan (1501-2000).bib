@inproceedings{ ISI:000392504700069,
Author = {Knag, Phil and Liu, Chester and Zhang, Zhengya},
Book-Group-Author = {{IEEE}},
Title = {{A 1.40mm(2) 141mW 898GOPS Sparse Neuromorphic Processor in 40nm CMOS}},
Booktitle = {{2016 IEEE SYMPOSIUM ON VLSI CIRCUITS (VLSI-CIRCUITS)}},
Series = {{Symposium on VLSI Circuits-Digest of Papers}},
