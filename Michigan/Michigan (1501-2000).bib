@article{ ISI:000368323100047,
Author = {Li, Si and Xu, Liguang and Ma, Wei and Wu, Xiaoling and Sun, Maozhong
   and Kuang, Hua and Wang, Libing and Kotov, Nicholas A. and Xu, Chuanlai},
Title = {{Dual-Mode Ultrasensitive Quantification of MicroRNA in Living Cells by
   Chiroplasmonic Nanopyramids Self-Assembled from Gold and Upconversion
   Nanoparticles}},
Journal = {{JOURNAL OF THE AMERICAN CHEMICAL SOCIETY}},
Year = {{2016}},
Volume = {{138}},
Number = {{1}},
Pages = {{306-312}},
Month = {{JAN 13}},
Abstract = {{Chiral self-assembled nanomaterials with biological applications have
   attracted great interest. In this study, DNA-driven gold-upconversion
   nanoparticle (Au-UCNP) pyramids were fabricated to detect intracellular
   microRNA (miRNA) in real time. The Au-UCNP pyramids are doubly optically
   active, displaying strong plasmonic circular dichroism (CD) at 521 nm
   and significant luminescence in 500-600 nm, and therefore can be
   monitored by both of them. CD will decrease while the luminescence
   intensity increases in the presence of miRNA. The experimental results
   show that the CD intensity had an outstanding linear range from 0.073 to
   43.65 fmol/10 mu g(RNA) and a limit of detection (LOD) of 0.03 fmol/10
   mu g(RNA), whereas the luminescence intensity ranged from 0.16 to 43.65
   fmol/10 mu g(RNA) with a LOD of 0.12 fmo1/10 mu g(RNA). These data
   indicate that the CD signal is much more sensitive to the concentration
   of miRNA than the luminescent signal, which is attributed to the strong
   CD intensity arising from the spin angular momentum of the photon
   interaction with chiral nanostructures and the plasmonic enhancement of
   the intrinsic chirality of DNA molecules in the pyramids. This approach
   opens up a new avenue to the ultrasensitive detection and quantification
   of miRNA in living cells.}},
