@inproceedings{ ISI:000388376103100,
Author = {Crocker, Stephanie J. and Mathieu, Johanna L.},
Book-Group-Author = {{IEEE}},
Title = {{Adaptive State Estimation and Control of Thermostatic Loads for
   Real-Time Energy Balancing}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2016}},
Pages = {{3557-3563}},
Note = {{American Control Conference (ACC), Boston, MA, JUL 06-08, 2016}},
Organization = {{Amer Automat Control Council}},
Abstract = {{This paper develops adaptive methods to control thermostatic loads that
   are subject to disturbances, with the broader purpose of providing
   real-time energy balancing for the electrical grid. For this
   application, most previous research has focused on the control problem -
   coordinating large populations of thermostatically controlled loads -
   without considering the effect that biased disturbances to the plant
   would have on system performance. To address this issue, we have
   designed online adaptive methods that manage such disturbances. One
   adaptive method uses state measurements to update the autonomous
   dynamics of the plant model, and the other method uses aggregate power
   consumption data. We consider three types of disturbances, and we
   compare the performance of the adaptive control systems to benchmark
   control systems when different combinations of these disturbances are
   active. The largest improvement in RMS tracking error due to adaptive
   control occurs when all three disturbance types are affecting the
   system. In this case, the best adaptive method results in an error of
   7.19\%, as compared with an error of 9.44\% with the best benchmark
   control system.}},
