@inproceedings{ ISI:000388376106141,
Author = {Hou, Jun and Sun, Jing and Hofmann, Heath},
Book-Group-Author = {{IEEE}},
Title = {{Integrated Control of Power Generation, Electric Motor and Hybrid Energy
   Storage for All-Electric Ships}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
