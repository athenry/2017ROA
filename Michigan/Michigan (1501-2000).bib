@inproceedings{ ISI:000388114607100,
Author = {Pierce, Leland},
Book-Group-Author = {{IEEE}},
Title = {{BIG DATA ISSUES FOR REMOTE SENSING: VARIETY}},
Booktitle = {{2016 IEEE INTERNATIONAL GEOSCIENCE AND REMOTE SENSING SYMPOSIUM (IGARSS)}},
Series = {{IEEE International Symposium on Geoscience and Remote Sensing IGARSS}},
Year = {{2016}},
Pages = {{7593-7596}},
Note = {{36th IEEE International Geoscience and Remote Sensing Symposium
   (IGARSS), Beijing, PEOPLES R CHINA, JUL 10-15, 2016}},
Organization = {{Inst Elect \& Elect Engineers; Inst Elect \& Elect Engineers, Geoscience
   \& Remote Sensing Soc; NSSC}},
Abstract = {{There are a number of Big Data issues that are relevant for dealing with
   remotely-sensed data. The major ones are: Volume: How much data ( TB),
   Variety: How variable the organization and meaning of the data is,
   Velocity: Data coming in very fast (TB/sec), Variability: Data varying
   in format or composition over time.
   While each of these is being dealt with by a number of researchers and
   techniques, this paper concerns the second issue: Variety. Dealing with
   the variety of the data that results from remote sensing instruments is
   a daunting task, one that most analysts avoid by using one or a few
   well-known datasets. In order to tackle this problem head-on, we are
   developing an open-source software framework that enables the entire
   remote sensing community to contribute to the solution of this problem.
   In particular, the framework provides a set of standard tools for
   development of software that can then be used to ingest any format of
   remotely-sensed data. Once ingested, this data, including a standard set
   of metadata, can then be used in subsequent processing without writing
   sensor-specific processing codes.
   The framework tools that are being developed to accomplish this are
   detailed, along with some first successes in writing ``ingesters{''} for
   some of the popular datasets.}},
