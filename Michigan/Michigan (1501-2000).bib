@inproceedings{ ISI:000392692000007,
Author = {Zhao, L. and Landi, E. and Fisk, L. A. and Lepri, S. T.},
Editor = {{Wang, L and Bruno, R and Mobius, E and Vourlidas, A and Zank, G}},
Title = {{The Coherent Relation between the Solar Wind Proton Speed and O7+/O6+
   Ratio and its Coronal Sources}},
Booktitle = {{PROCEEDINGS OF THE FOURTEENTH INTERNATIONAL SOLAR WIND CONFERENCE (SOLAR
   WIND 14)}},
Series = {{AIP Conference Proceedings}},
Year = {{2016}},
