@inproceedings{ ISI:000388376106072,
Author = {Meerkov, Semyon M. and Ravichandran, Maruthi T.},
Book-Group-Author = {{IEEE}},
Title = {{Combating Curse of Dimensionality in Resilient Monitoring Systems:
   Condition for Lossless Decentralized Inferences}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2016}},
Pages = {{6381-6386}},
Note = {{American Control Conference (ACC), Boston, MA, JUL 06-08, 2016}},
Organization = {{Amer Automat Control Council}},
Abstract = {{Resilient monitoring systems (RMS) are sensor networks that degrade
   gracefully under cyber-attacks on their sensors. The recently developed
   RMS, while being effective in the attacked sensors identification and
   isolation, exhibited a drawback in their operation - an exponentially
   increasing assessment time as a function of the number of sensors in the
   network. To combat this curse of dimensionality, a decomposition
   approach has been proposed, which led to a dramatic reduction of the
   assessment time, irrespective of the sensor network dimensionality.
   However, the information losses due to this approach, which involves
   decentralized inference calculations, have not been investigated. The
   current paper is intended to carry out such an investigation, and,
   specifically, derive a condition under which the inference calculations
   lead to no loss of information. The development is based on matrices,
   which characterize the coupling of process variables being monitored,
   and on the Renyi-2 entropy.}},
