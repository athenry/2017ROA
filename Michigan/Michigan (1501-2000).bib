@inproceedings{ ISI:000392486303041,
Author = {Essink, B. and Inman, D.},
Editor = {{Sas, P and Moens, D and VanDeWalle, A}},
Title = {{A Comparison of Damping and Vibration Absorption in Metastructures}},
Booktitle = {{PROCEEDINGS OF ISMA2016 INTERNATIONAL CONFERENCE ON NOISE AND VIBRATION
   ENGINEERING AND USD2016 INTERNATIONAL CONFERENCE ON UNCERTAINTY IN
   STRUCTURAL DYNAMICS}},
Year = {{2016}},
Pages = {{2025-2030}},
Note = {{International Conference on Noise and Vibration Engineering (ISMA) /
   International Conference on Uncertainty in Structural Dynamics (USD),
   Leuven, BELGIUM, SEP 19-21, 2016}},
Organization = {{KU Leuven, Dept Mech Engn}},
Abstract = {{In recent months, a new passive metamaterial-inspired vibration
   suppression method has been defined and investigated. This system is
   called a metastructure and is a design consisting of multiple
   oscillators that absorb vibration from the host structure.
   Metastructures integrate vibration suppression into the initial design,
   saving time and other resources that are often spent on adding
   structural damping after manufacturing. Previous metastructure designs
   have proved to be effective in vibration suppression and recent research
   has focused on optimization. Due to the high amount of inherent material
   damping that many of these structures incorporate, these optimizations
   may be gaining some of their advantages from material damping instead of
   solely from the design. This research focuses on finding the dividing
   line between effectiveness of absorber design and the damping properties
   of chosen materials.}},
