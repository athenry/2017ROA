@inproceedings{ ISI:000388376107079,
Author = {Zlotnik, David E. and Forbes, James R. and Aldrich, Jack B.},
Book-Group-Author = {{IEEE}},
Title = {{Control Bandwidth Recovery of Flexible Pointing Systems}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
