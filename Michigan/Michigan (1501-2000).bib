@inproceedings{ ISI:000400601400080,
Author = {Wagenmaker, Andrew J. and Ozay, Necmiye},
Book-Group-Author = {{IEEE}},
Title = {{A Bisimulation-like Algorithm for Abstracting Control Systems}},
Booktitle = {{2016 54TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2016}},
