@article{ ISI:000369456400015,
Author = {Taheri, Ehsan and Abdelkhalik, Ossama},
Title = {{Initial three-dimensional low-thrust trajectory design}},
Journal = {{ADVANCES IN SPACE RESEARCH}},
Year = {{2016}},
Volume = {{57}},
Number = {{3}},
Pages = {{889-903}},
Month = {{FEB 1}},
Abstract = {{This paper presents a method for rapid generation of three-dimensional
   low-thrust trajectories that utilizes Fourier series for shaping the
   position vector. The generated trajectories are feasible with respect to
   the given thrust acceleration constraints. An objective function is
   defined representing the overall mission cost, i.e. minimum AV. Four
   missions from Earth to Mars, the near Earth asteroid 1989ML, comet
   Tempel 1 and asteroid Dionysus are considered for assessing the
   performance of the algorithm. The selected missions present a range of
   various difficulties with different levels of thrust acceleration
   constraints. The Fourier series technique is flexible in generating
   various shapes rather than using one global shape. The proposed method
   is capable of rapid generation of sub-optimal feasible trajectories that
   are totally different from and comparable to the solutions of the
