@inproceedings{ ISI:000388376105004,
Author = {Cruz, Gerardo and Bernstein, Dennis S.},
Book-Group-Author = {{IEEE}},
Title = {{Noncolocated Adaptive Attitude Control of a Planar Two-Body Linkage with
   Nonminimum-Phase Dynamics}},
