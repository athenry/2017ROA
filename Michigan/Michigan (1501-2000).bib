@inproceedings{ ISI:000388442000019,
Author = {Shen, Yanfeng and Cesnik, Carlos E. S.},
Editor = {{Kundu, T}},
Title = {{Modeling of Fatigue Crack Induced Nonlinear Ultrasonics Using a Highly
   Parallelized Explicit Local Interaction Simulation Approach}},
Booktitle = {{HEALTH MONITORING OF STRUCTURAL AND BIOLOGICAL SYSTEMS 2016}},
