@inproceedings{ ISI:000391641300336,
Author = {Wang, J. and Nunovero, N. and Lin, Z. and Nidetz, R. and Buggaveeti, S.
   and Zhan, C. and Kurabayashi, K. and Steinecker, W. H. and Zellers, E.
