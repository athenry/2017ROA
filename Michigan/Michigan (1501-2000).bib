@inproceedings{ ISI:000392361800003,
Author = {Kim, Jinki and Harne, R. L. and Wang, K. W.},
Book-Group-Author = {{ASME}},
Title = {{PREDICTING NON-STATIONARY AND STOCHASTIC ACTIVATION OF SADDLE-NODE
   BIFURCATION}},
Booktitle = {{PROCEEDINGS OF THE ASME CONFERENCE ON SMART MATERIALS, ADAPTIVE
   STRUCTURES AND INTELLIGENT SYSTEMS, 2016, VOL 2}},
