@inproceedings{ ISI:000388377100114,
Author = {Tan, Shurun and Tsang, Leung},
Book-Group-Author = {{IEEE}},
Title = {{The Broadband Green's Funtion Applied to Band Diagram Simulation of 2D
   Perodic Noncircular Dielectric Scatterers}},
Booktitle = {{2016 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2016}},
Pages = {{237-238}},
