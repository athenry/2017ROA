@inproceedings{ ISI:000388376103145,
Author = {Caverly, Ryan James and Forbes, James Richard},
Book-Group-Author = {{IEEE}},
Title = {{Robust Controller Design using the Large Gain Theorem: The Full-State
   Feedback Case}},
