@inproceedings{ ISI:000403301400033,
Author = {Taheri, Ehsan and Kolmanovsky, Ilya and Atkins, Ella},
Editor = {{Zanetti, R and Russell, RP and Ozimek, MT and Bowes, AL}},
Title = {{ON THE APPLICATION OF EXTENDED LOGARITHMIC SMOOTHING TECHNIQUE FOR
   INDIRECT OPTIMIZATION OF MINIMUM-FUEL TRAJECTORIES}},
Booktitle = {{SPACEFLIGHT MECHANICS 2016, PTS I-IV}},
