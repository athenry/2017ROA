@article{ ISI:000323769000046,
Author = {Kang, M. and Wu, J. H. and Sofferman, D. L. and Beskin, I. and Chen, H.
   Y. and Thornton, K. and Goldman, R. S.},
Title = {{Origins of ion irradiation-induced Ga nanoparticle motion on GaAs
   surfaces}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{103}},
Number = {{7}},
Month = {{AUG 12}},
Abstract = {{We have examined the origins of ion irradiation-induced nanoparticle
   (NP) motion. Focused-ion-beam irradiation of GaAs surfaces induces
   random walks of Ga NPs, which are biased in the direction opposite to
   that of ion beam scanning. Although the instantaneous NP velocities are
   constant, the NP drift velocities are dependent on the off-normal
   irradiation angle, likely due to a difference in surface
   non-stoichiometry induced by the irradiation angle dependence of the
   sputtering yield. It is hypothesized that the random walks are initiated
   by ion irradiation-induced thermal fluctuations, with biasing driven by
   anisotropic mass transport. (C) 2013 AIP Publishing LLC.}},
