@article{ ISI:000326432000005,
Author = {Woodall, Jonathan C. and Gosselin, Tracy and Boswell, Amy and Murr,
   Michael and Denton, Brian T.},
Title = {{Improving Patient Access to Chemotherapy Treatment at Duke Cancer
   Institute}},
Journal = {{INTERFACES}},
Year = {{2013}},
Volume = {{43}},
Number = {{5, SI}},
Pages = {{449-461}},
Month = {{SEP-OCT}},
Abstract = {{This paper describes how we applied simulation and optimization in
   combination to improve patient flow within the Duke Cancer Institute, a
   large cancer center. We first developed a discrete-event simulation
   model to predict patient waiting time and resource utilization
   throughout various parts of the center, including the outpatient clinic,
   radiology, the pharmacy, laboratory services, and the oncology treatment
   facility. Simulation model studies showed that nurse unavailability
