@article{ ISI:000323689000025,
Author = {Zou, Ling and Zhang, Hongbin and Gehin, Jess and Kochunas, Brendan},
Title = {{COUPLED THERMAL-HYDRAULIC/NEUTRONICS/CRUD FRAMEWORK IN PREDICTION OF
   CRUD-INDUCED POWER SHIFT PHENOMENON}},
Journal = {{NUCLEAR TECHNOLOGY}},
Year = {{2013}},
Volume = {{183}},
Number = {{3}},
Pages = {{535-542}},
Month = {{SEP}},
Note = {{International Topical Meeting on Reactor Physics, Oak Ridge Knoxville
   Local Sect ANS, Knoxville, TN, APR 15-20, 2012}},
Abstract = {{A thermal-hydraulics (TH)/neutronics/crud multi-physics coupling
   framework to simulate the crud deposits' impact on crud-induced power
   shift (CIPS) phenomenon is proposed in this paper. The coupling among
   three essential physics (i.e., TH, crud, and neutronics) was implemented
   by coupling the computational fluid dynamics software STAR-CCM+, a newly
   developed crud module, and the neutronics code DeCART. A typical 3 x 3
   pressurized water reactor fuel pin problem was analyzed with this
   framework and simulation results are presented. Time-dependent results
   are provided for a 12-month simulation. Simulation results provide the
   history of crud deposits inventory and their distributions on fuel rods,
   boron hideout amount inside crud deposits, and power shape changing over
   time. The obtained results clearly showed the power shape suppression in
   regions where crud deposits exist, a clear indication of CIPS
   phenomenon.}},
