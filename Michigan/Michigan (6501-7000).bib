@article{ ISI:000322406600090,
Author = {Liao, Zhaoliang and Gao, Peng and Stadler, Shane and Jin, Rongying and
   Pan, Xiaoqing and Plummer, E. W. and Zhang, Jiandi},
Title = {{Tuning properties of columnar nanocomposite oxides}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{103}},
Number = {{4}},
Month = {{JUL 22}},
Abstract = {{One major challenge for engineering functional nanocomposites is how to
   tune the geometry structure and control the chemical composition. We
   demonstrate here that columnar nanocomposite films can be grown by using
