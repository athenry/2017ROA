@article{ ISI:000326148000002,
Author = {Chakrabarty, Ayan and Konya, Andrew and Wang, Feng and Selinger,
   Jonathan V. and Sun, Kai and Wei, Qi-Huo},
Title = {{Brownian Motion of Boomerang Colloidal Particles}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2013}},
Volume = {{111}},
Number = {{16}},
Month = {{OCT 18}},
Abstract = {{We investigate the Brownian motion of boomerang colloidal particles
   confined between two glass plates. Our experimental observations show
   that the mean displacements are biased towards the center of
   hydrodynamic stress (CoH), and that the mean-square displacements
   exhibit a crossover from short-time faster to long-time slower diffusion
   with the short-time diffusion coefficients dependent on the points used
   for tracking. A model based on Langevin theory elucidates that these
   behaviors are ascribed to the superposition of two diffusive modes: the
   ellipsoidal motion of the CoH and the rotational motion of the tracking
   point with respect to the CoH.}},
