@article{ ISI:000326120000011,
Author = {Dowling, David R. and Dowling, Thomas R.},
Title = {{Scaling of impact craters in unconsolidated granular materials}},
Journal = {{AMERICAN JOURNAL OF PHYSICS}},
Year = {{2013}},
Volume = {{81}},
Number = {{11}},
Pages = {{875-878}},
Month = {{NOV}},
Abstract = {{A simple experiment suitable for introductory physical science
   laboratory instruction involves forming impact craters by dropping
   spherical objects into a bed of unconsolidated granular material. The
   experiment is straightforward, and laboratory results for different
   impact energies, spheres, and granular materials may be collapsed to a
   single power-law using parametric scaling determined from dimensional
   analysis. The resulting power law is extrapolated over more than 16
   orders of magnitude to produce an estimate of the impact energy that
   formed the 1.2-km-diameter Barringer Meteor Crater in northern Arizona.
   (C) 2013 American Association of Physics Teachers.}},
