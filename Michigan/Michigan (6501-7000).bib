@article{ ISI:000323589000005,
Author = {Weber, Jr., Walter J. and Kim, Han S.},
Title = {{Removal of greases and lubricating oils from metal parts of machinery
   processes by subcritical water treatment}},
Journal = {{JOURNAL OF SUPERCRITICAL FLUIDS}},
Year = {{2013}},
Volume = {{80}},
Pages = {{30-37}},
Month = {{AUG}},
Abstract = {{Toxic or persistent solvents have been widely used to remove greases and
   lubricants from various machine elements in the washing processes. In
   this study, an alternative degreasing method that employed subcritical
   state water was assessed. This environmentally benign solvent has
   significant potential for various degreasing applications. The operation
   time and temperature and flow rate of subcritical water had markedly
   positive impacts on the degreasing efficiency. However, the effect of
   pressure of subcritical water flow was minimal. The degreasing
   efficiency was also highly dependent on the physical characteristics and
   chemical composition of grease. The subcritical water treatment
   demonstrated a better degreasing efficiency than conventional degreasing
   methods. Only minor physical damage was observed on the metal parts
   after the subcritical water treatment. Conclusively, it was found that
   the subcritical water degreasing system can be used as an effective
   degreasing technology for machinery operations. (C) 2013 Elsevier B.V.
   All rights reserved.}},
