@article{ ISI:000325284500040,
Author = {Huang, S. and Kim, S. J. and Levy, R. and Pan, X. Q. and Goldman, R. S.},
Title = {{Mechanisms of InAs/GaAs quantum dot formation during annealing of In
   islands}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{103}},
Number = {{13}},
Month = {{SEP 23}},
Abstract = {{We have examined the formation mechanisms of InAs quantum dots (QDs) via
   annealing In islands under As flux. We report two distinct mechanisms,
   droplet epitaxy (DE) and solid phase epitaxy (SPE), which depend on As
   surface coverage. On c(4 x 4) GaAs surfaces, QDs form by DE. For c(4 x
   4)alpha, one-to-one conversion from In islands to InAs QDs is observed.
   For c(4 x 4)beta, lower densities of larger QDs are observed, presumably
   due to enhanced In surface diffusion in the absence of metastable Ga-As
   dimers. For the As capped surface, In deposition leads to an amorphous
   film, from which QDs nucleate by SPE during annealing. (C) 2013 AIP
   Publishing LLC.}},
