@article{ ISI:000303964900024,
Author = {Frieberg, Bradley and Glynos, Emmanouil and Sakellariou, Georgios and
   Green, Peter F.},
Title = {{Physical Aging of Star-Shaped Macromolecules}},
Journal = {{ACS MACRO LETTERS}},
Year = {{2012}},
Volume = {{1}},
Number = {{5}},
Pages = {{636-640}},
Month = {{MAY}},
Abstract = {{Time-dependent structural relaxations, physical aging, of films with
   thicknesses in the range 0.4 mu m < H < 2 mu m of star-shaped
   polystyrene (SPS) macromolecules were investigated. Our studies reveal
   that the aging rates of star-shaped PS macromolecules are appreciably
   slower than their linear chain analogs. The magnitude of the difference
   between the aging rates of the linear and star-shaped macromolecules
   increases with increasing functionality, f, and decreasing molecular
   weight per arm, M-n(arm), of the stars. Our results are consistent with
   the notion that constraints imposed due to the architecture of the
   macromolecule suppress relaxations associated with and accommodate the
   reduction of the free volume of the system.}},
