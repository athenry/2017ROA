@article{ ISI:000304851600028,
Author = {Williams, Ceri A. and Smith, George D. W. and Marquis, Emmanuelle A.},
Title = {{The effect of Ti on the coarsening behavior of oxygen-rich nanoparticles
   in oxide-dispersion-strengthened steels after annealing at 1200 degrees
   C}},
