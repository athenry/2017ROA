@article{ ISI:000306247400013,
Author = {Wang, Yonggang and Zhou, Xiuli},
Title = {{Investigation of High Gain Metal Optical Antennas Working at 10.6 mu m}},
Journal = {{JOURNAL OF COMPUTATIONAL AND THEORETICAL NANOSCIENCE}},
Year = {{2012}},
Volume = {{9}},
Number = {{5}},
Pages = {{711-715}},
Month = {{MAY}},
Abstract = {{Optical antennas working at 10.6 mu m with different dipole geometries
   are designed. The dipole antenna is gradually evolved into two-steps,
   three-steps, four-steps and finally evolved into a bow-tie antenna
   structure. The near-field enhancement properties at the resonant
   wavelength of 10.6 mu m are studied with finite-difference-time-domain
   (FDTD) based calculation. Strong electric field (E-field) intensity
   distribution can be observed in the gap regions. Sharp angle effects are
   observed from the E-field distribution and it supports the strongest
   E-field distribution and the highest enhancement efficiency of the
   bow-tie infrared antenna. The dipole antenna with width of 100 nm is
   found to be the optimal geometry through exploring the relation between
   the width of dipole antenna and the antenna resolution. The high gain
   optical properties of the designed antenna motivate its application in
   infrared (IR) communication system.}},
