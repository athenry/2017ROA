@article{ ISI:000305822300010,
Author = {Zhang, D. and Waas, A. M. and Pankow, M. and Yen, C. F. and Ghiorse, S.},
Title = {{Flexural Behavior of a Layer-to-Layer Orthogonal Interlocked
