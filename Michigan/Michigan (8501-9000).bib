@article{ ISI:000303872800011,
Author = {Hawkins, Taviare L. and Mirigian, Matthew and Li, Jingqiang and Yasar,
   M. Selcuk and Sackett, Dan L. and Sept, David and Ross, Jennifer L.},
Title = {{Perturbations in Microtubule Mechanics from Tubulin Preparation}},
