@article{ ISI:000305483000013,
Author = {Tao, Fei and Cheng, Ying and Zhang, Lin and Zhao, Dongming},
Title = {{Utility modelling, equilibrium, and coordination of resource service
   transaction in service-oriented manufacturing system}},
