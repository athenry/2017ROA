@inproceedings{ ISI:000349551500014,
Author = {Guo, Yi and Liu, Xuanchen and Yan, Yu and Zhang, Ni and Su, Wencong},
Book-Group-Author = {{IEEE}},
Title = {{Economic Analysis of Plug-in Electric Vehicle Parking Deck with Dynamic
   Pricing}},
