@incollection{ ISI:000337342300026,
Author = {Orosz, Gabor},
Editor = {{Vyhlidal, T and Lafay, JF and Sipahi, R}},
Title = {{Decomposing the Dynamics of Delayed Hodgkin-Huxley Neurons}},
Booktitle = {{DELAY SYSTEMS: FROM THEORY TO NUMERICS AND APPLICATIONS}},
Series = {{Advances in Delays and Dynamics}},
Year = {{2014}},
Volume = {{1}},
Pages = {{343-357}},
Abstract = {{The effects of time delays on the nonlinear dynamics of neural networks
   are investigated. A decomposition method is utilized to derive modal
   equations that allow one to analyze the dynamics around synchronous
   states. The D-subdivision method is used to study the stability of
   equilibria while the stability of periodic orbits is investigated using
   Floquet theory. These methods are applied to a system of delay coupled
   Hodgkin-Huxley neurons to map out stable and unstable synchronous
   states. It is shown that for sufficiently strong coupling there exist
   delay ranges where stable equilibria coexist with stable oscillations
   which allow neural systems to respond to different environmental stimuli
   with different spatiotemporal patterns.}},
