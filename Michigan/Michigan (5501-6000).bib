@inproceedings{ ISI:000362533100049,
Author = {Rao, Abhinav and Tawfickl, Sameh and Shlian, Matthew and Hart, A. John},
Book-Group-Author = {{ASME}},
Title = {{FOLD MECHANICS OF NATURAL AND SYNTHETIC ORIGAMI PAPERS}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
   CONFERENCES AND COMPUTERS AND INFORMATION IN ENGINEERING CONFERENCE,
   2013, VOL 6B}},
Year = {{2014}},
