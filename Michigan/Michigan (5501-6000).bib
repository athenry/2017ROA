@article{ ISI:000344997500043,
Author = {Feng, Xuejiao and Yang, Jun and Bie, Yitian and Wang, Jiulin and Nuli,
   Yanna and Lu, Wei},
Title = {{Nano/micro-structured Si/CNT/C composite from nano-SiO2 for high power
   lithium ion batteries}},
Journal = {{NANOSCALE}},
Year = {{2014}},
Volume = {{6}},
Number = {{21}},
Pages = {{12532-12539}},
Abstract = {{Nano/micro-structured pSi and pSi/CNT particles were synthesized from
   nano-SiO2 as both a template and silicon precursor via a combination of
   spray drying and magnesiothermic reduction, followed by a nano-layer
   carbon coating by chemical vapor deposition to obtain a
   nano/micro-structured pSi/C and pSi/CNT/C composite. In the hierarchical
   microstructure of the pSi/CNT/C composite, Si nanoparticles less than 20
   nm in size were homogenously dispersed in an electronically conductive
   and porous network of multiwall carbon nanotubes, which can accommodate
   the volume changes in Si and improve the structural and conductive
   stability during repeated cycles leading to excellent electrochemical
