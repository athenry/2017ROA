@inproceedings{ ISI:000346492604011,
Author = {Storms, Justin G. and Tilbury, Dawn M.},
Book-Group-Author = {{IEEE}},
Title = {{Blending of Human and Obstacle Avoidance Control for a High Speed Mobile
   Robot}},
Booktitle = {{2014 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2014}},
Pages = {{3488-3493}},
Note = {{American Control Conference, Portland, OR, DEC 04-06, 2014}},
