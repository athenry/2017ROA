@inproceedings{ ISI:000344110800027,
Author = {Hobeck, Jared D. and Geslain, Damien and Inman, Daniel J.},
Editor = {{Lynch, JP and Wang, KW and Sohn, H}},
Title = {{The Dual Cantilever Flutter Phenomenon: A Novel Energy Harvesting Method}},
Booktitle = {{SENSORS AND SMART STRUCTURES TECHNOLOGIES FOR CIVIL, MECHANICAL, AND
   AEROSPACE SYSTEMS 2014}},
Series = {{Proceedings of SPIE}},
