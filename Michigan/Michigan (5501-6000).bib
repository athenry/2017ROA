@inproceedings{ ISI:000361167200072,
Author = {Storms, Justin G. and Tilbury, Dawn M.},
Book-Group-Author = {{ASME}},
Title = {{DYNAMIC WEIGHT-SHIFTING TO REDUCE ROLLOVER RISK IN HIGH SPEED MOBILE
   MANIPULATORS}},
Booktitle = {{7TH ANNUAL DYNAMIC SYSTEMS AND CONTROL CONFERENCE, 2014, VOL 3}},
Year = {{2014}},
Note = {{ASME 7th Annual Dynamic Systems and Control ConferenceResearch Workshop
   Israel Science Foundation on Infinite Products of Operators and their
   Applications, San Antonio, TXTechnion, Israel Inst Technol, Haifa,
   ISRAEL, OCT 22-24, 2014MAY 21-24, 2012}},
Organization = {{ASME, Dynam Syst \& Control Div}},
Organization = {{Technion, Ctr Math Sci}},
Abstract = {{Small unmanned ground vehicles risk rollover at high speeds. This paper
   proposes use of a manipulator arm on the robot in a dynamic
   weight-shifting arrangement to reduce rollover risk. A simple linear
   control law for dynamic weight-shifting of the manipulator arm is
   proposed. A linear dynamic model is used to analyze the effect of the
   arm design (link length, mass, etc.) on the roll dynamics, and a more
   complex nonlinear simulation in Simulink SimMechanics is used to
   evaluate roll reduction for various steering angles and velocities.
   Simulations given the same steering input with dynamic weight-shifting
   showed a roll reduction factor of 12\%. For the same radius turn, a roll
   reduction factor of 29\% was observed. A 50\% reduction in steering
   angle was used to achieve a similar turning radius with dynamic
   weight-shifting activated. By increasing rollover stability, dynamic
   weight-shifting has the potential to increase safe operating speeds for
   mobile robots.}},
