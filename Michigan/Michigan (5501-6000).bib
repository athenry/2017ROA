@inproceedings{ ISI:000352217500211,
Author = {Kim, Brian J. and Washabaugh, E. Peter and Meng, Ellis},
Book-Group-Author = {{IEEE}},
Title = {{ANNEALING EFFECTS ON FLEXIBLE MULTI-LAYERED PARYLENE-BASED SENSORS}},
