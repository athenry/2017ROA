@inproceedings{ ISI:000346492605088,
Author = {Weiss, Avishai and Leve, Frederick and Baldwin, Morgan and Forbes, James
   R. and Kolmanovsky, Ilya},
Book-Group-Author = {{IEEE}},
Title = {{Spacecraft Constrained Attitude Control using Positively Invariant
   Constraint Admissible Sets on SO(3) x R-3}},
Booktitle = {{2014 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2014}},
Pages = {{4955-4960}},
Note = {{American Control Conference, Portland, OR, DEC 04-06, 2014}},
Organization = {{American Automat Control Council; Boeing; GE Global Res; Honeywell;
