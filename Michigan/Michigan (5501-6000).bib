@inproceedings{ ISI:000346492602058,
Author = {Chitalia, Yash and Zhang, Weijia and Hyun, Baro and Girard, Anouck},
Book-Group-Author = {{IEEE}},
Title = {{A Revisit-based Mixed-Initiative Nested Classification Scheme for
   Unmanned Aerial Vehicles}},
Booktitle = {{2014 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2014}},
Note = {{American Control Conference, Portland, OR, DEC 04-06, 2014}},
