@article{ ISI:000331644100016,
Author = {Zhang, Lei and Lee, Leung-Kway and Teng, Chu-Hsiang and Hill, Tyler A.
   and Ku, Pei-Cheng and Deng, Hui},
Title = {{How much better are InGaN/GaN nanodisks than quantum wells-Oscillator
   strength enhancement and changes in optical properties}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2014}},
Volume = {{104}},
Number = {{5}},
Month = {{FEB 3}},
Abstract = {{We show over 100-fold enhancement of the exciton oscillator strength as
   the diameter of an InGaN nanodisk in a GaN nanopillar is reduced from a
   few micrometers to less than 40 nm, corresponding to the quantum dot
   limit. The enhancement results from significant strain relaxation in
   nanodisks less than 100 nm in diameter. Meanwhile, the radiative decay
   rate is only improved by 10 folds due to strong reduction of the local
   density of photon states in small nanodisks. Further increase in the
   radiative decay rate can be achieved by engineering the local density of
   photon states, such as adding a dielectric coating. (C) 2014 AIP
