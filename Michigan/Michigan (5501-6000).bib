@inproceedings{ ISI:000361249800002,
Author = {Lee, S. Shawn and Shao, Chenhui and Kim, Tae Hyung and Hu, S. Jack and
   Kannatey-Asibu, Elijah and Cai, Wayne W. and Spicer, J. Patrick and
   Abell, Jeffrey A.},
