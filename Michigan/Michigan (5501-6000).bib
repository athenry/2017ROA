@inproceedings{ ISI:000361249700083,
Author = {Weng, Xin and Jin, Xiaoning and Ni, Jun},
Book-Group-Author = {{ASME}},
Title = {{MULTI-SCALE SURFACE CHARACTERIZATION AND CONTROL BASED HIGH DENSITY
   MEASUREMENTS}},
Booktitle = {{PROCEEDINGS OF THE ASME 9TH INTERNATIONAL MANUFACTURING SCIENCE AND
