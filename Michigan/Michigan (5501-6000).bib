@inproceedings{ ISI:000354965500306,
Author = {Lee, Doowon and Parikh, Ritesh and Bertacco, Valeria},
Book-Group-Author = {{IEEE}},
Title = {{Brisk and Limited-Impact NoC Routing Reconfiguration}},
Booktitle = {{2014 DESIGN, AUTOMATION AND TEST IN EUROPE CONFERENCE AND EXHIBITION
   (DATE)}},
