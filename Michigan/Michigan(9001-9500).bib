Author = {Kumar, Karthik and Bernal, Luis P. and Yee, Seow Yuen and Besharatian,
   Ali and Najafi, Khalil},
Book-Group-Author = {{ASME}},
Title = {{TRANSIENT PERFORMANCE \& COUPLED ACOUSTIC STRUCTURAL \& ELECTROSTATIC
   MODELING OF A MULTISTAGE VACUUM MICROPUMP}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL MECHANICAL ENGINEERING CONGRESS
