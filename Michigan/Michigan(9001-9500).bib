@inproceedings{ ISI:000310776203044,
Author = {Ersal, Tulga and Ahn, Changsun and Hiskens, Ian A. and Peng, Huei and
   Stefanopoulou, Anna and Stein, Jeffrey L.},
Book-Group-Author = {{IEEE}},
Title = {{On the Effect of DC Source Voltage on Inverter-Based Frequency and
   Voltage Regulation in a Military Microgrid}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
