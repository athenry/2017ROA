@inproceedings{ ISI:000310776202060,
Author = {Larimore, Jacob and Hellstroem, Erik and Sterniak, Jeff and Jiang, Li
   and Stefanopoulou, Anna G.},
Book-Group-Author = {{IEEE}},
Title = {{Experiments and Analysis of High Cyclic Variability at the Operational
   Limits of Spark-Assisted HCCI Combustion}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
