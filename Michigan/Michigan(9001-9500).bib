Author = {Kumon, R. E. and Gudur, M. S. R. and Zhou, Y. and Deng, C. X.},
Editor = {{Muratore, R and Konofagou, EE}},
Title = {{High-Frequency Ultrasound M-mode Monitoring of HIFU Ablation in Cardiac
   Tissue}},
Booktitle = {{11TH INTERNATIONAL SYMPOSIUM ON THERAPEUTIC ULTRASOUND}},
Series = {{AIP Conference Proceedings}},
Year = {{2012}},
Volume = {{1481}},
