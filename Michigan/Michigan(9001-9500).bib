@inproceedings{ ISI:000312409000011,
Author = {Lee, Tae-Kyung and Filipi, Zoran S.},
Editor = {{Barsoum, N and Faiman, D and Vasant, P}},
Title = {{Approaches for the Computationally Efficient Assessment of the Plug-in
   HEV Impact on the Grid}},
Booktitle = {{PROCEEDINGS OF THE SIXTH GLOBAL CONFERENCE ON POWER CONTROL AND
   OPTIMIZATION}},
Series = {{AIP Conference Proceedings}},
Year = {{2012}},
Volume = {{1499}},
