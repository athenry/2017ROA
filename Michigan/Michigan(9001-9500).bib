@inproceedings{ ISI:000310776200098,
Author = {D'Amato, Anthony M. and Bernstein, Dennis S.},
Book-Group-Author = {{IEEE}},
Title = {{Adaptive Forward-Propagating Input Reconstruction for Nonminimum-Phase
   Systems}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
