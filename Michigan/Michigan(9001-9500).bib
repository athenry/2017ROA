@inproceedings{ ISI:000327200402156,
Author = {Nersesov, Sergey G. and Haddad, Wassim M. and Bernstein, Dennis S.},
Book-Group-Author = {{IEEE}},
Title = {{Poincare Recurrence and Output Reversibility in Linear Dynamical Systems}},
Booktitle = {{2012 IEEE 51ST ANNUAL CONFERENCE ON DECISION AND CONTROL (CDC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2012}},
Pages = {{2587-2592}},
