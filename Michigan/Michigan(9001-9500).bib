@article{ ISI:000301397100020,
Author = {Evangelopoulos, Apostolos E. A. S. and Glynos, Emmanouil and
   Madani-Grasset, Frederic and Koutsos, Vasileios},
Title = {{Elastic Modulus of a Polymer Nanodroplet: Theory and Experiment}},
Journal = {{LANGMUIR}},
Year = {{2012}},
Volume = {{28}},
Number = {{10}},
Pages = {{4754-4767}},
Month = {{MAR 13}},
Abstract = {{We redevelop a theoretical model that, in conjunction with atomic force
   microscopy (AFM), can be used as a noninvasive method for determination
   of the elastic modulus of a polymer nanodroplet residing on a flat,
   rigid substrate. The model is a continuum theory that combines surface
   and elasticity theories for prediction of the droplet's elastic modulus,
   given experimental measurement of its adsorbed height. Utilization of
   AFM-measured heights for relevant droplets reported in the literature
   and from our own experiments illustrated the following: the significance
   of both surface and elasticity effects in determining a polymer
   droplet's spreading behavior; the extent of a continuum theory's
   validity as one approaches the nanoscale; and a droplet size effect on
   the elastic modulus.}},
