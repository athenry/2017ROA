Author = {Chun, Se Young and Fessler, Jeffrey A. and Dewaraja, Yuni K.},
Editor = {{Yu, B}},
Title = {{Non-Local Means Methods Using CT Side Information for I-131 SPECT Image
   Reconstruction}},
