@inproceedings{ ISI:000310776201134,
Author = {Harvey, Philip S. and Scruggs, Jeffrey T. and Gavin, Henri P.},
Book-Group-Author = {{IEEE}},
Title = {{A Dual Method for Determining the Performance Limits of a Semiactively
   Constrained Control System}},
