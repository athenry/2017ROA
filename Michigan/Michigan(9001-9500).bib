@inproceedings{ ISI:000324347700082,
Author = {Tai, Bruce L. and Wang, Hui and Hai Nguyen and Hu, S. Jack and Shih,
   Albert},
Book-Group-Author = {{ASME}},
Title = {{SURFACE VARIATION REDUCTION FOR FACE MILLING BASED ON HIGH-DEFINITION
   METROLOGY}},
