@inproceedings{ ISI:000309166203058,
Author = {Kumar, Shyam Sunder and Sun, Min and Savarese, Silvio},
Book-Group-Author = {{IEEE}},
Title = {{Mobile Object Detection through Client-Server based Vote Transfer}},
