Author = {Li, Yangming and Olson, Edwin B.},
Book-Group-Author = {{IEEE
   Robotics Society of Japan}},
Title = {{IPJC: The Incremental Posterior Joint Compatibility Test for Fast
   Feature Cloud Matching}},
