@article{ ISI:000298978700187,
Author = {Liu, Dongan and Sudik, Andrea and Yang, Jun and Ferro, Patrick and
   Wolverton, Chris},
Title = {{Hydrogen Storage Properties of 3Mg(NH2)(2)-2Li(3)AlH(6)}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2012}},
Volume = {{116}},
Number = {{1}},
Pages = {{1485-1492}},
Month = {{JAN 12}},
Abstract = {{Recent efforts have been made to develop high-capacity complex hydride
   composites by combining alanates and amides. The hydrogen storage
   mechanisms in those composites are not unambiguously clarified because
   of chemical reactions during the sample preparation process. In this
   Article, we have studied the effects of sample preparation conditions on
   the phase stability of a mixture of 3Mg(NH2)(2)-2Li(3)AlH(6) and
   identified that unlike high-energy ball-milling light mixing generates a
   physical mixture of the reactants without decomposition. Subsequently,
   the hydrogen storage properties, the desorption pathway, and the
   reversible reaction mechanism of the composite were investigated through
   a combination of kinetic measurements and phase and microstructure
   analyses. The results reveal that the first step of hydrogen release
   (initiated at 170 degrees C) involves decomposition of Li3AlH6 to LiH
   and Al. The second step of hydrogen release occurs as the temperature
   increases (to 230 degrees C) when Mg(NH2)(2) reacts with LiH to form
   Li2Mg(NH)(2). If desorption of the 3Mg(NH2)(2)-2Li(3)AlH(6) mixture is
   limited to a temperature of 400 degrees C, then the reversible reaction
   takes place between Li2Mg(NH)(2) (plus H-2) and LiH and Mg(NH2)(2). We
   find that the Al generated from the first hydrogen release step does not
   participate in the reversible hydrogen storage process and instead
   inhibits mass transfer that results in higher desorption temperatures
   (and low desorption rates) and lowers the overall reversible capacity
   (2.7 wt \%) as compared with the neat reaction (i.e., similar to 4.3 wt
   \% without the presence of Al).}},
