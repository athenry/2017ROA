Author = {Johnson, David R. and Wang, K. W.},
Book-Group-Author = {{ASME}},
Title = {{ANALYSIS OF A FLEXIBLY MOUNTED SHAFT INCORPORATING A NON-CONSTANT
   VELOCITY COUPLING WITH DYNAMIC MISALIGNMENT}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
