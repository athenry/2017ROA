Author = {Seok, Jinwoo and Hyun, Baro and Kasa-Vubu, Josephine and Girard, Anouck},
Book-Group-Author = {{IEEE}},
Title = {{Automated Classification System for Bone Age X-ray Images}},
Booktitle = {{PROCEEDINGS 2012 IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN, AND
   CYBERNETICS (SMC)}},
Series = {{IEEE International Conference on Systems Man and Cybernetics Conference
   Proceedings}},
Year = {{2012}},
