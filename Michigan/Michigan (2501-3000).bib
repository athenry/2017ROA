@article{ ISI:000357751200043,
Author = {Yin, Xunyuan and Li, Zhaojian and Zhang, Lixian and Wang, Changhong and
   Shammakh, Wafa and Ahmad, Bashir},
Title = {{Model reduction of A class of Markov jump nonlinear systems with
   time-varying delays via projection approach}},
Journal = {{NEUROCOMPUTING}},
Year = {{2015}},
Volume = {{166}},
Pages = {{436-446}},
Month = {{OCT 20}},
Abstract = {{In this paper, the problem of H-infinity model reduction for a class of
   discrete-time Markov jump nonlinear systems with time-varying delays is
   investigated. Based on the Lyapunov-Krasovskii functional method,
   sufficient stability conditions in terms of strict LMIs are derived for
   the model error system. Projection approach is employed to construct a
   reduced-order model such that the model error system is asymptotically
   stable and preserves a guaranteed H-infinity performance. A numerical
   example is presented to illustrate the effectiveness of the developed
   theoretical results and to make comparisons among systems with different
   structures and parameters. (C) 2015 Elsevier B.V. All rights reserved.}},
