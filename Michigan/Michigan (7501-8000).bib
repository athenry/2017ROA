@inproceedings{ ISI:000329526900047,
Author = {Teng, Chu-Hsiang and Zhang, Lei and Deng, Hui and Ku, Pei-Cheng},
Editor = {{Chyi, JI and Nanishi, Y and Morkoc, H and Piprek, J and Yoon, E and Fujioka, H}},
Title = {{Linewidth reduction of site-controlled InGaN quantum dots by surface
   passivation}},
Booktitle = {{GALLIUM NITRIDE MATERIALS AND DEVICES VIII}},
Series = {{Proceedings of SPIE}},
Year = {{2013}},
Volume = {{8625}},
Note = {{Symposium on Gallium Nitride Materials and Devices VIII, San Francisco,
   CA, FEB 04-07, 2013}},
Organization = {{SPIE}},
Abstract = {{Indium gallium nitride (InGaN) semiconductor quantum dots are an
   attractive candidate for scalable room temperature quantum photonics
   applications owing to their large exciton binding energy and large
   oscillation strength. Previously, we reported single photon emission
   from site-controlled InGaN quantum dot structures. However, large
   homogeneous linewidth and significant non-radiative recombination were
   thought to be linked to the nearby surface charge centers. These charge
   centers can lead to spectral diffusion and excessive non-radiative
   recombinations at high temperature. In this work, ammonium sulfide
   passivation was investigated. Nitrogen vacancies were successfully
   passivated by ammonium sulfide ((NH4)(2)S-x) treatment, and the emission
   linewidth of a single quantum dot was reduced by 5 meV. Furthermore, the
   linewidth broadening with an increasing temperature was suppressed in
   the temperature range from 9 K to 95 K in this study. Satellite emission
   peak believed to be associated with the nitrogen vacancy was observed
   for un-passivated quantum dots. The satellite peak was 55 similar to 80
   meV away from the main InGaN emission peak and was eliminated after
   sulfide passivation.}},
