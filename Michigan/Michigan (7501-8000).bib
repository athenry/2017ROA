@inproceedings{ ISI:000331367402137,
Author = {Hinkle, Lauren and Olson, Edwin},
Editor = {{Amato, N}},
Title = {{Predicting Object Functionality Using Physical Simulations}},
Booktitle = {{2013 IEEE/RSJ INTERNATIONAL CONFERENCE ON INTELLIGENT ROBOTS AND SYSTEMS
   (IROS)}},
Series = {{IEEE International Conference on Intelligent Robots and Systems}},
Year = {{2013}},
