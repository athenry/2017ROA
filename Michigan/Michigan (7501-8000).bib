@article{ ISI:000314488200019,
Author = {Gillespie, Justin W. and Singer, David J.},
Title = {{Identifying drivers of general arrangements through the use of network
   measures of centrality and hierarchy}},
Journal = {{OCEAN ENGINEERING}},
Year = {{2013}},
Volume = {{57}},
Pages = {{230-239}},
Month = {{JAN 1}},
Abstract = {{Developing three-dimensional space and system arrangements is a
   challenging and time-consuming task because ship layouts include
   hundreds or thousands of elements with multiple interconnections and
   dependencies governed by functional characteristics. Existing automated
   arrangements tools aimed at early-stage design are capable of generating
   feasible layouts, but remain a bit of a ``black box,{''} lacking methods
   to elucidate the mechanisms that drive or limit the quality of those
   layouts. By knowing which items drive or constrain an arrangement,
   designers can more judiciously decide when and where to position
   objects. Methods that provide this information are particularly valuable
   for novel ship types, designs containing emerging technologies, or
   densely outfitted vessels such as naval combatants and submersibles. The
   use of networks to represent the interconnections allows designers to
   focus on the underlying relationships input into the system rather than
   just the resultant layouts. The authors use the complex network measures
   of centrality and hierarchy to identify layout drivers based solely on
   the relationships among the spaces to be arranged. Moreover, the methods
   are shown to permit identification of drivers without ever creating
   geometric arrangements. (C) 2012 Elsevier Ltd. All rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
Type = {{Article}},
