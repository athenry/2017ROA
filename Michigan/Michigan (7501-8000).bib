@inproceedings{ ISI:000326900100082,
Author = {Hu, Yue and Ramani, Sathish and Jacob, Mathews},
Book-Group-Author = {{IEEE}},
Title = {{A FAST MAJORIZE MINIMIZE ALGORITHM FOR HIGHER DEGREE TOTAL VARIATION
   REGULARIZATION}},
Booktitle = {{2013 IEEE 10TH INTERNATIONAL SYMPOSIUM ON BIOMEDICAL IMAGING (ISBI)}},
Series = {{IEEE International Symposium on Biomedical Imaging}},
Year = {{2013}},
Pages = {{326-329}},
Note = {{IEEE 10th International Symposium on Biomedical Imaging - From Nano to
   Macro (ISBI), San Francisco, CA, APR 07-11, 2013}},
Organization = {{IEEE; IEEE Signal Processing Soc; Engn Med Biol Soc}},
Abstract = {{The main focus of this paper is to introduce a computationally efficient
   algorithm for solving image recovery problems, regularized by the
   recently introduced higher degree total variation (HDTV) penalties. The
   anisotropic HDTV penalty is the fully separable L 1 semi-norm of the
   directional image derivatives; the use of this penalty is seen to
   considerably improve image quality in biomedical inverse problems. We
   introduce a novel majorize minimize algorithm to solve the HDTV
   optimization problem, thus considerably speeding it over the previous
   implementation. Specifically, comparisons with previous iterative
   reweighted algorithm show an approximate ten fold speedup. The new
   algorithm enables us to obtain reconstructions that are free of patchy
