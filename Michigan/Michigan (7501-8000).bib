@inproceedings{ ISI:000327210206027,
Author = {Sadrpour, Amir and Jin, J. and Ulsoy, A. G.},
Book-Group-Author = {{IEEE}},
Title = {{Experimental Validation of Mission Energy Prediction Model for Unmanned
   Ground Vehicles}},
Booktitle = {{2013 AMERICAN CONTROL CONFERENCE (ACC)}},
