Author = {Chen, Chia-Hsiang and Bowman, Keith and Augustine, Charles and Zhang,
   Zhengya and Tschanz, Jim},
Book-Group-Author = {{IEEE}},
Title = {{Minimum Supply Voltage for Sequential Logic Circuits in a 22nm
   Technology}},
Booktitle = {{2013 IEEE INTERNATIONAL SYMPOSIUM ON LOW POWER ELECTRONICS AND DESIGN
   (ISLPED)}},
Year = {{2013}},
