@inproceedings{ ISI:000331367404078,
Author = {Bishop-Moser, Joshua and Krishnan, Girish and Kota, Sridhar},
Editor = {{Amato, N}},
Title = {{Force and Moment Generation of Fiber-reinforced Pneumatic Soft Actuators}},
Booktitle = {{2013 IEEE/RSJ INTERNATIONAL CONFERENCE ON INTELLIGENT ROBOTS AND SYSTEMS
   (IROS)}},
Series = {{IEEE International Conference on Intelligent Robots and Systems}},
Year = {{2013}},
