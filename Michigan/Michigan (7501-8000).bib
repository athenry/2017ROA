@inproceedings{ ISI:000327210204117,
Author = {Brown, W. Robert and Ulsoy, A. Galip},
Book-Group-Author = {{IEEE}},
Title = {{Maneuver Based Design of a Passive-Assist Device for Augmenting Linear
   Motion Drives}},
Booktitle = {{2013 AMERICAN CONTROL CONFERENCE (ACC)}},
