@article{ ISI:000314400000008,
Author = {Wu, Yi-Kuei Ryan and Hollowell, Andrew E. and Zhang, Cheng and Guo, L.
   Jay},
Title = {{Angle-Insensitive Structural Colours based on Metallic Nanocavities and
   Coloured Pixels beyond the Diffraction Limit}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2013}},
Volume = {{3}},
Month = {{FEB 1}},
Abstract = {{To move beyond colorant-based pigmentation display technologies, a
   variety of photonic and plasmonic crystal based structures have been
   designed and applied as colour filters. Nanostructure based colour
   filtering offers increased efficiencies, low power consumption, slim
   dimensions, and enhanced resolution. However, incident angle tolerance
   still needs to be improved. In this work, we propose a new scheme
   through localized resonance in metallic nanoslits by light funneling.
   Angle insensitive colour filters up to +/- 80 degrees have been
   achieved, capable of wide colour tunability across the entire visible
   band with pixel size beyond the diffraction limit (similar to lambda/2).
   This work opens the door to angle insensitive manipulation of light with
   structural filtering.}},
