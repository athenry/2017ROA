Author = {Chwa, Hoon Sung and Lee, Jinkyu and Phan, Kieu-My and Easwaran, Arvind
   and Shin, Insik},
Book-Group-Author = {{IEEE}},
Title = {{Global EDF Schedulability Analysis for Synchronous Parallel Tasks on
   Multicore Platforms}},
Booktitle = {{PROCEEDINGS OF THE 2013 25TH EUROMICRO CONFERENCE ON REAL-TIME SYSTEMS
   (ECRTS 2013)}},
