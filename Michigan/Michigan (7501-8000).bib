@article{ ISI:000314151000005,
Author = {Nadakuditi, Raj Rao and Newman, M. E. J.},
Title = {{Spectra of random graphs with arbitrary expected degrees}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2013}},
Volume = {{87}},
Number = {{1}},
Month = {{JAN 10}},
Abstract = {{We study random graphs with arbitrary distributions of expected degree
   and derive expressions for the spectra of their adjacency and modularity
   matrices. We give a complete prescription for calculating the spectra
   that is exact in the limit of large network size and large vertex
   degrees. We also study the effect on the spectra of hubs in the network,
   vertices of unusually high degree, and show that these produce isolated
   eigenvalues outside the main spectral band, akin to impurity states in
   condensed matter systems, with accompanying eigenvectors that are
   strongly localized around the hubs. We give numerical results that
   confirm our analytic expressions. DOI: 10.1103/PhysRevE.87.012803}},
