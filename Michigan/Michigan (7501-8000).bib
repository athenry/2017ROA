@inproceedings{ ISI:000327210201137,
Author = {Aljanaideh, Khaled and Coffer, Benjamin J. and Bernstein, Dennis S.},
Book-Group-Author = {{IEEE}},
Title = {{Closed-Loop Identification of Unstable Systems Using Noncausal FIR
   Models}},
Booktitle = {{2013 AMERICAN CONTROL CONFERENCE (ACC)}},
