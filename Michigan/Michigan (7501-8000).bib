Author = {Ersal, Tulga and Brudnak, Mark and Stein, Jeffrey L.},
Book-Group-Author = {{ASME}},
Title = {{AN ITERATIVE LEARNING CONTROL APPROACH TO IMPROVING FIDELITY IN
   INTERNET-DISTRIBUTED HARDWARE-IN-THE-LOOP SIMULATION}},
Booktitle = {{PROCEEDINGS OF THE ASME 5TH ANNUAL DYNAMIC SYSTEMS AND CONTROL DIVISION
