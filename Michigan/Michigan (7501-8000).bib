@article{ ISI:000315431900014,
Author = {Hoepfner, Michael P. and Limsakoune, Vipawee and Chuenmeechao, Varun and
   Maqbool, Tabish and Fogler, H. Scott},
Title = {{A Fundamental Study of Asphaltene Deposition}},
Journal = {{ENERGY \& FUELS}},
Year = {{2013}},
Volume = {{27}},
Number = {{2}},
Pages = {{725-735}},
Month = {{FEB}},
Abstract = {{Asphaltenes are a solubility class of petroleum crude oil that can
   destabilize and deposit in both upstream and downstream processes. In
   this study, asphaltene deposits were generated in metal capillaries by
   heptane addition to crude oils, and it was found that deposition is
   caused by submicrometer asphaltene aggregates. Deposits were generated
   at heptane concentrations above and significantly below the
   instantaneous onset point. Analysis of the results reveals that the
   governing factor controlling the magnitude of asphaltene deposition is
   the concentration of insoluble asphaltenes present in a crude
   oil-precipitant mixture and the instantaneous onset point is irrelevant
   to the deposition process. Electron microscopy images of the deposits
   represent the first images and confirmation of arterial growth in
   laboratory generated asphaltene deposits. The axial deposit profile was
   found to be highly nonuniform. In addition, deposits formed shortly
   after when oil and heptane mix, revealing that the destabilization of
   asphaltenes occurs virtually immediately after a precipitant is added.
   The results were reproduced with a second crude oil, indicating that
   asphaltene deposition behavior is broadly applicable.}},
