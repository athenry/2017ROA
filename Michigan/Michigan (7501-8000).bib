@inproceedings{ ISI:000327210201086,
Author = {McDonough, Kevin and Kolmanovsky, Ilya and Filev, Dimitar and Yanakiev,
   Diana and Szwabowski, Steve and Michelini, John},
Book-Group-Author = {{IEEE}},
Title = {{Stochastic Dynamic Programming Control Policies for Fuel Efficient
   Vehicle Following}},
Booktitle = {{2013 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
