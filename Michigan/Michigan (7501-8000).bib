@article{ ISI:000315665100012,
Author = {Hodapp, David P. and Kim, Dae-Hyun and Troesch, Armin W.},
Title = {{On the finite approximation of a Gaussian process and its effect on
   extreme value theory}},
Journal = {{OCEAN ENGINEERING}},
Year = {{2013}},
Volume = {{58}},
Pages = {{135-143}},
Month = {{JAN 15}},
Abstract = {{The finite approximation of a Gaussian process is incorporated
   throughout a variety of engineering applications. Central to the marine
   industry is the reconstruction of a stochastic seaway using a finite
   number of Fourier components. When coupled with Monte-Carlo methods, a
   pragmatic approach is often taken to determine an appropriate number of
   Fourier components. In this paper, an analytical solution to the
   probability distribution of a finite summation of sinusoids with
   identical amplitudes is developed and then extended to the general case.
   The resulting probability distribution is then evaluated through the
   context of extreme value theory and compared to that of a true Gaussian
   process. As might be expected, the finite approximation underpredicts
   the theoretical value by an often significant margin. The relative
   magnitude of this deviation, however, provides a theoretical basis for
   readily determining a sufficiency condition for the number of Fourier
   components to be used in any specific application. (C) 2012 Elsevier
   Ltd. All rights reserved.}},
