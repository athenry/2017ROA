@inproceedings{ ISI:000320654000181,
Author = {Xue, Mengran and Hiskens, Ian A.},
Book-Group-Author = {{IEEE}},
Title = {{An Enhanced MPC-Based Strategy for Non-Disruptive Load Shedding}},
Booktitle = {{2012 50TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2012}},
