Author = {Wang, Ya and Inman, Daniel J.},
Book-Group-Author = {{ASME}},
Title = {{EXPERIMENTAL VALIDATION OF SIMULTANEOUS GUST ALLEVIATION AND ENERGY
   HARVESTING FOR MULTIFUNCTIONAL COMPOSITE WING SPARS}},
Booktitle = {{PROCEEDINGS OF THE ASME CONFERENCE ON SMART MATERIALS, ADAPTIVE
   STRUCTURES AND INTELLIGENT SYSTEMS, VOL 1}},
