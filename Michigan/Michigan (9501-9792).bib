@inproceedings{ ISI:000306560400048,
Author = {Rais-Zadeh, Mina},
Editor = {{George, T and Islam, MS and Dutta, A}},
Title = {{Gallium nitride micromechanical resonators for IR detection}},
Booktitle = {{MICRO- AND NANOTECHNOLOGY SENSORS, SYSTEMS, AND APPLICATIONS IV}},
