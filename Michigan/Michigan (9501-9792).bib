@article{ ISI:000298327500006,
Author = {Kim, Myung-Chul and Lee, Dong-Jin and Markov, Igor L.},
Title = {{SimPL: An Effective Placement Algorithm}},
Journal = {{IEEE TRANSACTIONS ON COMPUTER-AIDED DESIGN OF INTEGRATED CIRCUITS AND
   SYSTEMS}},
Year = {{2012}},
Volume = {{31}},
Number = {{1}},
Pages = {{50-60}},
Month = {{JAN}},
Abstract = {{We propose a self-contained, flat, quadratic global placer that is
   simpler than existing placers and easier to integrate into
   timing-closure flows. It maintains lower-bound and upper-bound
   placements that converge to a final solution. The upper-bound placement
   is produced by a novel look-ahead legalization algorithm. Our placer
   SimPL outperforms mPL6, FastPlace3, NTUPlace3, APlace2, and Capo
   simultaneously in runtime and solution quality, running 7.10 times
   faster than mPL6 (when using a single thread) and reducing wirelength by
   3\% on the ISPD 2005 benchmark suite. More significant improvements are
   achieved on larger benchmarks. The new algorithm is amenable to
   parallelism, and we report empirical studies with SSE2 instructions and
   up to eight parallel threads.}},
