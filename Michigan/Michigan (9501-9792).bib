@inproceedings{ ISI:000304768000154,
Author = {Tripathi, Anurag and Marentis, Theodore Cosmo and Chronis, Nikos},
Editor = {{Pelc, NJ and Nishikawa, RM and Whiting, BR}},
Title = {{Microfabricated Instrument Tag for the Radiographic Detection of
