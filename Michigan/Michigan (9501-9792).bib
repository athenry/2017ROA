@article{ ISI:000298858000011,
Author = {Larson, Kindra A. and Luo, Jiajia and Yousuf, Aisha and Ashton-Miller,
   James A. and Delancey, John O. L.},
Title = {{Measurement of the 3D geometry of the fascial arches in women with a
   unilateral levator defect and ``architectural distortion{''}}},
Journal = {{INTERNATIONAL UROGYNECOLOGY JOURNAL}},
Year = {{2012}},
Volume = {{23}},
Number = {{1}},
Pages = {{57-63}},
Month = {{JAN}},
Note = {{30th Annual Scientific Meeting for the American-Urogynecologic-Society,
   Hollywood, FL, SEP 24-26, 2009}},
Organization = {{Amer Urogynecol Soc}},
Abstract = {{The arcus tendineus fascia pelvis (ATFP) and arcus tendineus levator ani
   (ATLA) are elements of anterior vaginal support. This study describes
   their geometry in women with unilateral levator ani muscle defects and
   associated ``architectural distortion.{''}
   Fourteen subjects with unilateral defects underwent MRI. 3D models of
   the arcus were generated. The locations of these relative to an ilial
   reference line were compared between the unaffected and affected sides.
   Pronounced changes occurred on the defect sides' ventral region. The
   furthest point of the ATLA lays up to a mean of 10 mm (p = 0.01) more
   inferior and 6.5 mm (p = 0.02) more medial than that on the intact side.
   Similarly, the ATFP lays 6 mm (p = 0.01) more inferior than on the
   unaffected side.
   The ventral arcus anatomy is significantly altered in the presence of
   levator defects and architectural distortion. Alterations of these key
   fixation points will change the supportive force direction along the
   lateral anterior vaginal wall, increasing the risk for anterior vaginal
   wall prolapse.}},
