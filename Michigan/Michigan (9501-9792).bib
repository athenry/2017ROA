@article{ ISI:000301084800045,
Author = {Lee, J. Y. and Cho, D. K. and Lee, M. S. and Kook, D. H. and Choi, H. J.
   and Choi, J. W. and Wang, L. M.},
Title = {{Efficiency analyses of the CANDU spent fuel repository using modified
   disposal canisters for a deep geological disposal system design}},
Journal = {{NUCLEAR ENGINEERING AND DESIGN}},
Year = {{2012}},
Volume = {{242}},
Pages = {{433-444}},
Month = {{JAN}},
Abstract = {{Deep geological disposal concept is considered to be the most preferable
   for isolating high-level radioactive waste (HLW), including nuclear
   spent fuels, from the biosphere in a safe manner. The purpose of deep
   geological disposal of HLW is to isolate radioactive waste and to
   inhibit its release of for a long time, so that its toxicity does not
   affect the human beings and the biosphere. One of the most important
   requirements of HLW repository design for a deep geological disposal
   system is to keep the buffer temperature below 100 degrees C in order to
   maintain the integrity of the engineered barrier system. In this study,
   a reference disposal concept for spent nuclear fuels in Korea has been
   reviewed, and based on this concept, efficient alternative concepts that
   consider modified CANDU spent fuels disposal canister, were developed.
   To meet the thermal requirement of the disposal system, the spacing of
   the disposal tunnels and that of the disposal pits for each alternative
   concept, were drawn following heat transfer analyses. From the result of
   the thermal analyses, the disposal efficiency of the alternative
   concepts was reviewed and the most effective concept suggested. The
   results of these analyses can be used for a deep geological repository
   design and detailed analyses, based on exact site characteristics data,
   will reduce the uncertainty of the results. (C) 2011 Elsevier B.V. All
   rights reserved.}},
