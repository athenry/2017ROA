@inproceedings{ ISI:000307676800046,
Author = {Wille, K. and El-Tawil, S. and Naaman, A. E.},
Editor = {{ParraMontesinos, GJ and Reinhardt, HW and Naaman, AE}},
Title = {{Strain Rate Dependent Tensile Behavior of Ultra-High Performance Fiber
   Reinforced Concrete}},
Booktitle = {{HIGH PERFORMANCE FIBER REINFORCED CEMENT COMPOSITES 6}},
Series = {{RILEM Bookseries}},
Year = {{2012}},
Volume = {{2}},
Pages = {{381+}},
