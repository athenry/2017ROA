@article{ ISI:000298362600018,
Author = {Davidson, Paul and Waas, Anthony M. and Yerramalli, Chandra S.},
Title = {{Experimental determination of validated, critical interfacial modes I
   and II energy release rates in a composite sandwich panel}},
Journal = {{COMPOSITE STRUCTURES}},
Year = {{2012}},
Volume = {{94}},
Number = {{2}},
Pages = {{477-483}},
Month = {{JAN}},
Abstract = {{A validated experimental approach to obtaining critical mode I and mode
   II energy release rates for interfacial failure in a sandwich composite
   panel is outlined in this paper. By modifying the geometry of the
   sandwich structure to align the face sheet-core interface to coincide
   with the neutral axis, it is possible to obtain critical mode I and mode
   II energy release rates by conducting Double Cantilever Beam (DCB) and
   End Notch Flexure (ENF) tests, respectively. The values so obtained were
   used to predict the crack growth histories of modified DCB and ENF
   tests, and a Single Leg Bend (SLB) test, using a discrete cohesive zone
   method (DCZM). In addition, the influence of material and geometry were
   also analyzed. (C) 2011 Elsevier Ltd. All rights reserved.}},
