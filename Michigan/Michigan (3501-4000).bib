@inproceedings{ ISI:000380485800014,
Author = {He, Xiaozheng and Liu, Henry X. and Peeta, Srinivas},
Editor = {{Kuwahara, M and Kita, H and Asakura, Y}},
Title = {{A generalized flow splitting model for day-to-day traffic assignment}},
Booktitle = {{PAPERS SELECTED FOR POSTER SESSIONS AT THE 21ST INTERNATIONAL SYMPOSIUM
   ON TRANSPORTATION AND TRAFFIC THEORY}},
