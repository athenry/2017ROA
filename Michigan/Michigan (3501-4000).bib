@inproceedings{ ISI:000380614500240,
Author = {Zhang, Yao and Michalowski, Radoslaw L.},
Editor = {{Oka, F and Murakami, A and Uzuoka, R and Kimoto, S}},
Title = {{Modeling frost heaving and thaw settlement in frost-susceptible soils}},
Booktitle = {{Computer Methods and Recent Advances in Geomechanics}},
Year = {{2015}},
Pages = {{1487-1492}},
Note = {{Computer Methods and Recent Advances in Geomechanics, KYOTO, JAPAN, SEP
   22-25, 2014}},
Abstract = {{Frost susceptible soils in both seasonal freezing and permafrost regions
   are vulnerable to frost action and may experience large displacements
   upon extreme temperature changes. Frost heave and strengthening occurs
   as the soil freezes, whereas settlement and thaw weakening is expected
   during the melting process. As a result, significant damage to
   infrastructure built in these regions can be caused by large thermally
   induced deformation of soils. Therefore, accounting for soil behavior
   upon freezing and thawing is of great importance. In this paper, a
   Thermal-Hydro-Mechanical (THM) framework is used, in which a temperature
   dependent elastic-plastic constitutive relationship and porosity rate
   function (for ice lens formation) are included. The model is implemented
   into an FE system and then calibrated. A chilled gas pipeline buried in
   a seasonally freezing soil is simulated. The simulation results are
   reasonable and the model appears to be a good tool in dealing with
   problems associated with frost action.}},
Publisher = {{CRC PRESS-TAYLOR \& FRANCIS GROUP}},
Address = {{6000 BROKEN SOUND PARKWAY NW, STE 300, BOCA RATON, FL 33487-2742 USA}},
Type = {{Proceedings Paper}},
Language = {{English}},
Affiliation = {{Zhang, Y (Reprint Author), Univ Michigan, Dept Civil \& Environm Engn, Ann Arbor, MI 48109 USA.
