@inproceedings{ ISI:000381622900025,
Author = {Ledva, Gregory S. and Balzano, Laura and Mathieu, Johanna L.},
Book-Group-Author = {{IEEE}},
Title = {{Inferring the Behavior of Distributed Energy Resources with Online
   Learning}},
