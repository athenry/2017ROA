Title = {{Robust H-infinity Control for a Class of Networked Uncertain Systems
   with Multiple Channels Subject to Markovian Switching}},
Booktitle = {{2015 54TH IEEE CONFERENCE ON DECISION AND CONTROL (CDC)}},
Year = {{2015}},
