@article{ ISI:000348054700073,
Author = {Shao, Lei and Pipe, Kevin P.},
Title = {{Amplification and directional emission of surface acoustic waves by a
   two-dimensional electron gas}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{106}},
Number = {{2}},
Month = {{JAN 12}},
Abstract = {{Amplification of surface acoustic waves (SAWs) by electron drift in a
   two-dimensional electron gas (2DEG) is analyzed analytically and
   confirmed experimentally. Calculations suggest that peak power gain per
   SAW radian occurs at a more practical carrier density for a 2DEG than
   for a bulk material. It is also shown that SAW emission with tunable
   directionality can be achieved by modulating a 2DEG's carrier density
   (to effect SAW generation) in the presence of an applied DC field that
   amplifies SAWs propagating in a particular direction while attenuating
   those propagating in the opposite direction. (C) 2015 AIP Publishing
