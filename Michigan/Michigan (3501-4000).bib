@inproceedings{ ISI:000380430700085,
Author = {Karatas, Aslihan and Menassa, Carol C. and Stoiko, Allisandra},
Editor = {{Chong, WO and Chang, J and Parrish, K and Berardi, U}},
Title = {{A Framework for Delivering Targeted Occupancy Interventions to Reduce
   Energy Usage in Buildings}},
