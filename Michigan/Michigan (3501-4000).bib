@inproceedings{ ISI:000377904400164,
Author = {Lemieux, Joe and Ma, Yuan},
Book-Group-Author = {{IEEE}},
Title = {{Vehicle Speed Prediction using Deep Learning}},
Booktitle = {{2015 IEEE VEHICLE POWER AND PROPULSION CONFERENCE (VPPC)}},
Series = {{IEEE Vehicle Power and Propulsion Conference}},
Year = {{2015}},
Note = {{12th IEEE Vehicle Power and Propulsion Conference (VPPC), Montreal,
   CANADA, OCT 19-22, 2015}},
Organization = {{IEEE; TM4; OPAL RT Technol}},
Abstract = {{Global optimization of the energy consumption of dual power source
   vehicles such as hybrid electric vehicles, plug-in hybrid electric
   vehicles, and plug in fuel cell electric vehicles requires knowledge of
   the complete route characteristics at the beginning of the trip. One of
   the main characteristics is the vehicle speed profile across the route.
   The profile will translate directly into energy requirements for a given
   vehicle. However, the vehicle speed that a given driver chooses will
   vary from driver to driver and from time to time, and may be slower,
   equal to, or faster than the average traffic flow. If the specific
   driver speed profile can be predicted, the energy usage can be optimized
   across the route chosen. The purpose of this paper is to research the
   application of Deep Learning techniques to this problem to identify at
   the beginning of a drive cycle the driver specific vehicle speed profile
   for an individual driver repeated drive cycle, which can be used in an
   optimization algorithm to minimize the amount of fossil fuel energy used
   during the trip.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
Language = {{English}},
Affiliation = {{Lemieux, J (Reprint Author), Univ Michigan, Dept Elect \& Comp Engn, Dearborn, MI 48128 USA.
