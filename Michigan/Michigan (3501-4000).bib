@inproceedings{ ISI:000368452404007,
Author = {Liu, Yang and Liu, Mingyan},
Book-Group-Author = {{IEEE}},
Title = {{DETECTING HIDDEN CLIQUES FROM NOISY OBSERVATIONS}},
Booktitle = {{2015 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH, AND SIGNAL
   PROCESSING (ICASSP)}},
