@article{ ISI:000350300900025,
Author = {Gulsoy, Gokce and Was, Gary S.},
Title = {{Enhanced Oxygen Diffusion Within the Internal Oxidation Zone of Alloy
   617 in Controlled Impurity Helium Environments from 1023 K to 1123 K
   (750 A degrees C to 850 A degrees C)}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS A-PHYSICAL METALLURGY AND
   MATERIALS SCIENCE}},
Year = {{2015}},
Volume = {{46A}},
Number = {{4}},
Pages = {{1628-1638}},
Month = {{APR}},
Note = {{Symposium on High Entropy Alloy, Univ Tennessee, San Antonio, TX, MAR
   15-19, 2015}},
Organization = {{Alcoa Tech Ctr; Natl Energy Technol Lab; US Army Res Off; TMS Struct Mat
   Div; TMS ASM}},
Abstract = {{Alloy 617 was exposed to He-CO-CO2 environments with of either 9 or 1320
   at temperatures from 1023 K to 1123 K (750 A degrees C to 850 A degrees
   C) to determine the oxygen diffusion coefficients within the internal
   oxidation zone of the alloy. The oxygen diffusion coefficients
   determined based on both intergranular and transgranular oxidation rates
   were several orders of magnitude greater than those reported in pure
   nickel and in nickel-based binary alloys, indicating that the rapid
   internal aluminum oxidation of Alloy 617 was primarily due to enhanced
   oxygen diffusion along the incoherent Al2O3-alloy interfaces. The range
   of activation energy values determined for oxygen diffusion associated
   with the intergranular aluminum oxidation was from 149.6 to 154.7
   kJ/mol, and that associated with the transgranular aluminum oxidation
   was from 244.7 to 283.5 kJ/mol. (C) The Minerals, Metals \& Materials
   Society and ASM International 2015}},
