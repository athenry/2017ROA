@inproceedings{ ISI:000380529100166,
Author = {Zheng, Junxing and Hryciw, Roman D. and Ohm, Hyon-Sohk},
Editor = {{Soga, K and Kumar, K and Biscontin, G and Kuo, M}},
Title = {{Three-Dimensional Translucent Segregation Table (3D-TST) test for soil
   particle size and shape distribution}},
Booktitle = {{Geomechanics from Micro to Macro, Vols I and II}},
Year = {{2015}},
Pages = {{1037-1042}},
Note = {{3rd International Symposium on Geomechanics from Micro to Macro, Univ
   Cambridge, Cambridge, ENGLAND, SEP 01-03, 2014}},
Organization = {{Int Soc Soil Mech \& Geotechn Engn, Geo Mech Micro Macro Tech Comm 105}},
Abstract = {{The TST is a 0.9mx0.9m backlit tilting surface upon which coarse sand
   and gravel particles are spread out prior to image capture by a
   ceiling-mounted camera. The function of the TST is to somewhat segregate
   the particles by size so that small particles cannot be hidden from
   camera view behind larger ones. A method called watershed segmentation
   digitally detaches the particles thus eliminating the laborious task of
   physically separating the particles. Early TST test results were
   promising but did not perfectly match sieve results because the camera
   captured only two-dimensional particle projections. The third (vertical)
   dimension was only estimated. The problem has been overcome through
   stereophotography. This paper details the new three-dimensional TST
   (3D-TST) test and compares results to two-dimensional TST tests and to
   sieving. Distributions of particle shapes for large statistically valid
   numbers of particles are also presented.}},
