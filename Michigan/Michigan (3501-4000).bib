@inproceedings{ ISI:000380429500013,
Author = {Hirose, Mitsuhito and Xiao, Yong and Zuo, Zhiyuan and Kamat, Vineet R.
   and Zekkos, Dimitrios and Lynch, Jerome},
Book-Group-Author = {{IEEE}},
Title = {{Implementation of UAV Localization Methods for a Mobile Post-Earthquake
   Monitoring System}},
Booktitle = {{2015 IEEE WORKSHOP ON ENVIRONMENTAL, ENERGY AND STRUCTURAL MONITORING
