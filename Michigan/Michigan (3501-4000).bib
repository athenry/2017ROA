@inproceedings{ ISI:000381554507071,
Author = {Niendorf, Moritz and Kabamba, Pierre T. and Girard, Anouck R.},
Book-Group-Author = {{IEEE}},
Title = {{Stability analysis of multi-objective planning problems for unmanned
   aircraft}},
Booktitle = {{2015 54TH IEEE CONFERENCE ON DECISION AND CONTROL (CDC)}},
Year = {{2015}},
Pages = {{7238-7243}},
Note = {{54th IEEE Conference on Decision and Control (CDC), Osaka, JAPAN, DEC
   15-18, 2015}},
Organization = {{Kozo Keikaku Engn; MathWorks; Springer; CYBERNET Syst; Mitsubishi Elect;
   Soc Ind \& Appl Math; Altair; Int Journal Automat \& Comp; IEEE CAA
   Journal Automatica Sinica; Cogent Engn; Now; IHI; IEEE}},
Abstract = {{By performing sensitivity analysis on multi-objective mission planning
   problems for unmanned aircraft, this paper provides insight into
   properties of optimal solutions and their robustness with respect to
   trade-offs between objectives and also perturbations in the objective
   functions. We consider mission planning for unmanned aircraft in hostile
   environments, where a trade-off between short mission execution time and
   low risk must be made. This paper develops a method to compute stability
   regions for a set of schedules for a prototypical mission for unmanned
   aircraft, the weighted multi-objective symmetric traveling salesman
   problem. A stability region associated with an optimal solution is the
   set of all perturbations to the input parameters for which that solution
   remains optimal. In particular, this paper studies perturbations to the
   weights and costs in the objective functions. This work has multiple
   potential benefits such as providing insight into how robust a solution
   is with respect to modeling uncertainties, how limited intelligence
   resources should be allocated, and the influence of each objective on
   the solution. A numerical example is given to demonstrate the methods
   developed in this paper.}},
