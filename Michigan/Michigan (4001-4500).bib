@inproceedings{ ISI:000370259200137,
Author = {Ansari, Ahmad and Bernstein, Dennis S.},
Book-Group-Author = {{IEEE}},
Title = {{Adaptive Control of an Aircraft with Uncertain Nonminimum-Phase Dynamics}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
