@article{ ISI:000358554500004,
Author = {Min, Junying and Li, Yongqiang and Carlson, Blair E. and Hu, S. Jack and
   Li, Jingjing and Lin, Jianping},
Title = {{A new single-sided blind riveting method for joining dissimilar
   materials}},
Journal = {{CIRP ANNALS-MANUFACTURING TECHNOLOGY}},
Year = {{2015}},
Volume = {{64}},
Number = {{1}},
Pages = {{13-16}},
Abstract = {{A new single-sided, one-step, blind riveting method is developed for
   joining dissimilar materials. The rivet rotates at a high speed when it
   cuts and penetrates the workpieces. When fabricating lap joints of
   AA6111 and AA6022, it was observed that both the torque and force
   increased as feed rate increased during the rivet penetration. Joints
   produced by the new method exhibited approximately a 20\% higher maximum
   tensile load compared to joints made by conventional blind riveting and
   a 11\% higher maximum tensile load compared to joints made with friction
   stir blind riveting (FSBR). The new riveting process is compared to the
   FSBR due to their process similarity and the mechanism for the success
   of the new method is discussed. (C) 2015 CIRP.}},
