@inproceedings{ ISI:000370076903062,
Author = {Stein, Aaron and Hofmann, Heath},
Book-Group-Author = {{IEEE}},
Title = {{Resonant Inverter Design for Stand-Alone Dynamic Active Piezoelectric
   Energy Harvesting}},
Booktitle = {{2015 THIRTIETH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC 2015)}},
Series = {{Annual IEEE Applied Power Electronics Conference and Exposition (APEC)}},
Year = {{2015}},
