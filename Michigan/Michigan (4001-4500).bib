Author = {Dragnea, Horatiu C. and Boyd, Iain D. and Lee, Brian C. and Yalin, Azer
   P.},
Title = {{Characterization of Eroded Boron Atoms in the Plume of a Hall Thruster}},
Journal = {{IEEE TRANSACTIONS ON PLASMA SCIENCE}},
