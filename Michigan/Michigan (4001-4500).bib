@inproceedings{ ISI:000370259203016,
Author = {Ma, Zhongjing and Zou, Suli and Liu, Xiangdong and Hiskens, Ian},
Book-Group-Author = {{IEEE}},
Title = {{Efficient Coordination of Electric Vehicle Charging using a Progressive
   Second Price Auction}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2015}},
Pages = {{2999-3006}},
