@inproceedings{ ISI:000370259205114,
Author = {Zidek, Robert A. E. and Kolmanovsky, Ilya V.},
Book-Group-Author = {{IEEE}},
Title = {{Approximate Optimal Control of Nonlinear Systems with Quadratic
   Performance Criteria}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2015}},
Pages = {{5587-5592}},
Note = {{American Control Conference, Chicago, IL, JUL 01-03, 2015}},
