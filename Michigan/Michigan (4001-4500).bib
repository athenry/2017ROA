@inproceedings{ ISI:000370259200065,
Author = {Yin, Xiang and Lafortune, Stephane},
Book-Group-Author = {{IEEE}},
Title = {{On the Relationship between Codiagnosability and Coobservability under
   Dynamic Observations}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2015}},
Pages = {{390-395}},
Note = {{American Control Conference, Chicago, IL, JUL 01-03, 2015}},
