@inproceedings{ ISI:000359433200024,
Author = {Gentry, S. P. and Thornton, K.},
Book-Group-Author = {{IOP}},
Title = {{Simulating recrystallization in titanium using the phase field method}},
Booktitle = {{36TH RISO INTERNATIONAL SYMPOSIUM ON MATERIALS SCIENCE}},
Series = {{IOP Conference Series-Materials Science and Engineering}},
Year = {{2015}},
Volume = {{89}},
Note = {{36th Riso International Symposium on Materials Science, Riso, DENMARK,
   SEP 07-11, 2015}},
Organization = {{Tech Univ Denmark, Dept Wind Energy; Sect Mat Sci \& Adv Characterizat}},
Abstract = {{Integrated computational materials engineering (ICME) links
   physics-based models to predict performance of materials based on their
   processing history. The recrystallization phase field model is developed
   and parameterized for commercially pure titanium. Stored energy and
   nucleation of dislocation-free grains are added into a phase field
   grain-growth model. A two-dimensional simulation of recrystallization in
   titanium at 800 degrees C was performed; the recrystallized volume
   fraction was measured from the simulated microstructures. Fitting the
   recrystallized volume fraction to the Avrami equation gives the time
   exponent n as 1.8 and the annealing time to reach 50\% recrystallization
   (t(0.5)) as 71 s. As expected, the microstructure evolves faster when
   driven by stored energy than when driven by grain boundary energy.}},
