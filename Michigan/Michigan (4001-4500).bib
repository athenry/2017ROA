@article{ ISI:000347105700005,
Author = {Levy, Rachel and Hill, David B. and Forest, M. Gregory and Grotberg,
   James B.},
Title = {{Pulmonary Fluid Flow Challenges for Experimental and Mathematical
   Modeling}},
Journal = {{INTEGRATIVE AND COMPARATIVE BIOLOGY}},
Year = {{2014}},
Volume = {{54}},
Number = {{6}},
Pages = {{985-1000}},
Month = {{DEC}},
Note = {{Symposium on Shaking, Dripping, and Drinking - Surface Tension Phenomena
   in Organismal Biology at the Annual Meeting of the
   Society-for-Integrative-and-Comparative-Biology, Austin, TX, JAN 03-07,
   2014}},
Organization = {{Soc Integrat \& Comparat Biol}},
Abstract = {{Modeling the flow of fluid in the lungs, even under baseline healthy
   conditions, presents many challenges. The complex rheology of the
   fluids, interaction between fluids and structures, and complicated
   multi-scale geometry all add to the complexity of the problem. We
   provide a brief overview of approaches used to model three aspects of
   pulmonary fluid and flow: the surfactant layer in the deep branches of
   the lung, the mucus layer in the upper airway branches, and closure/
   reopening of the airway. We discuss models of each aspect, the potential
   to capture biological and therapeutic information, and open questions
   worthy of further investigation. We hope to promote multi-disciplinary
   collaboration by providing insights into mathematical descriptions of
   fluid-mechanics in the lung and the kinds of predictions these models
   can make.}},
