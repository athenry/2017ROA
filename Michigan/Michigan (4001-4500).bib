@inproceedings{ ISI:000392737000105,
Author = {Wei, Dan and Tardieu, Tasha and McCormick, Jason},
Editor = {{Ingraffea, N and Libby, M}},
Title = {{Cyclic Performance of Hollow Structural Sections Filled with High
   Damping, Light Weight Materials}},
Booktitle = {{Structures Congress 2015}},
Year = {{2015}},
Pages = {{1225-1236}},
Note = {{Structures Congress, Portland, OR, APR 23-25, 2015}},
Organization = {{Amer Soc Civil Engineers, Struct Engn Inst}},
Abstract = {{Seismic steel moment frames rely on the formation of plastic hinges at
   the beam ends and at the column bases to dissipate seismic input energy.
   Local buckling and accumulation of damage associated with this inelastic
   behavior may lead to inconsistent performance, residual deformation, and
   significant repair needs. To take advantage of the properties of
   light-weight, non-traditional materials that offer high damping, the
   integration of these materials into the hollow portion of hollow
   structural section members is considered to enhance their performance
   under large cyclic loads associated with a seismic event. The use of
   these materials as fill in hollow structural sections is explored
   through beam bending tests under cyclic load. The bending tests allow
   the cyclic behavior to be assessed with respect to fill material type
   (polymer foam and rubber), fill length, member width-thickness ratio,
   and member depth-thickness ratio. The results are used to evaluate the
   ability of the fill materials to control the behavior of structures
   during a seismic event and will benefit design professionals, code
   writers, and researchers in developing new ways to better integrate
   non-traditional materials into structural systems.}},
