@inproceedings{ ISI:000370259203018,
Author = {Zhang, Yiling and Shen, Siqian and Mathieu, Johanna L.},
Book-Group-Author = {{IEEE}},
Title = {{Data-driven Optimization Approaches for Optimal Power Flow with
   Uncertain Reserves from Load Control}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2015}},
Pages = {{3013-3018}},
Note = {{American Control Conference, Chicago, IL, JUL 01-03, 2015}},
