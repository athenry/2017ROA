@inproceedings{ ISI:000355726100071,
Author = {Suzuki, Akiko and Kurata, Masahiro and Xiaohua, Li and Minegishi, Kaede
   and Tang Zhenyun and Burton, Andrew},
Editor = {{Lynch, JP and Wang, KW and Sohn, H}},
Title = {{Quantification of seismic damage in steel beam-column connection using
   PVDF strain sensors and model-updating technique}},
Booktitle = {{SENSORS AND SMART STRUCTURES TECHNOLOGIES FOR CIVIL, MECHANICAL, AND
   AEROSPACE SYSTEMS 2015}},
Series = {{Proceedings of SPIE}},
