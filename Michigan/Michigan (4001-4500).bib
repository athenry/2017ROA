@article{ ISI:000346880300005,
Author = {Touran, Nicholas W. and Lee, John C.},
Title = {{A Modal Expansion Equilibrium Cycle Perturbation Method for Optimizing
   Fast Reactors}},
Journal = {{NUCLEAR SCIENCE AND ENGINEERING}},
Year = {{2015}},
Volume = {{179}},
Number = {{1}},
Pages = {{85-103}},
Month = {{JAN}},
Abstract = {{We developed a simulation tool that accelerates the evaluation of design
   changes on the equilibrium cycle of fast-spectrum nuclear reactors.
   Within the tool, an implicit equilibrium cycle search is accelerated by
   a modal expansion perturbation method that expands arbitrary flux
   perturbations on a large basis of lambda-eigenmode harmonics. The
   harmonics are computed only at the reference state using Krylov subspace
   iterative methods, and substantial perturbations from this state are
   shown to be well approximated by computationally efficient algebraic
   expressions. The modal expansion method is coupled to the equilibrium
   method to produce the later-in-time response of each design
   perturbation, resulting in an explicit perturbation-accelerated
   equilibrium cycle method.
   Because the method determines the perturbed flux explicitly, a wide
   variety of core performance metrics may be tracked within optimization
   frameworks, including the performance of thermal hydraulics, fuel,
   economics, core mechanical, and transients. This capability strongly
   differentiates the method from traditional generalized perturbation
   theory approaches. The motivating end-use of the method is to evaluate
   objective functions in multidisciplinary optimization of advanced
   reactor designs, though many other applications are envisioned.}},
