@inproceedings{ ISI:000370259201081,
Author = {Mehra, Aakar and Ma, Wen-Loong and Berg, Forrest and Tabuada, Paulo and
   Grizzle, Jessy W. and Ames, Aaron D.},
Book-Group-Author = {{IEEE}},
Title = {{Adaptive Cruise Control: Experimental Validation of Advanced Controllers
   on Scale-Model Cars}},
Booktitle = {{2015 AMERICAN CONTROL CONFERENCE (ACC)}},
Series = {{Proceedings of the American Control Conference}},
Year = {{2015}},
Pages = {{1411-1418}},
Note = {{American Control Conference, Chicago, IL, JUL 01-03, 2015}},
Organization = {{Amer Automat Control Council; IFAC; Adaptics Inc; Altair; dSPACE; Eaton
