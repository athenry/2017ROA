@inproceedings{ ISI:000370259204106,
Author = {Hsu, Shao-Chen and Xu, Xiangru and Ames, Aaron D.},
Book-Group-Author = {{IEEE}},
Title = {{Control Barrier Function based Quadratic Programs with Application to
   Bipedal Robotic Walking}},
