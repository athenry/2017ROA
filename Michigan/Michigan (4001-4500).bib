Author = {van der Meel, Roy and Fens, Marcel H. A. M. and Vader, Pieter and van
   Solinge, Wouter W. and Eniola-Adefeso, Omolola and Schiffelers, Raymond
   M.},
Title = {{Extracellular vesicles as drug delivery systems: Lessons from the
   liposome field}},
Journal = {{JOURNAL OF CONTROLLED RELEASE}},
