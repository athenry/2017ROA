Title = {{Traditional soil particle sphericity, roundness and surface roughness by
   computational geometry}},
Journal = {{GEOTECHNIQUE}},
Year = {{2015}},
Volume = {{65}},
Number = {{6}},
Pages = {{494-506}},
Abstract = {{Definitions of soil particle sphericity, roundness and roughness have
