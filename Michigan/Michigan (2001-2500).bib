
@inproceedings{ ISI:000389514300152,
Author = {Zhang, Bowen and Li, Wei and Chen, Tao and Su, Wencong},
Book-Group-Author = {{IEEE}},
Title = {{A Simulation Platform for Energy-Efficient Dynamic Commuter Transit
   using Electric Vehicles}},
Booktitle = {{2016 IEEE TRANSPORTATION ELECTRIFICATION CONFERENCE AND EXPO,
   ASIA-PACIFIC (ITEC ASIA-PACIFIC)}},
Year = {{2016}},
Pages = {{812-817}},
