@inproceedings{ ISI:000382153000011,
Author = {Wang, Muzhi and Lin, Feng and Rais-Zadeh, Mina},
Book-Group-Author = {{IEEE}},
Title = {{An X-Band Reconfigurable Bandpass Filter Using Phase Change RF Switches}},
Booktitle = {{2016 IEEE 16TH TOPICAL MEETING ON SILICON MONOLITHIC INTEGRATED CIRCUITS
   IN RF SYSTEMS (SIRF)}},
Year = {{2016}},
Pages = {{38-41}},
Note = {{16th IEEE Topical Meeting on Silicon Monolithic Integrated Circuits in
   RF Systems (SiRF), Austin, TX, JAN 24-27, 2016}},
Organization = {{IEEE; IEEE MTT S; IEEE APS; IEEE EMB}},
Abstract = {{In this paper, we report on a reconfigurable bandpass filter for X-band
   applications. The filter is composed of coupled lambda/2 microstrip line
   resonators. Center frequency tuning of each resonator is achieved using
   an RF switch based on a phase change chalcogenide compound, germanium
   telluride, which switches in and out a loading capacitor. A combination
   of electric and magnetic coupling between the resonators realizes a near
   constant absolute-bandwidth as the filter is tuned. The center frequency
   of the filter is switched between 7.45 and 8.07 GHz, with a 3-dB
   bandwidth of similar to 500 MHz, insertion loss of less than 3.2 dB, and
   return loss of better than 18 dB. The measured third-order inter
   modulation intercept point (IIP3) and 1-dB power compression point
   (P-1dB) are better than 30 dBm at frequency offset of 1 kHz and 25 dBm,
   respectively. Measured and simulated results are in good agreement. To
   our best knowledge, this is the first time implementation of a tunable
   filter using germanium telluride based phase change switches.}},
