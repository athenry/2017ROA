@article{ ISI:000379577800048,
Author = {Cheng, Peifu and Sun, Kai and Hu, Yun Hang},
Title = {{Mechanically-induced reverse phase transformation of MoS2 from stable 2H
   to metastable 1T and its memristive behavior}},
Journal = {{RSC ADVANCES}},
Year = {{2016}},
Volume = {{6}},
Number = {{70}},
Pages = {{65691-65697}},
Abstract = {{The 1T phase of MoS2 is attracting much attention due to its metallic
   property and potential applications for supercapacitors, photothermal
   agents, and memristors. However, because the 1T phase is metastable, it
   can easily be transferred to the stable 2H phase by heating. In this
   work, it is the first time we observe the mechanically-induced reverse
   phase transformation (from stable 2H to metastable 1T) for MoS2 by
   HAADF-STEM images. Furthermore, XPS revealed that the content of the 1T
   phase increased with increasing mechanical ball-milling time.
   Furthermore, the mechanically generated 1T MoS2 exhibited memristive
   behavior.}},
