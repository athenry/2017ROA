@article{ ISI:000379688000013,
Author = {Samad, Nassim A. and Kim, Youngki and Siegel, Jason B. and
   Stefanopoulou, Anna G.},
Title = {{Battery Capacity Fading Estimation Using a Force-Based Incremental
   Capacity Analysis}},
