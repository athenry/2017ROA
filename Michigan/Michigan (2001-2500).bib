@inproceedings{ ISI:000379884200067,
Author = {Nafari, Alireza and Bowland, Christopher C. and Sodano, Henry A.},
Book-Group-Author = {{ASME}},
Title = {{VERTICALLY ALIGNED LEAD TITANATE NANOWIRE ARRAYS FOR HIGH TEMPERATURE
   ENERGY HARVESTING}},
Booktitle = {{ASME CONFERENCE ON SMART MATERIALS, ADAPTIVE STRUCTURES AND INTELLIGENT
   SYSTEMS, 2015, VOL 2}},
Year = {{2016}},
Note = {{ASME Conference on Smart Materials, Adaptive Structures and Intelligent
   Systems, Colorado Springs, CO, SEP 21-23, 2015}},
Organization = {{ASME, Aerospace Div}},
Abstract = {{Over the past few decades, the increasing demand for self powered
   devices has led to an immense amount of research in the field of energy
   harvesting from renewable mechanical energies. Often, most of thee
   abundant energy sources are wasted in the form of structural vibrations,
   acoustic waves or impact energy. Recently, nano-electromechanical sys
   tens (NEMS) consisting of piezoelectric nanowires have shown excellent
   electromechanical coupling coefficients which can efficiently convert
   small amplitude vibrations into useful electrical energy for compact and
   low power wireless electronic devices. Specifically, high aspect ratio
   piezoelectric nanowires have shown to have a better deformability and
   hence produce higher piezoelectric response to low level induced stress.
   However, current materials are not well suited for energy harvesting
   from extreme environments. In this study, ultra-long high aspect ratio
   vertically aligned lead titanate nanowires (45 gm long, AR = 75) are
   synthesized through a two-step hydrothermal reaction in which s odium
   titanate nanowires serve as the precursor. Their application in
   harvesting vibrational energy at temperatures above 300 degrees C is
   demonstrated through the characterization of open circuit voltage and
   power measurements. The results show that the ultra-long vertically
   aligned lead titanate nanowire array energy harvester can produce up to
   22.3 mW m(-2) at room temperature and up to 13 mW m(-2) at 375 degrees
   C. Thus, it is shown that the energy harvester can provide enough energy
   density for many self-powered, high-temperature applications.}},
