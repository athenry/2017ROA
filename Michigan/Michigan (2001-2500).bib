Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
Language = {{English}},
Affiliation = {{Yin, X (Reprint Author), Univ Michigan, Dept Elect Engn \& Comp Sci, Ann Arbor, MI 48109 USA.
   Yin, Xiang; Lafortune, Stephane, Univ Michigan, Dept Elect Engn \& Comp Sci, Ann Arbor, MI 48109 USA.}},
