@article{ ISI:000368443900010,
Author = {Wu, Fan and Stark, Eran and Ku, Pei-Cheng and Wise, Kensall D. and
   Buzsaki, Gyorgy and Yoon, Euisik},
Title = {{Monolithically Integrated mu LEDs on Silicon Neural Probes for
   High-Resolution Optogenetic Studies in Behaving Animals}},
Journal = {{NEURON}},
Year = {{2015}},
Volume = {{88}},
Number = {{6}},
Pages = {{1136-1148}},
Month = {{DEC}},
Abstract = {{We report a scalable method to monolithically integrate microscopic
   light emitting diodes (mu LEDs) and recording sites onto silicon neural
   probes for optogenetic applications in neuroscience. Each mu LED and
   recording site has dimensions similar to a pyramidal neuron soma,
   providing confined emission and electrophysiological recording of action
   potentials and local field activity. We fabricated and implanted the
   four-shank probes, each integrated with 12 mu LEDs and 32 recording
   sites, into the CA1 pyramidal layer of anesthetized and freely moving
   mice. Spikes were robustly induced by 60 nW light power, and fast
   population oscillations were induced at the microwatt range. To
   demonstrate the spatiotemporal precision of parallel stimulation and
   recording, we achieved independent control of distinct cells similar to
   50 mu m apart and of differential somato-dendritic compartments of
   single neurons. The scalability and spatiotemporal resolution of this
   monolithic optogenetic tool provides versatility and precision for
   cellular-level circuit analysis in deep structures of intact, freely
   moving animals.}},
