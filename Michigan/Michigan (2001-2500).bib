@article{ ISI:000366404200010,
Author = {Choi, Yun-Jung and Ingram, Patrick N. and Yang, Kun and Coffman, Lan and
   Iyengar, Mangela and Bai, Shoumei and Thomas, Dafydd G. and Yoon, Euisik
   and Buckanovich, Ronald J.},
Title = {{Identifying an ovarian cancer cell hierarchy regulated by bone
