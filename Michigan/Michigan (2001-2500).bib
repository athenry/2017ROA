@article{ ISI:000366790500031,
Author = {Chen, Min-Hua and Puchala, Brian and Van der Ven, Anton},
Title = {{High-temperature stability of delta'-ZrO}},
Journal = {{CALPHAD-COMPUTER COUPLING OF PHASE DIAGRAMS AND THERMOCHEMISTRY}},
Year = {{2015}},
Volume = {{51}},
Pages = {{292-298}},
Month = {{DEC}},
Abstract = {{We investigate phase stability in the Zr-O system from first principles,
   focusing in particular on the finite temperature stability of the newly
   discovered delta'-ZrO phase. We present a first-principles phase diagram
   of the Zr-O binary that accounts for both configurational and
   vibrational excitations. Phonon calculations within the quasi-harmonic
   approximation predict that delta'-ZrO is stable to at least 1500 K.
   Quasi-harmonic phonon calculations also predict an alpha- to beta-ZrO2
   transition temperature of 1440 K. Stiff Zr-O bonds are responsible for a
   low vibrational entropy in the delta'-ZrO phase compared to competing
   oxides in the Zr-O binary, resulting in a reduction in its stability
   relative to ZrO1/2 and ZrO2 with increasing temperature. The calculated
   thermodynamic properties are relevant to understanding oxidation
   mechanisms of Zr-based alloys. (C) 2015 Elsevier Ltd. All rights
   reserved.}},
