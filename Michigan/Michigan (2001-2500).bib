@incollection{ ISI:000375648700007,
Author = {Luker, Kathryn E. and Luker, Gary D.},
Editor = {{Handel, TM}},
Title = {{Dual-Color Luciferase Complementation for Chemokine Receptor Signaling}},
Booktitle = {{CHEMOKINES}},
Series = {{Methods in Enzymology}},
Year = {{2016}},
Volume = {{570}},
Pages = {{119-129}},
Abstract = {{Chemokine receptors may share common ligands, setting up potential
   competition for ligand binding, and association of activated receptors
   with downstream signaling molecules such as beta-arrestin. Determining
   the ``winner{''} of competition for shared effector molecules is
   essential for understanding integrated functions of chemokine receptor
   signaling in normal physiology, disease, and response to therapy. We
   describe a dual-color click beetle luciferase complementation assay for
   cell-based analysis of interactions of two different chemokine
   receptors, CXCR4 and ACKR3, with the intracellular scaffolding protein
   beta-arrestin 2. This assay provides real-time quantification of
   receptor activation and signaling in response to chemokine CXCL12. More
   broadly, this general imaging strategy can be applied to quantify
   interactions of any set of two proteins that interact with a common
   binding partner.}},
