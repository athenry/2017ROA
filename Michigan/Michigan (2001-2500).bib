@inproceedings{ ISI:000380592200005,
Author = {Wu, Z. and Harne, R. L. and Wang, K. W.},
Editor = {{Park, G}},
Title = {{Energy exchange and localization in a modular metastructure under
   impulsive excitation}},
Booktitle = {{ACTIVE AND PASSIVE SMART STRUCTURES AND INTEGRATED SYSTEMS 2016}},
Series = {{Proceedings of SPIE}},
Year = {{2016}},
