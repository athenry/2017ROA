@inproceedings{ ISI:000382319400022,
Author = {Burton, Andrew R. and Kurata, Masahiro and Nishino, Hiromichi and Lynch,
   Jerome P.},
Editor = {{Lynch, JP}},
Title = {{Fully integrated patterned carbon nanotube strain sensors on flexible
   sensing skin substrates for structural health monitoring}},
Booktitle = {{SENSORS AND SMART STRUCTURES TECHNOLOGIES FOR CIVIL, MECHANICAL, AND
   AEROSPACE SYSTEMS 2016}},
