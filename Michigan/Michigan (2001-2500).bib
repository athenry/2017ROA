Author = {Abbas, Adel T. and Aly, Mohamed and Hamza, Karim},
Book-Group-Author = {{ASME}},
Title = {{MULTIOBJECTIVE OPTIMIZATION UNDER UNCERTAINTY IN ADVANCED ABRASIVE
   MACHINING PROCESSES VIA A FUZZY-EVOLUTIONARY APPROACH}},
Booktitle = {{INTERNATIONAL DESIGN ENGINEERING TECHNICAL CONFERENCES AND COMPUTERS AND
   INFORMATION IN ENGINEERING CONFERENCE, 2015, VOL 2A}},
