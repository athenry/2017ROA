Author = {Bateman, Julie and Zekkos, Dimitrios and Olson, Edwin and Messenger,
   Sean M. and Kershaw, Carl and Fei, Xunchang and Lynch, Jerry},
Editor = {{Yesiller, N and Zekkos, D and Farid, A and De, A and Reddy, KR}},
Title = {{Preliminary Observations from Robot-Enabled Surface Methane
   Concentration Monitoring at a MSW Landfill}},
Booktitle = {{GEO-CHICAGO 2016: SUSTAINABLE WASTE MANAGEMENT AND REMEDIATION}},
Series = {{Geotechnical Special Publication}},
