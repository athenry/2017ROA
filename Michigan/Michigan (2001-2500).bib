@inproceedings{ ISI:000379883700026,
Author = {Abd El-Aziz, Khalid M. and Kim, Jihun and Hamza, Karim and El Morsi,
   Mohamed and Nassef, Ashraf O. and Metwalli, Sayed M. and Saitou,
   Kazuhiro},
