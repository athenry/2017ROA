@inproceedings{ ISI:000379883700002,
Author = {El-Morsi, Mohamed and Hamza, Karim},
Book-Group-Author = {{ASME}},
Title = {{MULTI-OBJECTIVE OPTIMIZATION OF GAS BLEND ALTERNATIVE REFRIGERANTS FOR
   VAPOR-COMPRESSION REFRIGERATION SYSTEMS}},
Booktitle = {{INTERNATIONAL DESIGN ENGINEERING TECHNICAL CONFERENCES AND COMPUTERS AND
   INFORMATION IN ENGINEERING CONFERENCE, 2015, VOL 2A}},
