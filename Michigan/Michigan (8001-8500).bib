@inproceedings{ ISI:000350825600052,
Author = {Wei, Dennis and Hero, III, Alfred O.},
Book-Group-Author = {{IEEE}},
Title = {{A Performance Guarantee for Adaptive Estimation of Sparse Signals}},
Booktitle = {{2013 IEEE GLOBAL CONFERENCE ON SIGNAL AND INFORMATION PROCESSING
   (GLOBALSIP)}},
