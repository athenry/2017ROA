@article{ ISI:000311570900010,
Author = {Peerani, Paolo and Tomanin, Alice and Pozzi, Sara and Dolan, Jennifer
   and Miller, Eric and Flaska, Marek and Battaglieri, Marco and De Vita,
   Raffaella and Ficini, Luisa and Ottonello, Giacomo and Ricco, Giovanni
