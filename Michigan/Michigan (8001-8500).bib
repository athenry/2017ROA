
@article{ ISI:000312002200008,
Author = {Kim, John P. and Vlahopoulos, Nickolas and Zhang, Geng},
Title = {{Development of a blast event simulation process for multi-scale
   modelling of composite armour for lightweight vehicles}},
Journal = {{INTERNATIONAL JOURNAL OF VEHICLE DESIGN}},
Year = {{2013}},
Volume = {{61}},
Number = {{1-4}},
Pages = {{157-176}},
Abstract = {{This paper presents the development of a multi-scale simulation process
   for modelling the response of a vehicle with composite armour to the
   blast loads from an explosive threat. The new process can be used for
   improving the blast resistant capabilities of the composite armour by
   configuring its properties at the micro-level. A Blast Event Simulation
   sysTem (BEST), for conducting a complete sequence of explosive
   simulations, comprises one of the two main foundation components of the
   new development. The Micromechanics Analysis Code (MAC) comprises the
   second main foundation component. The development of the new multi-scale
   simulation capability and an associated case study are presented.}},
