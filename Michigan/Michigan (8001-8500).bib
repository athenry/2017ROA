@article{ ISI:000311562500056,
Author = {Ganjeh, Y. and Song, B. and Pagadala, K. and Kim, K. and Sadat, S. and
   Jeong, W. and Kurabayashi, K. and Meyhofer, E. and Reddy, P.},
Title = {{A platform to parallelize planar surfaces and control their spatial
   separation with nanometer resolution}},
Journal = {{REVIEW OF SCIENTIFIC INSTRUMENTS}},
Year = {{2012}},
Volume = {{83}},
Number = {{10, 1}},
Month = {{OCT}},
Abstract = {{Parallelizing planar surfaces and manipulating them into close proximity
   with spatial separation of nanoscale dimensions is critical for probing
   phenomena such as near-field radiative heat transport and Casimir
   forces. Here, we report on a novel platform, with an integrated
   reflected light microscope, that is capable of parallelizing two planar
   surfaces such that the angular deviation is <6 mu rad, while
   simultaneously allowing control of the gap from 15 mu m down to contact
   with similar to 0.15 nm resolution. The capabilities of this platform
   were verified by using two custom-fabricated micro-devices with planar
   surfaces, 60 x 60 mu m(2) each, whose flatness and surface roughness
   were experimentally quantified. We first parallelized the two
   micro-devices by using the developed platform in conjunction with a
   simple optical approach that relies on the shallow depth of field
   (similar to 2 mu m) of a long working distance microscope objective.
   Subsequently, we experimentally tested the parallelism achieved via the
   optical alignment procedure by taking advantage of electrodes integrated
   into the micro-devices. Our measurements unambiguously show that the
   simple depth-of-field based optical approach enables parallelization
   such that the angular deviation between the two surfaces is within
   similar to 500 mu rad. This ensures that the separation between any two
   corresponding points on the parallel surfaces deviate by similar to 30
   nm or less from the expected value. Further, we show that improved
   parallelization can be achieved using the integrated micro-electrodes
   which enable surface roughness limited parallelization with deviations
   of similar to 5 nm from parallelism. (C) 2012 American Institute of
   Physics. {[}http://dx.doi.org/10.1063/1.4754643]}},
