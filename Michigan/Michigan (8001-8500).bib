Year = {{2012}},
Volume = {{101}},
Number = {{13}},
Month = {{SEP 24}},
Abstract = {{We demonstrate large-area organic photovoltaic cells (OPVs) based on
   boron-subphthalocyanine chloride (SubPc)/C-60 and
   2,4-bis{[}4-(N,N-diphenylamino)-2,6 dihydroxyphenyl] squaraine/C-60
