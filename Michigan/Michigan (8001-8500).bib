@article{ ISI:000305166000007,
Author = {Hills, D. A. and Thaitirarot, A. and Barber, J. R. and Dini, D.},
Title = {{Correlation of fretting fatigue experimental results using an asymptotic
   approach}},
Journal = {{INTERNATIONAL JOURNAL OF FATIGUE}},
Year = {{2012}},
Volume = {{43}},
Pages = {{62-75}},
Month = {{OCT}},
Abstract = {{Small refinements are made to the bounded asymptotic forms recently
   developed to encapsulate the state of slip and stress at the edges of
   incomplete contacts. They are then used to re-analyse several sets of
   publicly available fretting fatigue data. Several different combinations
   of the asymptotes are used and it is shown that two are viable and give
