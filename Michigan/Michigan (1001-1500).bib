@article{ ISI:000371296000026,
Author = {Liu, Bin and Lei, Qian and Xie, Liuqun and Wang, Mingpu and Li, Zhou},
Title = {{Microstructure and mechanical properties of high product of strength and
   elongation Al-Zn-Mg-Cu-Zr alloys fabricated by spray deposition}},
Journal = {{MATERIALS \& DESIGN}},
Year = {{2016}},
Volume = {{96}},
Pages = {{217-223}},
Month = {{APR 15}},
Abstract = {{Large-scale Al-Zn-Mg-Cu-Zr alloys rods were fabricated by spray
   deposition. Microstructure observations reveal that equiaxed grains
   formed in the ingots, and the grain size in spray-deposited ingot was
   smaller than of the commercial product. High product of
   strength-elongation (11.9 GPa .\%.) alloy was obtained through spray
   deposition and followed by hot extrusion and solution treatment. This
   resulted in a good combination of strength and elongation (Ultimate
   tensile strength: 684 MPa; Elongation: 17.4\%). Hot extrusion improved
   the mechanical properties, while T6 treatment improved the tensile
   strength but decreased the elongation. The high-density nano-scale
   precipitates (eta' and Al3Zr precipitates) caused the improved strength
   (723 MPa), but were detrimental to the specimens' ductility (11\%).
   Intergranular fracture and transgranular fracture are main fracture
   mechanisms in extrusion state and T6 treatment samples, respectively.
   Brittle secondary-phase particles cracked during tension test process
   owing to their limited ductility. (C) 2016 Elsevier Ltd. All rights
   reserved.}},
