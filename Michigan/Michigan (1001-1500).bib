@article{ ISI:000374442300051,
Author = {Byon, Eunshin and Choe, Youngjun and Yampikulsakul, Nattavut},
Title = {{Adaptive Learning in Time-Variant Processes With Application to Wind
   Power Systems}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATION SCIENCE AND ENGINEERING}},
Year = {{2016}},
Volume = {{13}},
Number = {{2}},
Pages = {{997-1007}},
Month = {{APR}},
Abstract = {{This study develops new adaptive learning methods for a dynamic system
   where the dependency among variables changes over time. In general, many
   statistical methods focus on characterizing a system or process with
   historical data and predicting future observations based on a developed
   time-invariant model. However, for a nonstationary process with
   time-varying input-to-output relationship, a single baseline curve may
   not accurately characterize the system's dynamic behavior. This study
   develops kernel-based nonparametric regression models that allow the
   baseline curve to evolve over time. Applying the proposed approach to a
   real wind power system, we investigate the nonstationary nature of wind
   effect on the turbine response. The results show that the proposed
   methods can dynamically update the time-varying dependency pattern and
   can track changes in the operational wind power system.}},
