@article{ ISI:000372615400015,
Author = {Zhou, Guan and Ma, Zheng-Dong and Gu, Jichao and Li, Guangyao and Cheng,
   Aiguo and Zhang, Wei},
Title = {{Design optimization of a NPR structure based on HAM optimization method}},
Journal = {{STRUCTURAL AND MULTIDISCIPLINARY OPTIMIZATION}},
Year = {{2016}},
Volume = {{53}},
Number = {{3}},
Pages = {{635-643}},
Month = {{MAR}},
Abstract = {{Lower mass and higher performances of structure with Negative Poisson's
   Ratio (NPR) make it a promising option in vehicle design. In this work,
   a new NPR structure is introduced and analyzed. The new NPR structure
   has high shear strength and its effective Young's modulus increases as
   the load increases. Mathematical models are firstly established to study
   the unique properties of the NPR structure. The parameterized model of
   the structure is then built to study the performance of the structure
   under large deformations condition. Due to the complexities of the
   nonlinear properties, the hybrid and adaptive meta-model (HAM)
   optimization method is employed for optimizing the NPR structure. The
   optimization results are compared with those of traditional optimization
