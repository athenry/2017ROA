@article{ ISI:000374079300023,
Author = {Ramazani, A. and Abbasi, M. and Kazemiabnavi, S. and Schmauder, S. and
   Larson, R. and Prahl, U.},
Title = {{Development and application of a microstructure-based approach to
   characterize and model failure initiation in DP steels using XFEM}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2016}},
Volume = {{660}},
Pages = {{181-194}},
Month = {{APR 13}},
Abstract = {{We develop a microstructure-based model to characterize and model
   failure initiation in DP steels using an extended finite element method
   (XFEM) to simulate martensite cracking on the mesoscale combined with
   representative volume element (RVE) modeling. A mini tensile test with
   digital image correlation (DIC) analysis is linked to local SEM analysis
   to identify the local strain at which failure is initiated. In-situ
   bending tests in SEM with electron backscatter diffraction (EBSD)
   measurements before and after the test are carried out to validate that
   the crack initiates in the martensite islands. Empirical equations for
   XFEM parameters as functions of local carbon content in martensite are
   fit to experimental results for laboratory-annealed DP600 steels with
