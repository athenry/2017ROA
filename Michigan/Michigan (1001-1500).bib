@article{ ISI:000370636900013,
Author = {Yang, Chunyu and Zhang, Lingli and Sun, Jing},
Title = {{Anti-windup controller design for singularly perturbed systems subject
   to actuator saturation}},
Journal = {{IET CONTROL THEORY AND APPLICATIONS}},
Year = {{2016}},
Volume = {{10}},
Number = {{4}},
Pages = {{469-476}},
Month = {{FEB 26}},
Abstract = {{This study considers the problem of anti-windup (AW) controller design
   for singularly perturbed systems with actuator saturation. The AW
   controller consists of a dynamic state feedback (DSF) controller and an
   AW compensator. A convex optimisation problem in terms of linear matrix
   inequalities is formulated to simultaneously design both the DSF
   controller and the AW compensator. The resulting AW controller depends
   on the singular perturbation parameter e and is shown to be
   well-conditioned for any e of interest. Furthermore, a two-stage design
   method is proposed to handle the case that e is unknown. An
   e-independent DSF controller is designed at the first stage, and then an
   e-independent AW compensator is constructed by solving a convex
   optimisation problem. Both of the methods can achieve a desired
   stability bound and enlarge the basin of attraction at the same time.
   Finally, examples are given to show the advantages and effectiveness of
   the obtained results.}},
