@article{ ISI:000353634000014,
Author = {Tsiligkaridis, Theodoros and Sadler, Brian M. and Hero, III, Alfred O.},
Title = {{On Decentralized Estimation With Active Queries}},
Journal = {{IEEE TRANSACTIONS ON SIGNAL PROCESSING}},
Year = {{2015}},
Volume = {{63}},
Number = {{10}},
Pages = {{2610-2622}},
Month = {{MAY 15}},
Abstract = {{We consider the problem of decentralized 20 questions with noise for
   multiple players/agents under the minimum entropy criterion in the
   setting of stochastic search over a parameter space, with application to
   target localization. We propose decentralized extensions of the active
   query-based stochastic search strategy that combines elements from the
   20 questions approach and social learning. We prove convergence to
   correct consensus on the value of the parameter. This framework provides
   a flexible and tractable mathematical model for decentralized parameter
   estimation systems based on active querying. We illustrate the
   effectiveness and robustness of the proposed decentralized collaborative
   20 questions algorithm for random network topologies with information
   sharing.}},
