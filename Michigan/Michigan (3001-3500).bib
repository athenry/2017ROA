@article{ ISI:000354980300006,
Author = {Kim, Soo Chang and Kim, Young Sun and Kanicki, Jerzy},
Title = {{Density of states of short channel amorphous In-Ga-Zn-O thin-film
   transistor arrays fabricated using manufacturable processes}},
Journal = {{JAPANESE JOURNAL OF APPLIED PHYSICS}},
Year = {{2015}},
Volume = {{54}},
Number = {{5}},
Month = {{MAY}},
Abstract = {{The effect of temperature on the electrical characteristics of the short
   channel amorphous In-Ga-Zn-O (a-IGZO) thin-film transistor (TFT) arrays
   fabricated using manufacturable processes was investigated. This work
   shows that the fabricated TFTarrays are acceptable and stable enough for
   manufacturing of the ultra high definition (UHD) active matrix liquid
   crystal displays in size larger than 55 in. We observed that studied
   a-IGZO TFT arrays obeyed the Meyer-Neldel (MN) rule over a broad range
   of gate bias voltages. The MN rule and exponential subgap density of
   states (DOS) model were combined to extract the DOS distribution for the
   investigated a-IGZO TFT arrays. The results were consistent with the
   previous works on single a-IGZO TFTs. This study demonstrates that this
   method of DOS extraction can be applied to both single devices and
   arrays, and is reproducible from lab to lab. We believe that this
   approach of DOS extraction is useful for further development of UHD flat
   panel display technology. (C) 2015 The Japan Society of Applied Physics}},
