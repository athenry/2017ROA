@article{ ISI:000357802000028,
Author = {Hobeck, Jared D. and Inman, Daniel J.},
Title = {{Low-Cost Pressure Probe Sensor for Predicting Turbulence-Induced
   Vibration From Invasive Low-Velocity Turbulent Flow Measurements}},
Journal = {{IEEE SENSORS JOURNAL}},
Year = {{2015}},
Volume = {{15}},
Number = {{8}},
Pages = {{4373-4379}},
Month = {{AUG}},
Abstract = {{Measuring high-intensity turbulence with fast response pressure probe
   sensors has proved to be an effective and accurate technique;
   unfortunately, according to the available literature, most pressure
   probe research focuses primarily on high-velocity (>100 m/s)
   applications. Commercially available pressure probe sensor systems are
   available; however, most designs are intended for not only high-velocity
   flow but also for high bandwidth (similar to 10 kHz) and
   multidirectional applications, which drastically increase cost and
   complexity. Extremely limited options exist for those seeking a simple
   yet sensitive and accurate unidirectional turbulent flow measurement
   method; therefore, the intended purpose of this paper is to provide a
   means for future scientists and engineers to develop a pressure probe
   sensor tailored to their specific needs. Research presented in this
   paper focuses on the design and analysis of two high-sensitivity,
   low-cost pressure probes for invasive measurements in low-velocity (<20
   m/s) highly turbulent air flow. The pressure probe design, calibration
   techniques, and modeling are discussed and experimentally validated.}},
