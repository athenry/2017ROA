@article{ ISI:000356737300027,
Author = {Gunasekar, Kumarasamy and Chakravarthi, Nallan and Cho, Woosum and Lee,
   Jae Wook and Kim, Seong Woo and Cha, Sang-Ho and Kotov, Nicholas A. and
   Jin, Sung-Ho},
Title = {{Optimization of polymer solar cells performance by incorporated
   scattering of ZnO nanoparticles with different particle geometry}},
Journal = {{SYNTHETIC METALS}},
Year = {{2015}},
Volume = {{205}},
Pages = {{185-189}},
Month = {{JUL}},
Abstract = {{The photovoltaic performance of polymer solar cells (PSCs) can be
   improved by incorporating zinc oxide (ZnO) nanostructures of different
   particle morphology in combination with poly(3-hexylthiophene) (P3HT)
   and {[}6,6]-phenyl-C71-butyric acid methyl ester (PC71BM) in the active
   layer of the bulk heterojunction (BHJ) PSCs. Special emphasis is made to
   study the effect of the stabilizer-free ZnO nanoparticles (NPs) geometry
   - nanospheres, nanoplates and nanopyramids in the active layer of BHJ
   PSCs. The concentration of ZnO NPs in the active layer of BHJ blends was
   varied from 1 to 6 wt\%. Incorporation of NPs in the blend of active
   layer material (P3HT:PC71BM) increases the absorptivity with increasing
   concentration of ZnO NPs due to the light scattering effect of ZnO,
   which enhances the photovoltaic performance of the resultant polymer
   solar cells. The influence of the morphology and the concentration of
   the ZnO NPs on the general performance of the PSC devices are discussed
   in detail. (C) 2015 Elsevier B.V. All rights reserved.}},
