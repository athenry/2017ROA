@article{ ISI:000354592300079,
Author = {Chichester, David L. and Thompson, Scott J. and Kinlaw, Mathew T. and
   Johnson, James T. and Dolan, Jennifer I. and Flaska, Marek and Pozzi,
   Sara A.},
Title = {{Statistical estimation of the performance of a fast-neutron multiplicity
   system for nuclear material accountancy}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
Year = {{2015}},
Volume = {{784}},
