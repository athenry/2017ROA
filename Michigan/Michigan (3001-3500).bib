@article{ ISI:000356618700051,
Author = {Hobeck, J. D. and Inman, D. J.},
Title = {{The Jd number: An empirical constant for predicting dual cantilever
   flutter velocity}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{106}},
Number = {{24}},
Month = {{JUN 15}},
Abstract = {{In this letter, the authors present a semi-empirical relationship used
   to predict the velocity at which two adjacent identical cantilevers
   placed in steady cross-flow begin to experience persistent large
   amplitude vibrations at a particular combination of flow velocity and
   separation distance. This recently discovered flow-induced vibration
   phenomenon is called dual cantilever flutter (DCF). The only DCF models
   available can be used to predict time-series dynamics; however, they are
   complex and require extensive model updating procedures. The model
   proposed in this letter requires known parameters and only a single
   nondimensional empirical value called the Jd number. The Jd number was
   found to be an approximately constant single value for four case
   studies, each performed with a different cantilever design. Derivation
   of the Jd number and the flutter prediction model are discussed and
   experimentally validated. (C) 2015 AIP Publishing LLC.}},
