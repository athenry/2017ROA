@article{ ISI:000351663100014,
Author = {O'Brien, William A. and Qi, Meng and Yan, Lifan and Stephenson, Chad A.
   and Protasenko, Vladimir and Xing, Huili and Millunchick, Joanna M. and
   Wistey, Mark A.},
Title = {{Self-assembled Ge QDs Formed by High-Temperature Annealing on Al(Ga)As
   (001)}},
Journal = {{JOURNAL OF ELECTRONIC MATERIALS}},
Year = {{2015}},
Volume = {{44}},
Number = {{5}},
Pages = {{1338-1343}},
Month = {{MAY}},
Note = {{56th Electronic Materials Conference, Univ Calif Santa Barbara, Santa
   Barbara, CA, JUN 25-27, 2014}},
Organization = {{Amer Elements; Sandia Natl Labs}},
Abstract = {{This work studies the spontaneous self-assembly of Ge QDs on AlAs, GaAs
   and AlGaAs by high-temperature ina situ pound annealing using molecular
   beam epitaxy (MBE). The morphology of Ge dots formed on AlAs were
   observed by atom probe tomography, which revealed nearly spherical QDs
   with diameters approaching 10 nm and confirmed the complete absence of a
   wetting layer. Reflection high-energy electron diffraction and atomic
   force microscopy of Ge annealed under similar conditions on GaAs and
   Al0.3Ga0.7As surfaces revealed the gradual suppression of QD formation
   with decreasing Al-content of the buffer. To investigate the prospects
   of using encapsulated Ge dots for upconverting photovoltaics, in which
   photocurrent can still be generated from photons with energy less than
   the host bandgap, Ge QDs were embedded into the active region of III-V
   PIN diodes by MBE. It was observed that orders of magnitude higher
   short-circuit current is obtained at photon energies below the GaAs
   bandgap compared with a reference PIN diode without Ge QDs. These
   results demonstrate the promise of Ge QDs for upconverting solar cells
   and the realization of device-quality integration of group IV and III-V
   semiconductors.}},
