@article{ ISI:000357549200094,
Author = {Van Sluytman, Jason S. and Moceri, Charles J. and Pollock, Tresa M.},
Title = {{A Pt-modified Ni-base superalloy with high temperature precipitate
   stability}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2015}},
Volume = {{639}},
Pages = {{747-754}},
Month = {{JUL 15}},
Abstract = {{Nickel-base alloys display exceptional mechanical properties to high
   fractions of their melting point due to the presence of L1(2) gamma'
   precipitates. However, above 1000 degrees C, the precipitates typically
   coarsen or rapidly undergo shape changes along with dissolution into the
   matrix, thereby degrading the mechanical performance of the alloy at
   higher temperatures. In this research, a Pt-modified Ni-base alloy with
   exceptional high temperature stability has been identified. Coarsening
   studies conducted in the temperature range from 1050-1200 degrees C
   reveal unusually high volume fractions of morphologically stable gamma'
   precipitates up to 1200 degrees C. Electron-probe micro-analysis
   demonstrates strong partitioning of Pt to the gamma' phase. The presence
   of a high volume fraction of slowly coarsening precipitates at 1200
   degrees C suggests that the alloy would have excellent performance as a
   bond coat and/or single crystal blade. (C) 2015 Elsevier B.V. All rights
   reserved.}},
