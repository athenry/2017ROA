@article{ ISI:000355776100009,
Author = {Yu Yi and Shu XiaoLin and Liu YiNan and Niu LiangLiang and Jin Shuo and
   Gao Fei and Lu GuangHong},
Title = {{Effect of hydrogen on grain boundary migration in tungsten}},
Journal = {{SCIENCE CHINA-PHYSICS MECHANICS \& ASTRONOMY}},
Year = {{2015}},
Volume = {{58}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{Motivated by a grain boundary (GB) healing mechanism that GB turns into
   a mobile sink through migration to eliminate the vacancies in a bulk, we
   have further investigated the influence of the retained hydrogen (H) on
   the GB migration in tungsten using a molecular dynamics simulation. We
   show that H hinders the GB migration at different H concentrations and
   temperatures, and such friction of GB migration due to the presence of H
   increases with the H concentration and decreases with temperature. We
   demonstrate that H follows the GB-migration as the temperature is higher
   than 300 K. Most importantly, the presence of H induces a disordering of
   GB, which affects the GB migration significantly.}},
