@article{ ISI:000352820700006,
Author = {McAllister, Andrew and Aberg, Daniel and Schleife, Andre and Kioupakis,
   Emmanouil},
Title = {{Auger recombination in sodium-iodide scintillators from first principles}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2015}},
Volume = {{106}},
Number = {{14}},
Month = {{APR 6}},
Abstract = {{Scintillator radiation detectors suffer from low energy resolution that
   has been attributed to nonlinear light yield response to the energy of
   the incident gamma rays. Auger recombination is a key non-radiative
   recombination channel that scales with the third power of the excitation
   density and may play a role in the non-proportionality problem of
   scintillators. In this work, we study direct and phonon-assisted Auger
   recombination in NaI using first-principles calculations. Our results
   show that phonon-assisted Auger recombination, mediated primarily by
   short-range phonon scattering, dominates at room temperature. We discuss
   our findings in light of the much larger values obtained by numerical
   fits to z-scan experiments. (C) 2015 AIP Publishing LLC.}},
