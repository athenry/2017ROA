@article{ ISI:000352365800042,
Author = {Aktakka, Ethem Erkan and Woo, Jong-Kwan and Egert, Daniel and Gordenker,
   Robert J. M. and Najafi, Khalil},
Title = {{A Microactuation and Sensing Platform With Active Lockdown for In Situ
   Calibration of Scale Factor Drifts in Dual-Axis Gyroscopes}},
Journal = {{IEEE-ASME TRANSACTIONS ON MECHATRONICS}},
Year = {{2015}},
Volume = {{20}},
Number = {{2}},
Pages = {{934-943}},
Month = {{APR}},
Abstract = {{This paper presents the design and experimental results of a
   microvibratory actuation and sensing platform to provide on-chip
   physical stimulus for in situ calibration of long-term scale factor
   drifts in multiaxis microelectromechanical systems (MEMS) inertial
   sensors. The platform consists of a three degrees-of-freedom micromotion
   stage that can provide piezoelectric actuation for X/Y-tilting reference
   stimuli, compensation of undesired off-axis motion, integrated sensing
   of applied periodic stimulus, and electrostatic position lock-down for
   shock protection. A dual-axis MEMS gyroscope is mounted on top of the
   microplatform, and its electrical interconnects are provided through
   microfabricated highly flexible parylene cables with virtually
   zero-loading. The piezoelectric stage is measured to provide up to 280
   degrees/s angular ac excitation to a 25-mg inertial sensor payload at an
   expense of <100 mu W, while providing an analog sensing signal (11
   mV/degrees/s) to determine the applied rate with a precision of 1.2
   degrees/s. The estimated scale factor has < 0.8\% deviation from
   rate-table characterized values on the same-model gyroscope samples.
   With further improvements in control precision and angular velocity
   estimation, the introduced platform is expected enable on-chip
   self-calibration of long-term scale-factor drifts to < 100 ppm.}},
Publisher = {{IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC}},
Address = {{445 HOES LANE, PISCATAWAY, NJ 08855-4141 USA}},
Type = {{Article}},
