@article{ ISI:000354139700004,
Author = {Whetstone, Z. D. and Kearfott, K. J.},
Title = {{A method for using neutron elastic scatter to create a variable energy
   neutron beam from a nearly monoenergetic neutron source}},
Journal = {{RADIATION PHYSICS AND CHEMISTRY}},
Year = {{2015}},
Volume = {{112}},
Pages = {{22-28}},
Month = {{JUL}},
Abstract = {{This work describes preliminary investigation into the design of a
   compact, portable, variable energy neutron source. The proposed method
   uses elastic neutron scatter at specific angles to reduce the energy of
   deuterium-deuterium or deuterium-tritium (D-T) neutrons. The research
   focuses on D-T Monte Carlo simulations, both in idealized and more
   realistic scenarios. Systematic uncertainty of the method is also
   analyzed. The research showed promise, but highlighted the need for
   discrimination of multiply-scattered neutrons, either through a pulsed
   generator or associated particle imaging. (C) 2015 Elsevier Ltd. All
   rights reserved.}},
