@inproceedings{ ISI:000371297400011,
Author = {Laurenzano, Michael A. and Tiwari, Ananta and Jundt, Adam and Peraza,
   Joshua and Ward, Jr., William A. and Campbell, Roy and Carrington, Laura},
Editor = {{Silva, F and Dutra, I and Costa, VS}},
Title = {{Characterizing the Performance-Energy Tradeoff of Small ARM Cores in HPC
   Computation}},
Booktitle = {{EURO-PAR 2014 PARALLEL PROCESSING}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2014}},
Volume = {{8632}},
