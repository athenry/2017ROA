@article{ ISI:000331169400010,
Author = {Liu, Jing and Wu, Junxiao and He, Xiaoyi and Rao, Madhukar and Monroe,
   Charles W.},
Title = {{Thermodynamics and Kinetics of Liquid Removal from Nano-Features during
   Single-Wafer Spin Cleaning}},
Journal = {{ECS JOURNAL OF SOLID STATE SCIENCE AND TECHNOLOGY}},
Year = {{2014}},
Volume = {{3}},
Number = {{1}},
Pages = {{N3118-N3124}},
Abstract = {{Liquid removal from nano-scale features on semiconductor wafers is
   studied from the perspectives of thermodynamics and transport. The
   thermodynamic model considers various cylindrically symmetrical states
   of liquid in a feature and calculates their free energies. A phase
   diagram is developed to show what configurations of liquid in a
   cylindrical feature are most stable given the aspect ratio of the
   feature, the volume fraction occupied by liquid, and the interior
   contact angle. Energies needed to remove liquid from a feature are
   computed in terms of these parameters, as well as the contact angle on
   the wafer surface exterior to the feature. The transport model is used
   to estimate drying times by accounting for liquid evaporation kinetics
   and vapor-phase transport. Drying is controlled by the evaporation rate
   of the liquid. Periods required for drying depend strongly on the
   evaporation coefficient, as well as ambient temperature and ambient
   solvent vapor pressure. (C) 2014 The Electrochemical Society. All rights
   reserved.}},
