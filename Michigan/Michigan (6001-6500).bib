@inproceedings{ ISI:000338768500023,
Author = {Xie, Zhixing and Tian, Chao and Chen, Sung-Liang and Ling, Tao and
   Zhang, Cheng and Guo, L. Jay and Carson, Paul L. and Wang, Xueding},
Editor = {{Oraevsky, AA and Wang, LV}},
Title = {{3D high resolution photoacoustic imaging based on pure optical
   photoacoustic microscopy with microring resonator}},
Booktitle = {{PHOTONS PLUS ULTRASOUND: IMAGING AND SENSING 2014}},
