@article{ ISI:000330342400019,
Author = {Lemasson, Benjamin and Chenevert, Thomas L. and Lawrence, Theodore S.
   and Tsien, Christina and Sundgren, Pia C. and Meyer, Charles R. and
   Junck, Larry and Boes, Jennifer and Galban, Stefanie and Johnson,
   Timothy D. and Rehemtulla, Alnawaz and Ross, Brian D. and Galban, Craig
   J.},
Title = {{Impact of Perfusion Map Analysis on Early Survival Prediction Accuracy
   in Glioma Patients}},
Journal = {{TRANSLATIONAL ONCOLOGY}},
Year = {{2013}},
Volume = {{6}},
Number = {{6}},
Pages = {{766-774}},
Month = {{DEC}},
Abstract = {{Studies investigating dynamic susceptibility contrast magnetic resonance
   imaging-determined relative cerebral blood volume (rCBV) maps as a
   metric of treatment response assessment have generated conflicting
   results. We evaluated the potential of various analytical techniques to
   predict survival of patients with glioma treated with chemoradiation.
   rCBV maps were acquired in patients with high-grade gliomas at 0, 1, and
   3 weeks into chemoradiation therapy. Various analytical techniques were
   applied to the same cohort of serial rCBV data for early assessment of
   survival. Three different methodologies were investigated: 1) percentage
   change of whole tumor statistics (i.e., mean, median, and percentiles),
   2) physiological segmentation (low rCBV, medium rCBV, or high rCBV), and
   3) a voxel-based approach, parametric response mapping (PRM). All
   analyses were performed using the same tumor contours, which were
   determined using contrast-enhanced T1-weighted and fluid attenuated
   inversion recovery images. The predictive potential of each response
   metric was assessed at 1-year and overall survival. PRM was the only
   analytical approach found to generate a response metric significantly
   predictive of patient 1-year survival. Time of acquisition and contour
   volume were not found to alter the sensitivity of the PRM approach for
   predicting overall survival. We have demonstrated the importance of the
   analytical approach in early response assessment using serial rCBV maps.
   The PRM analysis shows promise as a unified early and robust imaging
   biomarker of treatment response in patients diagnosed with high-grade
   gliomas.}},
