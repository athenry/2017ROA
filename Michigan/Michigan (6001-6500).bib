@article{ ISI:000335847600001,
Author = {An, Seungdo and Gupta, Naveen K. and Gianchandani, Yogesh B.},
Title = {{Vacuum sealing using atomic layer deposition of Al2O3 at 250 degrees C}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY A}},
Year = {{2014}},
Volume = {{32}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{This paper describes the use of low-temperature atomic layer deposition
   (ALD) of Al2O3, for vacuum seals in wafer-level vacuum packaging and
   other applications. The conformal coverage provided by ALD Al2O3 is
   shown to seal circular micromachined cavities. The cavities are 0.8 mu m
   in height, 400 mu m in diameter, and are capped by porous
   plasma-enhanced chemical vapor deposited dielectrics that form a
   membrane. The ALD Al2O3 film, of thickness approximate to 0.2 mu m, is
   deposited at a temperature of 250 degrees C on this membrane. The
   retention of vacuum is indicated by the deflection of the membrane.
   Lifetime tests extending out to 19 months are reported. (C) 2014
   American Vacuum Society.}},
