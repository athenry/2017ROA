@article{ ISI:000384016500016,
Author = {Xue, Xiang and Ramakrishnan, Sadeesh K. and Weisz, Kevin and Triner,
   Daniel and Xie, Liwei and Attili, Durga and Pant, Asha and Gyorffy,
   Balazs and Zhan, Mingkun and Carter-Su, Christin and Hardiman, Karin M.
   and Wang, Thomas D. and Dame, Michael K. and Varani, James and Brenner,
   Dean and Fearon, Eric R. and Shah, Yatrik M.},
Title = {{Iron Uptake via DMT1 Integrates Cell Cycle with JAK-STAT3 Signaling to
   Promote Colorectal Tumorigenesis}},
Journal = {{CELL METABOLISM}},
Year = {{2016}},
Volume = {{24}},
Number = {{3}},
Pages = {{447-461}},
Month = {{SEP 13}},
Abstract = {{Dietary iron intake and systemic iron balance are implicated in
   colorectal cancer (CRC) development, but the means by which iron
   contributes to CRC are unclear. Gene expression and functional studies
   demonstrated that the cellular iron importer, divalent metal transporter
   1 (DMT1), is highly expressed in CRC through hypoxia-inducible factor 2
   alpha-dependent transcription. Colon-specific Dmt1 disruption resulted
   in a tumor-selective inhibitory effect of proliferation in mouse colon
   tumor models. Proteomic and genomic analyses identified an
   iron-regulated signaling axis mediated by cyclin-dependent kinase 1
   (CDK1), JAK1, and STAT3 in CRC progression. A pharmacological inhibitor
   of DMT1 antagonized the ability of iron to promote tumor growth in a CRC
   mouse model and a patient-derived CRC enteroid orthotopic model. Our
   studies implicate a growth-promoting signaling network instigated by
   elevated intracellular iron levels in tumorigenesis, offering molecular
   insights into how a key dietary component may contribute to CRC.}},
