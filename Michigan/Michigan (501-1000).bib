@article{ ISI:000377729200002,
Author = {Liu, Lehao and Xie, Fan and Lyu, Jing and Zhao, Tingkai and Li, Tiehu
   and Choi, Bong Gill},
Title = {{Tin-based anode materials with well-designed architectures for
   next-generation lithium-ion batteries}},
