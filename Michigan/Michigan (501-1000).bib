@article{ ISI:000388294800004,
Author = {Fampa, Marcia and Lee, Jon and Maculan, Nelson},
Title = {{An overview of exact algorithms for the Euclidean Steiner tree problem
   in n-space}},
Journal = {{INTERNATIONAL TRANSACTIONS IN OPERATIONAL RESEARCH}},
Year = {{2016}},
Volume = {{23}},
Number = {{5, SI}},
Pages = {{861-874}},
Month = {{SEP}},
Abstract = {{The Euclidean Steiner tree problem (ESTP) in R-n is to find a shortest
   network interconnecting p given points in n-dimensional Euclidean space.
   The problem was first described in the plane and an algorithm with very
   good practical efficiency has been proposed to solve this particular
   case. The generalization for higher dimensions was proposed in the 19th
   century, however the numerical solution of the problem remains very
   challenging when n >= 3. We give an overview of the exact algorithms
   presented in the literature for the ESTP when n >= 3 and discuss their
   common and distinguished features, their advantages and drawbacks, and
   some possible directions for improvement toward the numerical solution
   of large instances of the problem.}},
