@article{ ISI:000380295700030,
Author = {Liang, Sai and Wang, Hongxia and Qu, Shen and Feng, Tiantian and Guan,
   Dabo and Fang, Hong and Xu, Ming},
Title = {{Socioeconomic Drivers of Greenhouse Gas Emissions in the United States}},
