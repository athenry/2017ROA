@article{ ISI:000380942600023,
Author = {Shiozawa, Yusuke and Berry, Janice E. and Eber, Matthew R. and Jung,
   Younghun and Yumoto, Kenji and Cackowski, Frank C. and Yoon, Hyeun Joong
   and Parsana, Princy and Mehra, Rohit and Wang, Jingcheng and McGee,
   Samantha and Lee, Eunsohl and Nagrath, Sunitha and Pienta, Kenneth J.
   and Taichman, Russell S.},
Title = {{The marrow niche controls the cancer stem cell phenotype of disseminated
   prostate cancer}},
Journal = {{ONCOTARGET}},
Year = {{2016}},
Volume = {{7}},
Number = {{27}},
Pages = {{41217-41232}},
Month = {{JUL 5}},
Abstract = {{Dissemination of cancer stem cells (CSCs) serves as the basis of
   metastasis. Recently, we demonstrated that circulating prostate cancer
   targets the hematopoietic stem cell (HSCs) `niche' in marrow during
   dissemination. Once in the niche, disseminated tumor cells (DTCs) may
   remain dormant for extended periods. As the major function of the HSC
   niche is to maintain stem cell functions, we hypothesized that the niche
   regulates CSC activities of DTCs. Here we show that DTCs recovered from
   marrow were significantly enriched for a CSC phenotype. Critically, the
   conversion of DTCs to CSCs is regulated by niche-derived GAS6 through
   the Mer/mTOR; molecules previously shown to regulate dormancy. The data
   demonstrate that the niche plays a significant role in maintaining
   tumor-initiating prostate cancer in marrow and suggests a functional
   relationship between CSCs and dormancy. Understanding how the marrow
   niche regulates the conversion of DTCs to CSCs is critical for the
   development of therapeutics specifically targeting skeletal bone
   metastasis and dormancy.}},
