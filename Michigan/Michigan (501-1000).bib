@article{ ISI:000382683100006,
Author = {Clarizia, Maria Paola and Ruf, Christopher S.},
Title = {{On the Spatial Resolution of GNSS Reflectometry}},
Journal = {{IEEE GEOSCIENCE AND REMOTE SENSING LETTERS}},
Year = {{2016}},
Volume = {{13}},
Number = {{8}},
Pages = {{1064-1068}},
Month = {{AUG}},
Abstract = {{A method for defining the spatial resolution of a Global Navigation
   Satellite System reflectometry delay-Doppler map (DDM) and of any
   derived geophysical product is proposed. An effective spatial resolution
   is derived as a function of measurement geometry and delay-Doppler (DD)
   interval, and as a more appropriate representation of resolution than
   the geometric resolution previously used in the literature. The
   definition more accurately accounts for variations in the scattered
   power across different pixels of the DDM and more accurately includes
   the power spreading effect caused by the Woodward ambiguity function.
   The dependence of the effective resolution on incidence angle, receiver
   altitude, and DD interval is analyzed and compared with the dependence
   of the geometric resolution with similar parameters.}},
