@article{ ISI:000379026100016,
Author = {Luo, Jiajia and Betschart, Cornelia and Ashton-Miller, James A. and
   DeLancey, John O. L.},
Title = {{Quantitative analyses of variability in normal vaginal shape and
   dimension on MR images}},
Journal = {{INTERNATIONAL UROGYNECOLOGY JOURNAL}},
Year = {{2016}},
Volume = {{27}},
Number = {{7}},
Pages = {{1087-1095}},
Month = {{JUL}},
Abstract = {{We present a technique for quantifying inter-individual variability in
   normal vaginal shape, axis, and dimension, and report findings in
   healthy women.
   Eighty women (age: 28 similar to 70 years) with normal pelvic organ
   support underwent supine, multi-planar proton-density MRI. Vaginal width
   was assessed at five evenly-spaced locations, and vaginal axis, length,
   and surface area were quantified via ImageJ and MATLAB.
   The mid-sagittal plane angles, relative to the horizontal, of three
   vaginal axes were 90 +/- 11, 72 +/- 21, and 41 +/- 22A degrees (caudal
   to cranial, p < 0.001). The mean (+/- SD) vaginal widths were 17 +/- 5,
   24 +/- 4, 30 +/- 7, 41 +/- 9, and 45 +/- 12 mm at the five locations
   (caudal to cranial, p < 0.001). Mid-sagittal lengths for anterior and
   posterior vaginal walls were 63 +/- 9 and 98 +/- 18 mm respectively. The
   vaginal surface area was 72 +/- 21 cm(2) (range: 34 similar to aEuro
   parts per thousand 164 cm(2)). The coefficient of determination between
   any demographic variable and any vaginal dimension did not exceed 0.16.
   Large variations in normal vaginal shape, axis, and dimensions were not
   explained by body size or other demographic variables. This variation
   has implications for reconstructive surgery, intravaginal and surgical
   product design, and vaginal drug delivery.}},
