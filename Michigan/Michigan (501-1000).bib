@article{ ISI:000375814200015,
Author = {Jin, K. and Lu, C. and Wang, L. M. and Qu, J. and Weber, W. J. and
   Zhang, Y. and Bei, H.},
Title = {{Effects of compositional complexity on the ion-irradiation induced
   swelling and hardening in Ni-containing equiatomic alloys}},
Journal = {{SCRIPTA MATERIALIA}},
Year = {{2016}},
Volume = {{119}},
Pages = {{65-70}},
Month = {{JUL 1}},
Abstract = {{The impact of compositional complexity on the ion-irradiation induced
   swelling and hardening is studied in Ni and six Ni-containing equiatomic
   alloys with face-centered cubic structure. The irradiation resistance at
   the temperature of 500 degrees C is improved by controlling the number
   and, especially, the type of alloying elements. Alloying with Fe and Mn
   has a stronger influence on swelling reduction than does alloying with
   Co and Cr. The quinary alloy NiCoFeCrMn, with known excellent mechanical
   properties, has shown 40 times higher swelling tolerance than nickel.
   (C) 2016 Elsevier Ltd. All rights reserved.}},
