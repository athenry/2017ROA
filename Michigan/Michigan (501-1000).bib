@article{ ISI:000373612500024,
Author = {Zhang, F. X. and Tracy, C. L. and Lang, M. and Ewing, R. C.},
Title = {{Stability of fluorite-type La2Ce2O7 under extreme conditions}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2016}},
Volume = {{674}},
Pages = {{168-173}},
Month = {{JUL 25}},
Abstract = {{The structural stability of fluorite-type La2Ce2O7 was studied at
   pressure up to similar to 40 GPa and under hydrothermal conditions of 1
   GPa and up to 350 degrees C, respectively, using synchrotron X-ray
   diffraction (XRD) and Raman scattering measurements. XRD measurements
   indicated that the fluorite-type La2Ce2O7 is not stable at pressures
   greater than 22.6 GPa and gradually transformed to a high-pressure
   phase. The high-pressure phase is not stable and changeed back to the
   fluorite-type structure when pressure is released. The La2Ce2O7 fluorite
   is also not stable under hydrothermal conditions and began to react with
   water at 200-250 degrees C. Both Raman and XRD results suggest that
   lanthanum hydroxide La(OH)(3) and La3+-doped CeO2 fluorite are the
   dominant products after hydrothermal treatment. (C) 2016 Elsevier B.V.
   All rights reserved.}},
