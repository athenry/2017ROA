@article{ ISI:000379175000004,
Author = {Tang, J. J. and Bai, Y. and Liu, K. and Zhang, P. and Li, B. Q. and
   Yang, J. F.},
Title = {{Microstructural Evolution of SAPS/HVOF CoNiCrAlY Alloy Coating During
   Thermal Cycling Test}},
Journal = {{OXIDATION OF METALS}},
Year = {{2016}},
Volume = {{86}},
Number = {{1-2}},
Pages = {{75-87}},
Month = {{AUG}},
Abstract = {{In this paper, CoNiCrAlY alloy coatings were deposited by
   high-efficiency supersonic atmospheric plasma spraying (SAPS) and
   high-velocity oxygen fuel (HVOF) spraying. The microstructural evolution
   of coatings during thermal cycling test was investigated. The results
   suggested that the as-sprayed SAPS coating consisted of lamellar
   structures and unmelted particles. However, the as-sprayed HVOF coating
   primarily consisted of the unmelted particles. The beta-NiAl phase
   mainly existed in the unmelted particles, and its content increased with
   the increase of unmelted particles. The thermal cycling life of SAPS
   coating was 258 cycles, about 117 \% higher than that of HVOF coating.
   During thermal cycling, significant internal oxidation and large cracks
   formed in the HVOF coating, which was one of the reasons that led to the
   spallation of HVOF coating.}},
