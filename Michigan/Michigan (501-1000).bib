@article{ ISI:000381456000001,
Author = {Churchill, Nathan W. and Spring, Robyn and Grady, Cheryl and Cimprich,
   Bernadine and Askren, Mary K. and Reuter-Lorenz, Patricia A. and Jung,
   Mi Sook and Peltier, Scott and Strother, Stephen C. and Berman, Marc G.},
Title = {{The suppression of scale-free fMRI brain dynamics across three different
   sources of effort: aging, task novelty and task difficulty}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2016}},
Volume = {{6}},
Month = {{AUG 8}},
Abstract = {{There is growing evidence that fluctuations in brain activity may
   exhibit scale-free ({''}fractal{''}) dynamics. Scale-free signals follow
   a spectral-power curve of the form P(f) proportional to f(-beta), where
   spectral power decreases in a power-law fashion with increasing
   frequency. In this study, we demonstrated that fractal scaling of BOLD
   fMRI signal is consistently suppressed for different sources of
   cognitive effort. Decreases in the Hurst exponent (H), which quantifies
   scale-free signal, was related to three different sources of cognitive
   effort/task engagement: 1) task difficulty, 2) task novelty, and 3)
   aging effects. These results were consistently observed across multiple
   datasets and task paradigms. We also demonstrated that estimates of H
   are robust across a range of time-window sizes. H was also compared to
   alternative metrics of BOLD variability (SDBOLD) and global connectivity
   (Gconn), with effort-related decreases in H producing similar decreases
   in SDBOLD and Gconn. These results indicate a potential global brain
   phenomenon that unites research from different fields and indicates that
   fractal scaling may be a highly sensitive metric for indexing cognitive
   effort/task engagement.}},
