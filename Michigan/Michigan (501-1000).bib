@article{ ISI:000375186700099,
Author = {Yang, Le and Li, Yongdan and Savage, Phillip E.},
Title = {{Near- and supercritical ethanol treatment of biocrude from hydrothermal
   liquefaction of microalgae}},
Journal = {{BIORESOURCE TECHNOLOGY}},
Year = {{2016}},
Volume = {{211}},
Pages = {{779-782}},
Month = {{JUL}},
Abstract = {{Biocrude produced from algae by hydrothermal liquefaction was treated
   with near- and supercritical ethanol and ethanol-water mixtures at
   210-290 degrees C for 0.5-4 h. Longer reaction times and higher
   temperatures better promoted esterification reactions. Dilution of the
   ethanol with water led to lower yields of treated biocrude and reduced
   ester content. The viscosity of treated biocrude was an order of
   magnitude lower than that of the crude bio-oil, and the treated biocrude
   exhibited the characteristics of a Newtonian fluid. Overall, treatment
   of biocrude with near-and supercritical ethanol generated a treated
   bio-oil with lower viscosity, more ester content, and in nearly 100 wt\%
   yield. (C) 2016 Elsevier Ltd. All rights reserved.}},
