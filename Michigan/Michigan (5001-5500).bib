@article{ ISI:000335887100009,
Author = {Hashemi, Mohammed Reza M. and Berry, Christopher W. and Merced,
   Emmanuelle and Sepulveda, Nelson and Jarrahi, Mona},
Title = {{Direct Measurement of Vanadium Dioxide Dielectric Properties in W-band}},
Journal = {{JOURNAL OF INFRARED MILLIMETER AND TERAHERTZ WAVES}},
Year = {{2014}},
Volume = {{35}},
Number = {{5}},
Pages = {{486-492}},
Month = {{MAY}},
Abstract = {{In this paper we directly measure the refractive index and extinction
   coefficient of vanadium dioxide (VO2) in W-band (75-110 GHz) near the
   insulator-to-metal phase transition. A frequency-domain interferometry
   setup is used to obtain the transmission amplitude and phase shift of a
   transmitted wave though a 200 nm-thick VO2 film deposited on a silicon
   substrate. Using the transmission matrix technique, the measured data in
   the 58-100 A degrees C temperature range are used to obtain the
   refractive index and extinction coefficient of VO2 as a function of
   temperature and frequency. The experimental results show a significant
   increase in the refractive index and extinction coefficient of VO2
   beyond the transition temperature of similar to 68 A degrees C.}},
