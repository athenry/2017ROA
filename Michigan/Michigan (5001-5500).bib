@article{ ISI:000337137900020,
Author = {Kundu, Soumya and Hiskens, Ian A.},
Title = {{Overvoltages due to Synchronous Tripping of Plug-in Electric-Vehicle
   Chargers Following Voltage Dips}},
Journal = {{IEEE TRANSACTIONS ON POWER DELIVERY}},
Year = {{2014}},
Volume = {{29}},
Number = {{3}},
Pages = {{1147-1156}},
Month = {{JUN}},
Abstract = {{Plug-in electric vehicle (PEV) charging equipment incorporates
   protection that ensures that grid disturbances do not damage the charger
   or vehicle. When the grid voltage sags below 80\% of nominal,
   undervoltage protection is likely to disconnect the charging load from
   the grid. Most PEV charging will occur overnight, when non-PEV load is
   at a minimum. This paper argues that PEV voltage-sag response, when
   synchronized across large numbers of PEVs, could result in the loss of a
   significant proportion of the total load. It is shown that this load
   loss can lead to unacceptably high voltages once the initiating event
   has been cleared. This paper explores the nature of this voltage-rise
   phenomenon. Analysis tools are developed to assist in determining PEV
   loading conditions that demarcate acceptable postdisturbance voltage
   response from unacceptable outcomes. Two examples, based on standard
   distribution test systems, are used to illustrate PEV-induced
   overvoltage behavior, and demonstrate applications of the analysis
   tools.}},
