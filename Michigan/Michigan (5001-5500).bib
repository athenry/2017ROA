@article{ ISI:000334526200026,
Author = {Hendarto, Erwin and Gianchandani, Yogesh B.},
Title = {{Thermocapillary Actuation of Millimeter-Scale Rotary Structures}},
Journal = {{JOURNAL OF MICROELECTROMECHANICAL SYSTEMS}},
Year = {{2014}},
Volume = {{23}},
Number = {{2}},
Pages = {{494-499}},
Month = {{APR}},
Abstract = {{In this paper, we describe the rotary motion, by Marangoni flow, of a
   millimeter-scale rotary structure immersed in a thin layer of liquid. A
   16 x 8 array of 1 x 0.8 x 0.3 mm(3) surface-mount resistors is suspended
   approximate to 500 mu m above the liquid to serve as a programmable heat
   source. The continuous operation of resistor elements is used to impose
   a spatially-defined temperature gradient on the surface of the liquid.
   With a maximum temperature gradient of 36.6 K/mm at the surface of a 2
   mm-thick film of liquid with viscosity 5 cSt, a stainless steel rotary
   structure with a weight of approximate to 10 mg, a diameter of 4.1 mm,
   and blade angle of 34 degrees takes 28 s to make a 360 degrees rotation.
   In general, the angular velocity of the rotary structure is affected by
   the temperature gradient of the liquid surface and liquid viscosity
   among several factors. {[}2013-0034]}},
Publisher = {{IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC}},
Address = {{445 HOES LANE, PISCATAWAY, NJ 08855-4141 USA}},
Type = {{Article}},
