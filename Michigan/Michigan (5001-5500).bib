@article{ ISI:000332523300011,
Author = {Zhou, Hongjian and Kim, Jong-Pil and Bahng, Joong Hwan and Kotov,
   Nicholas A. and Lee, Jaebeom},
Title = {{Self- Assembly Mechanism of Spiky Magnetoplasmonic Supraparticles}},
Journal = {{ADVANCED FUNCTIONAL MATERIALS}},
