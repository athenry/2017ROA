@article{ ISI:000331782500012,
Author = {Pojanavaraphan, Chinchanop and Nakaranuwattana, Warapun and
   Luengnaruemitchai, Apanee and Gulari, Erdogan},
Title = {{Effect of support composition and metal loading on Au/Ce1-xZrxO2
   catalysts for the oxidative steam reforming of methanol}},
Journal = {{CHEMICAL ENGINEERING JOURNAL}},
Year = {{2014}},
Volume = {{240}},
Pages = {{99-108}},
Month = {{MAR 15}},
Abstract = {{Gold (Au) deposited on CeO2-ZrO2 mixed oxides, prepared by the
   co-precipitation (CP) technique, was tested for oxidative steam
   reforming of methanol (OSRM) in the temperature range of 200-400 degrees
   C. An active uniform Ce1-xZrxO2 solid solution was found to be dependent
   on the Zr/(Ce + Zr) ratio and the calcination temperature of the
   support, where a ratio of 0.25 calcined at 400 degrees C efficiently
   performed Zr4+ incorporation inside the Ce4+ lattice to produce smaller
   ceria crystallites and higher surface area. The highest methanol
   conversion obtained was 92\% at 400 degrees C, while Hy and CO
