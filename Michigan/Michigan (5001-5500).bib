@article{ ISI:000336456500006,
Author = {Liu, Xun and Lan, Shuhuai and Ni, Jun},
Title = {{Analysis of process parameters effects on friction stir welding of
   dissimilar aluminum alloy to advanced high strength steel}},
Journal = {{MATERIALS \& DESIGN}},
Year = {{2014}},
Volume = {{59}},
Pages = {{50-62}},
Month = {{JUL}},
Abstract = {{Thin sheets of aluminum alloy 6061-T6 and one type of Advanced high
   strength steel, transformation induced plasticity (TRIP) steel have been
   successfully butt joined using friction stir welding (FSW) technique.
   The maximum ultimate tensile strength can reach 85\% of the base
   aluminum alloy. Intermetallic compound (IMC) layer of FeAl or Fe3Al with
   thickness of less than 1 mu m was formed at the Al-Fe interface in the
   advancing side, which can actually contribute to the joint strength.
   Tensile tests and scanning electron microscopy (SEM) results indicate
   that the weld nugget can be considered as aluminum matrix composite,
   which is enhanced by dispersed sheared-off steel fragments encompassed
   by a thin intermetallic layer or simply intermetallic particles. Effects
   of process parameters on the joint microstructure evolution were
   analyzed based on mechanical welding force and temperature that have
   been measured during the welding process. (C) 2014 Elsevier Ltd. All
   rights reserved.}},
