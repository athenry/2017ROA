@article{ ISI:000336249600017,
Author = {Kang, M. and Beskin, I. and Al-Heji, A. A. and Shende, O. and Huang, S.
   and Jeon, S. and Goldman, R. S.},
Title = {{Evolution of ion-induced nanoparticle arrays on GaAs surfaces}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2014}},
Volume = {{104}},
Number = {{18}},
Month = {{MAY 5}},
Abstract = {{We have examined the evolution of irradiation-induced Ga nanoparticle
   (NP) arrays on GaAs surfaces. Focused-ion-beam irradiation of
   pre-patterned GaAs surfaces induces monotonic increases in the NP volume
   and aspect ratio up to a saturation ion dose, independent of NP location
   within the array. Beyond the saturation ion dose, the NP volume
   continues to increase monotonically while the NP aspect ratio decreases
   monotonically. In addition, the NP volumes (aspect ratios) are highest
   (lowest) for the corner NPs. We discuss the relative influences of bulk
   and surface diffusion on the evolution of Ga NP arrays. (C) 2014 AIP
   Publishing LLC.}},
