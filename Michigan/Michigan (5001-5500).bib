@article{ ISI:000337018800024,
Author = {Jiao, Z. and Was, G. S.},
Title = {{Precipitate behavior in self-ion irradiated stainless steels at high
   doses}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2014}},
Volume = {{449}},
Number = {{1-3}},
Pages = {{200-206}},
Month = {{JUN}},
Note = {{Proceedings of the Sixth Symposium on Microstructural Processes in
   Irradiated Materials (MPIM) Preface, San Antonio, TX, MAR 03-07, 2013}},
Abstract = {{To study radiation-induced precipitation at high doses, solution
   annealed 304L SS and cold worked 316 SS were irradiated to 46 and 260
   dpa at 380 degrees C using 5 MeV Fe++ and the radiation-induced
   precipitates were examined using atom probe tomography. Ni/Si-rich
   clusters were observed in all examined conditions. G-phase precipitates
   were observed in 316 SS at 46 dpa but only appeared in 3041 SS at 260
   dpa. Using the neutron irradiation to 46 dpa at 320 degrees C as a
   reference, the temperature shift for cold worked 316 SS appeared to be
   smaller than that of solution annealed 304L SS, probably due to the high
   density of dislocations, which served as defect sinks and mitigated the
   effect of high dose rate. (C) 2014 Elsevier B.V. All rights reserved.}},
