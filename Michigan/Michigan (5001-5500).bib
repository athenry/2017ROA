@article{ ISI:000337984100018,
Author = {Su, Weihua and Cesnik, Carlos E. S.},
Title = {{Strain-Based Analysis for Geometrically Nonlinear Beams: A Modal
   Approach}},
Journal = {{JOURNAL OF AIRCRAFT}},
Year = {{2014}},
Volume = {{51}},
Number = {{3}},
Pages = {{890-903}},
Month = {{MAY-JUN}},
Note = {{53rd AIAA/ASME/ASCE/AHS/ASC Structures, Structural Dynamics and
   Materials Conference, Honolulu, HI, APR 22-26, 2012}},
Organization = {{AIAA; ASME; ASCE; AHS; ASC}},
Abstract = {{A strain-based geometrically nonlinear finite element beam formulation
   for structural dynamic and aeroelastic analysis of slender beams and
   wings has been successfully applied to study the coupled nonlinear
   aeroelasticity and flight dynamics of different, very flexible aircraft.
   To improve the ability to use such aeroelastic framework in control
   design, a modal-based approach is developed in this paper to solve the
   strain-based nonlinear beam equation's with a reduced number of degrees
   of freedom. The modal approach is applied to study geometrically
   nonlinear static and transient problems of constrained and free slender
   beams, subject to external structural or aerodynamic excitations.
   Accuracy and performance of the new formulation are compared with the
   direct nonlinear finite element procedure.}},
