@article{ ISI:000336344200002,
Author = {Kim, Wonsuk and Argento, Alan and Mohanty, Pravansu S.},
Title = {{Damping characteristics of a spray-deposited shape memory alloy beam}},
Journal = {{JOURNAL OF SOUND AND VIBRATION}},
Year = {{2014}},
Volume = {{333}},
Number = {{15}},
Pages = {{3356-3366}},
Month = {{JUL 21}},
Abstract = {{The damping characteristics of an Ni-Ti shape memory alloy (SMA) beam
   are theoretically and experimentally studied with interest in
   identifying an appropriate damping model for the material. The SMA beam
   is manufactured by a spray deposition method followed by heat treatment
   and found to have nanocrystalline structure in which damping capacity is
   high. The beam is then tested to obtain an impulse response and the
   frequency response function (FRF). By using the Hilbert transform
   technique it is shown that damping of the beam is almost amplitude
   independent in the tested range of displacement. It is also shown from
   the FRF that the damping of the spray-deposited shape memory alloy beam
   is well represented by a model including both linear viscous and
   hysteretic dampings. (C) 2014 Elsevier Ltd. All rights reserved.}},
