@article{ ISI:000320130500049,
Author = {Luengo-Kovac, Marta and Saucer, Timothy W. and Martin, Andrew J. and
   Millunchick, Joanna and Sih, Vanessa},
Title = {{Analyzing pattern retention for multilayer focused ion beam induced
   quantum dot structures}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY B}},
Year = {{2013}},
Volume = {{31}},
Number = {{3}},
Month = {{MAY}},
Abstract = {{Atomic force microscopy was used to investigate the effects of
   templating parameters on focused ion beam patterned single-, two-, and
   three-layer InAs/GaAs(001) quantum dot structures. The number of layers,
   focused ion beam dwell time, and pattern spacing affected the fidelity
   of the quantum dots. The highest single dot fidelities were found in
   regions with 1 and 3ms dwell times and 1 and 2 mu m pattern spacings. A
   two-layer region patterned with 1ms dwell time and 1 mu m spacing was
   found to have 100\% single quantum dot fidelity with no off-site dot
   nucleation in a 20 x 20 mu m(2) scan. Holes that were milled with 6 and
   9ms dwell times and 0.25 mu m spacing became faceted, that is, deep,
   tightly packed, and rhombic, by the third layer. Autocorrelation of the
   images was used to analyze the periodicity and size of the features. (C)
   2013 American Vacuum Society.}},
