   Wei, H. and Eccles, V. and Horton, W. and Mays, M. L. and Gannon, J.},
Title = {{Geospace environment modeling 2008-2009 challenge: D-st index}},
Journal = {{SPACE WEATHER-THE INTERNATIONAL JOURNAL OF RESEARCH AND APPLICATIONS}},
