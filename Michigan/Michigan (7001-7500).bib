Language = {{English}},
Affiliation = {{Gong, W (Reprint Author), Tsinghua Univ, State Key Lab Hydrosci \& Engn, Beijing 100084, Peoples R China.
   Gong, Wei; Yang, Dawen, Tsinghua Univ, State Key Lab Hydrosci \& Engn, Beijing 100084, Peoples R China.
   Gupta, Hoshin V., Univ Arizona, Dept Hydrol \& Water Resources, Tucson, AZ 85721 USA.
   Sricharan, Kumar; Hero, Alfred O., III, Univ Michigan, Dept Elect Engn \& Comp Sci EECS, Ann Arbor, MI 48109 USA.}},
DOI = {{10.1002/wrcr.20161}},
ISSN = {{0043-1397}},
Keywords = {{information theory; model structure adequacy; uncertainty analysis;
