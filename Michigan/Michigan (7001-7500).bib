@article{ ISI:000319004700001,
Author = {Engel, Michael and Anderson, Joshua A. and Glotzer, Sharon C. and Isobe,
   Masaharu and Bernard, Etienne P. and Krauth, Werner},
Title = {{Hard-disk equation of state: First-order liquid-hexatic transition in
   two dimensions with three simulation methods}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2013}},
Volume = {{87}},
Number = {{4}},
Month = {{APR 30}},
Abstract = {{We report large-scale computer simulations of the hard-disk system at
   high densities in the region of the melting transition. Our simulations
   reproduce the equation of state, previously obtained using the
   event-chain Monte Carlo algorithm, with a massively parallel
   implementation of the local Monte Carlo method and with event-driven
   molecular dynamics. We analyze the relative performance of these
   simulation methods to sample configuration space and approach
   equilibrium. Our results confirm the first-order nature of the melting
   phase transition in hard disks. Phase coexistence is visualized for
   individual configurations via the orientational order parameter field.
   The analysis of positional order confirms the existence of the hexatic
   phase.}},
