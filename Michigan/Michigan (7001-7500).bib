@article{ ISI:000316469500007,
Author = {Chen, Zheng and Fu, Yuhong and Mi, Chunting Chris},
Title = {{State of Charge Estimation of Lithium-Ion Batteries in Electric Drive
   Vehicles Using Extended Kalman Filtering}},
Journal = {{IEEE TRANSACTIONS ON VEHICULAR TECHNOLOGY}},
Year = {{2013}},
Volume = {{62}},
Number = {{3}},
Pages = {{1020-1030}},
Month = {{MAR}},
Abstract = {{In this paper, a more accurate battery state of charge (SOC) estimation
   method for electric drive vehicles is developed based on a nonlinear
   battery model and an extended Kalman filter (EKF) supported by
   experimental data. A nonlinear battery model is constructed by
   separating the model into a nonlinear open circuit voltage and a
   two-order resistance-capacitance model. EKF is used to eliminate the
   measurement and process noise and remove the need of prior knowledge of
   initial SOC. A hardware-in-the-loop test bench was built to validate the
   method. The experimental results show that the proposed method can
   estimate the battery SOC with high accuracy.}},
