@article{ ISI:000379630200011,
Author = {Zhao, Wanqin and Wang, Wenjun and Li, Ben Q. and Jiang, Gedong and Mei,
   Xuesong},
Title = {{Wavelength effect on hole shapes and morphology evolution during
   ablation by picosecond laser pulses}},
Journal = {{OPTICS AND LASER TECHNOLOGY}},
Year = {{2016}},
Volume = {{84}},
Pages = {{79-86}},
Month = {{OCT}},
Abstract = {{An experimental study is presented of the effect of wavelength on the
   shape and morphology evolution of micro holes ablated on stainless steel
   surface by a 10 ps Q-switched Nd:VAN pulsed laser. Two routes of hole
   development are associated with the visible (532 nm) and near-infrared
   (1064 nm) laser beams, respectively. The evolution of various geometric
