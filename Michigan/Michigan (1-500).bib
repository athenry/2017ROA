@article{ ISI:000381528900033,
Author = {Kern, William T. and Kim, Wonsuk and Argento, Alan and Lee, Ellen C. and
   Mielewski, Deborah F.},
Title = {{Finite element analysis and microscopy of natural fiber composites
   containing microcellular voids}},
Journal = {{MATERIALS \& DESIGN}},
Year = {{2016}},
Volume = {{106}},
Pages = {{285-294}},
Month = {{SEP 15}},
Abstract = {{Mechanical behaviors of chopped, natural fiber composites with and
   without closed-cell microcellular foamed structures are investigated
   using finite element analysis (FEA), with references to previously
   conducted experimental measurements of the stress-strain characteristics
   of analogous test specimens and their microscopic images. Analytical and
   FEA models are compared for a simple, aligned fiber case; the FEA models
   are then extended to composites having more general fiber arrangements.
   These FEA results are compared to experimental stress strain data.
   Scanning electron microscopy (SEM) images of fracture surfaces from test
   specimens are analyzed, and correlations between numerically predicted
   behaviors and fracture paths, and fracture surface images are discussed.
   The FEA results reflect experimental trends well including the effects
   of fibers and the presence of voids. These have also been found to
   induce specific regions of stress concentration. Inspection of fracture
   surfaces reveals transition from ductile to brittle behavior at low
   strain rates, generally brittle failure modes, and little evidence of
   fibers stressed to their tensile limits. (C) 2016 Elsevier Ltd. All
   rights reserved.}},
