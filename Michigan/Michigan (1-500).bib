@article{ ISI:000388500900031,
Author = {Wang, Xiaoyu and Mortazawi, Amir},
Title = {{Bandwidth Enhancement of RF Resonators Using Duffing Nonlinear Resonance
   for Wireless Power Applications}},
Journal = {{IEEE TRANSACTIONS ON MICROWAVE THEORY AND TECHNIQUES}},
Year = {{2016}},
Volume = {{64}},
Number = {{11, 1}},
Pages = {{3695-3702}},
Month = {{NOV}},
Abstract = {{This paper presents a nonlinear resonator circuit based on the Duffing
   equation to enhance the bandwidth of wireless power harvesting and
   wireless power transfer systems. The Duffing resonator circuit can be
   implemented with passive components. In this paper, analysis of an
   electrical Duffing resonator is presented. The detailed discussion of
   the Duffing resonator's properties is provided. As a proof of principle,
   a wireless power harvester employing such a nonlinear resonator has been
   designed and tested. It is shown that the Duffing resonator circuit
   enhances the bandwidth of the harvester by 2.7 times compared with a
   harvester using a linear resonator with the same quality factor, while
   achieving a similar amplitude level near the resonance frequency.
   Furthermore, the bandwidth of the nonlinear resonator is independent of
   its quality factor (Q).}},
