@article{ ISI:000384713100074,
Author = {Gamage, Nipuni-Dhanesha H. and McDonald, Kyle A. and Matzger, Adam J.},
Title = {{MOF-5-Polystyrene: Direct Production from Monomer, Improved Hydrolytic
   Stability, and Unique Guest Adsorption}},
Journal = {{ANGEWANDTE CHEMIE-INTERNATIONAL EDITION}},
Year = {{2016}},
Volume = {{55}},
Number = {{39}},
Pages = {{12099-12103}},
Month = {{SEP 19}},
Abstract = {{An unprecedented mode of reactivity of Zn4O-based metal-organic
   frameworks (MOFs) offers a straightforward and powerful approach to
   polymer-hybridized porous solids. The concept is illustrated with the
   production of MOF-5-polystyrene wherein polystyrene is grafted and
   uniformly distributed throughout MOF-5 crystals after heating in pure
   styrene for 4-24 h. The surface area and polystyrene content of the
   material can be fine-tuned by controlling the duration of heating
   styrene in the presence of MOF-5. Polystyrene grafting significantly
   alters the physical and chemical properties of pristine MOF-5, which is
   evident from the unique guest adsorption properties (solvatochromic dye
   uptake and improved CO2 capacity) as well as the dramatically improved
   hydrolytic stability of composite. Based on the fact that MOF-5 is the
   best studied member of the structure class, and has been produced at
   scale by industry, these findings can be directly leveraged for a range
   of current applications.}},
