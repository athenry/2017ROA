@article{ ISI:000389199800001,
Author = {Walker, II, R. C. and Shi, T. and Silva, E. C. and Jovanovic, I. and
   Robinson, J. A.},
Title = {{Radiation effects on two-dimensional materials}},
Journal = {{PHYSICA STATUS SOLIDI A-APPLICATIONS AND MATERIALS SCIENCE}},
Year = {{2016}},
Volume = {{213}},
Number = {{12}},
Pages = {{3065-3077}},
Month = {{DEC}},
Abstract = {{The effects of electromagnetic and particle irradiation on
   twodimensional materials (2DMs) are discussed in this review. Radiation
   creates defects that impact the structure and electronic performance of
   materials. Determining the impact of these defects is important for
   developing 2DM-based devices for use in high-radiation environments,
   such as space or nuclear reactors. As such, most experimental studies
   have been focused on determining total ionizing dose damage to 2DMs and
   devices. Total dose experiments using X-rays, gamma rays, electrons,
   protons, and heavy ions are summarized in this review. We briefly
   discuss the possibility of investigating single event effects in 2DMs
   based on initial ion beam irradiation experiments and the development of
   2DM-based integrated circuits. Additionally, beneficial uses of
   irradiation such as ion implantation to dope materials or electron-beam
   and helium-beam etching to shape materials have begun to be used on 2DMs
   and are reviewed as well. For non-ionizing radiation, such as low-energy
   photons, we review the literature on 2DM-based photo-detection from
   terahertz to UV. The majority of photo-detecting devices operate in the
   visible and UV range, and for this reason they are the focus of this
   review. However, we review the progress in developing 2DMs for detecting
   infrared and terahertz radiation. (C) 2016 WILEY-VCH Verlag GmbH \& Co.
   KGaA, Weinheim}},
