@article{ ISI:000383296300014,
Author = {Leistner, T. and Embrechts, M. and Leissner, T. and Chelgani, S. Chehreh
   and Osbahr, I. and Moeckel, R. and Peuker, U. A. and Rudolph, M.},
Title = {{A study of the reprocessing of fine and ultrafine cassiterite from
   gravity tailing residues by using various flotation techniques}},
Journal = {{MINERALS ENGINEERING}},
Year = {{2016}},
Volume = {{96-97}},
Number = {{SI}},
Pages = {{94-98}},
Month = {{OCT}},
Note = {{7th International Flotation Conference (Flotation), Cape Town, SOUTH
   AFRICA, NOV 16-19, 2015}},
Abstract = {{This study investigates the reprocessing of once disposed tin tailings
   from a historic German tin operation through different surface
   properties based processing techniques. Froth flotation and
   agglomeration flotation, by using commercially available cassiterite
   collectors from Clariant, were chosen as processing techniques.
   Isooctane as a nonpolar oil was used to promote the collection of
   ultrafine (-10 mu m) cassiterite by selective agglomeration, and thus,
   size enlargement. Results indicate that by using sulfosuccinamates as a
   collector, around 80\% of the fine (-50 mu m) to very fine (-20 mu m)
   cassiterite can be recovered (representing similar to 50\% of the total
   cassiterite in the tailings sample). Agglomeration flotation experiments
   showed inferior results for recovering the ultrafine cassiterite (for
   feed fractions in the similar to 25 mu m size range). Oil/froth
   interaction and increased pulp ion content are considered as the main
   contributing causes. (C) 2016 Elsevier Ltd. All rights reserved.}},
