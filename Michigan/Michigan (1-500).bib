@article{ ISI:000390072100015,
Author = {Jiang, Tonghu and Rudraraju, Shiva and Roy, Anindya and Van der Ven,
   Anton and Garikipati, Krishna and Falk, Michael L.},
Title = {{Multiphysics SimulatiOns of Lithiation-Induced Stress in Li1+xTi2O4
   Electrode Particles}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2016}},
Volume = {{120}},
Number = {{49}},
Pages = {{27871-27881}},
Month = {{DEC 15}},
Abstract = {{Cubic spinel Li1+xTi2O4 is a promising electrode material because it
   exhibits a high lithium diffusivity and undergoes minimal changes in
   lattice parameters during lithiation and delithiation, thereby ensuring
   favorable cyclability. The present work is a multiphysics and multiscale
   study of Li1+xTi2O4 that combines first-principles computations of
   thermodynamic and kinetic properties with continuum scale modeling of
   lithiationdelithiation kinetics. Density functional theory calculations
   and statistical mechanics methods are used to calculate lattice
   parameters, elastic coefficients, thermodynamic potentials, migration
   barriers, and Li diffusion coefficients. These computations were
   performed at a temperature of 800 K to magnify the kinetic effects.
