@article{ ISI:000388414600017,
Author = {Fisher, Alexander A. and Dreyer, Elizabeth F. C. and Chakrabarty, Ayan
   and Rand, Stephen C.},
Title = {{Optical magnetization, Part II: Theory of induced optical magnetism}},
Journal = {{OPTICS EXPRESS}},
Year = {{2016}},
Volume = {{24}},
Number = {{23}},
Pages = {{26064-26079}},
Month = {{NOV 14}},
Abstract = {{A fully quantized analysis is presented on the origin of induced
   magnetic dipole (MD) scattering in two-level diatomic molecules. The
   interaction is driven by dual optical fields, E and H{*}, and is
   universally allowed in dielectric optical materials, including
   centrosymmetric media. Leading terms of the interaction are shown to be
   quadratic and cubic with respect to the intensity, predicting an upper
   limit for the induced magnetic dipole scattering intensity (I-MD
   proportional to m(2)) that is equal to the electric dipole scattering
   (I-ED proportional to p(2)). The optical dynamics proceed by first
   establishing an electric polarization in the system. Then the magnetic
   field exerts torque on the orbital angular momentum of the excited
   state, mediating an exchange of orbital and rotational angular momenta
   that enhances the magnetic moment. The magneto-electric interaction also
   accounts for second-order, unpolarized scattering from high-frequency
   librations previously ascribed to third-order, all-electric processes.
   (C) 2016 Optical Society of America.}},
