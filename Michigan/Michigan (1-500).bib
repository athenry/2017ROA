@article{ ISI:000385339900002,
Author = {Fu, Pingqing and Kawamura, Kimitaka and Seki, Osamu and Izawa, Yusuke
   and Shiraiwa, Takayuki and Ashworth, Kirsti},
Title = {{Historical Trends of Biogenic SOA Tracers in an Ice Core from Kamchatka
   Peninsula}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY LETTERS}},
Year = {{2016}},
Volume = {{3}},
Number = {{10}},
Pages = {{351-358}},
Month = {{OCT}},
Abstract = {{Biogenic secondary organic aerosol (SOA) is ubiquitous in the Earth's
   atmosphere, influencing climate and air quality. However, the historical
   trend of biogenic SOA is not well known. Here, we report for the first
   time the major isoprene- and monoterpene-derived SOA tracers preserved
   in an ice core from the Kamchatka Peninsula. Significant variations are
   recorded during the past 300 years with lower concentrations in the
   early-to-middle 19th century and higher concentrations in the
   preindustrial period and the present day. We discovered that isoprene
   SOA tracers were more abundant in the preindustrial period than the
   present day, while monoterpene SOA tracers stay almost unchanged. The
   causes of the observed variability are complex, depending on atmospheric
   circulation, changes in emissions, and other factors such as
   tropospheric oxidative capacity. Our data presents an unprecedented
   opportunity to shed light on the formation, evolution, and fate of
   atmospheric aerosols and to constrain the uncertainties associated with
   modeling their atmospheric concentrations.}},
