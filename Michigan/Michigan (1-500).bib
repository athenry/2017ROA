@article{ ISI:000388221300002,
Author = {DeJarld, Matt and Teran, Alan and Luengo-Kovac, Marta and Yan, Lifan and
   Moon, Eun Seong and Beck, Sara and Guillen, Cristina and Sih, Vanessa
   and Phillips, Jamie and Milunchick, Joanna Mirecki},
Title = {{The effect of doping on low temperature growth of high quality GaAs
   nanowires on polycrystalline films}},
Journal = {{NANOTECHNOLOGY}},
Year = {{2016}},
Volume = {{27}},
Number = {{49}},
Month = {{DEC 9}},
Abstract = {{The increasing demand for miniature autonomous sensors requires low cost
   integration methods, but to date, material limitations have prevented
   the direct growth of optically active III-V materials on CMOS devices.
   We report on the deposition of GaAs nanowires on polycrystalline
   conductive films to allow for direct integration of optoelectronic
   devices on dissimilar materials. Undoped, Si-doped, and Be-doped
   nanowires were grown at T-s = 400 degrees C on oxide (indium tin oxide)
   and metallic (platinum and titanium) films. Be-doping is shown to
   significantly reduce the nanowire diameter and improve the nanowire
   aspect ratio to 50:1. Photoluminescence measurements of Be-doped
   nanowires are 1-2 orders of magnitude stronger than undoped and Si-doped
   nanowires and have a thermal activation energy of 14 meV, which is
   comparable to nanowires grown on crystalline substrates. Electrical
   measurements confirm that the metal-semiconductor junction is Ohmic.
   These results demonstrate the feasibility of integrating nanowire-based
   optoelectronic devices directly on CMOS chips.}},
