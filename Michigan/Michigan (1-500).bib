
@article{ ISI:000391026200002,
Author = {Mertens, Stephan and Ziff, Robert M.},
Title = {{Percolation in finite matching lattices}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2016}},
Volume = {{94}},
Number = {{6}},
Month = {{DEC 30}},
Abstract = {{We derive an exact, simple relation between the average number of
   clusters and the wrapping probabilities for two-dimensional percolation.
   The relation holds for periodic lattices of any size. It generalizes a
   classical result of Sykes and Essam, and it can be used to find exact or
   very accurate approximations of the critical density. The criterion that
   follows is related to the criterion used by Scullard and Jacobsen to
   find precise approximate thresholds, and our work provides a different
   perspective on their approach.}},
