@article{ ISI:000391853800003,
Author = {Fischer, E. and Martinez, G. M. and Renno, N. O.},
Title = {{Formation and Persistence of Brine on Mars: Experimental Simulations
   throughout the Diurnal Cycle at the Phoenix Landing Site}},
Journal = {{ASTROBIOLOGY}},
Year = {{2016}},
Volume = {{16}},
Number = {{12}},
Pages = {{937-948}},
Month = {{DEC}},
Abstract = {{In the last few years, water ice and salts capable of melting this ice
   and producing liquid saline water (brine) have been detected on Mars.
   Moreover, indirect evidence for brine has been found in multiple areas
   of the planet. Here, we simulate full diurnal cycles of temperature and
   atmospheric water vapor content at the Phoenix landing site for the
   first time and show experimentally that, in spite of the low Mars-like
   chamber temperature, brine forms minutes after the ground temperature
   exceeds the eutectic temperature of salts in contact with water ice.
   Moreover, we show that the brine stays liquid for most of the diurnal
   cycle when enough water ice is available to compensate for evaporation.
   This is predicted to occur seasonally in areas of the polar region where
   the temperature exceeds the eutectic value and frost or snow is
   deposited on saline soils, or where water ice and salts coexist in the
   shallow subsurface. This is important because the existence of liquid
   water is a key requirement for habitability.}},
