@article{ ISI:000385907400054,
Author = {Lee, Jiwon and Zhou, Menglian and Zhu, Hongbo and Nidetz, Robert and
   Kurabayashi, Katsuo and Fan, Xudong},
Title = {{Fully Automated Portable Comprehensive 2-Dimensional Gas Chromatography
   Device}},
Journal = {{ANALYTICAL CHEMISTRY}},
Year = {{2016}},
Volume = {{88}},
Number = {{20}},
Pages = {{10266-10274}},
Month = {{OCT 18}},
Abstract = {{We developed a fully automated portable 2-dimensional (2-D) gas
   chromatography (GC x GC) device, which had a dimension of 60 cm x 50 cm
   x 10 cm and weight less than 5 kg. The device incorporated a
   micropreconcentrator/injector, commercial columns, micro-Deans switches,
   microthermal injectors, microphotoionization detectors, data acquisition
   cards, and power supplies, as well as computer control and user
   interface. It employed multiple channels (4 channels) in the second
   dimension (D-2) to increase the D-2 separation time (up to 32 s) and
   hence D-2 peak capacity. In addition, a nondestructive flow-through
   vapor detector was installed at the end of the D-1 column to monitor the
   eluent from D-1 and assist in reconstructing D-1 elution peaks. With the
   information obtained jointly from the D-1 and D-2 detectors, D-1 elution
   peaks could be reconstructed with significantly improved D-1 resolution.
   In this Article, we first discuss the details of the system operating
   principle and the algorithm to reconstruct D-1 elution peaks, followed
   by the description and characterization of each component. Finally, 2-D
   separation of 50 analytes, including alkane (C-6-C-12), alkene, alcohol,
   aldehyde, ketone, cycloalkane, and aromatic hydrocarbon, in 14 min is
   demonstrated, showing the peak capacity of 430-530 and the peak capacity
   production of 40-80/min.}},
