@article{ ISI:000390227100010,
Author = {Hu, Hao and Ziff, Robert M. and Deng, Youjin},
Title = {{No-Enclave Percolation Corresponds to Holes in the Cluster Backbone}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2016}},
Volume = {{117}},
Number = {{18}},
Month = {{OCT 27}},
Abstract = {{The no-enclave percolation (NEP) model introduced recently by Sheinman
   et al. can be mapped to a problem of holes within a standard percolation
   backbone, and numerical measurements of such holes give the same
   size-distribution exponent tau = 1.82(1) as found for the NEP model. An
   argument is given that t = 1 + d(B)/2 approximate to 1.822 for backbone
   holes, where d(B) is the backbone dimension. On the other hand, a model
   of simple holes within a percolation cluster yields tau = 1 + d(f)/2
   187/96 approximate to 1.948, where d(f) is the fractal dimension of the
   cluster, and this value is consistent with the experimental results of
   gel collapse of Sheinman et al., which give tau = 1.91(6). This suggests
   that the gel clusters are of the universality class of percolation
   cluster holes. Both models give a discontinuous maximum hole size at
   p(c), signifying explosive percolation behavior.}},
