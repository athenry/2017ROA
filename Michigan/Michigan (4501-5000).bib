@article{ ISI:000343797500027,
Author = {Spak, Kaitlin S. and Agnes, Gregory S. and Inman, Daniel J.},
Title = {{Bakeout Effects on Dynamic Response of Spaceflight Cables}},
Journal = {{JOURNAL OF SPACECRAFT AND ROCKETS}},
Year = {{2014}},
Volume = {{51}},
Number = {{5}},
Pages = {{1721-1734}},
Month = {{SEP-OCT}},
Abstract = {{Spaceflight cables are investigated to determine the effect of bakeout
   on their dynamic response, including resonant frequencies and damping
   ratios. The addition of cable harnesses to spacecraft structures can
   affect the dynamic response of the entire structure, especially for
   lightweight structures with high cable mass ratios. Bakeout, a heat and
   vacuum treatment that spaceflight components must undergo, may change
   the dynamic stiffness of flight cables and thus the dynamics of the
   cabled host structure. Bakeout effects are examined by experimentally
   identifying natural frequencies and damping values for spaceflight
   cables before and after the bakeout process. After bakeout, the first
   natural frequency decreases by an average of 14\% for all single-strand
   cables and by 24\% for multistrand cables. The second natural frequency
   decreases by 8 to 17\% for all cables. Bakeout also increases the
   damping percentage for single and multistrand cables. These results show
   that bakeout affects the dynamic response of spaceflight cables
   significantly and should be taken into account when using cable data for
   design purposes.}},
