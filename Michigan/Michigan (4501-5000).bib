@article{ ISI:000345470400022,
Author = {Kim, Jae Hyung and Ahn, Young Ju and Jang, Yong Hoon and Barber, J. R.},
Title = {{Contact problems involving beams}},
Journal = {{INTERNATIONAL JOURNAL OF SOLIDS AND STRUCTURES}},
Year = {{2014}},
Volume = {{51}},
Number = {{25-26}},
Pages = {{4435-4439}},
Month = {{DEC}},
Abstract = {{Elastic contact problems involving Euler Bernoulli beams or Kirchhoff
   plates generally involve concentrated contact forces. Linear elasticity
   (e.g. finite element) solutions of the same problems show that finite
   contact regions are actually developed, but these regions have
   dimensions that are typically of the order of the beam thickness. Thus
   if beam theory is appropriate for a given structural problem, the local
   elasticity fields can be explored by asymptotic methods and will have
   fairly general (problem independent) characteristics. Here we show that
   the extent of the contact region is a fixed ratio of the beam thickness
   which is independent of the concentrated load predicted by the beam
   theory, and that the distribution of contact pressure in this region has
   a universal form, which is well approximated by a simple algebraic
   expression. (C) 2014 Elsevier Ltd. All rights reserved.}},
