@article{ ISI:000341259900002,
Author = {Yan, Qimin and Kioupakis, Emmanouil and Jena, Debdeep and Van de Walle,
   Chris G.},
Title = {{First-principles study of high-field-related electronic behavior of
   group-III nitrides}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2014}},
Volume = {{90}},
Number = {{12}},
Month = {{SEP 5}},
Abstract = {{Based on accurate band structures of AlN, GaN, and InN, we report
   physical quantities related to high-field electron transport, including
   effective masses, energies of inflection points, and satellite valleys
   in the conduction band. The band structures are obtained from density
   functional theory with a hybrid functional, as well as many-body
   perturbation theory based on the G(0)W(0) approach. We also calculate
   the electron-energy relaxation time due to the
   electron-longitudinal-optical-phonon interaction within the Frohlich
   model. Our results provide insights into the physical origin of negative
   differential resistance and the high-frequency characteristics of
   group-III nitrides and their alloys under high-field operation.}},
