@article{ ISI:000341895700012,
Author = {Zhang, Lin and Li, Yan and Li, Longchao and Guo, Baolin and Ma, Peter X.},
Title = {{Non-cytotoxic conductive carboxymethyl-chitosan/aniline pentamer
   hydrogels}},
Journal = {{REACTIVE \& FUNCTIONAL POLYMERS}},
Year = {{2014}},
Volume = {{82}},
Pages = {{81-88}},
Month = {{SEP}},
Abstract = {{Cytocompatible electrically conducting hydrogels based on amphoteric
   carboxymethyl chitosan (CMCS) and aniline oligomers with bioactive
   molecule delivery properties were presented. A series of conductive CMCS
   hydrogels with different aniline pentamer (AP) content were synthesized
   by a one-pot reaction with the combination of grafting and crosslinking
   reaction via glutaraldehyde. The conductivities of the swollen hydrogels
   are between 1.14 x 10(-4) and 4.23 x 10(-4) S/cm by tuning the AP
   content. Swelling ratio was controlled by the AP content and
   crosslinking degree of the hydrogels. The hydrogels showed absorption
   capacity of diclofenac sodium (DCS) as a model molecule and released DCS
   in a controlled manner. Morphologies and mechanical properties of the
   hydrogels were characterized by SEM and rheometer, respectively. The
   biocompatibility of the hydrogels was confirmed by C2C12 myoblast cells
   using Live/Dead assay and Alamar blue assay. These conducting hydrogels
   with controlled release capacity as bioactive scaffolds have potential
   application for tissue regeneration. (C) 2014 Elsevier Ltd. All rights
   reserved.}},
