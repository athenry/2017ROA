@article{ ISI:000346037300004,
Author = {Knight, Joshua T. and Zahradka, Frank T. and Singer, David J. and
   Collette, Matthew D.},
Title = {{Multiobjective Particle Swarm Optimization of a Planing Craft with
   Uncertainty}},
Journal = {{JOURNAL OF SHIP PRODUCTION AND DESIGN}},
Year = {{2014}},
Volume = {{30}},
Number = {{4}},
Pages = {{194-200}},
Month = {{NOV}},
Abstract = {{Uncertainty exists in many of the design variables and system parameters
   for planing craft. This is especially true in the early stages of
   design. For this reason, and others, optimization of a craft's
   performance characteristics is often delayed until later in the design
   process, after uncertainties have been at least partially resolved.
   However, delaying optimization can also limit its potential, because
   freedom to make changes to a design is also highly limited in the later
   stages. This article demonstrates how uncertainty can be directly
   incorporated into optimization using particle swarm. A simple synthesis
   model for a planing craft is built, and a deterministic Pareto front of
   optimal solutions is found, minimizing two objectives: drag and vertical
   acceleration at the center of gravity. The craft's weight is then
   modeled as a normally distributed random variable and sampling methods
   are used to quantify the uncertainty in the estimated drag for points
   along the Pareto front. Preliminary results reveal that drag uncertainty
   is not constant along the Pareto front, presenting useful tradeoff
   information for designers and decision-makers.}},
