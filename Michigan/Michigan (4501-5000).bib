@article{ ISI:000345033900005,
Author = {Lee, Duck Hyun and Kobaku, Sai P. R. and Hong, Young-Rae and Kwon, Jae
   Young},
Title = {{An Efficient Photoelectrochemical Hydrogen Evolution System using
   Silicon Nanomaterials with Ultra-High Aspect Ratios}},
Journal = {{ENERGY TECHNOLOGY}},
Year = {{2014}},
Volume = {{2}},
Number = {{11}},
Pages = {{889-896}},
Month = {{NOV}},
Abstract = {{We fabricated ultra-high aspect ratio silicon nanomaterials, including a
   silicon nanomesh and silicon nanowire array, on a wafer scale for
   efficient photoelectrochemical hydrogen production. These silicon
   nanomaterials (feature size approximate to 20 nm) possess a high aspect
   ratio to increase the optical absorptivity of the cells to approximately
   95\% over a broad range of wavelengths. The silicon nanomesh and Si
   nanowire cells achieved high photocurrent values of 13 and 28 mAcm(-2),
   respectively, which are increased by 200\% and 570\% in comparison to
   their bulk counterparts. In addition, these scalable Si nanomaterials
   remained stable for up to 100 min of hydrogen evolution. Detailed
   studies on the doping and geometrical structures of the resulting
   hydrogen evolution cells suggest that both the n(+) pp(+) doping and
   thickness of nanostructures are keys to the enhancement of the hydrogen
   evolution efficiency. The results obtained in this work show that these
   silicon nanomaterials can be used for high-performance water-splitting
   system applications.}},
