@article{ ISI:000343010000011,
Author = {Rhew, Hyo-Gyuem and Jeong, Jaehun and Fredenburg, Jeffrey A. and Dodani,
   Sunjay and Patil, Parag G. and Flynn, Michael P.},
Title = {{A Fully Self-Contained Logarithmic Closed-Loop Deep Brain Stimulation
   SoC With Wireless Telemetry and Wireless Power Management}},
