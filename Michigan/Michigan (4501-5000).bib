@article{ ISI:000343702700010,
Author = {Zhao, Feng and Fessler, Jeffrey A. and Wright, Steven M. and Noll,
   Douglas C.},
Title = {{Regularized Estimation of Magnitude and Phase of Multi-Coil B-1 Field
   Via Bloch-Siegert B-1 Mapping and Coil Combination Optimizations}},
Journal = {{IEEE TRANSACTIONS ON MEDICAL IMAGING}},
Year = {{2014}},
Volume = {{33}},
Number = {{10}},
Pages = {{2020-2030}},
Month = {{OCT}},
Abstract = {{Parallel excitation requires fast and accurate map estimation.
   Bloch-Siegert (BS) B-1 mapping is very fast and accurate over a large
   dynamic range. When applied to multi-coil systems, however, this
   phase-based method may produce low signal-to-noise ratio estimates in
   low magnitude regions due to localized excitation patterns of parallel
   excitation systems. Also, the imaging time increases with the number of
   coils. In this work, we first propose to modify the standard BS B-1
   mapping sequence so that it avoids the scans required by previous B-1
   phase estimation methods. A regularized method is then proposed to
   jointly estimate the magnitude and phase of multi-coil B-1 maps from BS
   mapping data, improving estimation quality by using the prior knowledge
   of the smoothness of B-1 magnitude and phase. Lastly, we use Cramer-Rao
   lower bound analysis to optimize the coil combinations, to improve the
   quality of the raw data for B-1 estimation. The proposed methods are
   demonstrated by simulations and phantom experiments.}},
