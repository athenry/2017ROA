@article{ ISI:000342208400016,
Author = {Knisely, Katherine and Grosh, Karl},
Title = {{Effect of AC target power on AlN film quality}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY A}},
Year = {{2014}},
Volume = {{32}},
Number = {{5}},
Month = {{SEP}},
Abstract = {{The influence of alternating current (AC) target power on film stress,
   roughness, and x-ray diffraction rocking curve full width half maximum
   (FWHM) was examined for AlN films deposited using S-gun magnetron
   sputtering on insulative substrates consisting of Si wafers with 575 nm
   thermal oxide. As the AC target power was increased from 5 to 8 kW, the
   deposition rate increased from 9.3 to 15.9 A/s, film stress decreased
   from 81 to -170 MPa, and the rocking curve FWHM increased from 0.98 to
   1.03 degrees. AlN film behavior is observed to change with target life;
   films deposited at 200 kWh target life were approximately 40 MPa more
   compressive and had 0.02 degrees degree higher rocking curve FWHM values
   than films deposited at 130 kWh. AlN films deposited in two depositions
   were compared with films deposited in a single deposition, in order to
   better characterize the growth behavior and properties of AlN films
   deposited on an existing AlN film, which is not well understood. Two
   deposition films, when compared with single deposition films, showed no
   variation in residual stress trends or grain size behavior, but the
   average film roughness increased from 0.7 to 1.4 nm and rocking curve
   FWHM values increased by more than 0.25 degrees. (C) 2014 American
   Vacuum Society.}},
