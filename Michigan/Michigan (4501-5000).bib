@article{ ISI:000343010000018,
Author = {Cho, Jihyun and Choi, Jaehyuk and Kim, Seong-Jin and Park, Seokjun and
   Shin, Jungsoon and Kim, James D. K. and Yoon, Euisik},
Title = {{A 3-D Camera With Adaptable Background Light Suppression Using
   Pixel-Binning and Super-Resolution}},
Journal = {{IEEE JOURNAL OF SOLID-STATE CIRCUITS}},
Year = {{2014}},
