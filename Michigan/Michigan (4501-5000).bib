@article{ ISI:000340667800049,
Author = {Ma, Fuyin and Wu, Jiu Hui and Wu, Weiwei},
Title = {{Vibration and noise analysis of multiphysical coupling systems}},
Journal = {{JOURNAL OF MECHANICAL SCIENCE AND TECHNOLOGY}},
Year = {{2014}},
Volume = {{28}},
Number = {{8}},
Pages = {{3361-3370}},
Month = {{AUG}},
Abstract = {{Vibration and noise analysis of complex mechanical systems is very
   difficult by traditional technologies. We present a novel technology to
   analyze the vibration and noise of complex structures. This technology
   can be divided into three stages step by step. First, it is necessary to
   process the load simplification, modal analysis, vibration and noise
   testing in the preparation stage. Then, in the vibration analysis stage,
   a virtual far-field excitation method is presented to obtain more
   accurate vibration response results in the near-field and far-field
   response points, of which the nonlinear behaviors need not be
   considered. And the solver error caused by the distance between the
   excitation point and response points could be reduced. Last, in the
   noise analysis stage, a modal partition method is put forward to solve
   the structure radiated noise according to the distributions of the
   low-level modal shapes, which is a rapid and efficient method to obtain
   a radiated noise distribution for the complex structures. On the whole,
   this technology has potential applications in vibro-acoustic problems
   solving for the complex systems in practical engineering.}},
