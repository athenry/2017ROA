@article{ ISI:000345069700043,
Author = {Wen, Zhigang and Wang, Shuai and Fan, Chunxiao and Xiang, Weidong},
Title = {{Joint Transceiver and Power Splitter Design Over Two-Way Relaying
   Channel With Lattice Codes and Energy Harvesting}},
Journal = {{IEEE COMMUNICATIONS LETTERS}},
Year = {{2014}},
Volume = {{18}},
Number = {{11}},
Pages = {{2039-2042}},
Month = {{NOV}},
Abstract = {{This letter considers a compute-and-forward two-way relaying channel
   with simultaneous wireless information and power transfer based on
   nested lattice codes. Two single-antenna nodes exchange information via
   a multi-antenna intermediate relay station. Meanwhile, energies in
   wireless signals are beamed and flow from the relay to terminals for
   static circuit consumption Pc and uplink transmission power Pi. The
   optimization problem is formulated to minimize the transmit power P-r of
   relay, while guaranteeing the minimal transmission rate at each
   terminal. We propose an efficient iterative algorithm to jointly design
   the beam-former, receiver and power splitter, based on semi-definite
   relaxation and programming. Numerical results of transmission power
   comparisons validate our analysis.}},
