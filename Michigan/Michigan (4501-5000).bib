@article{ ISI:000344428900050,
Author = {Bachhav, Mukesh and Odette, G. Robert and Marquis, Emmanuelle A.},
Title = {{Microstructural changes in a neutron-irradiated Fe-15 at.\%Cr alloy}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2014}},
Volume = {{454}},
Number = {{1-3}},
Pages = {{381-386}},
Month = {{NOV}},
Abstract = {{Microstructural changes in a Fe-15 at.\%Cr model alloy neutron
   irradiated to 1.82 dpa at 290 degrees C were characterized by atom probe
   tomography. Homogenously distributed alpha' precipitates as well as
   fewer clusters containing Si, P, Ni, and Cr, were observed in the
   matrix. Grain boundary analyses before and after irradiation revealed
   segregation of Cr, with W-shape concentration profiles developing in the
   vicinity of grain boundary carbide and nitride particles. After
   irradiation, impurities such as C, Si and P were segregated to the grain
   boundaries. Zones depleted of alpha' clusters, and Si were found at the
   interfaces of carbide and nitride precipitates and along grain
   boundaries in the vicinity of these precipitates. (C) 2014 Elsevier B.V.
   All rights reserved.}},
