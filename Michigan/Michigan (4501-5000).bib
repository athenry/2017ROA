@article{ ISI:000345216100001,
Author = {Kim, Kyu Hyun and Fan, Xudong},
Title = {{Surface sensitive microfluidic optomechanical ring resonator sensors}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2014}},
Volume = {{105}},
Number = {{19}},
Month = {{NOV 10}},
Abstract = {{The microfluidic optomechanical ring resonator (lFOMRR) based on a
   thin-walled glass capillary supports high Q-factor (> 10(3)) mechanical
   modes in the presence of liquids. In this letter, the sensitivity of the
   mu FOMRR to the surface change is studied by layer-by-layer removal of
   SiO2 molecules from the mu FOMRR inner surface using various
   concentrations of hydrofluoric acid solutions. A frequency downshift is
   observed with a sensitivity for the surface density change of 1.2
   Hz/(pg/mm(2)), which translates to a detection limit of 83 pg/mm(2).
   This work opens a door to using the optomechanical mode for detection
   and characterization of molecules near the resonator surface. (C) 2014
   AIP Publishing LLC.}},
