@article{ ISI:000350216800004,
Author = {Wysokowski, Marcin and Petrenko, Iaroslav and Stelling, Allison L. and
   Stawski, Dawid and Jesionowski, Teofil and Ehrlich, Hermann},
Title = {{Poriferan Chitin as a Versatile Template for Extreme Biomimetics}},
Journal = {{POLYMERS}},
Year = {{2015}},
Volume = {{7}},
Number = {{2}},
Pages = {{235-265}},
Month = {{FEB}},
Abstract = {{In this mini-review, we shall first cover a short history of the
   discovery of chitin isolated from sponges; as well as its evolutionarily
   ancient roots. Next, we will delve into the unique structural,
   mechanical, and thermal properties of this naturally occurring polymer
   to illuminate how its physicochemical properties may find uses in
