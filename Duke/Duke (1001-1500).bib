@article{ ISI:000358427200023,
Author = {Gourc, E. and Seguy, S. and Michon, G. and Berlioz, A. and Mann, B. P.},
Title = {{Quenching chatter instability in turning process with a vibro-impact
   nonlinear energy sink}},
Journal = {{JOURNAL OF SOUND AND VIBRATION}},
Year = {{2015}},
Volume = {{355}},
Pages = {{392-406}},
Month = {{OCT 27}},
Abstract = {{This paper investigates the passive control of chatter instability in
   turning processes using a vibro-impact nonlinear energy sink (NES). The
   workpiece is assumed to be rigid and the tool is flexible. A dynamical
   model including a nonlinear cutting law is presented and the stability
   lobes diagram is obtained. The behavior of the system with the
   vibro-irnpact NES is investigated using an asymptotic analysis. A
   control mechanism by successive beating is revealed, similarly to the
   strongly modulated response in the case of NES with cubic stiffness. It
   is shown that such a response regime may be beneficial for chaffer
   mitigation. An original experimental procedure is proposed to verify the
   sizing of the vibro-impact NES. An experimental setup is developed with
   a vibro-impact NES embedded on the lathe tool and the results are
   analyzed and validated. (C) 2015 Elsevier Ltd. All rights reserved.}},
