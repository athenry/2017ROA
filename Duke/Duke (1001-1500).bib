@article{ ISI:000358957500008,
Author = {Polstein, Lauren R. and Perez-Pinera, Pablo and Kocak, D. Dewran and
   Vockley, Christopher M. and Bledsoe, Peggy and Song, Lingyun and Safi,
   Alexias and Crawford, Gregory E. and Reddy, Timothy E. and Gersbach,
   Charles A.},
Title = {{Genome-wide specificity of DNA binding, gene regulation, and chromatin
   remodeling by TALE- and CRISPR/Cas9-based transcriptional activators}},
Journal = {{GENOME RESEARCH}},
Year = {{2015}},
Volume = {{25}},
Number = {{8}},
Pages = {{1158-1169}},
Month = {{AUG}},
Abstract = {{Genome engineering technologies based on the CRISPR/Cas9 and TALE
   systems are enabling new approaches in science and biotechnology.
   However, the specificity of these tools in complex genomes and the role
   of chromatin structure in determining DNA binding are not well
   understood. We analyzed the genome-wide effects of TALE-and CRISPR-based
   transcriptional activators in human cells using ChIP-seq to assess
   DNA-binding specificity and RNA-seq to measure the specificity of
   perturbing the transcriptome. Additionally, DNase-seq was used to assess
   genome-wide chromatin remodeling that occurs as a result of their
   action. Our results show that these transcription factors are highly
   specific in both DNA binding and gene regulation and are able to open
   targeted regions of closed chromatin independent of gene activation.
   Collectively, these results underscore the potential for these
