@article{ ISI:000364468600005,
Author = {Chowdhury, Shwetadwip and Eldridge, Will J. and Wax, Adam and Izatt,
   Joseph A.},
Title = {{Spatial frequency-domain multiplexed microscopy for simultaneous,
   single-camera, one-shot, fluorescent, and quantitative-phase imaging}},
