@article{ ISI:000354559600002,
Author = {Wang, Hsin-Neng and Fales, Andrew M. and Vo-Dinh, Tuan},
Title = {{Plasmonics-based SERS nanobiosensor for homogeneous nucleic acid
   detection}},
