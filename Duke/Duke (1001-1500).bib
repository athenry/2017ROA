@article{ ISI:000368730300015,
Author = {Assouline, S. and Thompson, S. E. and Chen, L. and Svoray, T. and Sela,
   S. and Katul, G. G.},
Title = {{The dual role of soil crusts in desertification}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-BIOGEOSCIENCES}},
Year = {{2015}},
Volume = {{120}},
Number = {{10}},
Pages = {{2108-2119}},
Month = {{OCT}},
Abstract = {{Vegetation cover in dry regions is a key variable in determining
   desertification. Soils exposed to rainfall by desertification can form
   physical crusts that reduce infiltration, exacerbating water stress on
   the remaining vegetation. Paradoxically, field studies show that crust
   removal is associated with plant mortality in desert systems, while
   artificial biological crusts can improve plant regeneration. Here it is
   shown how physical crusts can act as either drivers of or buffers
   against desertification depending on their environmental context. The
   behavior of crusts is first explored using a simplified theory for water
   movement on a uniform, partly vegetated slope subject to stationary
   hydrologic conditions. Numerical model runs supplemented with field data
   from a semiarid Long-Term Ecological Research site are then applied to
   represent more realistic environmental conditions. When vegetation cover
   is significant, crusts can drive desertification, but this process is
   potentially self-limiting. For low vegetation cover, crusts mitigate
   against desertification by providing water subsidy to plant communities
   through a runoff-runon mechanism.}},
