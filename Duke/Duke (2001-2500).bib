@article{ ISI:000337967000050,
Author = {Razi, Abolfazl and Abedi, Ali},
Title = {{Convergence Analysis of Iterative Decoding for Binary CEO Problem}},
Journal = {{IEEE TRANSACTIONS ON WIRELESS COMMUNICATIONS}},
Year = {{2014}},
Volume = {{13}},
Number = {{5}},
Pages = {{2944-2954}},
Month = {{MAY}},
Abstract = {{Estimation of a binary source using multiple observers, a variant of the
   so called Chief Executive Officer (CEO) problem, is considered. A
   low-complexity Distributed Joint Source Channel Coding (D-JSCC) based on
   the Parallel Concatenated Convolutional Codes (PCCC) is implemented in a
   cluster of sensors in a distributed fashion. Convergence of the
   iterative decoder is analyzed by utilizing EXtrinsic Information
   Transfer (EXIT) chart technique to determine the convergence region in
   terms of the sensors observation accuracy and channel SNR, where the
   iterative decoder outperforms the non-iterative one.
   This leads to design of a bi-modal decoder that adaptively switches
   between the iterative and non-iterative modes in order to avoid
   inefficient iterative information exchange without compromising the
   resulting Bit Error Rate (BER). This adaptive decoding algorithm saves
   the computational power and decoding time by a factor of about 10 by
   avoiding unnecessary iterations.}},
