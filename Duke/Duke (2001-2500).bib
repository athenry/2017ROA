@article{ ISI:000330431000134,
Author = {Scheideler, William J. and Chen, Chuan-Hua},
Title = {{The minimum flow rate scaling of Taylor cone-jets issued from a nozzle}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2014}},
Volume = {{104}},
Number = {{2}},
Month = {{JAN 13}},
Abstract = {{A minimum flow rate is required to maintain steady Taylor cone-jets
   issued from an electrified nozzle. This letter reports that the
   well-known scaling law proportional to the charge relaxation time and
   independent of the nozzle diameter is only applicable to lowly viscous
   systems. For highly viscous systems with rapid viscous diffusion, the
   cone-jet transitional region spans a length scale comparable to the
   nozzle diameter; the minimum flow rate appears to be proportional to the
   capillary-viscous velocity and the cross-sectional area of the nozzle,
   but independent of the liquid conductivity. (C) 2014 AIP Publishing LLC.}},
