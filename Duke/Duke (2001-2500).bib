@article{ ISI:000332124200020,
Author = {Kuo, Chun-Yi and Shih, Chi-Jih and Lu, Yi-Chang and Li, James Chien-Mo
   and Chakrabarty, Krishnendu},
Title = {{Testing of TSV-Induced Small Delay Faults for 3-D Integrated Circuits}},
Journal = {{IEEE TRANSACTIONS ON VERY LARGE SCALE INTEGRATION (VLSI) SYSTEMS}},
Year = {{2014}},
Volume = {{22}},
Number = {{3}},
Pages = {{667-674}},
Month = {{MAR}},
Abstract = {{Through silicon via (TSV) is a widely used interconnect technology in
   3-D integrated circuits. This paper shows that defective TSVs can induce
   small delay faults in surrounding logic gates. We present simulation
   results of TSV-induced small delay fault (TSDF) because of mechanical
   stress or pinhole leakage. A test technique is proposed to detect TSDF
   using a physical-aware fault extractor and timing-aware automatic test
   pattern generation. This technique requires no DfT area overhead and no
   direct TSV probing. Experimental results on benchmark circuits show that
   test coverage can be improved by 22\% and 10\% for stress-induced and
   leakage-induced TSDF, respectively. In our results, the test length
   overheads of both TSDFs are <5\%.}},
