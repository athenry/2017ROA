@inproceedings{ ISI:000346496103031,
Author = {Nokleby, Matthew and Rodrigues, Miguel and Calderbank, Robert},
Book-Group-Author = {{IEEE}},
Title = {{Discrimination on the Grassmann Manifold: Fundamental Limits of Subspace
   Classifiers}},
Booktitle = {{2014 IEEE INTERNATIONAL SYMPOSIUM ON INFORMATION THEORY (ISIT)}},
Series = {{IEEE International Symposium on Information Theory}},
Year = {{2014}},
Pages = {{3012-3016}},
Note = {{IEEE International Symposium on Information Theory (ISIT), Honolulu, HI,
   JUN 29-JUL 04, 2014}},
Organization = {{IEEE; IEEE Informat Theory Soc; NSF; Qualcomm; IBM Res; LG}},
Abstract = {{Repurposing tools and intuitions from Shannon theory, we derive
   fundamental limits on the reliable classification of high-dimensional
   signals from low-dimensional features. We focus on the classification of
   linear and affine subspaces and suppose the features to be noisy linear
   projections. Leveraging a syntactic equivalence of discrimination
   between subspaces and communications over vector wireless channels, we
   derive asymptotic bounds on classifier performance. First, we define the
   classification capacity, which characterizes necessary and sufficient
   relationships between the signal dimension, the number of features, and
   the number of classes to be discriminated, as all three quantities
   approach infinity. Second, we define the diversity-discrimination
   tradeoff, which characterizes relationships between the number of
   classes and the misclassification probability as the signal-to-noise
   ratio approaches infinity. We derive inner and outer bounds on these
   measures, revealing precise relationships between signal dimension and
   classifier performance.}},
