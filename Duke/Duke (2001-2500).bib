@article{ ISI:000331541600004,
Author = {Eyerly, Stephanie A. and Bahnson, Tristram D. and Koontz, Jason I. and
   Bradway, David P. and Dumont, Douglas M. and Trahey, Gregg E. and Wolf,
   Patrick D.},
Title = {{Contrast in Intracardiac Acoustic Radiation Force Impulse Images of
   Radiofrequency Ablation Lesions}},
Journal = {{ULTRASONIC IMAGING}},
Year = {{2014}},
Volume = {{36}},
Number = {{2}},
Pages = {{133-148}},
Month = {{APR}},
Abstract = {{We have previously shown that intracardiac acoustic radiation force
   impulse (ARFI) imaging visualizes tissue stiffness changes caused by
   radiofrequency ablation (RFA). The objectives of this in vivo study were
   to (1) quantify measured ARFI-induced displacements in RFA lesion and
   unablated myocardium and (2) calculate the lesion contrast (C) and
   contrast-to-noise ratio (CNR) in two-dimensional ARFI and conventional
   intracardiac echo images. In eight canine subjects, an ARFI
   imaging-electroanatomical mapping system was used to map right atrial
   ablation lesion sites and guide the acquisition of ARFI images at these
   sites before and after ablation. Readers of the ARFI images identified
   lesion sites with high sensitivity (90.2\%) and specificity (94.3\%) and
   the average measured ARFI-induced displacements were higher at unablated
   sites (11.23 +/- 1.71 mu m) than at ablated sites (6.06 +/- 0.94 mu m).
   The average lesion C (0.29 +/- 0.33) and CNR (1.83 +/- 1.75) were
   significantly higher for ARFI images than for spatially registered
   conventional B-mode images (C = -0.03 +/- 0.28, CNR = 0.74 +/- 0.68).}},
