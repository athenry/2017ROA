@inproceedings{ ISI:000357020300050,
Author = {Matthews, Opeoluwa and Zhang, Meng and Sorin, Daniel J.},
Book-Group-Author = {{IEEE}},
Title = {{Scalably Verifiable Dynamic Power Management}},
