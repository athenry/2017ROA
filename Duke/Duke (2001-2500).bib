@article{ ISI:000329957700003,
Author = {Tweten, Dennis J. and Mann, Brian P.},
Title = {{Experimental investigation of colored noise in stochastic resonance of a
   bistable beam}},
Journal = {{PHYSICA D-NONLINEAR PHENOMENA}},
Year = {{2014}},
Volume = {{268}},
Pages = {{25-33}},
Month = {{FEB 1}},
Abstract = {{This paper describes an experimental investigation of stochastic
   resonance in a bistable, composite beam excited by colored noise.
   Experimental results for average up-crossing period, spectral power
   amplification (SPA), and signal-to-noise ratio (SNR) are compared with
   analytical methods for underdamped systems. These analytical methods
   include expressions developed from Kramers, Melnikov, and two-state
   theory. The effect of a modal mass on the analytical expressions is
   explored. Finally, an alternative approach for calculating the effect of
   a colored noise spectrum on the SPA and SNR of underdamped systems is
   proposed. (C) 2013 Elsevier B.V. All rights reserved.}},
