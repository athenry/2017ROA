@article{ ISI:000332233300022,
Author = {Polans, James and McNabb, Ryan P. and Izatt, Joseph A. and Farsiu, Sina},
Title = {{Compressed wavefront sensing}},
Journal = {{OPTICS LETTERS}},
Year = {{2014}},
Volume = {{39}},
Number = {{5}},
Pages = {{1189-1192}},
Month = {{MAR 1}},
Abstract = {{We report on an algorithm for fast wavefront sensing that incorporates
   sparse representation for the first time in practice. The partial
   derivatives of optical wavefronts were sampled sparsely with a
   Shack-Hartman wavefront sensor (SHWFS) by randomly subsampling the
   original SHWFS data to as little as 5\%. Reconstruction was performed by
