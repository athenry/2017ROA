Title = {{AN ADAPTIVE DETECTOR FOR DETECTING TARGET IN CLUTTER PLUS GAUSSIAN NOISE
   BACKGROUND}},
Booktitle = {{2014 12TH INTERNATIONAL CONFERENCE ON SIGNAL PROCESSING (ICSP)}},
Series = {{International Conference on Signal Processing}},
