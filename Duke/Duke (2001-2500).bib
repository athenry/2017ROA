@inproceedings{ ISI:000344037800002,
Author = {Quiroz, Felipe Garcia and Chilkoti, Ashutosh},
Editor = {{Lutz, JF and Meyer, TY and Ouchi, M and Sawamoto, M}},
Title = {{The Language of Protein Polymers}},
Booktitle = {{SEQUENCE-CONTROLLED POLYMERS: SYNTHESIS, SELF-ASSEMBLY, AND PROPERTIES}},
Series = {{ACS Symposium Series}},
Year = {{2014}},
Volume = {{1170}},
