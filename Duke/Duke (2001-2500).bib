@article{ ISI:000330605200032,
Author = {Sarkar, Swarnavo and Warner, James E. and Aquino, Wilkins and Grigoriu,
   Mircea D.},
Title = {{Stochastic reduced order models for uncertainty quantification of
   intergranular corrosion rates}},
Journal = {{CORROSION SCIENCE}},
Year = {{2014}},
Volume = {{80}},
Pages = {{257-268}},
Month = {{MAR}},
Abstract = {{We present a stochastic reduced order model (SROM) approach for
   quantifying uncertainty in systems undergoing corrosion. A SROM is a
   simple random element with a small number of samples that approximates
   the statistics of another target random element. The parameters of a
   SROM are selected through an optimization problem. SROMs can be used to
   propagate uncertainty through a mathematical model of a corroding system
   in the same way as in Monte Carlo methods. We use SROMs to estimate the
   statistics of corrosion current density, considering randomness in
   anode-cathode sizes. We compare the performance of SROMs against the
   more common Monte-Carlo approach. (C) 2013 Elsevier Ltd. All rights
   reserved.}},
