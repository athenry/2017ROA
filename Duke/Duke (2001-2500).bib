@article{ ISI:000338621900002,
Author = {Guilak, Farshid and Butler, David L. and Goldstein, Steven A. and
   Baaijens, Frank P. T.},
Title = {{Biomechanics and mechanobiology in functional tissue engineering}},
Journal = {{JOURNAL OF BIOMECHANICS}},
Year = {{2014}},
Volume = {{47}},
Number = {{9, SI}},
Pages = {{1933-1940}},
Month = {{JUN 27}},
Abstract = {{The field of tissue engineering continues to expand and mature, and
   several products are now in clinical use, with numerous other
   preclinical and clinical studies underway. However, specific challenges
   still remain in the repair or regeneration of tissues that serve a
   predominantly biomechanical function. Furthermore, it is now clear that
   mechanobiological interactions between cells and scaffolds can
   critically influence cell behavior, even in tissues and organs that do
   not serve an overt biomechanical role. Over the past decade, the field
   of ``functional tissue engineering{''} has grown as a subfield of tissue
   engineering to address the challenges and questions on the role of
   biomechanics and mechanobiology in tissue engineering. Originally posed
   as a set of principles and guidelines for engineering of load-bearing
   tissues, functional tissue engineering has grown to encompass several
   related areas that have proven to have important implications for tissue
   repair and regeneration. These topics include measurement and modeling
   of the in vivo biomechanical environment; quantitative analysis of the
   mechanical properties of native tissues, scaffolds, and repair tissues;
   development of rationale criteria for the design and assessment of
   engineered tissues; investigation of the effects biomechanical factors
   on native and repair tissues, in vivo and in vitro; and development and
