@article{ ISI:000326087800003,
Author = {Agapito, Luis A. and Ferretti, Andrea and Calzolari, Arrigo and
   Curtarolo, Stefano and Nardelli, Marco Buongiorno},
Title = {{Effective and accurate representation of extended Bloch states on finite
   Hilbert spaces}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2013}},
Volume = {{88}},
Number = {{16}},
Month = {{OCT 21}},
Abstract = {{We present a straightforward, noniterative projection scheme that can
   represent the electronic ground state of a periodic system on a finite
   atomic-orbital-like basis, up to a predictable number of electronic
   states and with controllable accuracy. By cofiltering the projections of
   plane-wave Bloch states with high-kinetic-energy components, the
   richness of the finite space and thus the number of exactly-reproduced
   bands can be selectively increased at a negligible computational cost,
   an essential requirement for the design of efficient algorithms for
   electronic structure simulations of realistic material systems and
   massive high-throughput investigations.}},
