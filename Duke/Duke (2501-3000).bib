@article{ ISI:000321523500021,
Author = {van Rhoon, Gerard C. and Samaras, Theodoros and Yarmolenko, Pavel S. and
   Dewhirst, Mark W. and Neufeld, Esra and Kuster, Niels},
Title = {{CEM43A degrees C thermal dose thresholds: a potential guide for magnetic
   resonance radiofrequency exposure levels?}},
Journal = {{EUROPEAN RADIOLOGY}},
Year = {{2013}},
Volume = {{23}},
Number = {{8}},
Pages = {{2215-2227}},
Month = {{AUG}},
Abstract = {{To define thresholds of safe local temperature increases for MR
   equipment that exposes patients to radiofrequency fields of high
   intensities for long duration. These MR systems induce heterogeneous
   energy absorption patterns inside the body and can create localised
   hotspots with a risk of overheating.
   The MRI + EUREKA research consortium organised a ``Thermal Workshop on
   RF Hotspots{''}. The available literature on thresholds for thermal
   damage and the validity of the thermal dose (TD) model were discussed.
   The following global TD threshold guidelines for safe use of MR are
   proposed: 1. All persons: maximum local temperature of any tissue
