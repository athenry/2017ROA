@article{ ISI:000324073200015,
Author = {Do Kang, Sa and Carlon, Tim A. and Jantzen, Alexandra E. and Lin,
   Fu-Hsiung and Ley, Melissa M. and Allen, Jason D. and Stabler, Thomas V.
   and Haley, N. Rebecca and Truskey, George A. and Achneck, Hardean E.},
Title = {{Isolation of Functional Human Endothelial Cells from Small Volumes of
   Umbilical Cord Blood}},
Journal = {{ANNALS OF BIOMEDICAL ENGINEERING}},
Year = {{2013}},
Volume = {{41}},
Number = {{10}},
Pages = {{2181-2192}},
Month = {{OCT}},
Abstract = {{Endothelial cells (ECs) isolated from endothelial progenitor cells in
   blood have great potential as a therapeutic tool to promote
   vasculogenesis and angiogenesis and treat cardiovascular diseases.
   However, current methods to isolate ECs are limited by a low yield with
   few colonies appearing during isolation. In order to utilize
   blood-derived ECs for therapeutic applications, a simple method is
   needed that can produce a high yield of ECs from small volumes of blood
   without the addition of animal-derived products. For the first time, we
   show that human ECs can be isolated without the prior separation of
   blood components through the technique of diluted whole blood incubation
   (DWBI) utilizing commercially available human serum. We isolated ECs
   from small volumes of blood (similar to 10 mL) via DWBI and
   characterized them with flow cytometry, immunohistochemistry, and uptake
   of DiI-labeled acetylated low density lipoprotein (DiI-Ac-LDL). These
   ECs are functional as demonstrated by their ability to form tubular
   networks in Matrigel, adhere and align with flow under physiological
   fluid shear stress, and produce increased nitric oxide under fluid flow.
   An average of 7.0 +/- A 2.5 EC colonies that passed all functional tests
   described above were obtained per 10 mL of blood as compared to only 0.3
   +/- A 0.1 colonies with the traditional method based on density
   centrifugation. The time until first colony appearance was 8.3 +/- A 1.2
   days for ECs isolated with the DWBI method and 12 +/- A 1.4 days for ECs
   isolated with the traditional isolation method. A simplified method,
   such as DWBI, in combination with advances in isolation yield could
   enable the use of blood-derived ECs in clinical practice.}},
