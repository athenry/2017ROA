@article{ ISI:000326148700035,
Author = {Jiao, Wenyuan and Kong, Wei and Li, Jincheng and Collar, Kristen and
   Kim, Tong-Ho and Brown, April S.},
Title = {{The relationship between depth-resolved composition and strain
   relaxation in InAlN and InGaN films grown by molecular beam epitaxy}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{103}},
Number = {{16}},
Month = {{OCT 14}},
Abstract = {{A study of the relationship between strain and the incorporation of
   group III elements in ternary InGaN and InAlN grown by molecular beam
   epitaxy is reported. Using X-ray Photoelectron Spectroscopy
   compositional depth profiles with x-ray diffraction, we are able to find
   a clear relationship between strain and In incorporation including
   tensile-strained InAlN which has, to date, not been studied. The results
   show that fully strained films contain homogeneous indium composition
   while partially relaxed films have a non-homogeneous indium composition
   with depth. These results can be interpreted by considering the impurity
   formation energies of indium in host lattices. (C) 2013 AIP Publishing
   LLC.}},
