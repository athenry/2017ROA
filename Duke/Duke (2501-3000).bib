@article{ ISI:000316813100020,
Author = {Xu, Yong and Scheideler, William and Liu, Chuan and Balestra, Francis
   and Ghibaudo, Gerard and Tsukagoshi, Kazuhito},
Title = {{Contact Thickness Effects in Bottom-Contact Coplanar Organic
   Field-Effect Transistors}},
Journal = {{IEEE ELECTRON DEVICE LETTERS}},
Year = {{2013}},
Volume = {{34}},
Number = {{4}},
Pages = {{535-537}},
Month = {{APR}},
Abstract = {{Influences of contact thickness on bottom-contact and bottom-gate
   coplanar organic transistors are studied. In transistors with
   poor-quality pentacene films, thick contacts improve mobility and lower
   contact resistance. However, in transistors with high-quality pentacene
   films, thick contacts significantly degrade performance by disrupting
   molecular self-organization at the contact edge. These results highlight
   the importance of contact thickness to such organic transistors and
   reveal that semiconductor morphology should be considered in designing
   devices with minimal contact effects.}},
