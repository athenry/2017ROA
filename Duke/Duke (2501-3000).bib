@article{ ISI:000327007800118,
Author = {Tsai, Yu-Ju and Larouche, Stephane and Tyler, Talmage and Llopis,
   Antonio and Royal, Matthew and Jokerst, Nan M. and Smith, David R.},
Title = {{Arbitrary birefringent metamaterials for holographic optics at
   lambda=1.55 mu m}},
Journal = {{OPTICS EXPRESS}},
Year = {{2013}},
Volume = {{21}},
Number = {{22}},
Pages = {{26620-26630}},
Month = {{NOV 4}},
Abstract = {{This paper presents an optical element capable of multiplexing two
   diffraction patterns for two orthogonal linear polarizations, based on
   the use of non-resonant metamaterial cross elements. The metamaterial
   cross elements provide unique building blocks for engineering arbitrary
   birefringence. As a proof-of-concept demonstration, we present the
   design and experimental characterization of a polarization multiplexed
   blazed diffraction grating and a polarization multiplexed
   computer-generated hologram, for the telecommunication wavelength of
   lambda = 1.55 mu m. A quantitative study of the polarization multiplexed
   grating reveals that this approach yields a very large polarization
   contrast ratio. The results show that metamaterials can form the basis
   for a versatile and compact platform useful in the design of
   multi-functional photonic devices. (C) 2013 Optical Society of America}},
