@article{ ISI:000320852300022,
Author = {Wang, Hsin-Neng and Fales, Andrew M. and Zaas, Aimee K. and Woods,
   Christopher W. and Burke, Thomas and Ginsburg, Geoffrey S. and Vo-Dinh,
   Tuan},
Title = {{Surface-enhanced Raman scattering molecular sentinel nanoprobes for
   viral infection diagnostics}},
Journal = {{ANALYTICA CHIMICA ACTA}},
Year = {{2013}},
Volume = {{786}},
Pages = {{153-158}},
Month = {{JUL 5}},
Abstract = {{In this paper, we describe a surface-enhanced Raman scattering
   (SERS)-based detection approach, referred to as ``molecular sentinel{''}
   (MS) plasmonic nanoprobes, to detect an RNA target related to viral
   infection. The MS method is essentially a label-free technique
   incorporating the SERS effect modulation scheme associated with silver
   nanoparticles and Raman dye-labeled DNA hairpin probes. Hybridization
   with target sequences opens the hairpin and spatially separates the
   Raman label from the silver surface thus reducing the SERS signal of the
   label. Herein, we have developed a MS nanoprobe to detect the human
   radical S-adenosyl methionine domain containing 2 (RSAD2) RNA target as
   a model system for method demonstration. The human RSAD2 gene has
   recently emerged as a novel host-response biomarker for diagnosis of
   respiratory infections. Our results showed that the RSAD2 MS nanoprobes
   exhibits high specificity and can detect as low as 1 nM target
