@article{ ISI:000319085200011,
Author = {Dewhirst, Mark W. and Landon, Chelsea D. and Hofmann, Christina L. and
   Stauffer, Paul R.},
Title = {{Novel Approaches to Treatment of Hepatocellular Carcinoma and Hepatic
   Metastases Using Thermal Ablation and Thermosensitive Liposomes}},
Journal = {{SURGICAL ONCOLOGY CLINICS OF NORTH AMERICA}},
Year = {{2013}},
Volume = {{22}},
Number = {{3}},
Pages = {{545+}},
Month = {{JUL}},
Abstract = {{Because of the limitations of surgical resection, thermal ablation is
   commonly used for the treatment of hepatocellular carcinoma and liver
   metastases. Current methods of ablation can result in marginal
   recurrences of larger lesions and in tumors located near large vessels.
   This review presents a novel approach for extending treatment out to the
   margins where temperatures do not provide complete treatment with
   ablation alone, by combining thermal ablation with drug-loaded
   thermosensitive liposomes. A history of the development of
   thermosensitive liposomes is presented. Clinical trials have shown that
   the combination of radiofrequency ablation and doxorubicin-loaded
   thermosensitive liposomes is a promising treatment.}},
