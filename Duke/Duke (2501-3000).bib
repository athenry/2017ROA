@article{ ISI:000326883000008,
Author = {Bernard, Brian P. and Mann, Brian P.},
Title = {{Passive band-gap reconfiguration born from bifurcation asymmetry}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2013}},
Volume = {{88}},
Number = {{5}},
Month = {{NOV 7}},
Abstract = {{Current periodic structures are constrained to have fixed energy
   transmission behavior unless active control or component replacement is
   used to alter their wave propagation characteristics. The introduction
   of nonlinearity to generate multiple stable equilibria is an alternative
   strategy for realizing distinct energy propagation behaviors. We
   investigate the creation of a reconfigurable band-gap system by
   implementing passive switching between multiple stable states of
   equilibrium, to alter the level of energy attenuation in response to
   environmental stimuli. The ability to avoid potentially catastrophic
   loads is demonstrated by tailoring the bandpass and band-gap regions to
   coalesce for two stable equilibria and varying an external load
   parameter to trigger a bifurcation. The proposed phenomenon could be
   utilized in remote or autonomous applications where component
   modifications and active control are impractical.}},
