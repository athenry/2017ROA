@article{ ISI:000319350500007,
Author = {McNulty, Amy L. and Rothfusz, Nicole E. and Leddy, Holly A. and Guilak,
   Farshid},
Title = {{Synovial fluid concentrations and relative potency of interleukin-1
   alpha and beta in cartilage and meniscus degradation}},
Journal = {{JOURNAL OF ORTHOPAEDIC RESEARCH}},
Year = {{2013}},
Volume = {{31}},
Number = {{7}},
Pages = {{1039-1045}},
Month = {{JUL}},
Abstract = {{Cartilage degeneration with osteoarthritis (OA) is believed to involve
   the activities of interleukin-1 (IL-1), which exists as alpha and beta
   isoforms. The goal of this study was to measure the concentrations of
   both isoforms of IL-1 in the synovial fluid of normal and spontaneously
   osteoarthritic porcine knees, and to test the hypothesis that
   physiologic concentrations of IL-1 and IL-1 exhibit different potencies
   in activating calcium signaling, the production of matrix
   metalloproteinases and nitric oxide, and the loss of proteoglycans and
   tissue mechanical properties in cartilage and meniscus. Median
   concentrations of IL-1 were 0.043ng/ml with mild OA and 0.288ng/ml with
   moderate OA, whereas IL-1 concentrations were 0.109ng/ml with mild OA
   and 0.122ng/ml with moderate OA. Both isoforms induced calcium signaling
   in chondrocytes and meniscal cells at all concentrations. Overall,
   cartilage and meniscus catabolism was significantly more sensitive to
   IL-1 than IL-1 at concentrations of 1ng/ml or less, while few
   differences were observed between the two forms at 10ng/ml. These data
   provide a range of physiologic IL-1 concentrations that can serve as a
   framework for the comparison of various in vitro studies, as well as
   providing further insight for the development of anti-cytokine therapies
   for OA. (c) 2013 Orthopaedic Research Society Published by Wiley
   Periodicals, Inc. J Orthop Res 31:10391045, 2013}},
