@article{ ISI:000326148700095,
Author = {Hawkes, Allen M. and Katko, Alexander R. and Cummer, Steven A.},
Title = {{A microwave metamaterial with integrated power harvesting functionality}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{103}},
Number = {{16}},
Month = {{OCT 14}},
Abstract = {{We present the design and experimental implementation of a power
   harvesting metamaterial. A maximum of 36.8\% of the incident power from
   a 900 MHz signal is experimentally rectified by an array of metamaterial
   unit cells. We demonstrate that the maximum harvested power occurs for a
   resistive load close to 70 Omega in both simulation and experiment. The
   power harvesting metamaterial is an example of a functional metamaterial
   that may be suitable for a wide variety of applications that require
   power delivery to any active components integrated into the
   metamaterial. (C) 2013 AIP Publishing LLC.}},
