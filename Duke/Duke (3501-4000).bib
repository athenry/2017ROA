@inproceedings{ ISI:000304768000023,
Author = {Bond, Jason and Frush, Jack and Hon, Sylvia and Eckersley, Chris and
   Williams, Cameron H. and Feng, Jianqiao and Tward, Daniel J. and
   Ratnanather, Tilak J. T. and Miller, M. I. and Frush, D. and Samei, E.
   and Segars, W. P.},
Editor = {{Pelc, NJ and Nishikawa, RM and Whiting, BR}},
Title = {{Series of 4D adult XCAT phantoms for imaging research and dosimetry}},
Booktitle = {{MEDICAL IMAGING 2012: PHYSICS OF MEDICAL IMAGING}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8313}},
Note = {{Conference on Medical Imaging - Physics of Medical Imaging, San Diego,
   CA, FEB 05-08, 2012}},
Organization = {{SPIE; Agilent Technol; Diamond SA; DQE Instruments Inc; eMagin; Isuzu
   Glass Co Ltd; Medtron Inc; Ocean Thin Films Inc}},
Abstract = {{Computerized phantoms are finding an increasingly important role in
   medical imaging research. With the ability to simulate various imaging
   conditions, they offer a practical means with which to quantitatively
   evaluate and improve imaging devices and techniques. This is especially
   true in CT due to the high radiation levels involved with it. Despite
   their utility, due to the time required to develop them, only a handful
   of computational models currently exist of varying detail. Most phantoms
   available are limited to 3D and not capable of modeling patient motion.
   We have previously developed a technique to rapidly create highly
   detailed 4D extended cardiac-torso (XCAT) phantoms based on patient CT
   data {[}1].
   In this study, we utilize this technique to generate 58 new adult XCAT
   phantoms to be added to our growing library of virtual patients
   available for imaging research. These computerized patients provide a
   valuable tool for investigating imaging devices and the effects of
