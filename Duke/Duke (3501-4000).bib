@article{ ISI:000307688200020,
Author = {Liu, Lijun and Shen, Yi and Dowell, Earl H. and Zhu, Chunhui},
Title = {{A general H-infinity fault tolerant control and management for a linear
   system with actuator faults}},
Journal = {{AUTOMATICA}},
Year = {{2012}},
Volume = {{48}},
Number = {{8}},
Pages = {{1676-1682}},
Month = {{AUG}},
Abstract = {{This brief paper investigates the optimal selection of online
   controllers for a linear system with actuator faults in the framework of
   H-infinity control. The occurrence of actuator faults is modeled as a
   Markov chain to predict future faults. A compatible relationship of
   H-infinity controllers is proposed for successive fault modes. In order
   to shrink the set of all H-infinity controllers, seven methods are
   presented to construct a small set of controllers for different demands.
   Then five selection schemes are proposed to choose a proper controller
   from a given set of controllers for a given fault mode. Another
   selection scheme and a pre-selection scheme are presented when there
   might be false fault detection and identification. Considered together,
   they provide a powerful solution for all kinds of demands. A motivating
   example is used to illustrate the proposed methods. (C) 2012 Elsevier
   Ltd. All rights reserved.}},
