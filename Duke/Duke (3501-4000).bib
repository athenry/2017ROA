@article{ ISI:000306411900022,
Author = {Lee, Whasil and Struempfer, Johan and Bennett, Vann and Schulten, Klaus
   and Marszalek, Piotr E.},
Title = {{Mutation of Conserved Histidines Alters Tertiary Structure and
   Nanomechanics of Consensus Ankyrin Repeats}},
Journal = {{JOURNAL OF BIOLOGICAL CHEMISTRY}},
Year = {{2012}},
Volume = {{287}},
Number = {{23}},
Pages = {{19115-19121}},
Month = {{JUN 1}},
Abstract = {{The conserved TPLH tetrapeptide motif of ankyrin repeats (ARs) plays an
   important role in stabilizing AR proteins, and histidine (TPL (H) under
   bar)-to-arginine (TPL (R) under bar) mutations in this motif have been
   associated with a hereditary human anemia, spherocytosis. Here, we used
   a combination of atomic force microscopy-based single-molecule force
   spectroscopy and molecular dynamics simulations to examine the
   mechanical effects of His -> Arg substitutions in TPLH motifs in a model
   AR protein, NI6C. Our molecular dynamics results show that the mutant
   protein is less mechanically stable than the WT protein. Our atomic
   force microscopy results indicate that the mechanical energy input
   necessary to fully unfold the mutant protein is only half of that
   necessary to unfold the WT protein (53 versus 106 kcal/mol). In
   addition, the ability of the mutant to generate refolding forces is also
   reduced. Moreover, the mutant protein subjected to cyclic stretch-relax
   measurements displays mechanical fatigue, which is absent in the WT
   protein. Taken together, these results indicate that the His -> Arg
   substitutions in TPLH motifs compromise mechanical properties of ARs and
   suggest that the origin of hereditary spherocytosis may be related to
   mechanical failure of ARs.}},
