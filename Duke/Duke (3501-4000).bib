@inproceedings{ ISI:000326960201225,
Author = {Jakovljevic, Marko and Hyun, Dongwoon and Byram, Brett and Trahey, Gregg
   E. and Dahl, Jeremy},
Book-Group-Author = {{IEEE}},
Title = {{Clinical Realization of Short-lag Spatial Coherence Imaging on 2D Arrays}},
Booktitle = {{2012 IEEE INTERNATIONAL ULTRASONICS SYMPOSIUM (IUS)}},
