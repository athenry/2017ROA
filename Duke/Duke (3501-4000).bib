@article{ ISI:000302038500013,
Author = {Owens, Benjamin A. M. and Mann, Brian P.},
Title = {{Linear and nonlinear electromagnetic coupling models in vibration-based
   energy harvesting}},
Journal = {{JOURNAL OF SOUND AND VIBRATION}},
Year = {{2012}},
Volume = {{331}},
Number = {{4}},
Pages = {{922-937}},
Month = {{FEB 13}},
Abstract = {{This paper investigates the response of an energy harvester that uses
   electromagnetic induction to convert ambient vibration into electrical
   energy. A unique aspect of the present study is the comparison of the
   system's response behavior when either a linear or a physically
   motivated form of nonlinear coupling is applied. The motivating
   hypothesis for this work was that nonlinear coupling could be used to
   improve the performance of an energy harvester by broadening its
   frequency response. Combined theoretical and numerical studies
   investigate the harvester's response for both single and multi-frequency
   base excitation. Our investigations unveil regions in the parameter
   space where nonlinear coupling is better than linear coupling and
   regions where the opposite is true. The meaningful conclusion is that
   nonlinear coupling can sometimes be detrimental, but it can also be
   beneficial if properly designed into the system. (C) 2011 Elsevier Ltd.
   All rights reserved.}},
