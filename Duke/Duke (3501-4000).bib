@article{ ISI:000299251800022,
Author = {Chen, Tao and Chang, Debby P. and Zhang, Jianming and Jordan, Rainer and
   Zauscher, Stefan},
Title = {{Manipulating the Motion of Gold Aggregates Using Stimulus-Responsive
   Patterned Polymer Brushes as a Motor}},
Journal = {{ADVANCED FUNCTIONAL MATERIALS}},
Year = {{2012}},
Volume = {{22}},
Number = {{2}},
Pages = {{429-434}},
Month = {{JAN 25}},
Abstract = {{An important goal and major challenge of material science and
   nanotechnology is building nanomotors for manipulating the motion of
   nanoparticles (NPs). Here, it is demonstrated that patterned,
   stimulus-responsive polymer brush microstructures can be used as motor
   arrays to manipulate the movement of gold NP aggregates in response to
   external stimuli that induce a conformational change in the brushes as
   the driving force. The motion of NP aggregates in the out-of-plane
   direction is achieved with displacements ranging from nanometers to
   sub-micrometers. These patterned polymer-brush microstructures can find
   applications as efficient motor arrays and nanosensors, and benefit the
   design of more complex nanodevices.}},
