@article{ ISI:000299868000001,
Author = {Levy, Ohad and Xue, Junkai and Wang, Shidong and Hart, Gus L. W. and
   Curtarolo, Stefano},
Title = {{Stable ordered structures of binary technetium alloys from first
   principles}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2012}},
Volume = {{85}},
Number = {{1}},
Month = {{JAN 30}},
Abstract = {{Technetium, element 43, is the only radioactive transition metal. It
   occurs naturally on earth in only trace amounts. Experimental
   investigation of its possible compounds is thus inherently difficult and
   limited. Half of the Tc-transition-metal systems (14 out of 28) are
   reported to be phase separating or lack experimental data. Using
   high-throughput first-principles calculations, we present a
   comprehensive investigation of the binary alloys of technetium with the
   transition metals. The calculations predict stable, ordered structures
   in nine of these 14 binary systems. They also predict additional
   compounds in all nine known compound-forming systems and in two of the
   five systems reported with disordered chi or sigma phases. These results
   accentuate the incompleteness of the current knowledge of technetium
   alloys and should guide experimental studies to obtain accurate
   structural information on potential compounds, obviating some of the
   difficulties associated with such work.}},
