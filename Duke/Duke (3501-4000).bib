@article{ ISI:000307551700028,
Author = {Jain, Shashank and Pitoc, George A. and Holl, Eda K. and Zhang, Ying and
   Borst, Luke and Leong, Kam W. and Lee, Jaewoo and Sullenger, Bruce A.},
Title = {{Nucleic acid scavengers inhibit thrombosis without increasing bleeding}},
Journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA}},
Year = {{2012}},
Volume = {{109}},
Number = {{32}},
Pages = {{12938-12943}},
Month = {{AUG 7}},
Abstract = {{Development of effective, yet safe, antithrombotic agents has been
   challenging because such agents increase the propensity of patients to
   bleed. Recently, naturally occurring polyphosphates such as
   extracellular DNA, RNA, and inorganic polyphosphates have been shown to
   activate blood coagulation. In this report, we evaluate the
   anticoagulant and antithrombotic activity of nucleic acid-binding
   polymers in vitro and in vivo. Such polymers bind to DNA, RNA, and
