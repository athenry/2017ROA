Title = {{In vivo pump-probe microscopy of melanoma and pigmented lesions}},
Booktitle = {{MULTIPHOTON MICROSCOPY IN THE BIOMEDICAL SCIENCES XII}},
Series = {{Proceedings of SPIE}},
