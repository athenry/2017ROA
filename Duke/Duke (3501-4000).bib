@inproceedings{ ISI:000326814202130,
Author = {Magana, Quetzalcoatl and Kapadia, Anuj and Agasthya, Greeshma and
   Balinskas, Stephen},
Editor = {{Yu, B}},
Title = {{Automated Hemochromatosis Spectra Analysis Using Neutron Stimulated
   Emission Tomography}},
Booktitle = {{2012 IEEE NUCLEAR SCIENCE SYMPOSIUM AND MEDICAL IMAGING CONFERENCE
   RECORD (NSS/MIC)}},
Series = {{IEEE Nuclear Science Symposium Conference Record}},
Year = {{2012}},
