@article{ ISI:000305098000009,
Author = {Tweten, Dennis J. and Lipp, Genevieve M. and Khasawneh, Firas A. and
   Mann, Brian P.},
Title = {{On the comparison of semi-analytical methods for the stability analysis
   of delay differential equations}},
Journal = {{JOURNAL OF SOUND AND VIBRATION}},
Year = {{2012}},
Volume = {{331}},
Number = {{17}},
Pages = {{4057-4071}},
Month = {{AUG 13}},
Abstract = {{This paper provides the first comparison of the semi-discretization,
   spectral element, and Legendre collocation methods. Each method is a
   technique for solving delay differential equations (DDEs) as well as
   determining regions of stability in the DDE parameter space. We present
   the necessary concepts, assumptions, and equations required to implement
   each method. To compare the relative performance between the methods,
   the convergence rate and computational time for each method is compared
   in three numerical studies consisting of a ship stability example, the
   delayed damped Mathieu equation, and a helicopter rotor control problem.
   For each study, we present one or more stability diagrams in the
   parameter space and one or more convergence plots. The spectral element
   method is demonstrated to have the quickes convergence rate while the
   Legendre collocation method requires the least computational time. The
   semi-discretization method on the other hand has both the slowest
   convergence rate and requires the most computational time. (C) 2012
   Elsevier Ltd. All rights reserved.}},
