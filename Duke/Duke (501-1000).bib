@inproceedings{ ISI:000392655800027,
Author = {Li, Zipeng and Lai, Kelvin Yi-Tse and Yu, Po-Hsien and Chakrabarty,
   Krishnendu and Ho, Tsung-Yi and Lee, Chen-Yi},
Book-Group-Author = {{IEEE}},
Title = {{Built-In Self-Test for Micro-Electrode-Dot-Array Digital Microfluidic
   Biochips}},
