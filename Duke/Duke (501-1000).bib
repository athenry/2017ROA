@inproceedings{ ISI:000401938000020,
Author = {Neu, Wanda Krassowska},
Editor = {{Ortuno, F and Rojas, I}},
Title = {{Uncertainty in 1D and 3D Models of a Fiber Stimulated by an External
   Electrode}},
