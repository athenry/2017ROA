@article{ ISI:000373778900040,
Author = {Zhou, Kevin C. and Huang, Brendan K. and Gamm, Ute A. and Bhandari,
   Vineet and Khokha, Mustafa K. and Choma, Michael A.},
Title = {{Particle streak velocimetry-optical coherence tomography: a novel method
   for multidimensional imaging of microscale fluid flows}},
Journal = {{BIOMEDICAL OPTICS EXPRESS}},
Year = {{2016}},
Volume = {{7}},
Number = {{4}},
Pages = {{1590-1603}},
Month = {{APR 1}},
Abstract = {{We present a new OCT method for flow speed quantification and
   directional velocimetry: particle streak velocimetry-OCT (PSV-OCT).
   PSV-OCT generates two-dimensional, 2.5-vector component (nu(x),vertical
   bar nu(y)vertical bar,nu(z)) maps of microscale flow velocity fields.
   Knowledge of 2.5-vector components also enables the estimation of total
   flow speed. The enabling insight behind PSV-OCT is that tracer particles
   in sparsely-seeded fluid flow trace out streaks in (x,z,t)-space. The
   streak orientations in x-t and z-t yield nu(x) and nu(z), respectively.
   The in-plane (x-z plane) residence time yields the out-of-plane speed
   vertical bar nu(y)vertical bar. Vector component values are generated by
   fitting streaks to a model of image formation that incorporates
   equations of motion in 3D space. We demonstrate cross-sectional
   estimation of (nu(x),vertical bar nu(y)vertical bar,nu(z)) in two
   important animal models in ciliary biology: Xenopus embryos (tadpoles)
   and mouse trachea. (C) 2016 Optical Society of America}},
