@article{ ISI:000372794900021,
Author = {Koo, Bonhyeong and Lee, June Hyuk and Shin, Donghyeop and Ahn, Byung Tae
   and Shin, Byungha},
Title = {{Photoluminescence study of high energy proton irradiation on
   Cu(In,Ga)Se-2 thin films}},
Journal = {{THIN SOLID FILMS}},
Year = {{2016}},
Volume = {{603}},
Pages = {{134-138}},
Month = {{MAR 31}},
Abstract = {{We have studied the effect of proton irradiation on Cu(In,Ga)Se-2 (CIGS)
   thin films using photoluminescence (PL). We used a 10 MeV proton beam
   with varying doses from 10(9) to 10(12) cm(-2). Intensity-dependent low
   temperature PL measurements suggest that the proton irradiation does not
   create a new defect level but instead changes the number of preexisting
   defects in the detection range of the PL system. By comparing PL spectra
   after the proton irradiation with those obtained after thermal annealing
   under inert gas aswell as under hydrogen gas ambient, we find that the
   irradiation-induced change in the defect structure does not originate
   from the incorporation of hydrogen but fromenergetics of the irradiating
   particles. Electrical resistivity of the proton irradiated CIGS thin
   films is shown to decrease after the proton irradiation, and this is
   explained by the reduction of the concentration of compensating
   donor-like defects, specifically selenium vacancies, based on the PL
   results. (C) 2016 Elsevier B.V. All rights reserved.}},
