@article{ ISI:000379620200025,
Author = {Theis, Thomas and Ortiz, Jr., Gerardo X. and Logan, Angus W. J. and
   Claytor, Kevin E. and Feng, Yesu and Huhn, William P. and Blum, Volker
   and Malcolmson, Steven J. and Chekmenev, Eduard Y. and Wang, Qiu and
   Warren, Warren S.},
Title = {{Direct and cost-efficient hyperpolarization of long-lived nuclear spin
   states on universal N-15(2)-diazirine molecular tags}},
Journal = {{SCIENCE ADVANCES}},
Year = {{2016}},
Volume = {{2}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{Conventional magnetic resonance (MR) faces serious sensitivity
   limitations which can be overcome by hyperpolarization methods, but the
   most common method (dynamic nuclear polarization) is complex and
   expensive, and applications are limited by short spin lifetimes
   (typically seconds) of biologically relevant molecules. We use a
   recently developed method, SABRE-SHEATH, to directly hyperpolarize
   N-15(2) magnetization and long-lived N-15(2) singlet spin order, with
   signal decay time constants of 5.8 and 23 min, respectively. We find
   >10,000-fold enhancements generating detectable nuclear MR signals that
   last for over an hour. N-15(2)-diazirines represent a class of
   particularly promising and versatile molecular tags, and can be
   incorporated into a wide range of biomolecules without significantly
   altering molecular function.}},
