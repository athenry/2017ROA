Title = {{The Relationship Between Software Bug Type and Number of Factors
   Involved in Failures}},
Booktitle = {{2016 IEEE 27TH INTERNATIONAL SYMPOSIUM ON SOFTWARE RELIABILITY
   ENGINEERING WORKSHOPS (ISSREW)}},
Series = {{IEEE International Symposium on Software Reliability Engineering
   Workshops}},
