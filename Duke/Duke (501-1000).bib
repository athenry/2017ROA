@inproceedings{ ISI:000391649800012,
Author = {O'Brien, Niall and Wiesner, Mark and Cummins, Enda},
Book-Group-Author = {{SGEM}},
Title = {{MODELLING ENGINEERED NANOPARTICLE FATE IN AQUATIC SYSTEMS: APPLICATION
   OF ATTACHMENT RATE}},
Booktitle = {{NANO, BIO AND GREEN - TECHNOLOGIES FOR A SUSTAINABLE FUTURE CONFERENCE
   PROCEEDINGS, SGEM 2016, VOL III}},
