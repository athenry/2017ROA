@inproceedings{ ISI:000388377100301,
Author = {Hu, Yunyun and Liu, Qing Huo},
Book-Group-Author = {{IEEE}},
Title = {{Enhanced Electromagnetic Measurement with High Contrast Nanoparticles
   Injection}},
Booktitle = {{2016 IEEE ANTENNAS AND PROPAGATION SOCIETY INTERNATIONAL SYMPOSIUM}},
Series = {{IEEE Antennas and Propagation Society International Symposium}},
Year = {{2016}},
Pages = {{623-624}},
