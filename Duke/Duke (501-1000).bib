@article{ ISI:000358929500012,
Author = {Liu, Zhe and Shen, Liang-Bo and Kuang, Xi-Wen and Zhu, Hong},
Title = {{Effect of Ag@C on the Dielectric Properties of Ag@C/PVDF-TrFE Composites}},
Journal = {{JOURNAL OF ADVANCED PHYSICS}},
Year = {{2015}},
Volume = {{4}},
Number = {{4}},
Pages = {{375-379}},
Month = {{DEC}},
Abstract = {{The core-shell Ag@C nanoparticles were prepared by hydrothermal method
   and the Ag@C/PVDF-TrFE composites were prepared by solution cast method.
   TEM showed that the silver cores with diameters in the range of 100-120
   nm are each covered with a carbon shell about 60-80 nm thick. SEM
   displayed that the Ag@C nanoparticles are homogenously dispersed in the
   PVDF-TrFE matrix. The DSC results indicated that the crystallinity of
   the composites decreased with the increasing content of the Ag@C
   nanoparticles. The dielectric tests showed that the permittivity of the
   composites increased obviously over the PVDF-TrFE with increasing
   content of Ag@C nanoparticles, owing to the interfacial polarization.
   The tan delta of the composites remained at a low level (similar to 0.03
   at 1000 Hz) because the carbon layer provides a barrier layer between
   the conductive fillers. Furthermore, the permittivity and the tan delta
   of the composites increased with increase of temperature.}},
