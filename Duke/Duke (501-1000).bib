@inproceedings{ ISI:000383223700048,
Author = {Malof, Jordan M. and Collins, Leslie M.},
Editor = {{Bishop, SS and Isaacs, JC}},
Title = {{Using queuing models to aid design and guide research effort for
   multi-modality buried target detection systems}},
Booktitle = {{DETECTION AND SENSING OF MINES, EXPLOSIVE OBJECTS, AND OBSCURED TARGETS
   XXI}},
