@article{ ISI:000370032600001,
Author = {Muralidharan, Sreraman and Li, Linshu and Kim, Jungsang and Luetkenhaus,
   Norbert and Lukin, Mikhail D. and Jiang, Liang},
Title = {{Optimal architectures for long distance quantum communication}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2016}},
Volume = {{6}},
Month = {{FEB 15}},
Abstract = {{Despite the tremendous progress of quantum cryptography, efficient
   quantum communication over long distances (>= 1000 km) remains an
   outstanding challenge due to fiber attenuation and operation errors
   accumulated over the entire communication distance. Quantum repeaters
   (QRs), as a promising approach, can overcome both photon loss and
   operation errors, and hence significantly speedup the communication
   rate. Depending on the methods used to correct loss and operation
   errors, all the proposed QR schemes can be classified into three
   categories (generations). Here we present the first systematic
   comparison of three generations of quantum repeaters by evaluating the
   cost of both temporal and physical resources, and identify the optimized
   quantum repeater architecture for a given set of experimental parameters
   for use in quantum key distribution. Our work provides a roadmap for the
   experimental realizations of highly efficient quantum networks over
   transcontinental distances.}},
