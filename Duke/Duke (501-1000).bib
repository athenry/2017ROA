@article{ ISI:000366491400001,
Author = {Mount, Emily and Kabytayev, Chingiz and Crain, Stephen and Harper, Robin
   and Baek, So-Young and Vrijsen, Geert and Flammia, Steven T. and Brown,
   Kenneth R. and Maunz, Peter and Kim, Jungsang},
Title = {{Error compensation of single-qubit gates in a surface-electrode ion trap
   using composite pulses}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2015}},
Volume = {{92}},
Number = {{6}},
Month = {{DEC 16}},
Abstract = {{The fidelity of laser-driven quantum logic operations on trapped ion
   qubits tend to be lower than microwave-driven logic operations due to
   the difficulty of stabilizing the driving fields at the ion location.
   Through stabilization of the driving optical fields and use of composite
   pulse sequences, we demonstrate high-fidelity single-qubit gates for the
   hyperfine qubit of a Yb-171(+) ion trapped in a microfabricated
   surface-electrode ion trap. Gate error is characterized using a
   randomized benchmarking protocol and an average error per randomized
   Clifford group gate of 3.6(3) x 10(-4) is measured. We also report
   experimental realization of palindromic pulse sequences that scale
   efficiently in sequence length.}},
