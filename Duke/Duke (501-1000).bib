@inproceedings{ ISI:000387497400459,
Author = {Rouze, Ned C. and Deng, Yufeng and Palmeri, Mark L. and Nightingale,
   Kathryn R.},
Book-Group-Author = {{IEEE}},
Title = {{Robust Characterization of Viscoelastic Materials from Measurements of
   Group Shear Wave Speeds}},
Booktitle = {{2016 IEEE INTERNATIONAL ULTRASONICS SYMPOSIUM (IUS)}},
Series = {{IEEE International Ultrasonics Symposium}},
Year = {{2016}},
Note = {{IEEE International Ultrasonics Symposium (IUS), Tours, FRANCE, SEP
   18-21, 2016}},
Organization = {{IEEE}},
Abstract = {{This study describes a new method to determine the stiffness mu(0) and
   viscosity eta of viscoelastic materials by observing shear wave
   propagation following localized, impulsive excitations and measuring the
   group shear wave speeds V-disp and V-vel determined using the shear wave
   displacement and velocity signals, respectively. The stiffness and
   viscosity are found by analytically solving the equation of motion
   describing shear wave propagation and calculating lookup tables mu(0)
   (V-disp, V-vel) and eta(V-disp, V-vel) to give mu(0) and eta from the
   measured shear wave speeds. Results are presented for three viscoelastic
   phantoms and one approximately elastic phantom. The method is robust
   because group shear wave speeds are easily measured experimentally, the
   lookup tables are relatively insensitive to the size and shape of the
   excitation, and the difference Delta V = V-vel - V-disp gives a first
   order measure of viscosity in the same way that the group shear wave
   speed gives a measure of the material stiffness.}},
