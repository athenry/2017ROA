@inproceedings{ ISI:000386515800085,
Author = {Liu, Hai and Zhang, Yuxian and Long, Zhijun and Han, Fen and Liu, Qing
   Huo},
Book-Group-Author = {{IEEE}},
Title = {{Reverse Time Migration For Subsurface Imaging}},
Booktitle = {{2016 URSI ASIA-PACIFIC RADIO SCIENCE CONFERENCE (URSI AP-RASC)}},
Year = {{2016}},
