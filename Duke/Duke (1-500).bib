@article{ ISI:000374698100012,
Author = {Liu, Xiaolei and Feng, Yu and Cui, Hongtao and Liu, Fangyang and Hao,
   Xiaojing and Conibeer, Gavin and Mitzi, David B. and Green, Martin},
Title = {{The current status and future prospects of kesterite solar cells: a
   brief review}},
Journal = {{PROGRESS IN PHOTOVOLTAICS}},
Year = {{2016}},
Volume = {{24}},
Number = {{6}},
Pages = {{879-898}},
Month = {{JUN}},
Abstract = {{Kesterite-based solar cells are attracting considerable attention in
   recent years, owing to the reduced toxicity and greater abundance of
   their constituent elements. In this brief review, we discuss the current
   status of this important technology by focusing on three key aspects of
   the device: (i) the interface between the kesterite absorber and the Mo
   back contact, (ii) the kesterite absorber bulk defects and grain
   boundaries and (iii) the interface between the kesterite absorber and
