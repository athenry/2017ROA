@article{ ISI:000383824600009,
Author = {Patki, Aniruddha and Frank-Ito, Dennis O.},
Title = {{Characterizing human nasal airflow physiologic variables by nasal index}},
Journal = {{RESPIRATORY PHYSIOLOGY \& NEUROBIOLOGY}},
Year = {{2016}},
Volume = {{232}},
Pages = {{66-74}},
Month = {{OCT}},
Abstract = {{Although variations in nasal index (NI) have been reported to represent
   adaptation to climatic conditions, assessments of NI with airflow
   variables have not been rigorously investigated. This study uses
   computational fluid dynamics modeling to investigate the relationship
   between NI and airflow variables in 16 subjects with normal nasal
   anatomy. Airflow simulations were conducted under constant inspiratory
   pressure. Nasal resistance (NR) against NI showed weak association from
   nostrils to anterior inferior turbinate (R-2 = 0.26) and nostril to
   choanae (R-2 = 0.12). NI accounted for 38\% and 41\% of the respective
   variation in wall shear stress (WSS) and heat flux (HF) at the nasal
   vestibule, and 52\% and 49\% of variability in WSS and HF across the
   entire nose. HF and WSS had strong correlation with NI < 80, and weakly
   correlated with NI > 80; these differences in HF and WSS for NI < 80 and
   NI > 80 were not statistically significant. Results suggest strong
   relationship between NI and both WSS and HF but not NR, particularly in
   subjects with NI < 80. (C) 2016 Elsevier B.V. All rights reserved.}},
