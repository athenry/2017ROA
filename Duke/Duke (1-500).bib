@article{ ISI:000384211900020,
Author = {Liu, Xinpei and Shen, Yiming and Zhang, Xuqian and Lin, Rui and Jia,
   Qiang and Chang, Yixiang and Liu, Wenge and Liu, Wentian},
Title = {{Brachytherapy Using Elastin-Like Polypeptides with I-131 Inhibit Tumor
   Growth in Rabbits with VX2 Liver Tumor}},
Journal = {{DIGESTIVE DISEASES AND SCIENCES}},
Year = {{2016}},
Volume = {{61}},
Number = {{10}},
Pages = {{2921-2927}},
Month = {{OCT}},
Abstract = {{Brachytherapy is a targeted type of radiotherapy utilized in the
   treatment of cancers. Elastin-like polypeptides are a unique class of
   genetically engineered peptide polymers that have several attractive
   properties for brachytherapy.
   To explore the feasibility and application of brachytherapy for VX2
   liver tumor using elastin-like polypeptides with I-131 so as to provide
   reliable experimental evidence for a new promising treatment of liver
   cancer.
   Elastin-like polypeptide as carrier was labeled with I-131 using the
   iodogen method. Ten eligible rabbits with VX2 liver tumor were randomly
   divided into the treatment group (n = 5) and control group (n = 5). The
   treatment group received brachytherapy using elastin-like polypeptide
   with I-131, and in the control group, elastin-like polypeptide was
   injected into the VX2 liver tumor as a control. Periodic biochemical and
   imaging surveillances were required to assess treatment efficacy.
   The stability of elastin-like polypeptide with I-131 in vitro was
   maintained at over 96.8 \% for 96 h. Biochemistry and imaging indicated
   brachytherapy using elastin-like polypeptide with I-131 for liver tumor
   can improve liver function and inhibit tumor growth (P < 0.05).
   Elastin-like polypeptide can be an ideal carrier of I-131 and have high
   labeling efficiency, radiochemical purity and stability. Brachytherapy
   using elastin-like polypeptide with I-131 for liver tumor is a useful
   therapy that possesses high antitumor efficacy advantages.}},
