@article{ ISI:000378428400040,
Author = {Shen, Chen and Xie, Yangbo and Li, Junfei and Cummer, Steven A. and
   Jing, Yun},
Title = {{Asymmetric acoustic transmission through near-zero-index and
   gradient-index metasurfaces}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2016}},
Volume = {{108}},
Number = {{22}},
Month = {{MAY 30}},
Abstract = {{We present a design of acoustic metasurfaces yielding asymmetric
   transmission within a certain frequency band. The design consists of a
   layer of gradient-index metasurface and a layer of low refractive index
   metasurface. Incident waves are controlled in a wave vector dependent
   manner to create strong asymmetric transmission. Numerical simulations
   show that the approach provides high transmission contrast between the
   two incident directions within the designed frequency band. This is
   further verified by experiments. Compared to previous designs, the
   proposed approach yields a compact and planar device. Our design may
   find applications in various scenarios such as noise control and
   therapeutic ultrasound. Published by AIP Publishing.}},
