Address = {{TIERGARTENSTRASSE 17, D-69121 HEIDELBERG, GERMANY}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Mycek, P (Reprint Author), Duke Univ, Dept Mech Engn \& Mat Sci, 144 Hudson Hall,Box 90300, Durham, NC 27708 USA.
