@article{ ISI:000383466500019,
Author = {Hooper, James A. and Baringer, Molly O. and St Laurent, Louis C. and
   Dewar, William K. and Nowacek, Doug},
Title = {{Dissipation processes in the Tongue of the Ocean}},
