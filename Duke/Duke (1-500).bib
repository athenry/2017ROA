@article{ ISI:000378415500015,
Author = {Padmanabhan, Madhusudan A. and Dowell, Earl H. and Thomas, Jeffrey P.
   and Pasiliao, Crystal L.},
Title = {{Store-Induced Limit-Cycle Oscillations Due to Nonlinear Wing-Store
   Attachment}},
Journal = {{JOURNAL OF AIRCRAFT}},
Year = {{2016}},
Volume = {{53}},
Number = {{3}},
Pages = {{778-789}},
Month = {{MAY-JUN}},
Note = {{56th AIAA/ASCE/AHS/ASC Structures, Structural Dynamics, and Materials
   Conference / 33rd Wind Energy Symposium, Kissimmee, FL, JAN 05-09, 2015}},
Organization = {{AIAA; ASCE; AHS; ASC}},
Abstract = {{Several high-performance fighter aircraft exhibit store-induced
   limit-cycle oscillations, leading to pilot discomfort, potential
   structural fatigue, and flight envelope restrictions. The roles of
   various aerodynamic and structural factors causing the limit-cycle
   oscillation are not sufficiently understood, and their numerical
   exploration via time marching is computationally expensive. In this
   paper, the effects of nonlinear stiffness and damping in the wing-store
   attachments of the F-16 aircraft are examined, in the presence of steady
   flow aerodynamic nonlinearity, using the computationally efficient
   harmonic balance method. Structural mechanisms including cubic restoring
   force of both softening and hardening types, freeplay, and Coulomb
   friction are systematically evaluated, and the most likely among these
   are identified by comparing the computed limit-cycle oscillation results
   to flight data. An extension of the harmonic balance method to handle
   nonlinear unsteady aerodynamics along with structural nonlinearity is
   also proposed to enable rapid and accurate limit-cycle oscillation
   assessment of candidate store configurations.}},
