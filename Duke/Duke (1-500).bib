@article{ ISI:000375809700037,
Author = {Arras, Matthias M. L. and Jana, Richard and Muhlstadt, Mike and Maenz,
   Stefan and Andrews, Joseph and Su, Zhiqiang and Grasl, Christian and
   Jandt, Klaus D.},
Title = {{In Situ Formation of Nanohybrid Shish-Kebabs during Electrospinning for
   the Creation of Hierarchical Shish-Kebab Structures}},
Journal = {{MACROMOLECULES}},
Year = {{2016}},
Volume = {{49}},
Number = {{9}},
Pages = {{3550-3558}},
Month = {{MAY 10}},
Abstract = {{We successfully tested the hypothesis that the nanohybrid shish-kebab
   (NHSK) structure, i.e., polymer kebabs on a CNT shish, can be created in
   situ during electrospinning. In addition, the resulting nanofibers
   containing the NHSKs were used to create a hierarchical structure by
   introducing a second level of shish-kebabs: the nanofiber shish-kebab
   (NFSK), i.e., polymer kebabs on the nanofiber shish. A multiwalled
   CNT/poly(epsilon-caprolactone) (MWCNT)/(PCL) solution was electrospun to
   form nanofibers of empty set approximate to 100 nm. The MWCNTs aligned
   in fiber direction within the MWCNT/PCL nanofibers. The first time
   creation of the NHSK during electrospinning was confirmed by bright
   field diffraction contrast in the transmission electron microscope. The
   NFSK was created by incubating the NHSK containing nanofibers in a
   supersaturated PCL solution. A simple model explaining the in situ NHSK
   formation is presented. The hierarchical structure based on the
   shish-kebab morphology is proposed as a high-performing building block
   in future advanced materials.}},
