@article{ ISI:000379164900048,
Author = {Popa, Bogdan-Ioan and Wang, Wenqi and Konneker, Adam and Cummer, Steven
   A. and Rohde, Charles A. and Martin, Theodore P. and Orris, Gregory J.
   and Guild, Matthew D.},
Title = {{Anisotropic acoustic metafluid for underwater operation}},
Journal = {{JOURNAL OF THE ACOUSTICAL SOCIETY OF AMERICA}},
Year = {{2016}},
Volume = {{139}},
Number = {{6}},
Pages = {{3324-3330}},
Month = {{JUN}},
Abstract = {{The paper presents a method to design and characterize mechanically
   robust solid acoustic metamaterials suitable for operation in dense
   fluids such as water. These structures, also called metafluids, behave
   acoustically as inertial fluids characterized by anisotropic mass
   densities and isotropic bulk modulus. The method is illustrated through
   the design and experimental characterization of a metafluid consisting
   of perforated steel plates held together by rubber coated magnetic
   spacers. The spacers are very effective at reducing the effective shear
   modulus of the structure, and therefore effective at minimizing the
   ensuing coupling between the shear and pressure waves inside the solid
