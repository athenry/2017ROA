@article{ ISI:000381847300005,
Author = {Bhatia, Swapnil and LaBoda, Craig and Yanez, Vanessa and
   Haddock-Angelli, Traci and Densmore, Douglas},
Title = {{Permutation Machines}},
Journal = {{ACS SYNTHETIC BIOLOGY}},
Year = {{2016}},
Volume = {{5}},
Number = {{8}},
Pages = {{827-834}},
Month = {{AUG}},
Note = {{21st International Conference on DNA Computing and Molecular Programming
   (DNA), Harvard Univ, Wyss Inst Biologically Inspired Engn, Boston, MA,
   AUG 17-21, 2015}},
Abstract = {{We define a new inversion-based machine called a permuton of n genetic
   elements, which allows the n elements to be rearranged in any of the
   nc(n - 1)center dot(n - 2)center dot center dot center dot 2 = n!
   distinct orderings. We present two design algorithms for architecting
   such a machine. We define a notion of a feasible design and use the
   framework to discuss the feasibility of the permuton architectures. We
   have implemented our design algorithms in a freely usable web-accessible
   software for exploration of these machines. Permutation machines could
   be used as memory elements or state machines and explicitly illustrate a
   rational approach to designing biological systems.}},
