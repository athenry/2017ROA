@article{ ISI:000383378500011,
Author = {Choi, H. C. and Ahn, B. H.},
Title = {{Image alignment by parameter hypersurface learning}},
Journal = {{ELECTRONICS LETTERS}},
Year = {{2016}},
Volume = {{52}},
Number = {{18}},
Pages = {{1526-1527}},
Month = {{SEP 2}},
Abstract = {{A new concept of image alignment method is introduced based on machine
   learning technique. The proposed method estimates image alignment
   parameters by using a regression hypersurface, which maps image feature
   space onto alignment parameter space. The regression hypersurface is
   obtained by learning an appropriate type of neural network from a large
   number of training samples, which consist of image features and
   corresponding alignment parameters. Compared with the previous method
   that utilises a regression hyperplane, this method achieved a
   significant improvement in convergence range to apply for alignment of
   rectangular region in long term image sequences in low frame rate.}},
