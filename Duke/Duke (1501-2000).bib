@article{ ISI:000343135200002,
Author = {Ho, Derek and Kim, Sanghoon and Drake, Tyler K. and Eldridge, Will J.
   and Wax, Adam},
Title = {{Wavelet transform fast inverse light scattering analysis for size
   determination of spherical scatterers}},
Journal = {{BIOMEDICAL OPTICS EXPRESS}},
Year = {{2014}},
Volume = {{5}},
Number = {{10}},
Pages = {{3292-3304}},
Month = {{OCT 1}},
Abstract = {{We present a fast approach for size determination of spherical
   scatterers using the continuous wavelet transform of the angular light
   scattering profile to address the computational limitations of
   previously developed sizing techniques. The potential accuracy, speed,
   and robustness of the algorithm were determined in simulated models of
   scattering by polystyrene beads and cells. The algorithm was tested
   experimentally on angular light scattering data from polystyrene bead
   phantoms and MCF-7 breast cancer cells using a 2D a/LCI system.
   Theoretical sizing of simulated profiles of beads and cells produced
   strong fits between calculated and actual size (r(2) = 0.9969 and r(2) =
   0.9979 respectively), and experimental size determinations were accurate
   to within one micron. (C)2014 Optical Society of America}},
