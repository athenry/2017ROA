@article{ ISI:000345218600009,
Author = {Cleveland, Zackary I. and Virgincar, Rohan S. and Qi, Yi and Robertson,
   Scott H. and Degan, Simone and Driehuys, Bastiaan},
Title = {{3D MRI of impaired hyperpolarized Xe-129 uptake in a rat model of
   pulmonary fibrosis}},
Journal = {{NMR IN BIOMEDICINE}},
Year = {{2014}},
Volume = {{27}},
Number = {{12, SI}},
Pages = {{1502-1514}},
Month = {{DEC}},
Abstract = {{A variety of pulmonary pathologies, in particular interstitial lung
   diseases, are characterized by thickening of the pulmonary blood-gas
   barrier, and this thickening results in reduced gas exchange. Such
   diffusive impairment is challenging to quantify spatially, because the
   distributions of the metabolically relevant gases (CO2 and O-2) cannot
   be detected directly within the lungs. Hyperpolarized (HP) Xe-129 is a
   promising surrogate for these metabolic gases, because MR spectroscopy
   and imaging allow gaseous alveolar Xe-129 to be detected separately from
   Xe-129 dissolved in the red blood cells (RBCs) and the adjacent tissues,
   which comprise blood plasma and lung interstitium. Because Xe-129
   reaches the RBCs by diffusing across the same barrier tissues (blood
   plasma and interstitium) as O-2, barrier thickening will delay Xe-129
   transit and, thus, reduce RBC-specific Xe-129 MR signal. Here we have
   exploited these properties to generate 3D, MR images of Xe-129 uptake by
   the RBCs in two groups of rats. In the experimental group, unilateral
   fibrotic injury was generated prior to imaging by instilling bleomycin
   into one lung. In the control group, a unilateral sham instillation of
   saline was performed. Uptake of Xe-129 by the RBCs, quantified as the
   fraction of RBC signal relative to total dissolved Xe-129 signal, was
   significantly reduced (P=0.03) in the injured lungs of bleomycin-treated
   animals. In contrast, no significant difference (P=0.56) was observed
   between the saline-treated and untreated lungs of control animals.
   Together, these results indicate that 3D MRI of HP Xe-129 dissolved in
   the pulmonary tissues can provide useful biomarkers of impaired
   diffusive gas exchange resulting from fibrotic thickening. Copyright (c)
   2014 John Wiley \& Sons, Ltd.}},
