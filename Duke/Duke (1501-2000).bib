@inproceedings{ ISI:000375810900042,
Author = {Meinzer, Christopher E. and Bittner, Simon L. and Schmitt, Stefan and
   Kielb, Robert E. and Seume, Joerg R.},
Book-Group-Author = {{ASME}},
Title = {{DESIGN OF A SINGLE STAGE TURBINE FOR THE QUANTIFICATION OF AERODYNAMIC
   DAMPING}},
Booktitle = {{PROCEEDINGS OF THE ASME TURBO EXPO: TURBINE TECHNICAL CONFERENCE AND
