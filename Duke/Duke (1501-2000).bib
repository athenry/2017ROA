@inproceedings{ ISI:000370747500042,
Author = {Vartziotis, Fotios and Kavousianos, Xrysovalantis and Georgiou,
   Panagiotis and Chakrabarty, Krishnendu},
Book-Group-Author = {{IEEE}},
Title = {{Test-Access-Mechanism Optimization for Multi-V-dd SoCs}},
Booktitle = {{2015 IEEE INTERNATIONAL TEST CONFERENCE (ITC)}},
Series = {{International Test Conference Proceedings}},
Year = {{2015}},
Note = {{46th IEEE International Test Conference (ITC), Anaheim, CA, OCT 06-08,
   2015}},
Organization = {{IEEE; IEEE Philadelphia Sect}},
Abstract = {{The use of multiple voltage levels introduces new challenges fin-testing
   Multi-V-dd SoCs. Time-Division-Multiplexing (TDM) tackles many of these
   challenges and offers very effective test-schedules. However, the
   effectiveness of TDM for minimizing test time depends on the
   lest-Access-Mechanism (TAM) in the SoC. Single-V-dd TAM optimization
   techniques consider neither the highly constrained test environment of
   multi-V-dd SoCs nor the benefits provided by TDM, therefore they are not
   suitable for multi-V-dd SoCs. In this paper, we propose the first TAXI
   optimization technique fir multi-V-dd SoCs. The proposed method exploits
   unique scheduling opportunities and flexibility offered by TDM and by
   the means of a Branch-\&-Bound approach, it quickly identifies the most
   effective TAM configurations. Experiments using SoCs from industry
   highlight the benefits of the proposed technique on multi-Vu designs,
   for both single-site and multi-site test applications.}},
