Title = {{Noncanonical Self-Assembly of Highly Asymmetric Genetically Encoded
   Polypeptide Amphiphiles into Cylindrical Micelles}},
Journal = {{NANO LETTERS}},
Year = {{2014}},
Volume = {{14}},
