@article{ ISI:000342687200088,
Author = {Srimani, Jaydeep K. and Yao, Guang and Neu, John and Tanouchi, Yu and
   Lee, Tae Jun and You, Lingchong},
Title = {{Linear Population Allocation by Bistable Switches in Response to
   Transient Stimulation}},
Journal = {{PLOS ONE}},
Year = {{2014}},
Volume = {{9}},
Number = {{8}},
Month = {{AUG 20}},
Abstract = {{Many cellular decision processes, including proliferation,
   differentiation, and phenotypic switching, are controlled by bistable
   signaling networks. In response to transient or intermediate input
   signals, these networks allocate a population fraction to each of two
   distinct states (e.g. OFF and ON). While extensive studies have been
   carried out to analyze various bistable networks, they are primarily
   focused on responses of bistable networks to sustained input signals. In
   this work, we investigate the response characteristics of bistable
   networks to transient signals, using both theoretical analysis and
   numerical simulation. We find that bistable systems exhibit a common
   property: for input signals with short durations, the fraction of
   switching cells increases linearly with the signal duration, allowing
   the population to integrate transient signals to tune its response. We
   propose that this allocation algorithm can be an optimal response
   strategy for certain cellular decisions in which excessive switching
   results in lower population fitness.}},
