@inproceedings{ ISI:000381622900131,
Author = {Beirami, Ahmad and Calderbank, Robert and Christiansen, Mark and Duffy,
   Ken and Makhdoumi, Ali and Medard, Muriel},
Book-Group-Author = {{IEEE}},
Title = {{A Geometric Perspective on Guesswork}},
