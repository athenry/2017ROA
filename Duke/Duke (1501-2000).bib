@inproceedings{ ISI:000355579600013,
Author = {Bottenus, Nick and Jakovljevic, Marko and Boctor, Emad and Trahey, Gregg
   E.},
Editor = {{Bosch, JG and Duric, N}},
Title = {{Implementation of swept synthetic aperture imaging}},
Booktitle = {{MEDICAL IMAGING 2015: ULTRASONIC IMAGING AND TOMOGRAPHY}},
