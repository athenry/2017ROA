@article{ ISI:000346686100087,
Author = {Ruhl, Laura S. and Dwyer, Gary S. and Hsu-Kim, Heileen and Hower, James
   C. and Vengosh, Avner},
Title = {{Boron and Strontium Isotopic Characterization of Coal Combustion
   Residuals: Validation of New Environmental Tracers}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2014}},
Volume = {{48}},
Number = {{24}},
Pages = {{14790-14798}},
Month = {{DEC 16}},
Abstract = {{In the U.S., coal fired power plants produce over 136 million tons of
   coal combustion residuals (CCRs) annually. CCRs are enriched in toxic
   elements, and their leachates can have significant impacts on water
   quality. Here we report the boron and strontium isotopic ratios of
   leaching experiments on CCRs from a variety of coal sources
   (Appalachian, Illinois, and Powder River Basins). CCR leachates had a
   mostly negative delta B-11, ranging from -17.6 to +6.3, and Sr-87/Sr-86
   ranging from 0.70975 to 0.71251. Additionally, we utilized these
   isotopic ratios for tracing CCR contaminants in different environments:
   (1) the 2008 Tennessee Valley Authority (TVA) coal ash spill affected
   waters; (2) CCR effluents from power plants in Tennessee and North
   Carolina; (3) lakes and rivers affected by CCR effluents in North
   Carolina; and (4) porewater extracted from sediments in lakes affected
   by CCRs. The boron isotopes measured in these environments had a
   distinctive negative delta B-11 signature relative to background waters.
   In contrast Sr-87/Sr-86 ratios in CCRs were not always exclusively
   different from background, limiting their use as a CCR tracer. This
   investigation demonstrates the validity of the combined geochemical and
   isotopic approach as a unique and practical identification method for
   delineating and evaluating the environmental impact of CCRs.}},
