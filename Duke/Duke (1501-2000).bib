@article{ ISI:000345000000015,
Author = {Crain, S. and Mount, E. and Baek, S. and Kim, J.},
Title = {{Individual addressing of trapped Yb-171(+) ion qubits using a
   microelectromechanical systems-based beam steering system}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2014}},
Volume = {{105}},
Number = {{18}},
Month = {{NOV 3}},
Abstract = {{The ability to individually manipulate the increasing number of qubits
   is one of the many challenges towards scalable quantum information
   processing with trapped ions. Using micro-mirrors fabricated with
   micro-electromechanical systems technology, we focus laser beams on
   individual ions in a linear chain and steer the focal point in two
   dimensions. We demonstrate sequential single qubit gates on multiple
   Yb-171(+) qubits and characterize the gate performance using quantum
   state tomography. Our system features negligible crosstalk to
   neighboring ions (<3 x 10(-4)), and switching speed comparable to
   typical single qubit gate times (<2 mu s). (C) 2014 AIP Publishing LLC.}},
