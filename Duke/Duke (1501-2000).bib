@inproceedings{ ISI:000377414600143,
Author = {Woods, Virginia and Wang, Charles and Bossi, Silvia and Insanally,
   Michele and Trumpis, Michael and Froemke, Robert and Viventi, Jonathan},
Book-Group-Author = {{IEEE}},
Title = {{A Low-cost, 61-channel mu ECoG Array for use in Rodents}},
Booktitle = {{2015 7TH INTERNATIONAL IEEE/EMBS CONFERENCE ON NEURAL ENGINEERING (NER)}},
Series = {{International IEEE EMBS Conference on Neural Engineering}},
Year = {{2015}},
Pages = {{573-576}},
Note = {{7th Annual International IEEE EMBS Conference on Neural Engineering
   (NER), Montpellier, FRANCE, APR 22-24, 2015}},
Organization = {{IEEE; EMBS}},
Abstract = {{Micro-Electrocorticography (mu ECoG) offers a minimally invasive, high
   resolution interface with large areas of cortex. A wide variety of mu
   ECoG designs have been developed and customized {[}1]-{[}4], including
   active, multiplexed arrays {[}5] and arrays on dissolving substrates for
   increased conformal contact {[}6]. However, designing and fabricating
   customized mu ECoG arrays requires access to microfabrication
   facilities, which many neuroscience labs do not have. Microfabrication
   is also typically labor intensive and expensive. Commercial mu ECoG
   arrays with 64 electrodes and coarser dimensions cost approximately
   \$1000, limiting their suitability for chronic implantation in large
   numbers of animals. Here we present a high density (406 mu m spacing),
   flexible (similar to 30 mu m thin), 61-contact mu ECoG electrode array
   fabricated using a low-cost, commercial manufacturing process. The array
   costs just \$26 when ordered in quantities of 100, with the cost per
   electrode increasing slightly when lower quantities are ordered. Fine
   pitch wires minimize the size of the interconnections, enabling chronic
   implantation in rodents. In-house post-processing of the fabricated mu
   ECoG arrays added optional electrode coatings, such as platinum black,
   to reduce the electrode impedance. Our electrode design and
   manufacturing process dramatically improves the accessibility and
   reduces the cost of high-volume, high-resolution neuroscience.}},
