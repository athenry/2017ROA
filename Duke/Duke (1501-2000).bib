@inproceedings{ ISI:000380904702002,
Author = {Mahdavifar, Hessam and Beirami, Ahmad},
Book-Group-Author = {{IEEE}},
Title = {{Diffusion Channel with Poisson Reception Process: Capacity Results and
   Applications}},
Booktitle = {{2015 IEEE INTERNATIONAL SYMPOSIUM ON INFORMATION THEORY (ISIT)}},
Series = {{IEEE International Symposium on Information Theory}},
Year = {{2015}},
Pages = {{1956-1960}},
