@article{ ISI:000348829500002,
Author = {Liu, Ningyu and Spiva, Nicholas and Dwyer, Joseph R. and Rassoul, Hamid
   K. and Free, Dwayne and Cummer, Steven A.},
Title = {{Upward electrical discharges observed above Tropical Depression Dorian}},
Journal = {{NATURE COMMUNICATIONS}},
Year = {{2015}},
Volume = {{6}},
Month = {{JAN}},
Abstract = {{Observation of upward electrical discharges from thunderstorms has been
   sporadically reported in the scientific literature. According to their
   terminal altitudes, they are classified as starters (20-30 km), jets
   (40-50 km) and gigantic jets (70-90 km). They not only have a
   significant impact on the occupied atmospheric volumes but also
   electrically couple different atmospheric regions. However, as they are
   rare and unpredictable, our knowledge of them has been built on
   observations that typically record only one type of such discharges.
   Here we report a close-distance observation of seven upward discharges
   including one starter, two jets and four gigantic jets above Tropical
   Depression Dorian. Our optical and electromagnetic data indicate that
   all events are of negative polarity, suggesting they are initiated in
   the same thundercloud charge region. The data also indicate that the
   lightning-like discharge channel can extend above thunderclouds by about
   30 km, but the discharge does not emit low-frequency electromagnetic
   radiation as normal lightning.}},
