@article{ ISI:000345343100022,
Author = {Manzoni, S. and Katul, G.},
Title = {{Invariant soil water potential at zero microbial respiration explained
   by hydrological discontinuity in dry soils}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2014}},
Volume = {{41}},
Number = {{20}},
Pages = {{7151-7158}},
Month = {{OCT 28}},
Abstract = {{Soil microbial respiration rates decrease with soil drying, ceasing
   below water potentials around -15MPa. A proposed mechanism for this
   pattern is that under dry conditions, microbes are substrate limited
   because solute diffusivity is halted due to breaking of water film
   continuity. However, pore connectivity estimated from hydraulic
   conductivity and solute diffusivity (at Darcy's scale) is typically
   interrupted at much less negative water potentials than microbial
   respiration (-0.1 to -1MPa). It is hypothesized here that the more
   negative respiration thresholds than at the Darcy's scale emerge because
   microbial activity is restricted to microscale soil patches that retain
   some hydrological connectivity even when it is lost at the macroscale.
   This hypothesis is explored using results from percolation theory and
   meta-analyses of respiration-water potential curves and hydrological
   percolation points. When reducing the spatial scale from macroscale to
   microscale, hydrological and respiration thresholds become consistent,
   supporting the proposed hypothesis.}},
