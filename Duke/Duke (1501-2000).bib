@article{ ISI:000348437700033,
Author = {Rafaels, Karin A. and Cutcliffe, Hattie C. and Salzar, Robert S. and
   Davis, Martin and Boggess, Brian and Bush, Bryan and Harris, Robert and
   Rountree, Mark Steve and Sanderson, Ellory and Campman, Steven and Koch,
   Spencer and Bass, Cameron R. Dale},
Title = {{Injuries of the Head from Backface Deformation of Ballistic Protective
   Helmets Under Ballistic Impact}},
Journal = {{JOURNAL OF FORENSIC SCIENCES}},
Year = {{2015}},
Volume = {{60}},
Number = {{1}},
Pages = {{219-225}},
Month = {{JAN}},
Abstract = {{Modern ballistic helmets defeat penetrating bullets by energy transfer
   from the projectile to the helmet, producing helmet deformation. This
   deformation may cause severe injuries without completely perforating the
   helmet, termed behind armor blunt trauma (BABT). As helmets become
   lighter, the likelihood of larger helmet backface deformation under
   ballistic impact increases. To characterize the potential for BABT,
   seven postmortem human head/neck specimens wearing a ballistic
   protective helmet were exposed to nonperforating impact, using a 9mm,
   full metal jacket, 124 grain bullet with velocities of 400-460m/s. An
   increasing trend of injury severity was observed, ranging from simple
   linear fractures to combinations of linear and depressed fractures.
   Overall, the ability to identify skull fractures resulting from BABT can
   be used in forensic investigations. Our results demonstrate a high risk
   of skull fracture due to BABT and necessitate the prevention of BABT as
   a design factor in future generations of protective gear.}},
