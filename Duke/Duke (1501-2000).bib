@article{ ISI:000345085200016,
Author = {Dausch, David E. and Gilchrist, Kristin H. and Carlson, James B. and
   Hall, Stephen D. and Castellucci, John B. and von Ramm, Olaf T.},
Title = {{In Vivo Real-Time 3-D Intracardiac Echo Using PMUT Arrays}},
Journal = {{IEEE TRANSACTIONS ON ULTRASONICS FERROELECTRICS AND FREQUENCY CONTROL}},
Year = {{2014}},
Volume = {{61}},
Number = {{10}},
Pages = {{1754-1764}},
Month = {{OCT}},
Abstract = {{Piezoelectric micromachined ultrasound transducer (PMUT) matrix arrays
   were fabricated containing novel through-silicon interconnects and
   integrated into intracardiac catheters for in vivo real-time 3-D
   imaging. PMUT arrays with rectangular apertures containing 256 and 512
   active elements were fabricated and operated at 5 MHz. The arrays were
   bulk micromachined in silicon-on-insulator substrates, and contained
   flexural unimorph membranes comprising the device silicon, lead
   zirconate titanate (PZT), and electrode layers. Through-silicon
   interconnects were fabricated by depositing a thin-film conformal copper
   layer in the bulk micromachined via under each PMUT membrane and
   photolithographically patterning this copper layer on the back of the
   substrate to facilitate contact with the individually addressable matrix
   array elements. Cable assemblies containing insulated 45-AWG copper
   wires and a termination silicon substrate were thermocompression bonded
   to the PMUT substrate for signal wire interconnection to the PMUT array.
   Side-viewing 14-Fr catheters were fabricated and introduced through the
   femoral vein in an adult porcine model. Real-time 3-D images were
   acquired from the right atrium using a prototype ultrasound scanner.
   Full 60 degrees x 60 degrees volume sectors were obtained with
   penetration depth of 8 to 10 cm at frame rates of 26 to 31 volumes per
   second.}},
