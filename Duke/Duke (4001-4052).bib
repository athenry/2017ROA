@article{ ISI:000300641900004,
Author = {Petersen, Thomas H. and Calle, Elizabeth A. and Colehour, Maegen B. and
   Niklason, Laura E.},
Title = {{Matrix Composition and Mechanics of Decellularized Lung Scaffolds}},
Journal = {{CELLS TISSUES ORGANS}},
