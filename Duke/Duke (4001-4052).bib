@inproceedings{ ISI:000335091200054,
Author = {McGehee, Clark C. and Sah, Si Mohamed and Mann, Brian P.},
Book-Group-Author = {{ASME}},
Title = {{REFINING THE METHOD OF AVERAGING FOR A MORE ACCURATE ANALYTICAL
   APPROXIMATION TO THE BEHAVIORS OF A DAMPED PENDULUM}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
   CONFERENCES AND COMPUTERS AND INFORMATION IN ENGINEERING CONFERENCE
   2012, VOL 1, PTS A AND B}},
Year = {{2012}},
