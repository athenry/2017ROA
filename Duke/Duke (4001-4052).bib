@inproceedings{ ISI:000309010000004,
Author = {Malladi, Krishna T. and Nothaft, Frank A. and Periyathambi, Karthika and
   Lee, Benjamin C. and Kozyrakis, Christos and Horowitz, Mark},
Book-Group-Author = {{IEEE}},
Title = {{Towards Energy-Proportional Datacenter Memory with Mobile DRAM}},
Booktitle = {{2012 39TH ANNUAL INTERNATIONAL SYMPOSIUM ON COMPUTER ARCHITECTURE (ISCA)}},
Series = {{Conference Proceedings Annual International Symposium on Computer
   Architecture}},
Year = {{2012}},
Pages = {{37-48}},
Note = {{39th Annual International Symposium on Computer Architecture (ISCA),
   Portland, OR, JUN 09-13, 2012}},
Organization = {{IEEE; ACM; SIGARCH; IEEE Comp Soc; AMD; ARM; Cavium; Google; HP; IBM
   Res; Intel; Intel Ind Educ; Microsoft Res; Oracle Labs; VMWare}},
Abstract = {{To increase datacenter energy efficiency, we need memory systems that
   keep pace with processor efficiency gains. Currently, servers use DDR3
   memory, which is designed for high bandwidth but not for energy
   proportionality. A system using 20\% of the peak DDR3 bandwidth consumes
   2.3x the energy per bit compared to the energy consumed by a system with
   fully utilized memory bandwidth. Nevertheless, many datacenter
   applications stress memory capacity and latency but not memory
   bandwidth. In response, we architect server memory systems using mobile
   DRAM devices, trading peak bandwidth for lower energy consumption per
   bit and more efficient idle modes. We demonstrate 3-5x lower memory
   power, better proportionality, and negligible performance penalties for
   datacenter workloads.}},
