Journal = {{ENVIRONMENTAL MODELING \& ASSESSMENT}},
Year = {{2013}},
Volume = {{18}},
Number = {{1}},
Pages = {{95-103}},
Month = {{FEB}},
Abstract = {{A method to optimally site river water quality sensors is expanded and
