@inproceedings{ ISI:000352223504060,
Author = {Lu, Wenjie and Ferrari, Silvia},
Book-Group-Author = {{IEEE}},
Title = {{An Approximate Dynamic Programming Approach for Model-free Control of
   Switched Systems}},
