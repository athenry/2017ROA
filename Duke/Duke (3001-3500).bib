@incollection{ ISI:000318253400006,
Author = {Gainza, Pablo and Roberts, Kyle E. and Georgiev, Ivelin and Lilien, Ryan
   H. and Keedy, Daniel A. and Chen, Cheng-Yu and Reza, Faisal and
   Anderson, Amy C. and Richardson, David C. and Richardson, Jane S. and
   Donald, Bruce R.},
Editor = {{Keating, AE}},
Title = {{OSPREY: Protein Design with Ensembles, Flexibility, and Provable
   Algorithms}},
Booktitle = {{METHODS IN PROTEIN DESIGN}},
Series = {{Methods in Enzymology}},
Year = {{2013}},
Volume = {{523}},
Pages = {{87-107}},
Abstract = {{We have developed a suite of protein redesign algorithms that improves
   realistic in silico modeling of proteins. These algorithms are based on
   three characteristics that make them unique: (1) improved flexibility of
   the protein backbone, protein side-chains, and ligand to accurately
   capture the conformational changes that are induced by mutations to the
   protein sequence; (2) modeling of proteins and ligands as ensembles of
   low-energy structures to better approximate binding affinity; and (3) a
   globally optimal protein design search, guaranteeing that the
   computational predictions are optimal with respect to the input model.
   Here, we illustrate the importance of these three characteristics. We
   then describe OSPREY, a protein redesign suite that implements our
   protein design algorithms. OSPREY has been used prospectively, with
   experimental validation, in several biomedically relevant settings. We
   show in detail how OSPREY has been used to predict resistance mutations
   and explain why improved flexibility, ensembles, and provability are
   essential for this application.}},
