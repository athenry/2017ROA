@inproceedings{ ISI:000333958600012,
Author = {Cetinguel, H. Ertan and Dumont, Laura and Nadar, Mariappan S. and
   Thompson, Paul M. and Sapiro, Guillermo and Lenglet, Christophe},
Book-Group-Author = {{IEEE}},
Title = {{Importance Sampling Spherical Harmonics to Improve Probabilistic
   Tractography}},
Booktitle = {{2013 3RD INTERNATIONAL WORKSHOP ON PATTERN RECOGNITION IN NEUROIMAGING
   (PRNI 2013)}},
Series = {{International Workshop on Pattern Recognition in Neuroimaging}},
Year = {{2013}},
Pages = {{46-49}},
Note = {{3rd International Workshop on Pattern Recognition in NeuroImaging
   (PRNI), Philadelphia, PA, JUN 22-24, 2013}},
Organization = {{Siemens; Univ Penn; MICCAI; IEEE; IEEE Comp Soc}},
Abstract = {{We consider the problem of improving the accuracy and reliability of
   probabilistic white matter tractography methods by improving the
   built-in sampling scheme, which randomly draws, from a diffusion model
   such as the orientation distribution function (ODF), a direction of
   propagation. Existing methods employing inverse transform sampling
   require an ad hoc thresholding step to prevent the less likely
   directions from being sampled. We herein propose to perform importance
   sampling of spherical harmonics, which redistributes an input point set
   on the sphere to match the ODF using hierarchical sample warping. This
   produces a point set that is more concentrated around the modes,
   allowing the subsequent inverse transform sampling to generate
   orientations that are in better accordance with the local fiber
   configuration. Integrated into a Kalman filter-based framework, our
   approach is evaluated through experiments on synthetic, phantom, and
   real datasets.}},
