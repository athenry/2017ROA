@article{ ISI:000321145200104,
Author = {Huang, J. C. and Bain, J. A. and Song, W. D. and Li, M. H. and Shi, L.
   P. and Schlesinger, T. E. and Chong, T. C.},
Title = {{Assessing diffusion barriers for phase change memory devices using the
   magnetization of Fe}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{102}},
Number = {{25}},
Month = {{JUN 24}},
Abstract = {{We show a method for studying diffusion barriers for phase change memory
   devices. The method involves placing a barrier layer between a phase
   change material like Ge2Sb2Te5 and Fe and taking note of the
   magnetization changes of the Fe layer before and after anneal. This
   results in a fast, sensitive but qualitative measure of various
   diffusion barriers. Based on this analysis, Ru was found to be good
   diffusion barrier comparable to the conventional TiW, while Pt is a poor
   barrier. These results were then reconfirmed by X-ray photoelectron
   spectroscopy depth studies. (C) 2013 AIP Publishing LLC.}},
