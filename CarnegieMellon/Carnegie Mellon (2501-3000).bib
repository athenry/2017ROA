@inproceedings{ ISI:000339389900054,
Author = {Arunachalam, Saravanan and Woody, Matthew and Rissman, Jeffrey and
   Binkowski, Francis and Wong, Hsi-Wu and Jathar, Shantanu and Robinson,
   Allan},
Editor = {{Steyn, DG and Builtjes, PJH and Timmermans, RMA}},
Title = {{An Enhanced Sub-grid Scale Approach to Characterize Air Quality Impacts
   of Aircraft Emissions}},
