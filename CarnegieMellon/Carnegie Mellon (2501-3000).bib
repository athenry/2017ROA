@inproceedings{ ISI:000393407200009,
Author = {Liu, Beiye and Li, Hai and Chen, Yiran and Li, Xin and Huang, Tingwen
   and Wu, Qing and Barnell, Mark},
Book-Group-Author = {{IEEE}},
Title = {{Reduction and IR-drop Compensations Techniques for Reliable Neuromorphic
   Computing Systems}},
Booktitle = {{2014 IEEE/ACM INTERNATIONAL CONFERENCE ON COMPUTER-AIDED DESIGN (ICCAD)}},
Series = {{ICCAD-IEEE ACM International Conference on Computer-Aided Design}},
Year = {{2014}},
