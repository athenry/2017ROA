@inproceedings{ ISI:000339389900081,
Author = {Fountoukis, Christos and Megaritis, A. G. and van der Gon, Hugo. D. and
   Charalampidis, P. E. and Pilinis, Christodoulos and Pandis, Spyros N.},
Editor = {{Steyn, DG and Builtjes, PJH and Timmermans, RMA}},
Title = {{Simulating Organic Aerosol Over Europe: Concentration, Chemical
   Composition and Sources}},
