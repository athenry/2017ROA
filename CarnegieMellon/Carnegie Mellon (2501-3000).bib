@article{ ISI:000325304600005,
Author = {Guo, Dongzhi and McGaughey, Alan J. H. and Gao, Jinsheng and Fedder,
   Gary K. and Lee, Minyoung and Yao, Shi-Chune},
Title = {{Multiphysics modeling of a micro-scale Stirling refrigeration system}},
Journal = {{INTERNATIONAL JOURNAL OF THERMAL SCIENCES}},
Year = {{2013}},
Volume = {{74}},
Pages = {{44-52}},
Month = {{DEC}},
Abstract = {{A new micro-scale refrigeration system composed of arrays of silicon
   MEMS cooling elements that operate on the Stirling cycle has been
   designed. In this paper, we describe a multiphysics computational
   approach for analyzing the system performance that considers
   compressible flow and heat transfer with a large deformable mesh. The
   regenerator pressure drop and effectiveness are first explored to
   determine the optimal porosity. A value near 0.9 is found to maximize
   the coefficient of performance. To overcome the computational complexity
   brought about by the fine pillar structure in the regenerator, a porous
   medium model is used to allow for modeling of a full element. Parametric
   studies demonstrate the effect of the operating frequency on the cooling
   capacity and the coefficient of performance. (C) 2013 Elsevier Masson
   SAS. All rights reserved.}},
