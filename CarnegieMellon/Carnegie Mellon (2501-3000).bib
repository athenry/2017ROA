@article{ ISI:000337718500002,
Author = {Keelan, Robert and Yamakawa, Soji and Shimada, Kenji and Rabin, Yoed},
Title = {{COMPUTERIZED TRAINING OF CRYOSURGERY - A SYSTEM APPROACH}},
Journal = {{CRYOLETTERS}},
Year = {{2013}},
Volume = {{34}},
Number = {{4}},
Pages = {{324-337}},
Month = {{JUL-AUG}},
Abstract = {{The objective of the current study is to provide the foundation for a
   computerized training platform for cryosurgery. Consistent with clinical
   practice, the training process targets the correlation of the frozen
   region contour with the target region shape, using medical imaging and
   accepted criteria for clinical success. The current study focuses on
   system design considerations, including a bioheat transfer model,
   simulation techniques, optimal cryoprobe layout strategy, and a
   simulation core framework. Two fundamentally different approaches were
   considered for the development of a cryosurgery simulator, based on a
   finite-elements (FE) commercial code (ANSYS) and a proprietary
   finite-difference (FD) code. Results of this study demonstrate that the
   FE simulator is superior in terms of geometric modeling, while the FD
   simulator is superior in terms of runtime. Benchmarking results further
   indicate that the FD simulator is superior in terms of usage of memory
   resources, preprocessing, parallel processing, and post-processing. It
   is envisioned that future integration of a human-interface module and
   clinical data into the proposed computer framework will make
   computerized training of cryosurgery a practical reality.}},
Publisher = {{CRYO LETTERS}},
Address = {{C/O ROYAL VETERINARY COLLEGE, ROYAL COLLEGE ST, LONDON NW1 0TU, ENGLAND}},
