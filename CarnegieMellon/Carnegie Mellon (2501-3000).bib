Author = {Tan, Jiaqi and Gandhi, Rajeev and Narasimhan, Priya},
Book-Group-Author = {{IEEE}},
Title = {{STOVE: Strict, Observable, Verifiable Data and Execution Models for
   Untrusted Applications}},
Booktitle = {{2014 IEEE 6TH INTERNATIONAL CONFERENCE ON CLOUD COMPUTING TECHNOLOGY AND
   SCIENCE (CLOUDCOM)}},
Series = {{International Conference on Cloud Computing Technology and Science}},
Year = {{2014}},
