@article{ ISI:000323093700048,
Author = {Zenyuk, I. V. and Kumbur, E. C. and Litster, S.},
Title = {{Deterministic contact mechanics model applied to electrode interfaces in
   polymer electrolyte fuel cells and interfacial water accumulation}},
Journal = {{JOURNAL OF POWER SOURCES}},
Year = {{2013}},
Volume = {{241}},
Pages = {{379-387}},
Month = {{NOV 1}},
Abstract = {{An elastic deterministic contact mechanics model is applied to the
   compressed micro-porous (MPL) and catalyst layer (CL) interfaces in
   polymer electrolyte fuel cells (PEFCs) to elucidate the interfacial
   morphology. The model employs measured two-dimensional surface profiles
   and computes local surface deformation and interfacial gap, average
   contact resistance, and percent contact area as a function of
   compression pressure. Here, we apply the model to one interface having a
   MPL with cracks and one with a crack-free MPL. The void size
   distributions and water retention curves for the two sets of CL vertical
   bar MPL interfaces under compression are also computed. The CL vertical
   bar MPL interfaces with cracks are observed to have higher roughness,
   resulting in twice the interfacial average gap compared to the
   non-cracked interface at a given level of compression. The results
   indicate that the interfacial contact resistance is roughly the same for
   cracked or non-cracked interfaces due to cracks occupying low percentage
   of overall area. However, the cracked CL vertical bar MPL interface
   yields higher liquid saturation levels at all capillary pressures,
   resulting in an order of magnitude higher water storage capacity
   compared to the smooth interface. The van Genuchten water retention
   curve correlation for log-normal void size distributions is found to fit
   non-cracked CL vertical bar MPL interfaces well. (C) 2013 Elsevier B.V.
   All rights reserved.}},
