@inproceedings{ ISI:000345275100040,
Author = {Mitra, Sumit and Garcia-Herreros, Pablo and Grossmann, Ignacio E.},
Editor = {{Klemes, JJ and Varbanov, PS and Liew, PY}},
Title = {{A Novel Cross-decomposition Multi-cut Scheme for Two-Stage Stochastic
   Programming}},
