@inproceedings{ ISI:000392750900095,
Author = {Kolouri, Soheil and Basu, Saurav and Rohde, Gustavo K.},
Book-Group-Author = {{IEEE}},
Title = {{LEARNING AND VISUALIZING STATISTICAL RELATIONSHIPS BETWEEN PROTEIN
   DISTRIBUTIONS FROM MICROSCOPY IMAGES}},
