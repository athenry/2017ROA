@inproceedings{ ISI:000370974903018,
Author = {Han, Yuanfeng and Marvi, Hamidreza and Sitti, Metin},
Book-Group-Author = {{IEEE}},
Title = {{Fiberbot: A Miniature Crawling Robot Using a Directional Fibrillar Pad}},
Booktitle = {{2015 IEEE INTERNATIONAL CONFERENCE ON ROBOTICS AND AUTOMATION (ICRA)}},
Series = {{IEEE International Conference on Robotics and Automation ICRA}},
Year = {{2015}},
Pages = {{3122-3127}},
Note = {{IEEE International Conference on Robotics and Automation (ICRA),
   Seattle, WA, MAY 26-30, 2015}},
Organization = {{IEEE}},
Abstract = {{Vibration-driven locomotion has been widely used for crawling robot
   studies. Such robots usually have a vibration motor as the actuator and
   a fibrillar structure for providing directional friction on the
   substrate. However, there has not been any studies about the effect of
   fiber structure on robot crawling performance. In this paper, we develop
   Fiberbot, a custom made mini vibration robot, for studying the effect of
   fiber angle on robot velocity, steering, and climbing performance. It is
   known that the friction force with and against fibers depends on the
   fiber angle. Thus, we first present a new fabrication method for making
   millimeter scale fibers at a wide range of angles. We then show that
   using 30 degrees angle fibers that have the highest friction anisotropy
   (ratio of backward to forward friction force) among the other fibers we
   fabricated in this study, Fiberbot speed on glass increases to 13.8 +/-
   0.4 cm/s (compared to v = 0.6 +/- 0.1 cm/s using vertical fibers). We
   also demonstrate that the locomotion direction of Fiberbot depends on
   the tilting direction of fibers and we can steer the robot by rotating
   the fiber pad. Fiberbot could also climb on glass at inclinations of up
   to 10 degrees when equipped with fibers of high friction anisotropy. We
   show that adding a rigid tail to the robot it can climb on glass at 25
   degrees inclines. Moreover, the robot is able to crawl on rough surfaces
   such as wood (v = 10.0 +/- 0.2 cm/s using 30 degrees fiber pad).
   Fiberbot, a low-cost vibration robot equipped with a custom-designed
   fiber pad with steering and climbing capabilities could be used for
   studies on collective behavior on a wide range of topographies as well
   as search and exploratory missions.}},
