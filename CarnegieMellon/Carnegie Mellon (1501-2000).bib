@inproceedings{ ISI:000370822100018,
Author = {Kang, Dongwoo and Baek, Seungjae and Choi, Jongmoo and Lee, Donghee and
   Noh, Sam H. and Mutlu, Onur},
Book-Group-Author = {{IEEE}},
Title = {{Amnesic Cache Management for Non-Volatile Memory}},
Booktitle = {{2015 31ST SYMPOSIUM ON MASS STORAGE SYSTEMS AND TECHNOLOGIES (MSST)}},
Series = {{IEEE Symposium on Mass Storage Systems and Technologies Proceedings-MSST}},
Year = {{2015}},
Note = {{31st Symposium on Massive Storage Systems and Technologies (MSST), Santa
   Clara Univ, Santa Clara, CA, MAY 30-JUN 05, 2015}},
Abstract = {{One characteristic of non-volatile memory (NVM) is that, even though it
   supports non-volatility, its retention capability is limited. To handle
   this issue, previous studies have focused on refreshing or advanced
   error correction code (ECC). In this paper, we take a different approach
   that makes use of the limited retention capability to our advantage.
   Specifically, we employ NVM as a file cache and devise a new scheme
   called amnesic cache management (ACM). The scheme is motivated by our
   observation that most data in a cache are evicted within a short time
   period after they have been entered into the cache, implying that they
   can be written with the relaxed retention capability. This retention
   relaxation can enhance the overall cache performance in terms of latency
   and energy since the data retention capability is proportional to the
   write latency. In addition, to prevent the retention relaxation from
   degrading the hit ratio, we estimate the future reference intervals
   based on the inter-reference gap (IRG) model and manage data adaptively.
   Experimental results with real-world workloads show that our scheme can
   reduce write latency by up to 40\% (30\% on average) and save energy
   consumption by up to 49\% (37\% on average) compared with the
   conventional LRU based cache management scheme.}},
