Author = {Lei, Tian and Liu, Xu and Wu, Lei and Chen, Tianjian and Wang, Yuhui and
   Xiong, Luyao and Wei, Shuaili},
Editor = {{Rau, PLP}},
Title = {{The Impact of Natural Utilization of Traditional Chinese Cultural
   Elements on the User Experience in Mobile Interaction Design}},
Booktitle = {{CROSS-CULTURAL DESIGN: APPLICATIONS IN MOBILE INTERACTION, EDUCATION,
   HEALTH, TRANSPORT AND CULTURAL HERITAGE, CCD 2015, PT II}},
