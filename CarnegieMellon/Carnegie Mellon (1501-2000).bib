Author = {Sangpetch, Akkarit and Kim, Hyong S.},
Editor = {{Pu, C and Mohindra, A}},
Title = {{VDEP: VM Dependency Discovery in Multi-tier Cloud Applications}},
Booktitle = {{2015 IEEE 8TH INTERNATIONAL CONFERENCE ON CLOUD COMPUTING}},
