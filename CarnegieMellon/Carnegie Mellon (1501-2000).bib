@inproceedings{ ISI:000380442800092,
Author = {Huang, Qicheng and Li, Xiao and Yang, Fan and Zeng, Xuan and Li, Xin},
Book-Group-Author = {{IEEE}},
Title = {{SIPredict: Efficient Post-layout Waveform Prediction via System
   Identification}},
Booktitle = {{2015 20TH ASIA AND SOUTH PACIFIC DESIGN AUTOMATION CONFERENCE (ASP-DAC)}},
Series = {{Asia and South Pacific Design Automation Conference Proceedings}},
Year = {{2015}},
Pages = {{460-465}},
Note = {{The 20th Asia and South Pacific Design Automation Conference, Chiba,
   JAPAN, JAN 19-22, 2015}},
Abstract = {{In this paper, we propose a post-layout waveform prediction method by
   System Identification (SI) based on the fact that the waveforms of
   pre-layout and post-layout are always correlated. Mathematical models
   are built to describe the relationships between the pre-layout and
   post-layout simulation results via SI techniques. The model parameters
   are calibrated by using the simulation results of the first few data
   points of pre-layout and post-layout stages. By taking the corresponding
