@article{ ISI:000357608200012,
Author = {Downs, Julie S. and de Bruin, Waendi Bruine and Fischhoff, Baruch and
   Murray, Pamela J.},
Title = {{Behavioral Decision Research Intervention Reduces Risky Sexual Behavior}},
Journal = {{CURRENT HIV RESEARCH}},
Year = {{2015}},
Volume = {{13}},
Number = {{5}},
Pages = {{439-446}},
Abstract = {{Although adolescents are at disproportionate risk for sexually
   transmitted infections, most sex education programs have shown little
   effect on sexual behavior. An interactive video intervention developed
   by our team has been identified as one of a few programs that have been
   documented to reduce sexually transmitted infections in this population.
   Building on behavioral decision research, we used a mental models
   approach to interview young women about their sexual decisions, finding,
   among other things, the strong role of perceived social norms. We based
   our intervention on these results, aiming to help young women identify
   and implement personally and socially acceptable decision strategies. A
   randomized controlled trial found that the video reduced risky sexual
   behavior and the acquisition of chlamydia infection. We recently revised
   the video to suit more diverse audiences, and upgraded it to modern
   standards of cinematography and interactivity. It is now in field trial.}},
