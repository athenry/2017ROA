@article{ ISI:000350160400015,
Author = {Li, Di and Kar, Soummya and Moura, Jose M. F. and Poor, H. Vincent and
   Cui, Shuguang},
Title = {{Distributed Kalman Filtering Over Massive Data Sets: Analysis Through
   Large Deviations of Random Riccati Equations}},
Journal = {{IEEE TRANSACTIONS ON INFORMATION THEORY}},
Year = {{2015}},
Volume = {{61}},
Number = {{3}},
Pages = {{1351-1372}},
Month = {{MAR}},
Abstract = {{This paper studies the convergence of the estimation error process and
   the characterization of the corresponding invariant measure in
   distributed Kalman filtering for potentially unstable and large linear
   dynamic systems. A gossip network protocol termed modified gossip
   interactive Kalman filtering ( M-GIKF) is proposed, where sensors
   exchange their filtered states ( estimates and error covariances) and
   propagate their observations via intersensor communications of rate.
   (gamma) over bar; (gamma) over bar is defined as the averaged number of
   intersensor message passages per signal evolution epoch. The filtered
   states are interpreted as stochastic particles swapped through local
   interaction. This paper shows that the conditional estimation error
   covariance sequence at each sensor under M-GIKF evolves as a random
   Riccati equation ( RRE) with Markov modulated switching. By formulating
   the RRE as a random dynamical system, it is shown that the network
   achieves weak consensus, i. e., the conditional estimation error
   covariance at a randomly selected sensor converges weakly ( in
   distribution) to a unique invariant measure. Further, it is proved that
   as.(gamma) over bar -> infinity this invariant measure satisfies the
   large deviation ( LD) upper and lower bounds, implying that this measure
   converges exponentially fast ( in probability) to the Dirac measure
   delta P{*}, where P{*} is the stable error covariance of the centralized
   ( Kalman) filtering setup. The LD results answer a fundamental question
   on how to quantify the rate at which the distributed scheme approaches
   the centralized performance as the intersensor communication rate
   increases.}},
