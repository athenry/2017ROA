@article{ ISI:000351036100001,
Author = {Meade, Jonathan and Bartlow, Patrick and Trivedi, Ram Narayan and
   Akhtar, Parvez and Ataai, Mohammad M. and Khan, Saleem A. and Domach,
   Michael M.},
Title = {{Effect of plasmid replication deregulation via inc mutations on E-coli
   proteome \& simple flux model analysis}},
Journal = {{MICROBIAL CELL FACTORIES}},
Year = {{2015}},
Volume = {{14}},
Month = {{MAR 8}},
Abstract = {{When the replication of a plasmid based on sucrose selection is
   deregulated via the inc1 and inc2 mutations, high copy numbers (7,000 or
   greater) are attained while the growth rate on minimal medium is
   negligibly affected. Adaptions were assumed to be required in order to
   sustain the growth rate. Proteomics indicated that indeed a number of
   adaptations occurred that included increased expression of ribosomal
   proteins and 2-oxoglutarate dehydrogenase. The operating space
   prescribed by a basic flux model that maintained phenotypic traits (e.g.
   growth, byproducts, etc.) within typical bounds of resolution was
   consistent with the flux implications of the proteomic changes.}},
