@article{ ISI:000358850700001,
Author = {Egan, Paul and Sinko, Robert and LeDuc, Philip R. and Keten, Sinan},
Title = {{The role of mechanics in biological and bio-inspired systems}},
Journal = {{NATURE COMMUNICATIONS}},
Year = {{2015}},
Volume = {{6}},
Month = {{JUL}},
Abstract = {{Natural systems frequently exploit intricate multiscale and multiphasic
   structures to achieve functionalities beyond those of man-made systems.
   Although understanding the chemical make-up of these systems is
   essential, the passive and active mechanics within biological systems
   are crucial when considering the many natural systems that achieve
   advanced properties, such as high strength-to-weight ratios and
   stimuli-responsive adaptability. Discovering how and why biological
   systems attain these desirable mechanical functionalities often reveals
   principles that inform new synthetic designs based on biological
   systems. Such approaches have traditionally found success in medical
   applications, and are now informing breakthroughs in diverse frontiers
   of science and engineering.}},
