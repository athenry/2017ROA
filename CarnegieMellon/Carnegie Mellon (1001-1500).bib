@inproceedings{ ISI:000381622900076,
Author = {Cvetkovic, Milos and Ilic, Marija},
Book-Group-Author = {{IEEE}},
Title = {{Interaction Variables for Distributed Numerical Integration of Nonlinear
   Power System Dynamics}},
Booktitle = {{2015 53RD ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2015}},
