@article{ ISI:000360274600017,
Author = {Chen, Yang and Eslick, John C. and Grossmann, Ignacio E. and Miller,
   David C.},
Title = {{Simultaneous process optimization and heat integration based on rigorous
   process simulations}},
Journal = {{COMPUTERS \& CHEMICAL ENGINEERING}},
