@article{ ISI:000358620700006,
Author = {Tam, Wing Chiu (Jason) and Blanton, Ronald D. (Shawn)},
Title = {{LASIC: Layout Analysis for Systematic IC-Defect Identification Using
   Clustering}},
Journal = {{IEEE TRANSACTIONS ON COMPUTER-AIDED DESIGN OF INTEGRATED CIRCUITS AND
   SYSTEMS}},
Year = {{2015}},
Volume = {{34}},
Number = {{8}},
Pages = {{1278-1290}},
Month = {{AUG}},
Abstract = {{Systematic defects within integrated circuits (ICs) are a significant
   source of failures in nanoscale technologies. Identification of
   systematic defects is therefore very important for yield improvement.
   This paper discusses a diagnosis-driven systematic defect identification
   methodology that we call layout analysis for systematic IC-defect
   identification using clustering (LASIC). By clustering images of the
   layout locations that correspond to diagnosed sites for a statistically
   large number of IC failures, LASIC uncovers the common layout features.
   To reduce computation time, only the dominant coefficients of a discrete
   cosine transform analysis of the layout images are used for clustering.
