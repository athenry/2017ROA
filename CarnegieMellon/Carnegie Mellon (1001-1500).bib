@article{ ISI:000354155800067,
Author = {Weis, Allison and Michalek, Jeremy J. and Jaramillo, Paulina and Lueken,
   Roger},
Title = {{Emissions and Cost Implications of Controlled Electric Vehicle Charging
   in the US PJM Interconnection}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2015}},
Volume = {{49}},
Number = {{9}},
Pages = {{5813-5819}},
Month = {{MAY 5}},
Abstract = {{We develop a unit commitment and economic dispatch model to estimate the
   operation costs and the air emissions externality costs attributable to
   new electric vehicle electricity demand under controlled vs uncontrolled
   charging schemes. We focus our analysis on the PJM Interconnection and
   use scenarios that characterize (1) the most recent power plant fleet
   for which sufficient data are available, (2) a hypothetical 2018 power
   plant fleet that reflects upcoming plant retirements, and (3) the 2018
   fleet with increased wind capacity. We find that controlled electric
   vehicle charging can reduce associated generation costs by 23\%-34\% in
   part by shifting loads to lower-cost, higher-emitting coal plants. This
   shift results in increased externality costs of health and environmental
   damages from increased air pollution. On balance, we find that
   controlled charging of electric vehicles produces negative net social
   benefits in the recent PJM grid but could have positive net social
   benefits in a future grid with sufficient coal retirements and wind
   penetration.}},
