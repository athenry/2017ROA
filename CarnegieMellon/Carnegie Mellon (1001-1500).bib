@article{ ISI:000360274600026,
Author = {Dowling, Alexander W. and Balwani, Cheshta and Gao, Qianwen and Biegler,
   Lorenz T.},
Title = {{Optimization of sub-ambient separation systems with embedded cubic
   equation of state thermodynamic models and complementarity constraints}},
Journal = {{COMPUTERS \& CHEMICAL ENGINEERING}},
Year = {{2015}},
Volume = {{81}},
Number = {{SI}},
Pages = {{323-343}},
Month = {{OCT 4}},
