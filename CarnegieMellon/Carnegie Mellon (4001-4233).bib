@inproceedings{ ISI:000320654000083,
Author = {Gopalakrishnan, Ajit and Raghunathan, Arvind U. and Nikovski, Daniel and
   Biegler, Lorenz T.},
Book-Group-Author = {{IEEE}},
Title = {{Global Optimization of Optimal Power Flow Using a Branch \& Bound
