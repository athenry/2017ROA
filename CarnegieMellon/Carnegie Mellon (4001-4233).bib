@inproceedings{ ISI:000320768400280,
Author = {Jakovetic, Dusan and Moura, Jose M. F. and Xavier, Joao},
Editor = {{Matthews, MB}},
Title = {{Fast Cooperative Distributed Learning}},
Booktitle = {{2012 CONFERENCE RECORD OF THE FORTY SIXTH ASILOMAR CONFERENCE ON
