@inproceedings{ ISI:000312381402192,
Author = {Zhang, June and Moura, Jose M. F.},
Book-Group-Author = {{IEEE}},
Title = {{ACCOUNTING FOR TOPOLOGY IN SPREADING CONTAGION IN NON-COMPLETE NETWORKS}},
Booktitle = {{2012 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
