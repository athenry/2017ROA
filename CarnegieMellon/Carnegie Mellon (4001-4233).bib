@article{ ISI:000309107200036,
Author = {Li, Wen and Pistorius, P. Chris},
Title = {{Effects of Thiocyanate on Anodic Dissolution of Iron, Chromium, Nickel
   and Type 304 Stainless Steel I. Electrochemical Measurements and Surface
   Morphology}},
Journal = {{JOURNAL OF THE ELECTROCHEMICAL SOCIETY}},
Year = {{2012}},
Volume = {{159}},
Number = {{11}},
Pages = {{C513-C518}},
Abstract = {{Recently published work investigated the mechanism of the catalytic
   effect of thiocyanate on the active dissolution of ferritic stainless
   steel (Type 430 stainless steel). This work has been extended to Type
   304 stainless steel. Potentiostatic and impedance measurements were
   performed on pure iron, pure chromium, pure nickel and Type 304
   stainless steel in 1 M SO42- solutions at pH = 2.5 and pH = 0. As
   proposed for Type 430 stainless steel, the catalytic effect of
   thiocyanate on anodic dissolution of Type 304 stainless steel also
   appears to be primarily caused by the increased dissolution rate of
   chromium, counteracting the formation of the Cr-O-Cr passivating network
   at ledges. Nickel does contribute to the corrosion resistance of Type
   304 stainless steel in the presence of thiocyanate, but its beneficial
   effect appears to be secondary to that of chromium. (C) 2012 The
   Electrochemical Society. {[}DOI: 10.1149/2.068211jes] All rights
   reserved.}},
