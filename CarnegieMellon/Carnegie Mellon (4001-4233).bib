@article{ ISI:000302247900003,
Author = {Tkacik, Daniel S. and Luna-Cruz, Yaitza and Clinton, Nicholas and Spak,
   Scott and Ryan, John},
Title = {{Atmospheric correction for MASTER image data using localized modelled
   and observed meteorology and trace gases}},
Journal = {{REMOTE SENSING LETTERS}},
Year = {{2012}},
Volume = {{3}},
Number = {{3}},
Pages = {{201-209}},
Abstract = {{Atmospheric correction for remote sensing-based studies typically does
   not use information from spatio-temporally resolved meteorological
   models. We assessed the effect of using observations and mesoscale
   weather and chemical transport models on multispectral retrievals of
   land and ocean properties. We performed two atmospheric corrections on
   image data acquired by the Moderate Resolution Imaging Spectroradiometer
   (MODIS)/Advanced Spaceborne Thermal Emission and Reflection Radiometer
   (ASTER) airborne simulator over Monterey Bay, California. One correction
   used local atmospheric profiles of meteorology and trace gases at
   overpass and the other used the 1976 US Standard default atmospheric
   profile in the MODTRAN4 radiative transfer model. We found only minor
   impacts from atmospheric correction in the Fluorescence Line Height
   index of ocean chlorophyll, but substantive differences in retrievals of
   surface temperature and the Normalized Difference Vegetation Index.
   Improvements in sea surface temperature retrieval were validated by in
   situ measurements. Results indicate that spatio-temporally specific
   atmospheric correction factors from mesoscale models can improve
   retrievals of surface properties from remotely sensed image data.}},
