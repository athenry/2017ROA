Author = {Marinack, Jr., Martin C. and Higgs, III, C. Fred},
Book-Group-Author = {{ASME}},
Title = {{A FRICTION INCLUSIVE CELLULAR AUTOMATA APPROACH FOR MODELING KINETIC AND
   TRANSITIONAL GRANULAR FLOWS}},
Booktitle = {{PROCEEDINGS OF THE ASME/STLE INTERNATIONAL JOINT TRIBOLOGY CONFERENCE -
