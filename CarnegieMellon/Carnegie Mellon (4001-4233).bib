@inproceedings{ ISI:000312381402190,
Author = {Schmidt, Aurora and Moura, Jose M. F.},
Book-Group-Author = {{IEEE}},
Title = {{Distributed Field Reconstruction With Model-Robust Basis Pursuit}},
Booktitle = {{2012 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
Series = {{International Conference on Acoustics Speech and Signal Processing
   ICASSP}},
