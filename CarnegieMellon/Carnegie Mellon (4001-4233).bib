@inproceedings{ ISI:000310776203140,
Author = {Arechiga, Nikos and Loos, Sarah M. and Platzer, Andre and Krogh, Bruce
   H.},
Book-Group-Author = {{IEEE}},
Title = {{Using Theorem Provers to Guarantee Closed-Loop System Properties}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
