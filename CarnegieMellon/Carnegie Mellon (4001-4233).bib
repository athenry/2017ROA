@inproceedings{ ISI:000310776204123,
Author = {Krishnamurthy, Yamuna and Messner, William},
Book-Group-Author = {{IEEE}},
Title = {{Root Locus Design with CPISL Compensation and the Parallel Inner Loop
   Form}},
Booktitle = {{2012 AMERICAN CONTROL CONFERENCE (ACC)}},
