@article{ ISI:000304027700034,
Author = {Alesi, Jr., W. Richard and Kitchin, John R.},
Title = {{Evaluation of a Primary Amine-Functionalized Ion-Exchange Resin for CO2
   Capture}},
Journal = {{INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH}},
Year = {{2012}},
Volume = {{51}},
Number = {{19}},
Pages = {{6907-6915}},
Month = {{MAY 16}},
Abstract = {{A primary amine-functionalized polymeric ion-exchange resin (Lewatit VP
   OC 1065, Lanxess) was evaluated for use in CO, capture applications. The
   polymeric resin was characterized by SEM, DRIFTS-IR, N-2 adsorption
   isotherms, and packed bed sorption measurements to determine some of the
   characteristic properties of the resin. Capture capacities ranging from
   1.85 to 1.15 mol CO2/kg sorbent were obtained in a packed bed reactor
   exposed to 10 vol \% CO2 in N-2 at adsorption temperatures ranging from
   30 to 70 degrees C. The capture capacity of the resin was stable over 18
   adsorption/regeneration cycles. The resin was evaluated through
   thermogravimetric analysis to have a low moisture adsorption (1.5 mol
   H2O/kg sorbent). It is possible to completely regenerate the resin under
   1 atm of CO2 at 200 degrees C.}},
