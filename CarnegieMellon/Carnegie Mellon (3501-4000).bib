@article{ ISI:000306082100002,
Author = {Grigoriadis, Christos and Nese, Alper and Matyjaszewski, Krzysztof and
   Pakula, Tadeusz and Butt, Hans-Juergen and Floudas, George},
Title = {{Dynamic Homogeneity by Architectural Design - Bottlebrush Polymers}},
Journal = {{MACROMOLECULAR CHEMISTRY AND PHYSICS}},
Year = {{2012}},
Volume = {{213}},
Number = {{13}},
Pages = {{1311-1320}},
Month = {{JUL 13}},
Abstract = {{The self-organization and dynamics of bottlebrush polymers consisting of
   poly(2-bromoisobutyryloxyethyl methacrylate) (PBiBEM) backbone and
   grafted n-butyl acrylate (PBA) chains are studied as a function of the
   side-chain length by X-ray scattering, dielectric spectroscopy, and
   rheology in relation to the respective homopolymers. All short-range
   correlations are dominated by the PBA side chains, whereas long-range
   correlations reflect the screening of the backbones by the side chains.
   Such correlations show up also in the segmental dynamics. The PBA
   segmental dynamics are slowed-down, whereas the backbone dynamics are
   plasticized by the side chains. Overall the bottle-brush architecture
   imparts dynamic homogeneity to the backbone and side-chain dynamics.}},
