@article{ ISI:000311460200004,
Author = {Iadicola, M. A. and Hu, L. and Rollett, A. D. and Foecke, T.},
Title = {{Crystal plasticity analysis of constitutive behavior of 5754 aluminum
   sheet deformed along bi-linear strain paths}},
