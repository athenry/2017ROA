@article{ ISI:000311819400002,
Author = {Orbay, Guenay and Yuemer, Mehmet Ersin and Kara, Levent Burak},
Title = {{Sketch-based aesthetic product form exploration from existing images
   using piecewise clothoid curves}},
Journal = {{JOURNAL OF VISUAL LANGUAGES AND COMPUTING}},
Year = {{2012}},
Volume = {{23}},
Number = {{6}},
Pages = {{327-339}},
Month = {{DEC}},
Abstract = {{We present a new sketch-based product form exploration technique that
   works from images and sketches of existing products. At the heart of our
   approach, is a multi-stroke curve beautification method and a
   curve-based image deformation algorithm. The proposed approach converts
   groups of strokes into piecewise clothoid curves in order to produce
   visually pleasing shapes. The deformation diffusion algorithm then
   spatially distributes the user specified deformations through out the
   image to produce smooth transformations from the original image to the
   resulting image. We demonstrate the technique on a variety of images
   including photo-realistic images, real product images, and sketches. (c)
   2012 Elsevier Ltd. All rights reserved.}},
