@article{ ISI:000300847100014,
Author = {Zheng, Peng and Greve, David W. and Oppenheim, Irving J. and Chin,
   Tao-Lun and Malone, Vanessa},
Title = {{Langasite Surface Acoustic Wave Sensors: Fabrication and Testing}},
Journal = {{IEEE TRANSACTIONS ON ULTRASONICS FERROELECTRICS AND FREQUENCY CONTROL}},
Year = {{2012}},
Volume = {{59}},
Number = {{2}},
Pages = {{295-303}},
Month = {{FEB}},
Abstract = {{We report on the development of harsh-environment surface acoustic wave
   sensors for wired and wireless operation. Surface acoustic wave devices
   with an interdigitated transducer emitter and multiple reflectors were
   fabricated on langasite substrates. Both wired and wireless temperature
   sensing was demonstrated using radar-mode (pulse) detection. Temperature
   resolution of better than +/- 0.5 degrees C was achieved between 200
   degrees C and 600 degrees C. Oxygen sensing was achieved by depositing a
   layer of ZnO on the propagation path. Although the ZnO layer caused
   additional attenuation of the surface wave, oxygen sensing was
   accomplished at temperatures up to 700 degrees C. The results indicate
   that langasite SAW devices are a potential solution for
   harsh-environment gas and temperature sensing.}},
