@inproceedings{ ISI:000324076700063,
Author = {Yumer, Mehmet Ersin and Kara, Levent Burak},
Book-Group-Author = {{ASME}},
Title = {{CONCEPTUAL DESIGN OF FREEFORM SURFACES FROM UNSTRUCTURED POINT SETS
   USING NEURAL NETWORK REGRESSION}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
