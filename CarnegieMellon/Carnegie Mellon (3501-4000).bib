@article{ ISI:000306548700019,
Author = {Verma, Neerav and Pistorius, Petrus C. and Fruehan, Richard J. and
   Potter, Michael S. and Oltmann, Helmut G. and Pretorius, Eugene B.},
Title = {{Calcium Modification of Spinel Inclusions in Aluminum-Killed Steel:
   Reaction Steps}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS B-PROCESS METALLURGY AND
   MATERIALS PROCESSING SCIENCE}},
Year = {{2012}},
Volume = {{43}},
Number = {{4}},
Pages = {{830-840}},
Month = {{AUG}},
Abstract = {{Calcium treatment is a well-established way to modify solid alumina
   inclusions to liquid or partially liquid calcium aluminates. Spinels
   (Al2O3 center dot xMgO) can also form in liquid steel after aluminum
   deoxidation. Like alumina, the spinels can be modified readily to liquid
   inclusions by a calcium treatment. The modification of spinels was
   studied by observing the transient evolution of inclusions, in
   laboratory and industrial heats. Spinel modification involves the
   preferential reduction of MgO from the spinel, with Mg dissolving in the
   steel, and it proceeds through transient calcium sulfide formation, just
   like in the case of alumina inclusions. Because magnesium dissolves in
   steel after the calcium treatment of spinels, the reoxidation of the
   melt will produce new spinels.}},
