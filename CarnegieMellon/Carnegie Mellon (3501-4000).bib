@article{ ISI:000306721000022,
Author = {Weimer, James and Krogh, Bruce H. and Small, Mitchel J. and Sinopoli,
   Bruno},
Title = {{An approach to leak detection using wireless sensor networks at carbon
   sequestration sites}},
Journal = {{INTERNATIONAL JOURNAL OF GREENHOUSE GAS CONTROL}},
Year = {{2012}},
Volume = {{9}},
Pages = {{243-253}},
Month = {{JUL}},
Abstract = {{This paper concerns the problem of detecting leaks at carbon
   sequestration sites through the monitoring of CO2 levels using a
   wireless sensor network (WSN). By applying a basic linear dynamic model
   for an advection-diffusion process, a model-based detection strategy
   called the Iterative Partial Sequential Probability Ratio Test (IPSPRT)
   can be employed to detect and localize multiple leaks. A 3-D CO2
   transport model is employed to provide a proof of concept simulated
   evaluation of the IPSPRT against a windowed-average approach in terms of
   time-to-decision vs. probability of false alarm and probability of a
   missed alarm. Despite using only a basic linear dynamic model, the
   IPSPRT is shown to achieve exponentially increasing better
   time-to-decision than the windowed-average approach as the probability
   of false alarm and probability of a missed alarm are decreased. A test
   bed implementation is described and employed to evaluate the performance
   of the IPSPRT in the presence of common WSN errors. (C) 2012 Elsevier
   Ltd. All rights reserved.}},
