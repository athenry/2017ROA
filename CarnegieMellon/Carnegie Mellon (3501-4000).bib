@article{ ISI:000301901200025,
Author = {Shin, E. J. and Jung, A. and Choi, S. -H. and Rollett, A. D. and Park,
   S. S.},
Title = {{A theoretical prediction of twin variants in extruded AZ31 Mg alloys
   using the microstructure based crystal plasticity finite element method}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2012}},
Volume = {{538}},
Pages = {{190-201}},
Month = {{MAR 15}},
Abstract = {{A resolved shear stress (RSS) criterion and the
   microstructure-based-crystal plasticity finite element method (MB-CPFEM)
   were used to analyze the activation of twin variants in extruded AZ31 Mg
   alloys during ex situ uniaxial compression. The RSS criterion, which is
   simply based on the Schmid factor, failed to predict the activation of
   twin variants consisting of the second-highest RSS and the third-highest
   RSS. In contrast to the RSS criterion, the MB-CPFEM based on a quasi-3D
   finite element mesh successfully predicted the activation of twin
   variants consisting of the highest RSS and the second-highest RSS. The
   MB-CPFEM demonstrated that local fluctuation of the stress field induces
   the activation of twin variants with the second-highest RSS during
   uniaxial compression. (C) 2012 Elsevier B.V. All rights reserved.}},
