@article{ ISI:000303282400265,
Author = {Vemuri, Sesha Hari and Chung, Pil Seung and Smith, Robert L. and Lee,
   Nae-Eung and Biegler, Lorenz T. and Jhon, Myung S.},
Title = {{Head-disk interface design in magnetic data storage}},
