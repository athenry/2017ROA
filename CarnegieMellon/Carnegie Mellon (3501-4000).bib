Title = {{A study on magnetoelastic properties of Tb-3 (Fe28-xCox) V-1.0 (x=0, 3,
   6) compounds}},
Journal = {{PHYSICA B-CONDENSED MATTER}},
Year = {{2012}},
Volume = {{407}},
