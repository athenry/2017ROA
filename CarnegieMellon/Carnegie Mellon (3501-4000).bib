@article{ ISI:000313695800006,
Author = {Fischbeck, Paul S. and Gerard, David and McCoy, Sean T.},
Title = {{Sensitivity analysis of the build decision for carbon capture and
   sequestration projects}},
Journal = {{GREENHOUSE GASES-SCIENCE AND TECHNOLOGY}},
Year = {{2012}},
Volume = {{2}},
Number = {{1}},
Pages = {{36-45}},
Month = {{FEB}},
Abstract = {{Carbon capture and sequestration (CCS) is a technology that could allow
   for continued use of fossil fuels in a carbon-constrained world,
   smoothing the transition to non-fossil energy sources. Our objective is
   to assess the commercial viability of CCS given pervasive future
   uncertainties, particularly uncertainties about future natural gas and
   CO2 prices. Using data from the Integrated Environmental Control Model
   (IECM), we develop an interactive Excel-based spreadsheet tool to
   compare levelized-average costs of four different new-construction 500
   MW power plants: natural gas combined cycle (NGCC) with CCS, NCGG
   without CCS, supercritical coal with CCS, and supercritical coal without
   CCS. With low natural gas prices, the NGCC without the sequestration
   option is the dominant technology. Overall, CCS projects for either
   natural gas or coal projects are unlikely to be the lowest-cost option
   for CO2 prices less than \$50 per ton. Finally, we examine several
   potential policy levers that could affect the deployment of CCS. (c)
   2012 Society of Chemical Industry and John Wiley \& Sons, Ltd}},
