@article{ ISI:000300214000040,
Author = {McGaughey, Alan J. H. and Jain, Ankit},
Title = {{Nanostructure thermal conductivity prediction by Monte Carlo sampling of
   phonon free paths}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2012}},
Volume = {{100}},
Number = {{6}},
Month = {{FEB 6}},
Abstract = {{We propose a method by which the thermal conductivity of a nanostructure
   with arbitrary geometry can be predicted through Monte Carlo sampling of
   the free paths associated with phonon-phonon and phonon-boundary
   scattering. The required inputs are the nanostructure geometry and the
   bulk phonon frequencies, group velocities, and mean free paths. The
   method is applied to a thin film in the in-plane and cross-plane
   directions and to a polycrystalline bulk material. For the film, a
   faster approach to the bulk thermal conductivity is found compared to
   predictions made using the Matthiessen rule with the bulk mean free path
   and an average phonon-boundary scattering length. (C) 2012 American
   Institute of Physics. {[}doi: 10.1063/1.3683539]}},
