@article{ ISI:000303282402006,
Author = {Wang, Junhua and Li, Jiangui and Ho, S. L. and Fu, W. N. and Zhao,
   Zhigang and Yan, Weili and Sun, Mingui},
Title = {{Analytical study and corresponding experiments for a new resonant
   magnetic charger with circular spiral coils}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2012}},
Volume = {{111}},
Number = {{7}},
Month = {{APR 1}},
Note = {{56th Annual Conference on Magnetism and Magnetic Materials, Scottsdale,
   AZ, OCT 30-30, 2011}},
Abstract = {{This study proposes a new resonant magnetic charger comprising circular
   spiral coils that operate with a strong coupling effect between the
   transmitter and the receiver. The two spiral coils are fitted with
   additional copper tapes to serve as resonant transmitter and receiver
   coils. The magnetic flux distributions are calculated using temporal
   coupled mode theory. Analysis results show that the proposed system can
   dramatically improve the efficiency and extend the power transfer
   distance. Experiments have been carried out in order to verify the
   performance of the system. In particular, the trends of output voltages
   when either the operating frequencies or the transfer distances are
   changing are reported. The system efficiency obtained experimentally is
   also given. Both calculated and measured results are in good agreement.
   (C) 2012 American Institute of Physics. {[}doi:10.1063/1.3670981]}},
