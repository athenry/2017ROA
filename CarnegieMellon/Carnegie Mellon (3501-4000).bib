@article{ ISI:000305861900025,
Author = {Choudhury, Mahbuboor R. and Hsieh, Ming-Kai and Vidic, Radisav D. and
   Dzombak, David A.},
Title = {{Corrosion management in power plant cooling systems using
   tertiary-treated municipal wastewater as makeup water}},
Journal = {{CORROSION SCIENCE}},
Year = {{2012}},
Volume = {{61}},
Pages = {{231-241}},
Month = {{AUG}},
Abstract = {{Additional tertiary treatment of secondary-treated municipal wastewater
   (MWW) is necessary to reduce its scaling potential for use in power
   plant cooling systems. In this study corrosiveness of three
   tertiary-treated municipal wastewaters (acidified MWW, MWW treated by
   nitrification-filtration, and MWW treated by nitrification-filtration
   and granular activated carbon adsorption) towards mild steel, copper,
   and cupronickel alloys was assessed. pH, ammonia, and organic matter
   were identified as important corrosion influencing parameter in
   tertiary-treated municipal wastewater. Corrosiveness of wastewater
   towards metal alloys increased after tertiary treatments. However,
   corrosion of mild steel and copper alloys was controlled within
   acceptable limits using the corrosion inhibitor tolyltriazole. (C) 2012
   Elsevier Ltd. All rights reserved.}},
