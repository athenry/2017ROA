@inproceedings{ ISI:000324076700066,
Author = {Arisoy, Erhan Batuhan and Orbay, Gunay and Kara, Levent Burak},
Book-Group-Author = {{ASME}},
Title = {{APPROXIMATE SURFACING OF CURVE CLOUDS FOR CONCEPTUAL SHAPE CREATION AND
   EVALUATION}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
   CONFERENCES AND COMPUTERS AND INFORMATION IN ENGINEERING CONFERENCE,
   2011, VOL 5, PTS A AND B}},
Year = {{2012}},
