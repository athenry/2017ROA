@article{ ISI:000306144800034,
Author = {Chua, E. K. and Zhao, R. and Shi, L. P. and Chong, T. C. and
   Schlesinger, T. E. and Bain, J. A.},
Title = {{Effect of metals and annealing on specific contact resistivity of
   GeTe/metal contacts}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2012}},
Volume = {{101}},
Number = {{1}},
Month = {{JUL 2}},
Abstract = {{The circular transfer length method was employed to extract the specific
   contact resistivity, rho(c) of GeTe (amorphous and crystalline state)
   with metals (Ni, W, TiW) to quantify the series contact resistance. The
   rho(c) of amorphous-GeTe with metals was also determined for different
   annealing conditions. Higher metal work functions produce lower rho(c)
   for both GeTe states and the rho(c) was reduced further for annealing
   temperatures greater than the GeTe crystallization temperature. This is
   suggested to be a consequence of the higher temperature required to
   diffuse sufficient interstitial metallic atoms to transform the GeTe
   covalent bonding at the interface to metallic bonding. (C) 2012 American
   Institute of Physics. {[}http://dx.doi.org/10.1063/1.4732787]}},
