@article{ ISI:000334983000042,
Author = {Urban, Nathan M. and Holden, Philip B. and Edwards, Neil R. and Sriver,
   Ryan L. and Keller, Klaus},
Title = {{Historical and future learning about climate sensitivity}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2014}},
Volume = {{41}},
Number = {{7}},
Pages = {{2543-2552}},
Month = {{APR 16}},
Abstract = {{Equilibrium climate sensitivity measures the long-term response of
   surface temperature to changes in atmospheric CO2. The range of climate
   sensitivities in the Intergovernmental Panel on Climate Change Fifth
   Assessment Report is unchanged from that published almost 30years
   earlier in the Charney Report. We conduct perfect model experiments
   using an energy balance model to study the rate at which uncertainties
   might be reduced by observation of global temperature and ocean heat
   uptake. We find that a climate sensitivity of 1.5 degrees C can be
   statistically distinguished from 3 degrees C by 2030, 3 degrees C from
   4.5 degrees C by 2040, and 4.5 degrees C from 6 degrees C by 2065.
   Learning rates are slowest in the scenarios of greatest concern (high
   sensitivities), due to a longer ocean response time, which may have
   bearing on wait-and-see versus precautionary mitigation policies.
   Learning rates are optimistic in presuming the availability of whole
   ocean heat data but pessimistic by using simple aggregated metrics and
   model physics.}},
