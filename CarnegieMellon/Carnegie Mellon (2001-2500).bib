Author = {Ding, Hangjun and Zhong, Mingjiang and Kim, Young Jo and Pholpabu,
   Pitirat and Balasubramanian, Aditya and Hui, Chin Ming and He, Hongkun
   and Yang, Huai and Matyjaszewski, Krzysztof and Bettinger, Christopher
   John},
Title = {{Biologically Derived Soft Conducting Hydrogels Using Heparin-Doped
   Polymer Networks}},
