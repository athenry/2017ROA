@article{ ISI:000333480100019,
Author = {Yim, Sehyuk and Jeon, Doyoung},
Title = {{Magnetic Mechanical Capsule Robot for Multiple Locomotion Mechanisms}},
Journal = {{INTERNATIONAL JOURNAL OF CONTROL AUTOMATION AND SYSTEMS}},
Year = {{2014}},
Volume = {{12}},
Number = {{2}},
Pages = {{383-389}},
Month = {{APR}},
Abstract = {{This paper proposes a magnetic mechanical capsule robot which crawls in
   a fluid-filled tube. The developed capsule robot employs two locomotion
   mechanisms simultaneously. It has spiral ribs at both ends, which are
   rotated by a small on-board motor. Such rotating spiral structures
   generate a driving force of the capsule robot. We invented a magnetic
   mechanical mechanism to transfer the rotational motion of the frontal
   part into the linear motion of the middle part. Using this original
   mechanism, the linearly moving part at the middle of the capsule robot
   generates a supportive driving force. The improved mobility is evaluated
   in experiments. The developed capsule robot employing multiple
   locomotion mechanisms moves 44\% faster than the spiral motion-based
   capsule robot. The developed magnetic mechanical mechanism and the
   mobile robotic platform could be used for pipe inspection robots or
   medical robots.}},
