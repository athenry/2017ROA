@article{ ISI:000338620400021,
Author = {Dougherty, Patrick S. M. and Pudjoprawoto, Randyka and Higgs, III, C.
   Fred},
Title = {{Bit cutter-on-rock tribometry: Analyzing friction and
   rate-of-penetration for deep well drilling substrates}},
Journal = {{TRIBOLOGY INTERNATIONAL}},
Year = {{2014}},
Volume = {{77}},
Pages = {{178-185}},
Month = {{SEP}},
Abstract = {{In this paper, techniques for studying the tribology of rock cutting
   were developed using bit cutter-on-rock tribometry (B-CORT).
   Tribological testing was carried out on water-jet fabricated rock disks
   representative of those found during deep well drilling. The tribometer
   was also retrofitted with a variable radius cutter assembly, and a
   system for the capture of in situ rate-of-penetration CROP). Results
   include in situ coefficient of friction (COF) and ROP for O1 tool steel
   cutters on Carthage Marble rock disks. Additionally, this work includes
   validation of the DOC measurement system with optical interferometry.
   The interdependence of interfacial cutting friction, ROP, and disk speed
   are discussed and qualitatively validated against existing studies. (C)
   2014 Elsevier Ltd. All rights reserved.}},
