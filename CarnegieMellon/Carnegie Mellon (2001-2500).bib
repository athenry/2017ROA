Author = {He, Yi-Jun and Zhang, Yan and Ma, Zi-Feng and Sahinidis, Nikolaos V. and
   Tan, Raymond R. and Foo, Dominic C. Y.},
Title = {{Optimal Source-Sink Matching in Carbon Capture and Storage Systems under
   Uncertainty}},
Journal = {{INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH}},
Year = {{2014}},
Volume = {{53}},
Number = {{2}},
Pages = {{778-785}},
Month = {{JAN 15}},
Abstract = {{This study addresses the robust optimal source-sink matching in carbon
   capture and storage (CCS) supply chains under uncertainty. A
   continuous-time uncertain mixed-integer linear programming (MILP) model
   with physical and temporal constraints is developed, where uncertainties
   are described as interval and uniform distributed stochastic parameters.
   A worst-case MILP formulation and a robust stochastic two-stage MILP
   formation are proposed to handle interval and stochastic uncertainties,
   respectively. Then, two illustrative case studies are solved to
   demonstrate the effectiveness of the proposed models for planning CCS
   deployment under uncertainty.}},
