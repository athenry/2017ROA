@article{ ISI:000335643700373,
Author = {Kim, Chang Soo and Jung, Jin-Won and Choi, Dooho and Sahashi, Masashi
   and Kryder, Mark H.},
Title = {{Tunneling magnetoresistance of perpendicular magnetic tunnel junction
   using L1(0) FePt electrodes on MgO/CrRu/TiN under-layers}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2014}},
Volume = {{115}},
Number = {{17}},
Month = {{MAY 7}},
Note = {{58th Annual Conference on Magnetism and Magnetic Materials, Denver, CO,
   NOV 04-08, 2013}},
Abstract = {{A perpendicular magnetic tunnel junction (pMTJ) device was fabricated
   using L1(0)ordered FePt electrodes, which were deposited on MgO(8
   nm)/CrRu(10 nm)/TiN(4 nm) under-layers. It was found that the
   MgO/CrRu/TiN under-layer helps lower the required FePt deposition
   temperature to below 400 degrees C, and provides a well-ordered bottom
   L1(0) FePt electrode with root-mean-square (RMS) surface roughness close
   to 0.4 nm. Magnetoresistance (MR) ratio and resistance-area (RA) were
   measured at room temperature by the current-in-plane tunneling (CIPT)
   method from a lithographically unpatterned PMTJ sample and 138\% and 6.4
   k Omega mu m(2) were obtained, respectively. A PMTJ test pattern, with a
   junction size of 80 X 40 mu m(2), was also fabricated and showed a MR
