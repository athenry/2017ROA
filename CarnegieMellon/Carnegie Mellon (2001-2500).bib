@inproceedings{ ISI:000349076600029,
Author = {Arisoy, Erhan Batuhan and Orbay, Gunay and Kara, Levent Burak},
Editor = {{Zhang, YJ and Tavares, JMRS}},
Title = {{Predictive Modeling for 2D Form Design}},
Booktitle = {{COMPUTATIONAL MODELING OF OBJECTS PRESENTED IN IMAGES: FUNDAMENTALS,
   METHODS, AND APPLICATIONS}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2014}},
