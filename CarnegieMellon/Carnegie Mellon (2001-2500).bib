@inproceedings{ ISI:000362380000012,
Author = {Sylcott, Brian and Michalek, Jeremy J. and Cagan, Jonathan},
Book-Group-Author = {{ASME}},
Title = {{TOWARDS UNDERSTANDING THE ROLE OF INTERACTION EFFECTS IN VISUAL CONJOINT
   ANALYSIS}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
   CONFERENCES AND COMPUTERS AND INFORMATION IN ENGINEERING CONFERENCE,
   2013, VOL 3A}},
