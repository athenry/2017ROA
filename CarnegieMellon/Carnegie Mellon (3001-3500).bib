@article{ ISI:000313329000090,
Author = {Ghosh, Siddhartha and Piazza, Gianluca},
Title = {{Photonic microdisk resonators in aluminum nitride}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2013}},
Volume = {{113}},
Number = {{1}},
Month = {{JAN 7}},
Abstract = {{We demonstrate integrated photonic microdisk resonators in sputtered
   c-axis oriented aluminum nitride (AlN) films. A 400nm thick layer of AlN
   was patterned in a single lithography step with silicon dioxide used for
   the top and bottom cladding layers. AlN disks with a radius of 20 mu m
   at various spacings to an 850 nm wide waveguide were tested. A loaded
   quality factor of 28 350 is shown in these microdisks, with an extracted
   peak shift over power ratio of 0.0495 pm/mu W. The demonstration of
   photonic resonators in a piezoelectric material can lead to novel
   optomechanical functionalities. (C) 2013 American Institute of Physics.
   {[}http://dx.doi.org/10.1063/1.4772601]}},
