@inproceedings{ ISI:000341702100073,
Author = {Perel, Sagi and Sadtler, Patrick T. and Godlove, Jason M. and Ryu,
   Stephen I. and Wang, Wei and Batista, Aaron P. and Chase, Steven M.},
Book-Group-Author = {{IEEE}},
Title = {{Direction and Speed Tuning of Motor-Cortex Multi-Unit Activity and Local
   Field Potentials During Reaching Movements}},
Booktitle = {{2013 35TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
   MEDICINE AND BIOLOGY SOCIETY (EMBC)}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
Year = {{2013}},
