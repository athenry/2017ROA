@inproceedings{ ISI:000320549200060,
Author = {Zaghloul, Usama and Piazza, Gianluca},
Book-Group-Author = {{IEEE}},
Title = {{10-25 NM PIEZOELECTRIC NANO-ACTUATORS AND NEMS SWITCHES FOR MILLIVOLT
   COMPUTATIONAL LOGIC}},
