@inproceedings{ ISI:000331072100012,
Author = {Qian, Zhiliang and Bogdan, Paul and Tsui, Chi-Ying and Marculescu, Radu},
Book-Group-Author = {{IEEE}},
Title = {{Performance Evaluation of Multicore Systems: From Traffic Analysis to
   Latency Predictions (Embedded Tutorial)}},
Booktitle = {{2013 IEEE/ACM INTERNATIONAL CONFERENCE ON COMPUTER-AIDED DESIGN (ICCAD)}},
Series = {{ICCAD-IEEE ACM International Conference on Computer-Aided Design}},
Year = {{2013}},
