@inproceedings{ ISI:000329495800097,
Author = {Garcia-Herreros, Pablo and Grossmann, Ignacio E. and Wassick, John},
Editor = {{Kraslawski, A and Turunen, I}},
Title = {{Design of Supply Chains under the Risk of Facility Disruptions}},
