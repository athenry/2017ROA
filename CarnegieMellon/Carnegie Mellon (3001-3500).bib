@article{ ISI:000313670200102,
Author = {Noman, Mohammad and Sharma, Abhishek A. and Lu, Yi Meng and Skowronski,
   Marek and Salvador, Paul A. and Bain, James A.},
Title = {{Transient characterization of the electroforming process in TiO2 based
   resistive switching devices}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{102}},
Number = {{2}},
Month = {{JAN 14}},
Abstract = {{The transient electroforming process of TiO2-based resistive switching
   devices is investigated using a pulsed voltage method, and the
   electroforming time is found to vary from 10(-8) s to 10(-1) s as
   function of pulse magnitude (3-8 V) and ambient temperature (25-100
   degrees C). Pulsed experiments and thermal simulations reveal that Joule
   self-heating has a significant effect on the electroforming dynamics,
   specially for electroforming voltages above 5.5V where there is little
   dependence on ambient temperature and the electroforming time (10-100
   ns) is much shorter than the device thermal time constant ( approximate
   to 2 mu s). (C) 2013 American Institute of Physics.
   {[}http://dx.doi.org/10.1063/1.4776693]}},
