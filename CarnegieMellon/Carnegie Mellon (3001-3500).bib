@inproceedings{ ISI:000329495800098,
Author = {Marchetti, Pablo A. and Grossmann, Ignacio E. and Bucey, Wiley and
   Majewski, Rita A.},
Editor = {{Kraslawski, A and Turunen, I}},
Title = {{A Multiproduct Feedstock Optimization Model for Polymer Production}},
