@article{ ISI:000314770300116,
Author = {Ricketts, David S. and Chabalko, Matthew J. and Hillenius, Andrew},
Title = {{Experimental demonstration of the equivalence of inductive and strongly
   coupled magnetic resonance wireless power transfer}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{102}},
Number = {{5}},
Month = {{FEB 4}},
Abstract = {{In this work, we show experimentally that wireless power transfer (WPT)
   using strongly coupled magnetic resonance (SCMR) and traditional
   induction are equivalent. We demonstrate that for a given coil
   separation, and to within 4\%, strongly coupled magnetic resonance and
   traditional induction produce the same theoretical efficiency of
   wireless power transfer versus distance. Moreover, we show that the
   difference between traditional induction and strongly coupled magnetic
   resonance is in the implementation of the impedance matching network
   where strongly coupled magnetic resonance uses the mini-loop impedance
   match. The mini-loop impedance mach provides a low-loss, high-ratio
   impedance transformation that makes it desirable for longer distance
   wireless power transfer, where large impedance transformations are
   needed to maximize power transfer. (C) 2013 American Institute of
   Physics. {[}http://dx.doi.org/10.1063/1.4788748]}},
