@article{ ISI:000316085200082,
Author = {Hussin, Rozana and Chen, Yixuan and Luo, Yi},
Title = {{Metal-semiconductor-metal heterojunction diodes consisting of a thin
   layer of crystal silicon}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{102}},
Number = {{9}},
Month = {{MAR 4}},
Abstract = {{Metal-semiconductor-metal heterojunction diodes consisting of thin
   (i.e., 10-70 nm, comparable to carrier mean-free-path) single-crystal
   silicon were investigated. The devices exhibited extremely low series
   resistance, excellent nonlinearity, and exceptionally high
   saturation-current-density (J(0)), up to 10(2)-10(3) times higher than
   the corresponding Schottky diodes. In addition to electrode materials,
   J(0) depends strongly on silicon thickness (D). Excluding the
   contribution from tunneling and image-charge barrier lowering, the
   remaining J(0) (attributed to thermionic emission) increases
   exponentially as D decreases. This is likely due to less scattering in
   silicon that becomes only a barrier, above which carriers are directly
   emitted and collected between metal electrodes. (C) 2013 American
   Institute of Physics. {[}http://dx.doi.org/10.1063/1.4794421]}},
