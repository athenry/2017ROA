@inproceedings{ ISI:000350825600170,
Author = {Mota, Joao F. C. and Xavier, Joao M. F. and Aguiar, Pedro M. Q. and
   Pueschel, Markus},
Book-Group-Author = {{IEEE}},
Title = {{DISTRIBUTED COMPRESSED SENSING ALGORITHMS: COMPLETING THE PUZZLE}},
Booktitle = {{2013 IEEE GLOBAL CONFERENCE ON SIGNAL AND INFORMATION PROCESSING
   (GLOBALSIP)}},
Series = {{IEEE Global Conference on Signal and Information Processing}},
Year = {{2013}},
Pages = {{629}},
Note = {{IEEE Global Conference on Signal and Information Processing (GlobalSIP),
   Austin, TX, DEC 03-05, 2013}},
Organization = {{Inst Elect \& Elect Engineers; IEEE Signal Proc Soc}},
Abstract = {{Reconstructing compressed sensing signals involves solving an
   optimization problem. An example is Basis Pursuit (BP) {[}1], which is
   applicable only in noise- free scenarios. In noisy scenarios, either the
   Basis Pursuit Denoising (BPDN) {[}1] or the Noise- Aware BP (NABP) {[}2]
   can be used. Consider a distributed scenario where the dictionary matrix
   and the vector of observations are spread over the nodes of a network.
   We solve the following open problem: design distributed algorithms that
   solve BPDN with a column partition, i.e., when each node knows only some
   columns of the dictionary matrix, and that solve NABP with a row
   partition, i.e., when each node knows only some rows of the dictionary
