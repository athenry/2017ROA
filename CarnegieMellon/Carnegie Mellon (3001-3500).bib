Book-Group-Author = {{IEEE}},
Title = {{Fast Statistical Analysis of Rare Circuit Failure Events via
   Scaled-Sigma Sampling for High-Dimensional Variation Space}},
Booktitle = {{2013 IEEE/ACM INTERNATIONAL CONFERENCE ON COMPUTER-AIDED DESIGN (ICCAD)}},
Series = {{ICCAD-IEEE ACM International Conference on Computer-Aided Design}},
Year = {{2013}},
