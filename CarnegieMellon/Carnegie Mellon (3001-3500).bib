@incollection{ ISI:000323894500011,
Author = {Armantier, Olivier and de Bruin, Waendi Bruine and Potter, Simon and
   Topa, Giorgio and van der Klaauw, Wilbert and Zafar, Basit},
Editor = {{Arrow, KJ and Bresnahan, TF}},
Title = {{Measuring Inflation Expectations}},
Booktitle = {{ANNUAL REVIEW OF ECONOMICS, VOL 5}},
Series = {{Annual Review of Economics}},
Year = {{2013}},
Volume = {{5}},
Pages = {{273-301}},
Abstract = {{To conduct monetary policy, central banks around the world increasingly
   rely on measures of public inflation expectations. In this article, we
   review findings from an ongoing initiative at the Federal Reserve Bank
   of New York aimed at improving the measurement and our understanding of
   household inflation expectations through surveys. We discuss the
   importance of question wording and the usefulness of new questions to
   elicit an individual's distribution of inflation beliefs. We present
   evidence suggesting that consumers update their inflation expectations
   in response to new information and that information dissemination may
   lead to more informed and reliable reporting of inflation expectations.
   Finally, we report on a financially incentivized experiment suggesting
   that expectations surveys are informative and that respondents generally
   act on their stated beliefs in a way consistent with expected utility
   theory.}},
