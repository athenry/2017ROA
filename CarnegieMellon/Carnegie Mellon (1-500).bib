@article{ ISI:000384282800004,
Author = {Low, Tze Meng and Igual, Francisco D. and Smith, Tyler M. and
   Quintana-Orti, Enrique S.},
Title = {{Analytical Modeling Is Enough for High-Performance BLIS}},
Journal = {{ACM TRANSACTIONS ON MATHEMATICAL SOFTWARE}},
Year = {{2016}},
Volume = {{43}},
Number = {{2}},
Month = {{SEP}},
Abstract = {{We show how the BLAS-like Library Instantiation Software (BLIS)
   framework, which provides a more detailed layering of the GotoBLAS (now
   maintained as OpenBLAS) implementation, allows one to analytically
   determine tuning parameters for high-end instantiations of the
   matrix-matrix multiplication. This is of both practical and scientific
   importance, as it greatly reduces the development effort required for
   the implementation of the level-3 BLAS while also advancing our
   understanding of how hierarchically layered memories interact with
   high-performance software. This allows the community to move on from
   valuable engineering solutions (empirically autotuning) to scientific
   understanding (analytical insight).}},
