@article{ ISI:000381331600039,
Author = {Kwon, Jonghan and Sharma, Abhishek A. and Chen, Chao-Yang and Fantini,
   Andrea and Jurczak, Malgorzata and Herzing, Andrew A. and Bain, James A.
   and Picard, Yoosuf N. and Skowronski, Marek},
Title = {{Transient Thermometry and High-Resolution Transmission Electron
   Microscopy Analysis of Filamentary Resistive Switches}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2016}},
Volume = {{8}},
Number = {{31}},
Pages = {{20176-20184}},
Month = {{AUG 10}},
Abstract = {{We present data on the filament size and temperature distribution
   Hf0.82Al0.18Ox-based Resistive Random Access Memory (RRAM) devices
   obtained by transient thermometry and high-resolution transmission
   electron microscopy (HRTEM). The thermometry shows that the temperature
   of the nonvolatile conducting filament can reach temperatures as high as
   1600 K at the onset of RESET at voltage of 0.8 V and power of 40 mu W.
   The size of the filament was estimated at about 1 mn in diameter. Hot
   filament increases the temperature of the surrounding high resistivity
   oxide, causing it to conduct and carry a significant fraction of the
   total current. The current spreading results in slowing down the
   filament temperature increase at higher power. The results of
   thermometry have been corroborated by HRTEM analysis of the
   as-fabricated and switched RRAM devices. The functional HfAlOx layer in
   as-fabricated devices is amorphous. In devices that were switched, we
   detected a small crystalline region of 10-15 nm in size. The
   crystallization temperature of the HfAlOx was determined to be 850 K in
   an independent annealing experiment. The size of the crystalline region
   agrees with thermal modeling based on the thermometry data. Scanning
   transmission electron microscopy (TEM) coordinated with electron energy
   loss spectroscopy could not detect changes in the chemical makeup of the
   filament.}},
