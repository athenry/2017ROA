@article{ ISI:000384563500013,
Author = {Wang, Hongfei and Blanton, R. D. (Shawn)},
Title = {{Ensemble Reduction via Logic Minimization}},
Journal = {{ACM TRANSACTIONS ON DESIGN AUTOMATION OF ELECTRONIC SYSTEMS}},
Year = {{2016}},
Volume = {{21}},
Number = {{4}},
Month = {{SEP}},
Abstract = {{An ensemble of machine learning classifiers usually improves
   generalization performance and is useful for many applications. However,
   the extra memory storage and computational cost incurred from the
   combined models often limits their potential applications. In this
   article, we propose a new ensemble reduction method called CANOPY that
   significantly reduces memory storage and computations. CANOPY uses a
   technique from logic minimization for digital circuits to select and
   combine particular classification models from an initial pool in the
   form of a Boolean function, through which the reduced ensemble performs
   classification. Experiments on 20 UCI datasets demonstrate that CANOPY
   either outperforms or is very competitive with the initial ensemble and
   one state-of-the-art ensemble reduction method in terms of
   generalization error, and is superior to all existing reduction methods
   surveyed for identifying the smallest numbers of models in the reduced
   ensembles.}},
