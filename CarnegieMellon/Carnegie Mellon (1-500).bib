@article{ ISI:000381117500021,
Author = {Jain, Abhishek and van der Meer, Andries D. and Papa, Anne-Laure and
   Barrile, Riccardo and Lai, Angela and Schlechter, Benjamin L. and
   Otieno, Monicah A. and Louden, Calvert S. and Hamilton, Geraldine A. and
   Michelson, Alan D. and Frelinger, III, Andrew L. and Ingber, Donald E.},
Title = {{Assessment of whole blood thrombosis in a microfluidic device lined by
   fixed human endothelium}},
Journal = {{BIOMEDICAL MICRODEVICES}},
Year = {{2016}},
Volume = {{18}},
Number = {{4}},
Month = {{AUG}},
Abstract = {{The vascular endothelium and shear stress are critical determinants of
   physiological hemostasis and platelet function in vivo, yet current
   diagnostic and monitoring devices do not fully incorporate endothelial
   function under flow in their assessment and, therefore, they can be
   unreliable and inaccurate. It is challenging to include the endothelium
   in assays for clinical laboratories or point-of-care settings because
   living cell cultures are not sufficiently robust. Here, we describe a
   microfluidic device that is lined by a human endothelium that is
   chemically fixed, but still retains its ability to modulate hemostasis
   under continuous flow in vitro even after few days of storage. This
   device lined with a fixed endothelium supports formation of
   platelet-rich thrombi in the presence of physiological shear, similar to
   a living arterial vessel. We demonstrate the potential clinical value of
   this device by showing that thrombus formation and platelet function can
   be measured within minutes using a small volume (0.5 mL) of whole blood
   taken from subjects receiving antiplatelet medications. The inclusion of
   a fixed endothelial microvessel will lead to biomimetic analytical
   devices that can potentially be used for diagnostics and point-of-care
   applications.}},
