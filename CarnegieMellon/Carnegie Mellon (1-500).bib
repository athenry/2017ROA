@article{ ISI:000380061700003,
Author = {Wang, Fa and Cachecho, Paolo and Zhang, Wangyang and Sun, Shupeng and
   Li, Xin and Kanj, Rouwaida and Gu, Chenjie},
Title = {{Bayesian Model Fusion: Large-Scale Performance Modeling of Analog and
   Mixed-Signal Circuits by Reusing Early-Stage Data}},
Journal = {{IEEE TRANSACTIONS ON COMPUTER-AIDED DESIGN OF INTEGRATED CIRCUITS AND
   SYSTEMS}},
Year = {{2016}},
Volume = {{35}},
Number = {{8}},
Pages = {{1255-1268}},
Month = {{AUG}},
Abstract = {{Efficient performance modeling of today's analog and mixed-signal
   circuits is an important yet challenging task, due to the
   high-dimensional variation space and expensive circuit simulation. In
   this paper, we propose a novel performance modeling algorithm that is
   referred to as Bayesian model fusion (BMF) to address this challenge.
   The key idea of BMF is to borrow the information collected from an early
   stage (e.g., schematic level) to facilitate efficient performance
   modeling at a late stage (e.g., post layout). Such a goal is achieved by
   statistically modeling the performance correlation between early and
   late stages through Bayesian inference. Furthermore, to make the
   proposed BMF method of practical utility, four implementation issues,
   including: 1) prior mapping; 2) missing prior knowledge; 3) fast solver;
   and 4) prior and hyper-parameter selection, are carefully considered in
   this paper. Two circuit examples designed in a commercial 32 nm CMOS
   silicon on insulator process demonstrate that the proposed BMF method
   achieves up to 9x runtime speedup over the traditional modeling
   technique without surrendering any accuracy.}},
