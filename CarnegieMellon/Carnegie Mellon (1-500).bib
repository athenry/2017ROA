@article{ ISI:000379924800037,
Author = {Chen, Lei and Wang, Yao and Qin, Fujian and Li, Dongyi},
Title = {{Enhanced Sensitivity With Five-Phase Heterostructure Magnetoelectric
   Sensor at Low Magnetic Bias Field}},
Journal = {{IEEE TRANSACTIONS ON MAGNETICS}},
Year = {{2016}},
Volume = {{52}},
Number = {{7}},
Month = {{JUL}},
Note = {{13th Joint Magnetism and Magnetic Materials (MMM)/Intermag Conference,
   San Diego, CA, JAN 11-15, 2016}},
Organization = {{Amer Inst Phys; IEEE Magnet soc}},
Abstract = {{In this paper, we have proposed a five-phase heterostructure
   magnetoelectric (ME) sensor with high-permeability Fe-based
   nanocrystalline soft-magnetic alloy FeCuNbSiB, ferromagnetic alloy with
   constant elasticity FeNi and piezoelectric ceramic lead zirconium
   titanate (PZT). A built-in magnetic field induced by a spatially varying
   magnetization within the heterostructure leads to a zero-biased ME
   response. The corresponding zero-biased ME coefficient reaches as high
   as 43.75 V/cm . Oe (3.5 V/Oe). Meanwhile, an additional stress produced
   by FeCuNbSiB is transferred to FeNi, which enhances the effective
   resonant strain coefficient for FeCuNbSiB/FeNi at low magnetic bias and
   decreases the required optimum dc magnetic bias H-opt,H- dc. Hence, the
   maximum resonant ME coefficient is found to reach 51.5 V/cm . Oe (4.12
   V/Oe) at H-opt,H- dc of only 33 Oe. H-opt,H- dc for our proposed ME
   sensor is about one-tenth of that for FeNi/PZT/FeNi. Correspondingly,
   the five-phase heterostructure ME sensor is able to possess the strong
   ME sensitivity at low bias or even zero bias.}},
