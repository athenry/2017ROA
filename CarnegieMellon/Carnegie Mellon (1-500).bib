@article{ ISI:000382392200002,
Author = {Shroff, Sameer S. and de Boer, Maarten P.},
Title = {{Full Assessment of Micromachine Friction Within the Rate-State
   Framework: Experiments}},
Journal = {{TRIBOLOGY LETTERS}},
Year = {{2016}},
Volume = {{63}},
Number = {{3}},
Month = {{SEP}},
Abstract = {{This work represents the first full mapping of the microscale kinetic
   friction phase diagram. A micromachined friction test platform that
   allows three orders of magnitude variation of normal load, spring
   constant, and puller velocity is used to achieve this objective. It
   enables transitions between stick-slip and steady sliding through each
   of these variables. Experimentally, the sliding behavior of surface
   micromachined interfaces coated with a
   tridecafluorotris(dimethylamino)silane (FOTAS,
   CF3(CF2)(5)(CH2)(2)Si(N(CH3)(2)) 3) monolayer is examined. Static aging,
   a friction decrease then increase with puller velocity, an instantaneous
   increase in friction in response to a step change in velocity, and
   stick-slip to steady sliding transitions with decreasing normal load and
   increasing velocity, respectively, are observed in a wear-free regime.
   The results form a complete data set that is used in the context of
   coupled rate-state theory equations to develop and validate model-based
   predictions for the friction phase diagram in a companion paper (Shroff
   et al. in Tribol Lett, 2016).}},
