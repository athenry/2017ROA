@article{ ISI:000385636200001,
Author = {Cheng, Ran and Zhu, Jian-Gang and Xiao, Di},
Title = {{Anomalous feedback and negative domain wall resistance}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{2016}},
Volume = {{49}},
Number = {{43}},
Month = {{NOV 2}},
Abstract = {{Magnetic induction can be regarded as a negative feedback effect, where
   the motive-force opposes the change of magnetic flux that generates the
   motive-force. In artificial electromagnetics emerging from spintronics,
   however, this is not necessarily the case. By studying the
   current-induced domain wall dynamics in a cylindrical nanowire, we show
   that the spin motive-force exerting on electrons can either oppose or
   support the applied current that drives the domain wall. The switching
   into the anomalous feedback regime occurs when the strength of the
   dissipative torque beta is about twice the value of the Gilbert damping
   constant alpha. The anomalous feedback manifests as a negative domain
   wall resistance, which has an analogy with the water turbine.}},
