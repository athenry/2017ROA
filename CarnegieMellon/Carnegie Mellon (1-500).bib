@article{ ISI:000377428700005,
Author = {Xin, Hongyi and Nahar, Sunny and Zhu, Richard and Emmons, John and
   Pekhimenko, Gennady and Kingsford, Carl and Alkan, Can and Mutlu, Onur},
Title = {{Optimal seed solver: optimizing seed selection in read mapping}},
Journal = {{BIOINFORMATICS}},
Year = {{2016}},
Volume = {{32}},
Number = {{11}},
Pages = {{1632-1642}},
Month = {{JUN 1}},
Abstract = {{Motivation: Optimizing seed selection is an important problem in read
   mapping. The number of non-overlapping seeds a mapper selects determines
   the sensitivity of the mapper while the total frequency of all selected
   seeds determines the speed of the mapper. Modern seed-and-extend mappers
   usually select seeds with either an equal and fixed-length scheme or
   with an inflexible placement scheme, both of which limit the ability of
   the mapper in selecting less frequent seeds to speed up the mapping
   process. Therefore, it is crucial to develop a new algorithm that can
   adjust both the individual seed length and the seed placement, as well
   as derive less frequent seeds.
   Results: We present the Optimal Seed Solver (OSS), a dynamic programming
   algorithm that discovers the least frequently-occurring set of x seeds
   in an L-base-pair read in O(X x L) operations on average and in O(X x
   L-2) operations in the worst case, while generating a maximum of O(L-2)
   seed frequency database lookups. We compare OSS against four
   state-of-the-art seed selection schemes and observe that OSS provides a
   3-fold reduction in average seed frequency over the best previous seed
   selection optimizations.}},
