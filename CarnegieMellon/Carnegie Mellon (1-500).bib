@article{ ISI:000388818100026,
Author = {Sapiro, David and Webler, Bryan},
Title = {{Fabrication of a Bronze Age Sword using Ancient Techniques}},
Journal = {{JOM}},
Year = {{2016}},
Volume = {{68}},
Number = {{12}},
Pages = {{3180-3185}},
Month = {{DEC}},
Abstract = {{A khopesh was cast and forged for the TMS 2016 Bladesmithing Symposium.
   The khopesh was the first sword style, originating during the Bronze Age
   in the Near East. The manufacturing process used in this study closely
   followed Bronze Age techniques to determine the plausibility of open
   mold casting coupled with cold work and annealing cycles. Forging and
   annealing cycles substantially increased blade strength and diminished
   intergranular delta-phase inclusions. While a functional blade was not
   completed due to casting defects, the process gives valuable insight
   into the effort required to fabricate a khopesh during the Bronze Age.
   Forging and annealing cycles following casting were necessary to produce
   the mechanical properties desired in a sword.}},
