@article{ ISI:000382986000015,
Author = {Zapata-Gonzalez, Ivan and Hutchinson, Robin A. and Payne, Kevin A. and
   Saldivar-Guerra, Enrique},
Title = {{Mathematical Modeling of the Full Molecular Weight Distribution in ATRP
