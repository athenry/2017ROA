@article{ ISI:000384399300033,
Author = {Levy, Elizabeth S. and Tajon, Cheryl A. and Bischof, Thomas S. and
   Iafrati, Pan and Fernandez-Bravo, Angel and Garfield, David J. and
   Chamanzar, Maysamreza and Maharbiz, Michel M. and Sohal, Vikaas S. and
   Schuck, P. James and Cohen, Bruce E. and Chan, Emory M.},
Title = {{Energy-Looping Nanoparticles: Harnessing Excited-State Absorption for
   Deep-Tissue Imaging}},
Journal = {{ACS NANO}},
Year = {{2016}},
Volume = {{10}},
Number = {{9}},
Pages = {{8423-8433}},
Month = {{SEP}},
Abstract = {{Near infrared (NIR) microscopy enables noninvasive imaging in tissue,
   particularly in the NIR-II spectral range (1000-1400 nm) where
   attenuation due to tissue scattering and absorption is minimized.
   Lanthanide-doped upconverting nanocrystals are promising deep-tissue
   imaging probes due to their photostable emission in the visible and NIR,
   but these materials are not efficiently excited at NIR-II wavelengths
   due to the dearth of lanthanide ground-state absorption transitions in
   this window. Here, we develop a class of lanthanide doped imaging probes
   that harness an energy-looping mechanism that facilitates excitation at
   NIR-II wavelengths, such as 1064 nm, that are resonant with
   excited-state absorption transitions but not ground-state absorption.
   Using computational methods and combinatorial screening, we have
   identified Tm3+-doped NaYF4 nanoparticles as efficient looping systems
   that emit at 800 nm under continuous-wave excitation at 1064 nm. Using
   this benign excitation with standard confocal microscopy, energy-looping
   nanoparticles (ELNPs) are imaged in cultured mammalian cells and through
   brain tissue without autofluorescence. The 1 nun imaging depths and 2 pm
   feature sizes are comparable to those demonstrated by state-of-the-art
   multiphoton techniques, illustrating that ELNPs are a promising class of
   NIR probes for high-fidelity visualization in cells and tissue.}},
