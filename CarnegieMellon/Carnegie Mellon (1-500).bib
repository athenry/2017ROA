Author = {Condessa, Filipe and Bioucas-Dias, Jose and Kovacevic, Jelena},
Title = {{Supervised Hyperspectral Image Classification With Rejection}},
Journal = {{IEEE JOURNAL OF SELECTED TOPICS IN APPLIED EARTH OBSERVATIONS AND REMOTE
   SENSING}},
