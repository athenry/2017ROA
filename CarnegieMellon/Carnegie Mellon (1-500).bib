Type = {{Article}},
Language = {{English}},
Affiliation = {{Yagan, O (Reprint Author), Carnegie Mellon Univ, Dept Elect \& Comp Engn, Pittsburgh, PA 15213 USA.
   Yagan, Osman, Carnegie Mellon Univ, Dept Elect \& Comp Engn, Pittsburgh, PA 15213 USA.}},
DOI = {{10.1109/TIT.2016.2574742}},
ISSN = {{0018-9448}},
