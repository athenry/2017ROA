@inproceedings{ ISI:000399937903154,
Author = {Baros, Stefanos and Ilic, Marija},
Book-Group-Author = {{IEEE}},
Title = {{Distributed Dynamic Dispatching of Deloaded Wind DFIGs via Consensus
   Torque Control}},
Booktitle = {{2016 IEEE POWER AND ENERGY SOCIETY GENERAL MEETING (PESGM)}},
Series = {{IEEE Power and Energy Society General Meeting PESGM}},
Year = {{2016}},
Note = {{IEEE-Power-and-Energy-Society General Meeting (PESGM), Boston, MA, JUL
   17-21, 2016}},
Organization = {{IEEE Power \& Energ Soc}},
Abstract = {{In this paper we propose a method for dispatching a fleet of wind
   doubly-fed induction generators (DFIGs) in a fair fashion according to a
   prespecified total power reference. In our context fair means that wind
   generators (WGs) are required to reach the same utilization levels after
   dispatched. We pose the problem as a constrained consensus problem on
   the utilization levels among the rotor-side converters (RSCs). Then, we
   propose a leader-follower protocol that RSCs can adopt to guarantee that
   the WGs reach asymptotic consensus on their utilization levels and that
   their total extracted power from the wind converges to a reference
   value. Finally, we develop a Control Lyapunov Function-based (CLF)
   controller for the rotor side converter (RSC) of each WG that realizes
   the protocol in practice. The effectiveness of our proposed protocol and
   the corresponding RSC controller is verified via simulations on the
   modified IEEE 24-bus RT system.}},
