@inproceedings{ ISI:000390098702106,
Author = {Eletreby, Rashad and Yagan, Osman},
Book-Group-Author = {{IEEE}},
Title = {{Minimum node degree in inhomogeneous random key graphs with unreliable
   links}},
Booktitle = {{2016 IEEE INTERNATIONAL SYMPOSIUM ON INFORMATION THEORY}},
Series = {{IEEE International Symposium on Information Theory}},
