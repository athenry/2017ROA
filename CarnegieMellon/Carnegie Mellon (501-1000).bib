@inproceedings{ ISI:000398536300018,
Author = {Wang, Chen and Kim, Hyong and Morla, Ricardo},
Book-Group-Author = {{IEEE}},
Title = {{QWatch: Detecting and Locating QoE anomaly for VoD in the Cloud}},
Booktitle = {{2016 8TH IEEE INTERNATIONAL CONFERENCE ON CLOUD COMPUTING TECHNOLOGY AND
   SCIENCE (CLOUDCOM 2016)}},
Series = {{International Conference on Cloud Computing Technology and Science}},
Year = {{2016}},
