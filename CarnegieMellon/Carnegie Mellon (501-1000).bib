@article{ ISI:000361249300006,
Author = {Weng, Jinzu and Chen, Xi and Biegler, Lorenz T.},
Title = {{A multi-thread parallel computation method for dynamic simulation of
   molecular weight distribution of multisite polymerization}},
Journal = {{COMPUTERS \& CHEMICAL ENGINEERING}},
Year = {{2015}},
Volume = {{82}},
Pages = {{55-67}},
Month = {{NOV 2}},
Abstract = {{Molecular weight distribution (MWD) is an important quality index of
   polymer products. Many methods have been proposed to dynamically
   simulate the MWD of polymerization, but these methods are normally
   designed for serial computations. In this paper, a multi-thread parallel
   computation method was proposed for multisite free-radical
   polymerization. Analysis of the relationship among different subtasks
   revealed a combined parallel strategy by fully exploiting the parallel
   feature of the process. A good performance was obtained to accelerate
   the dynamic simulation of MWD based on Flory method. We theoretically
