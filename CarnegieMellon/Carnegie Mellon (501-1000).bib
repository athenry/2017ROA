Title = {{Evaluation and Comparison of Different Virtual Reality Environments
   towards Supporting Tasks Done on a Virtual Construction Site}},
Booktitle = {{CONSTRUCTION RESEARCH CONGRESS 2016: OLD AND NEW CONSTRUCTION
   TECHNOLOGIES CONVERGE IN HISTORIC SAN JUAN}},
Year = {{2016}},
Pages = {{2371-2381}},
