Author = {Chang, Jen-Hao Rick and Sankaranarayanan, Aswin C. and Kumar, B. V. K.
   Vijaya},
Book-Group-Author = {{IEEE}},
Title = {{Random Features for Sparse Signal Classification}},
Booktitle = {{2016 IEEE CONFERENCE ON COMPUTER VISION AND PATTERN RECOGNITION (CPVR)}},
Series = {{IEEE Conference on Computer Vision and Pattern Recognition}},
Year = {{2016}},
Pages = {{5404-5412}},
Note = {{29th IEEE Conference on Computer Vision and Pattern Recognition
   Workshops (CVPRW), Las Vegas, NV, JUN 26-JUL 01, 2016}},
Organization = {{IEEE; IEEE Comp Soc}},
Abstract = {{Random features is an approach for kernel-based inference on large
   datasets. In this paper, we derive performance guarantees for random
   features on signals, like images, that enjoy sparse representations and
   show that the number of random features required to achieve a desired
   approximation of the kernel similarity matrix can be significantly
   smaller for sparse signals. Based on this, we propose a scheme termed
   compressive random features that first obtains low-dimensional
   projections of a dataset and, subsequently, derives random features on
   the low-dimensional projections. This scheme provides significant
   improvements in signal dimensionality, computational time, and storage
   costs over traditional random features while enjoying similar
   theoretical guarantees for achieving inference performance. We support
   our claims by providing empirical results across many datasets.}},
