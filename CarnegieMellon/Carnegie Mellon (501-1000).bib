@article{ ISI:000381091400001,
Author = {Chung, Chul E. and Chu, Jung-Eun and Lee, Yunha and van Noije, Twan and
   Jeoung, Hwayoung and Ha, Kyung-Ja and Marks, Marguerite},
Title = {{Global fine-mode aerosol radiative effect, as constrained by
   comprehensive observations}},
