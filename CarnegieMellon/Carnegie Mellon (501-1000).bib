@inproceedings{ ISI:000400601400012,
Author = {Eletreby, Rashad and Yagan, Osman},
Book-Group-Author = {{IEEE}},
Title = {{Node Isolation of Secure Wireless Sensor Networks under a Heterogeneous
   Channel Model}},
Booktitle = {{2016 54TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
Series = {{Annual Allerton Conference on Communication Control and Computing}},
Year = {{2016}},
