@inproceedings{ ISI:000400048105099,
Author = {Liu, Xiaofei and Weerakkody, Sean and Sinopoli, Bruno},
Book-Group-Author = {{IEEE}},
Title = {{Sensor Placement for Reliable Observability: a Structured Systems
   Approach}},
Booktitle = {{2016 IEEE 55TH CONFERENCE ON DECISION AND CONTROL (CDC)}},
