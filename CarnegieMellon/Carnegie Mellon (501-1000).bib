
@article{ ISI:000371371700060,
Author = {Moore, Joe D. and Stegemeier, John P. and Bibby, Kyle and Marinakos,
   Stella M. and Lowry, Gregory V. and Gregory, Kelvin B.},
Title = {{Impacts of Pristine and Transformed Ag and Cu Engineered Nanomaterials
   on Surficial Sediment Microbial Communities Appear Short-Lived}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2016}},
Volume = {{50}},
Number = {{5}},
Pages = {{2641-2651}},
Month = {{MAR 1}},
Abstract = {{Laboratory-based studies have shown that many soluble metal and metal
   oxide engineered nanomaterials (ENM) exert strong toxic effects on
   microorganisms. However, laboratory-based studies lack the complexity of
   natural systems and often use ``as manufactured{''} ENMs rather than
   more environmentally relevant transformed ENMs, leaving open the
   question of whether natural ligands and seasonal variation will mitigate
   ENM impacts. Because ENMs will accumulate in subaquatic sediments, we
   examined the effects of pristine and transformed Ag and Cu ENMs on
   surficial sediment microbial communities in simulated freshwater
   wetlands. Five identical mesocosms were dosed through the water column
   with either Ag-0, Ag2S, CuO or CuS ENMs (nominal sizes of 4.67 +/- 1.4,
   18.1 +/- 3.2, 31.1 +/- 12, and 12.4 +/- 4.1, respectively) or Cu2+.
   Microbial communities were examined at 0, 7, 30, 90, 180, and 300 d
   using qPCR and high-throughput 16S rRNA gene sequencing. Results suggest
   differential short-term impacts of Ag-0 and Ag2S, similarities between
   CuO and CuS, and differences between Cu ENMs and Cu2+. PICRUSt-predicted
   metagenomes displayed differential effects of Ag treatments on
   photosynthesis and of Cu treatments on methane metabolism. By 300 d, all
   metrics pointed to reconvergence of ENM-dosed mesocosm microbial
   community structure and composition, suggesting that the long-term
   microbial community impacts from a pulse of Ag or Cu ENMs are limited.}},
