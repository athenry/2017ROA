@inproceedings{ ISI:000406968900159,
Author = {Kandiraju, Anvitha and Garcia-Herreros, Pablo and Misra, Pratik and
   Arslan, Erdem and Mehta, Sanjay and Grossmann, Ignacio E.},
Editor = {{Kravanja, Z and Bogataj, M}},
Title = {{Capacity Planning with Rational Markets and Demand Uncertainty}},
Booktitle = {{26TH EUROPEAN SYMPOSIUM ON COMPUTER AIDED PROCESS ENGINEERING (ESCAPE),
   PT B}},
Series = {{Computer Aided Chemical Engineering}},
Year = {{2016}},
Volume = {{38B}},
Pages = {{2169-2174}},
Note = {{26th European Symposium on Computer Aided Process Engineering (ESCAPE),
   Portoroz, SLOVENIA, JUN 12-15, 2016}},
Abstract = {{Capacity planning for capital intensive industries is a challenging task
   because expansion plans are highly dependent on market behavior and
   future demands. A suboptimal expansion strategy may lead to unnecessary
   investments and loss of market share. To mitigate the risk associated
   with uncertain demands and to account for market preferences in the face
   of competition, we propose a stochastic bilevel optimization model. The
   model combines two-stage stochastic programming and bilevel optimization
   to consider uncertainty and the behavior of multiple decision-makers
   simultaneously. Uncertainty is considered in scenarios describing
   different realizations of demand. The upper level of the bilevel program
   maximizes the expected net present value obtained from the capacity
   expansion plan, while the lower level minimizes the cost paid by the
   markets in different scenarios. The bilevel model is transformed into a
   single level using a duality-based reformulation. The single-level
   reformulation is implemented on an illustrative example; the results
   demonstrate its advantages over the equivalent deterministic
   formulation.}},
