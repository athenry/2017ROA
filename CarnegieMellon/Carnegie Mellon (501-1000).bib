@inproceedings{ ISI:000382319400004,
Author = {Mirshekari, Mostafa and Pan, Shijia and Zhang, Pei and Noh, Hae Young},
Editor = {{Lynch, JP}},
Title = {{Characterizing Wave Propagation to Improve Indoor Step-Level Person
   Localization using Floor Vibration}},
Booktitle = {{SENSORS AND SMART STRUCTURES TECHNOLOGIES FOR CIVIL, MECHANICAL, AND
   AEROSPACE SYSTEMS 2016}},
Series = {{Proceedings of SPIE}},
Year = {{2016}},
Volume = {{9803}},
