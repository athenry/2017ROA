@inproceedings{ ISI:000390300900030,
Author = {Amini, M. H. and Rahmani, Mostafa and Boroojeni, Kianoosh G. and Atia,
   George and Iyengar, S. S. and Karabasoglu, O.},
Book-Group-Author = {{IEEE}},
Title = {{Sparsity-Based Error Detection in DC Power Flow State Estimation}},
Booktitle = {{2016 IEEE INTERNATIONAL CONFERENCE ON ELECTRO INFORMATION TECHNOLOGY
   (EIT)}},
Series = {{International Conference on Electro Information Technology}},
Year = {{2016}},
Pages = {{263-268}},
Note = {{IEEE International Conference on Electro Information Technology (EIT),
   Univ N Dakota, Grand Forks, ND, MAY 19-21, 2016}},
Organization = {{IEEE}},
Abstract = {{This paper presents a new approach for identifying the measurement error
   in the DC power flow state estimation problem. The proposed algorithm
   exploits the singularity of the impedance matrix and the sparsity of the
   error vector by posing the DC power flow problem as a sparse vector
   recovery problem that leverages the structure of the power system and
   uses l(1)-norm minimization for state estimation. This approach can
   provably compute the measurement errors exactly, and its performance is
   robust to the arbitrary magnitudes of the measurement errors. Hence, the
   proposed approach can detect the noisy elements if the measurements are
   contaminated with additive white Gaussian noise plus sparse noise with
