@article{ ISI:000388685600010,
Author = {Chesterman, J. P. and Amsden, B. G.},
Title = {{Triethylamine-based catalysts for the melt polymerization of carbonate
   monomers}},
Journal = {{POLYMER CHEMISTRY}},
Year = {{2016}},
Volume = {{7}},
Number = {{45}},
Pages = {{6946-6953}},
Abstract = {{This study determined that both triethylamine hydrochloride (TEA center
   dot HCl) and TEA can catalyze the melt polymerization of several
   carbonate monomers at 65-150 degrees C. TEA center dot HCl's catalytic
   activity is especially noteworthy because it is a common by-product in
   the synthesis of carbonate monomers. Melt polymerizations of
   trimethylene carbonate (TMC) catalyzed with TEA achieved monomer
   conversions ranging from 97\% in 6 h at 85 degrees C to 98\% in 1 h at
   110 degrees C and those catalyzed with TEA center dot HCl ranged from
   98\% in 12 h at 85 degrees C to 98\% in 1 h at 110 degrees C using a
   monomer : initiator : catalyst ratio of 50 : 1 : 0.1. By contrast, TMC
   polymerizations conducted with stannous octoate, a widely used catalyst,
   required at least twice as long to achieve the same monomer conversion
   (24 h at 85 degrees C and 2.5 h at 110 degrees C). Catalyst-free
   polymerization was also observed at temperatures and timespans below
   those previously reported, with 99\% conversion in 32 h at 85 degrees C
   and 99\% conversion in 2.5 h at 110 degrees C.}},
Publisher = {{ROYAL SOC CHEMISTRY}},
Address = {{THOMAS GRAHAM HOUSE, SCIENCE PARK, MILTON RD, CAMBRIDGE CB4 0WF, CAMBS,
   ENGLAND}},
Type = {{Article}},
