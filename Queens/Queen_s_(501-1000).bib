@inproceedings{ ISI:000393004000005,
Author = {Koukounian, Viken N. and Mechefske, Chris K.},
Book-Group-Author = {{ASME}},
Title = {{FEM-BEM MODELING AND EXPERIMENTAL VERIFICATION OF THE VIBRO-ACOUSTIC
