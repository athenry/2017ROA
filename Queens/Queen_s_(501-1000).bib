@inproceedings{ ISI:000391254900017,
Author = {McIntyre, David and Naeem, Wasif and Zhang, Cheng},
Book-Group-Author = {{IEEE}},
Title = {{Cooperative Mapping and Exploration using Counter-rotational Potential
   Fields}},
Booktitle = {{2016 27TH IRISH SIGNALS AND SYSTEMS CONFERENCE (ISSC)}},
Year = {{2016}},
Note = {{27th Irish Signals and Systems Conference (ISSC), Ulster Univ, Derry,
   NORTH IRELAND, JUN 21-22, 2016}},
Organization = {{IEEE; Rohde \& Schwarz; Xilinix; ExOrdo; Microelectron Circuits Ctr
   Ireland; VMForensics ltd; Visit Derry; MIDAS; AWR Grp; Natl Instruments;
   Ulster Univ}},
Abstract = {{This paper presents a novel technique for mapping and exploration using
   cooperating autonomous underwater vehicles. Rather than using the
   typical lawnmower sweep pattern to search an entire area, the proposed
   navigational plan involves guiding the formation directly towards each
   object of interest in turn, before arriving at a final goal position.
   This is achieved by the use of traditional artificial potential fields
   alongside counter-rotational potential fields. These clockwise and
   counter- clockwise fields are employed simultaneously by vehicles to
   ensure that the entire object is scanned rather than simply avoided as
   is the case with traditional collision avoidance techniques. The
   proposed methodology allows a formation to have fluid-like motion whilst
   a separation distance between cooperating agents (free of angular
   constraints) is maintained with a greater degree of flexibility than
   traditional formation control approaches. Owing to its nature, this
   technique is suited for applications such as exploration, mapping and
   underwater inspection to name a few. Simulation results demonstrate the
   efficacy of the proposed approach.}},
