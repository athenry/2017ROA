@inproceedings{ ISI:000386830000015,
Author = {Han, Jinan and Zhang, Chaozhu and Karaim, Malek},
Book-Group-Author = {{IEEE Comp Soc}},
Title = {{Design of Carrier Tracking Loop in Satellite Navigation Receiver based
   on High-order Doppler Parameter Detection Algorithm}},
Booktitle = {{PROCEEDINGS 2016 FIRST INTERNATIONAL CONFERENCE ON MULTIMEDIA AND IMAGE
   PROCESSING (ICMIP 2016)}},
Year = {{2016}},
Pages = {{78-82}},
Note = {{1st International Conference on Multimedia and Image Processing (ICMIP),
   Univ Brunei Darussalam, Fac Integrated Technologies, Bandar Seri
   Begawan, BRUNEI, JUN 01-03, 2016}},
Abstract = {{To improve the tracking performance of global positioning system
   receivers, we have proposed a carrier loop structure based on a
   high-order Doppler parameter detection algorithm. Applied when the input
   of the tracking loop is the carrier signal, this solution begins by
   establishing a relationship between the first and second order rate of
   change of the Doppler frequency shift and the output of the
   discriminator, after which the Doppler frequency shift can be estimated.
   Following this, we used the combined frequency-lock loop and phase-lock
   loop methods to build a carrier loop structure for the high-order
   Doppler parameter detection algorithm. Our results showed that the
   proposed carrier loop structure performs well in high-dynamic conditions
   compared with other carrier loop structures. Accordingly, the structure
   provides an improved design for the tracking module of satellite
   navigation receivers.}},
