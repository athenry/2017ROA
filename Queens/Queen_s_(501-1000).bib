@inproceedings{ ISI:000387075300041,
Author = {Kaviri, Sajjad M. and Najafabadi, T. A. and Mohammadpour, B. and Jain,
   Praveen and Bakhshai, Alireza},
Book-Group-Author = {{IEEE}},
Title = {{Modified Window, Recursive Least Square Estimator for Active and
   Reactive Powers in Single-Phase AC Systems}},
