@article{ ISI:000367531500048,
Author = {Chowdhury, Jahedul Islam and Bao Kha Nguyen and Thornhill, David},
Title = {{Modelling of Evaporator in Waste Heat Recovery System using Finite
   Volume Method and Fuzzy Technique}},
Journal = {{ENERGIES}},
Year = {{2015}},
Volume = {{8}},
Number = {{12}},
Pages = {{14078-14097}},
Month = {{DEC}},
Abstract = {{The evaporator is an important component in the Organic Rankine Cycle
   (ORC)-based Waste Heat Recovery (WHR) system since the effective heat
   transfer of this device reflects on the efficiency of the system. When
   the WHR system operates under supercritical conditions, the heat
   transfer mechanism in the evaporator is unpredictable due to the change
   of thermo-physical properties of the fluid with temperature. Although
   the conventional finite volume model can successfully capture those
   changes in the evaporator of the WHR process, the computation time for
   this method is high. To reduce the computation time, this paper develops
   a new fuzzy based evaporator model and compares its performance with the
   finite volume method. The results show that the fuzzy technique can be
   applied to predict the output of the supercritical evaporator in the
   waste heat recovery system and can significantly reduce the required
   computation time. The proposed model, therefore, has the potential to be
   used in real time control applications.}},
