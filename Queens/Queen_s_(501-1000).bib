@inproceedings{ ISI:000387075300057,
Author = {Mousavian, Hossein and Abnavi, Somayeh and Bakhshai, Alireza and Jain,
   Praveen},
Book-Group-Author = {{IEEE}},
Title = {{A Push-pull Class E converter with improved PDM Control}},
