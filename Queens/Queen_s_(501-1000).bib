@article{ ISI:000371654800028,
Author = {Jin, Chao and Mao, Yachun and Rooney, David W. and Sun, Wang and Zhang,
   Naiqing and Sun, Kening},
Title = {{Synthesis of Pr0.6Sr0.4FeO3-delta-xCe(0.9)Pr(0.1)O(2-delta) cobalt-free
   composite cathodes by a one-pot method for intermediate-temperature
   solid oxide fuel cells}},
Journal = {{INTERNATIONAL JOURNAL OF HYDROGEN ENERGY}},
Year = {{2016}},
Volume = {{41}},
Number = {{6}},
Pages = {{4005-4015}},
Month = {{FEB 19}},
Abstract = {{Cobalt-free composite cathodes consisting of
   Pr0.6Sr0.4FeO3-delta-xCe(0.9)Pr(0.1)O(2-delta) (PSFO-xCPO, x = 0-50
   wt\%) have been synthesized using a one-pot method. X-ray diffraction,
   scanning electron microscopy, thermal expansion coefficient,
   conductivity, and polarization resistance (R-p) have been used to
   characterize the PSFO-xCPO cathodes. Furthermore the discharge
   performance of the Ni-SSZ/SSZ/GDC/PSFO-xCPO cells has been measured. The
   experimental results indicate that the PSFO-xCPO composite materials
   fully consist of PSFO and CPO phases and posses a porous microstructure.
   The conductivity of PSFO-xCPO decreases with the increase of CPO
   content, but R-p of PSFO-40CPO shows the smallest value amongst all the
   samples. The power density of single cells with a PSFO-40CPO composite
   cathode is significantly improved compared with that of the PSFO
   cathode, exhibiting 0.43, 0.75, 1.08 and 1.30 W cm(-2) at 650, 700, 750
   and 800 degrees C, respectively. In addition, single cells with the
   PSFO-40CPO composite cathode show a stable performance with no obvious
   degradation over 100 h when operating at 750 degrees C. Copyright (C)
   2016, Hydrogen Energy Publications, LLC. Published by Elsevier Ltd. All
   rights reserved.}},
