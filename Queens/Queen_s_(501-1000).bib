@article{ ISI:000365206900001,
Author = {Li, Ye and Blostein, Steven and Chan, Wai-Yip},
Title = {{Systematic network coding for two-hop lossy transmissions}},
Journal = {{EURASIP JOURNAL ON ADVANCES IN SIGNAL PROCESSING}},
Year = {{2015}},
Month = {{NOV 14}},
Abstract = {{In this paper, we consider network transmissions over a single or
   multiple parallel two-hop lossy paths. These scenarios occur in
   applications such as sensor networks or WiFi offloading. Random linear
   network coding (RLNC), where previously received packets are re-encoded
   at intermediate nodes and forwarded, is known to be a capacity-achieving
   approach for these networks. However, a major drawback of RLNC is its
   high encoding and decoding complexity. In this work, a systematic
   network coding method is proposed. We show through both analysis and
   simulation that the proposed method achieves higher end-to-end rate as
   well as lower computational cost than RLNC for finite field sizes and
   finite-sized packet transmissions.}},
