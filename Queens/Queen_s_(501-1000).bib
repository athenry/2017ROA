
@article{ ISI:000373945100012,
Author = {Houston, G. and Quinn, D. and Murphy, A. and Bron, F.},
Title = {{Wing Panel Design with Novel Skin-Buckling Containment Features}},
Journal = {{JOURNAL OF AIRCRAFT}},
Year = {{2016}},
Volume = {{53}},
Number = {{2}},
Pages = {{416-426}},
Month = {{MAR-APR}},
Abstract = {{The impact of buckling containment features on the stability of
   thin-gauge fuselage, metallic stiffened panels has previously been
   demonstrated. With the continuing developments in manufacturing
   technology, such as welding, extrusion, machining, and additive layer
   manufacture, understanding the benefits of additional panel design
   features on heavier applications, such as wing panels, is timely. This
   compression testing of thick-gauge panels with and without buckling
   containment features has been undertaken to verify buckling and collapse
   behaviors and validate sizing methods. The experimental results
   demonstrated individual panel mass savings on the order of 9\%, and wing
   cover design studies demonstrated mass savings on the order of 4 to
   13\%, dependent on aircraft size and material choice.}},
