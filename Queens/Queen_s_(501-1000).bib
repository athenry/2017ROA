Affiliation = {{Zhao, SR (Reprint Author), Queens Univ, Dept Mech \& Mat Engn, Kingston, ON, Canada.
   Zhao, Stacey R.; Bryant, Tim; Li, Qingguo, Queens Univ, Dept Mech \& Mat Engn, Kingston, ON, Canada.
   Kingston Gen Hosp, Human Mobil Res Ctr, Kingston, ON, Canada.}},
ISBN = {{978-1-5090-0935-0}},
Keywords = {{trans-tibial prosthesis; prosthetic foot; vertical ground reaction
