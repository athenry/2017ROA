Author = {Gan, Shaojun and Liang, Shan and Li, Kang and Deng, Jing and Cheng,
   Tingli},
Book-Group-Author = {{IEEE}},
Title = {{Ship Trajectory Prediction for Intelligent Traffic Management using
   Clustering and ANN}},
Booktitle = {{2016 UKACC 11TH INTERNATIONAL CONFERENCE ON CONTROL (CONTROL)}},
Year = {{2016}},
Note = {{11th UKACC International Conference on Control (UKACC Control), Belfast,
