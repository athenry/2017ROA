@article{ ISI:000369166100028,
Author = {Khawaja, Bilal A. and Tarar, Munir A. and Tauqeer, Tauseef and Amir,
   Faisal and Mustaqim, Muhammad},
Title = {{A 1 x 2 triple-band printed antenna array for use in next generation
   flying AD-HOC networks (FANETs)}},
Journal = {{MICROWAVE AND OPTICAL TECHNOLOGY LETTERS}},
Year = {{2016}},
Volume = {{58}},
Number = {{3}},
Pages = {{606-610}},
Month = {{MAR}},
Abstract = {{A triple-band 1 x 2 microstrip patch antenna array is proposed for use
   in next generation flying ad-hoc network (FANET) communication in
   multi-UAV systems. The antenna array resonates in the 3.6 GHz, 5.2 GHz
   and 6.7 GHz frequency bands, respectively. The antenna was designed and
   simulated using Agilent ADS Momentum using FR4 epoxy glass substrate
   (epsilon(r)=4.2 and h=1.66mm). A hybrid feed technique is used for
   antenna arrays with quarter-wave transformer based network to match the
   impedance from the feed-point to the antenna to 50. The antenna is
   optimized to resonate at triple-bands by introducing two symmetrical
   slits at the non-radiating edges of the patch. The bandwidths achieved
   for the three bands are 130 MHz, 125 MHz and 180 MHz, respectively,
   which are very practical. The antenna array design is less complex and
   can be used for the advanced antenna beam-steering systems in next
   generation FANETs. (c) 2015 Wiley Periodicals, Inc. Microwave Opt
   Technol Lett 58:606-611, 2016}},
