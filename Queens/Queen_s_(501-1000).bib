@inproceedings{ ISI:000382315800039,
Author = {Carter, K. and Vaughan, T. and Holden, M. and Gauvin, G. and Pezeshki,
   P. and Lasso, A. and Ungi, T. and Morin, E. and Rudan, J. and Engel, C.
   J. and Fichtinger, G.},
Editor = {{Webster, RJ and Yaniv, ZR}},
Title = {{Visual feedback mounted on surgical tool: proof of concept}},
Booktitle = {{MEDICAL IMAGING 2016: IMAGE-GUIDED PROCEDURES, ROBOTIC INTERVENTIONS,
   AND MODELING}},
Series = {{Proceedings of SPIE}},
Year = {{2016}},
Volume = {{9786}},
