@article{ ISI:000362078400009,
Author = {Wu, Z. and Zaremba, E. and Smyrnakis, J. and Magiropoulos, M. and
   Efremidis, Nikolaos K. and Kavoulakis, G. M.},
Title = {{Mean-field yrast spectrum and persistent currents in a two-component
   Bose gas with interaction asymmetry}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2015}},
Volume = {{92}},
Number = {{3}},
Month = {{SEP 29}},
Abstract = {{We analyze the mean-field yrast spectrum of a two-component Bose gas in
   the ring geometry with arbitrary interaction asymmetry. Of particular
   interest is the possibility that the yrast spectrum develops local
   minima at which persistent superfluid flow can occur. By analyzing the
   mean-field energy functional, we show that local minima can be found at
   plane-wave states and arise when the system parameters satisfy certain
   inequalities. We then go on to show that these plane-wave states can be
   yrast states even when the yrast spectrum no longer exhibits a local
   minimum. Finally, we obtain conditions which establish when the
   plane-wave states cease to be yrast states. Specific examples
   illustrating the roles played by the various interaction asymmetries are
   presented.}},
