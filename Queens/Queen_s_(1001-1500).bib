@article{ ISI:000360781400025,
Author = {Arcagni, Marina and Rizzo, Andrea and Campbell, Linda M. and Arribere,
   Maria A. and Juncos, Romina and Reissig, Mariana and Kyser, Kurt and
   Barriga, Juan P. and Battini, Miguel and Ribeiro Guevara, Sergio},
Title = {{Stable isotope analysis of trophic structure, energy flow and spatial
   variability in a large ultraoligotrophic lake in Northwest Patagonia}},
Journal = {{JOURNAL OF GREAT LAKES RESEARCH}},
Year = {{2015}},
Volume = {{41}},
Number = {{3}},
Pages = {{916-925}},
Month = {{SEP}},
Abstract = {{The food web structure of the largest lake in northwest Patagonia, Lake
   Nahuel Huapi (area 557 km(2)), was investigated. Fish, benthic
   macroinvertebrates, plankton and primary producers were analyzed for
   stable nitrogen (delta N-15) and carbon (delta C-13) isotopes in three
   sites with contrasting morphometry and environmental characteristics.
   delta N-15 and delta C-13 revealed a similar food web structure between
   basins but with distinct isotopic baselines. The SIAR (Stable Isotope
   Analysis in R) mixing model was used to study potential food sources
   (benthic or pelagic) for adult fish. Results indicated that energy flow
   to fish from both habitats is different for each site and species, and
   does not depend on the basin morphometry. An ontogenetic shift in the
   carbon source and trophic level was observed in some of the fish
   species, revealing a coupling between the pelagic and bentho-littoral
   habitats of the lake. Our findings complement the existing literature on
   trophic interactions between introduced and native fish, and provide
   novel information on plankton composition, food web structure and energy
   flow in a large lake of North Patagonia that can be extrapolated to
   other understudied lakes in the area. (C) 2015 International Association
   for Great Lakes Research. Published by Elsevier B.V. All rights
   reserved.}},
