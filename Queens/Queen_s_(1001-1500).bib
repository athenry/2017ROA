@article{ ISI:000353860200004,
Author = {Phan, Duoc T. and Lim, James B. P. and Tanyimboh, Tiku T. and Wrzesien,
   A. M. and Sha, Wei and Lawson, R. M.},
Title = {{Optimal design of cold-formed steel portal frames for stressed-skin
   action using genetic algorithm}},
Journal = {{ENGINEERING STRUCTURES}},
Year = {{2015}},
Volume = {{93}},
Pages = {{36-49}},
Month = {{JUN 15}},
Abstract = {{This paper describes a stressed-skin diaphragm approach to the optimal
   design of the internal frame of a cold-formed steel portal framing
   system, in conjunction with the effect of semi-rigid joints. Both
   ultimate and serviceability limit states are considered. Wind load
   combinations are included. The designs are optimized using a real-coded
   niching genetic algorithm, in which both discrete and continuous
   decision variables are processed. For a building with two internal
   frames, it is shown that the material cost of the internal frame can be
