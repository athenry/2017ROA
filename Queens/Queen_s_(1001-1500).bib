@article{ ISI:000349998300026,
Author = {Ralphs, Kathryn and Chansai, Sarayute and Hardacre, Christopher and
   Burch, Robbie and Taylor, Sarah F. R. and James, Stuart L.},
Title = {{Mechanochemical preparation of Ag catalysts for the n-octane-SCR de-NOx
   reaction: Structural and reactivity effects}},
Journal = {{CATALYSIS TODAY}},
Year = {{2015}},
Volume = {{246}},
Pages = {{198-206}},
Month = {{MAY 15}},
Note = {{11th International Symposium on the Scientific Bases for the Preparation
   of Heterogeneous Catalysts (PREPA 11), Univ Cathol Louvain,
   Louvain-la-Neuve, BELGIUM, JUL 06-10, 2014}},
Abstract = {{Mechanochemical preparation of Ag/Al2O3 catalysts used for the selective
   catalytic reduction of NOx using(2)O(3) hydrocarbons has been shown to
   substantially increase the activity of the catalyst in comparison with
   Ag/Al2O3 prepared by wet impregnation. The effect of using different
   ball-milling experimental parameters on both the structure of the
   material as well as the catalyst activity has been investigated and the
   optimum conditions established. A phase transition from to gamma- to
   alpha-alumina was observed milling at high speeds which was found to
   result in lower catalyst activities. At lower milling speeds both
   fracturing and agglomeration of the alumina support can be observed
   depending on the grinding time. However, due to ball-milling, a general
   enhancement in the NOx reduction activity was observed for all catalysts
   compared with the conventionally prepared catalysts irrespective of the
   reductant used. Transient DRIFTS-MS experiments were performed to
   investigate the effect of H-2 in the absence and presence of water on
   the SCR reaction over catalysts prepared by both ball milling and wet
   impregnation. In-situ DRIFTS-MS analysis revealed significant
   differences in both gas phase and surface species. Most notably,
   isocyanate species were formed significantly more quickly and at higher
   surface concentration in the case of the mechanochemically prepared
   catalyst. (C) 2014 Elsevier B.V. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
