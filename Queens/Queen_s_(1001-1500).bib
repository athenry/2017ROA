@article{ ISI:000353950200014,
Author = {Ahmoye, Daniel and Krstic, Vladimir D.},
Title = {{Reaction sintering of SiC composites with in situ converted TiO2 to TiC}},
Journal = {{JOURNAL OF MATERIALS SCIENCE}},
Year = {{2015}},
Volume = {{50}},
Number = {{7}},
Pages = {{2806-2812}},
Month = {{APR}},
Abstract = {{Binary SiC-TiO2 ceramic composite was fabricated using reaction
   pressureless sintering at temperatures ranging from 1825 to 1900 degrees
   C. The raw materials for the reaction process were TiO2 and carbon
   powders, where yttria (Y2O3) and alumina (Al2O3) were used as sintering
   aids. The SiC grain size and morphology were analyzed as a function of
   TiC volume fraction. The presence of TiC particles in the SiC matrix
   inhibited the exaggerated grain growth of the SiC grains and activated
   additional mass transport mechanisms. The SiC grains were found to be
   roughly equiaxed with very fine TiC particles preventing significant
   elongation. The role the TiC particles play in the densification of SiC
   was also discussed.}},
