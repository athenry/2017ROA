@article{ ISI:000362418300070,
Author = {Gangaraj, S. Ali Hassani and Nemilentsau, Andrei and Hanson, George W.
   and Hughes, Stephen},
Title = {{Transient and steady-state entanglement mediated by three-dimensional
   plasmonic waveguides}},
Journal = {{OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{23}},
Number = {{17}},
Pages = {{22330-22346}},
Month = {{AUG 24}},
Abstract = {{Entanglement between two qubits (two level atoms) mediated by surface
   plasmons in three-dimensional plasmonic waveguides is studied using a
   quantum master equation formalism. Two types of waveguides, a nanowire
   and a V-shaped channel cut in a flat metal plane, are considered. The
   Green functions for the waveguides, which rigorously describes the
   dissipative qubit environment, are calculated numerically using a direct
   finite-difference time-domain (FDTD) solution of Maxwell's equations.
   Finite-length effects are shown to play a crucial role in enhancing
   entanglement, and resonant-length plasmonic waveguides can provide
   higher entanglement between qubits than infinite-length waveguides. It
   is also shown that coupling slots can improve entanglement via stronger
   qubit-waveguide coupling, for both the infinite-and finite-waveguide
   cases. The formalism used in the paper can be applied to a wide range of
   plasmonic waveguides. (C) 2015 Optical Society of America}},
