@article{ ISI:000358605000002,
Author = {Giacomin, A. Jeffrey and Saengow, Chaimongkol and Guay, Martin and
   Kolitawong, Chanyut},
Title = {{Pade approximants for large-amplitude oscillatory shear flow}},
Journal = {{RHEOLOGICA ACTA}},
Year = {{2015}},
Volume = {{54}},
Number = {{8}},
Pages = {{679-693}},
Month = {{AUG}},
Abstract = {{Analytical solutions for either the shear stress or the normal stress
   differences in large-amplitude oscillatory shear flow, both for
   continuum or molecular models, often take the form of the first few
   terms of a power series in the shear rate amplitude. Here, we explore
   improving the accuracy of these truncated series by replacing them with
   ratios of polynomials. Specifically, we examine replacing the truncated
   series solution for the corotational Maxwell model with its Pad,
   approximants for the shear stress response and for the normal stress
   differences. We find these Pad, approximants to agree closely with the
   corresponding exact solution, and we learn that with the right
   approximants, one can nearly eliminate the inaccuracies of the truncated
   expansions.}},
