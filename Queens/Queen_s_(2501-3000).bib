@inproceedings{ ISI:000356049700040,
Author = {Hayajneh, Khaled F. and Yousefi, Shahram and Valipour, Mehrdad},
Book-Group-Author = {{IEEE}},
Title = {{Left Degree Distribution Shaping for LT Codes Over the Binary Erasure
   Channel}},
