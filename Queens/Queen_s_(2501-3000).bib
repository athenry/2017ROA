@inproceedings{ ISI:000356331202042,
Author = {Elhoushi, Mostafa and Georgy, Jacques and Korenberg, Michael and
   Noureldin, Aboelmagd},
Book-Group-Author = {{Inst Navigat}},
Title = {{Broad Motion Mode Recognition for Portable Navigation}},
Booktitle = {{PROCEEDINGS OF THE 27TH INTERNATIONAL TECHNICAL MEETING OF THE SATELLITE
   DIVISION OF THE INSTITUTE OF NAVIGATION (ION GNSS 2014)}},
Series = {{Institute of Navigation Satellite Division Proceedings of the
