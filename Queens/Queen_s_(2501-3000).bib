@inproceedings{ ISI:000370073806146,
Author = {Sears, David and Rudie, Karen},
Book-Group-Author = {{IEEE}},
Title = {{On Computing Indistinguishable States of Nondeterministic Finite
   Automata with Partially Observable Transitions}},
Booktitle = {{2014 IEEE 53RD ANNUAL CONFERENCE ON DECISION AND CONTROL (CDC)}},
Year = {{2014}},
Pages = {{6731-6736}},
Note = {{IEEE 53rd Annual Conference on Decision and Control (CDC), Los Angeles,
