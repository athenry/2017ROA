@inproceedings{ ISI:000349212503034,
Author = {Zhang, Zhiliang and Cai, Yong-Yong and He, Xiao-Fei and Liu, Yan-Fei},
Book-Group-Author = {{IEEE}},
Title = {{A Distributed Architecture Based on Micro-Bank Modules with
   Self-Reconfiguration Control Regarding Battery Recovery Effect for DC
   Microgrids}},
Booktitle = {{2014 TWENTY-NINTH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC)}},
Series = {{Annual IEEE Applied Power Electronics Conference and Exposition (APEC)}},
Year = {{2014}},
Pages = {{3091+}},
Note = {{29th Annual IEEE Applied Power Electronics Conference and Exposition
   (APEC), Fort Worth, TX, MAR 16-20, 2014}},
Organization = {{IEEE}},
Abstract = {{This paper proposes a new distributed battery energy architecture based
   on the micro-bank modules for DC microgrids. The benefits of the
   proposed architecture include 1) no voltage sharing problem and no
   overcharge/overdischarge problem; 2) high compatibility and reliability
   3) high energy utilization efficiency; 4) reduced volume and weight of
   the BMS. Taking advantage of the battery recovery effect, a
   selfreconfiguration discharge strategy is also proposed to further
   enhance the battery performance and efficiency of the storage system.
