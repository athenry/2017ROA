Author = {Craig, Julie R. M. and Dunne, Nicholas},
Editor = {{Antoniac, I and Cavalu, S and Traistaru, T}},
Title = {{Biomechanics of Vertebroplasty: Effect of Cement Viscosity on Mechanical
   Behaviour}},
Booktitle = {{BIOCERAMICS, VOL 25}},
