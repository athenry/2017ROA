@article{ ISI:000326803800009,
Author = {Fitzhenry, Laurence and Manesiotis, Panagiotis and Duggan, Patrick and
   McLoughlin, Peter},
Title = {{Molecularly imprinted polymers for corticosteroids: impact of polymer
   format on recognition behaviour}},
Journal = {{MICROCHIMICA ACTA}},
Year = {{2013}},
Volume = {{180}},
Number = {{15-16}},
Pages = {{1421-1431}},
Month = {{NOV}},
Abstract = {{A comparative study was performed on different polymeric formats for
   targeting corticosteroids, focusing on the use of bulk monolith and
   precipitation polymerisation strategies. Hydrocortisone-17-butyrate was
   selected as the template and methacrylic acid as the functional monomer,
   following H-1 NMR investigation of the pre-polymerisation mixture. Three
   different cross-linkers were tested, ranging from moderate to highly
   hydrophobic. The synthesised bulk and precipitated imprinted polymers
   were physically characterised by nitrogen sorption and evaluated by
   means of HPLC and frontal chromatography against a range of template
   analogues. While some degree of selectivity for the template was
   achieved for all tested polymers, the ones based on the tri-functional
   cross-linking monomer trimethylolpropane trimethacrylate exhibited the
   longest retention for all corticosteroids, especially in the
   precipitated format, which suggested broader group selectivity.}},
