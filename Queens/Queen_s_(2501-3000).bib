@inproceedings{ ISI:000361921900057,
Author = {Cerantola, D. J. and Birk, A. M.},
Book-Group-Author = {{ASME}},
Title = {{EXPERIMENTAL ANALYSIS OF SWIRL IN SHORT ANNULAR DIFFUSERS WITH NEGATIVE
   WALL ANGLES}},
Booktitle = {{PROCEEDINGS OF THE ASME TURBO EXPO: TURBINE TECHNICAL CONFERENCE AND
   EXPOSITION, 2014, VOL 2A}},
Year = {{2014}},
Note = {{ASME Turbo Expo: Turbine Technical Conference and Exposition,
   Dusseldorf, GERMANY, JUN 16-20, 2014}},
Organization = {{Int Gas Turbine Inst}},
Abstract = {{Short annular diffuser systems consisting of a conical expansion section
   with negative wall angles and a solid (full outer diameter) diffusing
   section were tested experimentally at several inlet swirl angles of 0-40
   degrees. Tests analyzed were completed over a range of inlet Reynolds
   number of Re-t = 0.9-2.2 x 10(5) and considered fully turbulent.
   Performance back pressure coefficient, outlet velocity uniformity, and
   total pressure loss were appreciably depreciated for inlet swirl number
   larger than 0.7 and the average 10 degrees curved vane swirler
   out-performed its straight vaned equivalent. Three centre bodies with
   length half that of the diffuser and different curvatures were
   manufactured. Similar performance was achieved but each centre body
   provided marginal improvements to a particular objective. Most notably,
   the centre body that gave an initial flow expansion angle of 14 degrees
   resulted in 1-4\% lower back pressure than the other two whose expansion
   angles were 12 degrees and 16 degrees.}},
