@article{ ISI:000329144400008,
Author = {Stockdale, Aileen and MacLeod, Marsaili},
Title = {{Pre-retirement age migration to remote rural areas}},
Journal = {{JOURNAL OF RURAL STUDIES}},
Year = {{2013}},
Volume = {{32}},
Pages = {{80-92}},
Month = {{OCT}},
Abstract = {{Recent literature suggests that the increasingly blurred relationship
   between paid employment and retirement facilitates a retirement
   transition period, a life course stage, which may involve a change of
   residence. The role of such pre-retirement age mobility in the
   repopulation of rural areas has, however, received relatively little
   academic scrutiny. This paper draws upon findings from a two-year study
   conducted in three UK case study areas. It examines the extent of
   pre-retirement age (aged 50-64) migration into remote rural communities
   and the impacts this type of movement has upon economic activity, social
   and community engagement and service provision. It is argued that while
   this under-researched cohort offers opportunities to support the social
   and economic sustainability of rural communities (at least in the short
   and medium term), there are notable challenges which are likely to
   emerge as it ages in situ. The findings are particularly relevant given
   national trends on population ageing. (C) 2013 Elsevier Ltd. All rights
   reserved.}},
