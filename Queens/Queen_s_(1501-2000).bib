@inproceedings{ ISI:000377900900079,
Author = {Yoo, Seong Ki and Cotton, Simon L. and Sofotasios, Paschalis C. and
   Matthaiou, Michail and Valkama, Mikko and Karagiannidis, George K.},
Book-Group-Author = {{IEEE}},
Title = {{The kappa - mu/Inverse Gamma Fading Model}},
Booktitle = {{2015 IEEE 26TH ANNUAL INTERNATIONAL SYMPOSIUM ON PERSONAL, INDOOR, AND
   MOBILE RADIO COMMUNICATIONS (PIMRC)}},
