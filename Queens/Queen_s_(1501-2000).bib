@inproceedings{ ISI:000382389304094,
Author = {Fozooni, Milad and Matthaiou, Michail and Bjornson, Emil and Duong,
   Trung Q.},
Book-Group-Author = {{IEEE}},
Title = {{Performance Limits of MIMO Systems with Nonlinear Power Amplifiers}},
Booktitle = {{2015 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
Series = {{IEEE Global Communications Conference}},
