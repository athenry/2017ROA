@article{ ISI:000375842300025,
Author = {Guay, Martin and Bennett, Ryan and Hudon, Nicolas},
Title = {{Observer Design Using Potential Based Realizations}},
Journal = {{IFAC PAPERSONLINE}},
Year = {{2015}},
Volume = {{48}},
Number = {{8}},
Pages = {{141-146}},
Note = {{9th IFAC Symposium on Advanced Control of Chemical Processes ADCHEM
   2015, Whistler, CA, JUN 07-10, 2015}},
Abstract = {{This paper considers observer design for nonlinear dynamical systems
   which can be approximated by a dissipative Hamiltonian realization. The
   design approach decomposes the system associated one form of a given
   dynamical system over an indeterminate metric using the Homotopy
   operator to generate exact (potential driver) and anti-exact parts. Then
   the convexity of the potential system given by the exact part is
   assessed and we propose a metric equation which yields a Lyaptatov
   function for the potential driven observer system. An application of
   this method is demonstrated for a two-dimensional van der Pol
   oscillator. (C) 2015, IFAC (International Federation of Automatic
   Control) Hosting by Elsevier Ltd. All rights reserved.}},
