@inproceedings{ ISI:000374801300207,
Author = {Djon, Lionnel M. and Olivo, Gema R. and Miller, Jim D. and Stewart,
   Robert D.},
Editor = {{Andre-Mayer, AS and Cathelineau, M and Muchez, P and Pirard, E and Sindern, S}},
Title = {{The Layered Ultramafic North Lac Iles Intrusion, Ontario, Canada:
   Lithological Controls of the PGE Mineralization}},
Booktitle = {{MINERAL RESOURCES IN A SUSTAINABLE WORLD, VOLS 1-5}},
Year = {{2015}},
Pages = {{913-916}},
Note = {{13th SGA Biennial Meeting on Mineral Resources in a Sustainable World,
   Nancy, FRANCE, AUG 24-27, 2015}},
Organization = {{CNRS; Univ Lorraine Nancy; Katholieke Universiteit Leuven; Univ Liege;
   RWTH Aachen Univ}},
Abstract = {{The North Lac des Iles Intrusions is a large, (similar to 25 km2)
   multi-phase ultramafic component of the Lac des Iles Complex located in
   northwestern Ontario. It comprises two major intrusive centers that are
   distinguishable based on their dominant rock types and geometry of
   layering. Detailed mapping on the eastern flank of the Northern
   Ultramafic Centre (NUC) led to the identification of a marginal gabbro
   and a layered series. The layered series consists mainly of
   serpentinite, websterite, olivine-clinopyroxenite units, with lesser
   olivine-websterite and rarely gabbronorite. Two distinct cycles can be
   recognized based on the repetition of units and their mineral
   paragenesis. The contacts between the cyclic units are sharp or
   gradational. Within individual cycles, upward whole rock compositional
   variation is consistent with fractional crystallization, characterised
   by decrease in Mg contents in whole rock. However, near the top of
   cyclic units, the trend is reversed gradually or abruptly. The NUC hosts
   several anomalous occurrences of platinum- group element (PGE).
   Significantly, the highest PGE concentrations occur in zones of
   compositional reversal in the layered units. The presence of the
   PGE-bearing zones is interpreted to be the result of dynamic mixing
