@article{ ISI:000346565400008,
Author = {Banerjee, Sandeep and Kyser, T. Kurtis and Mitchell, Roger},
Title = {{Nitrogen isotopic compositions and concentrations in MARID xenoliths}},
Journal = {{CHEMICAL GEOLOGY}},
Year = {{2015}},
Volume = {{391}},
Pages = {{83-89}},
Month = {{JAN 6}},
Abstract = {{Nitrogen isotopic compositions and concentrations in micas from
   kimberlite-hosted MARID (mica-amphibole-rutile-ilmenite-diopside) suite
   of xenoliths from the Kimberley area of the Kaapvaal Craton, South
   Africa, were measured using continuous-flow isotope-ratio mass
   spectrometry and Fourier transform infrared spectroscopy. Fourier
   transform infrared spectroscopy data suggest that the nitrogen in MARID
   phlogopite is from ammonium in the mica structure. The replacement of
   potassium by ammonium is the major mechanism for the storage of nitrogen
   within these phlogopites. The delta N-15 values of micas from MARID
   xenoliths range from -11 to +9 parts per thousand, and nitrogen contents
   range between 113 and 272 ppm. The N-15 enrichment of most samples
   relative to the average upper mantle delta N-15 value of -5 parts per
   thousand suggests that the nitrogen reflects a recycled reservoir from
   subducted material within their source region. The presence of nitrogen
   from a recycled reservoir indicates that sedimentary organic nitrogen in
   the sub-continental lithosphere is not effectively cycled to the surface
   resulting in a geochemically heterogeneous upper mantle. (C) 2014
   Elsevier B.V. All rights reserved.}},
