@inproceedings{ ISI:000367591500011,
Author = {Ma, Jianbing and Liu, Weiru and Dubois, Didier},
Editor = {{Zhang, S and Wirsing, M and Zhang, Z}},
Title = {{Rational Partial Choice Functions and Their Application to Belief
   Revision}},
Booktitle = {{KNOWLEDGE SCIENCE, ENGINEERING AND MANAGEMENT, KSEM 2015}},
