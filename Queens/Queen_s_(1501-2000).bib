Author = {Lockwood, Kenneth and Grover, Patrick and Da Silva, Ana Maria Ferreira
   and Lenz, Mareen A.},
Editor = {{Mynett, A}},
Title = {{FLOW CHARACTERISTICS OVER EQUILIBRIUM AND NON-EQUILIBRIUM DUNES}},
