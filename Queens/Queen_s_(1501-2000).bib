@article{ ISI:000344985900033,
Author = {Webster, P. J. L. and Wright, L. G. and Ji, Y. and Galbraith, C. M. and
   Kinross, A. W. and Van Vlack, C. and Fraser, J. M.},
Title = {{Automatic laser welding and milling with in situ inline coherent imaging}},
Journal = {{OPTICS LETTERS}},
Year = {{2014}},
Volume = {{39}},
Number = {{21}},
Pages = {{6217-6220}},
Month = {{NOV 1}},
Abstract = {{Although new affordable high-power laser technologies enable many
   processing applications in science and industry, depth control remains a
   serious technical challenge. In this Letter we show that inline coherent
   imaging (ICI), with line rates up to 312 kHz and microsecond-duration
   capture times, is capable of directly measuring laser penetration depth,
   in a process as violent as kW-class keyhole welding. We exploit ICI's
   high speed, high dynamic range, and robustness to interference from
   other optical sources to achieve automatic, adaptive control of laser
   welding, as well as ablation, achieving 3D micron-scale sculpting in
   vastly different heterogeneous biological materials. (C) 2014 Optical
   Society of America}},
