@inproceedings{ ISI:000309167700003,
Author = {Yang, Aolei and Naeem, Wasif and Irwin, George W. and Li, Kang},
Book-Group-Author = {{IEEE}},
Title = {{Novel Decentralised Formation Control for Unmanned Vehicles}},
Booktitle = {{2012 IEEE INTELLIGENT VEHICLES SYMPOSIUM (IV)}},
Year = {{2012}},
Pages = {{13-18}},
Note = {{IEEE Intelligent Vehicles Symposium (IV), Alcala de Henares, SPAIN, JUN
   03-07, 2012}},
Organization = {{IEEE}},
Abstract = {{This paper proposes a new methodology for solving the unmanned
   multi-vehicle formation control problem. It employs a unique
   ``extension-decomposition-aggregation{''} scheme to transform the
   overall complex formation control problem to a group of sub-problems
   which work via boundary interactions. The H robust control strategy is
