@inproceedings{ ISI:000335720900094,
Author = {Cerantola, David J. and Birk, A. M.},
Book-Group-Author = {{ASME}},
Title = {{NUMERICALLY OPTIMIZING AN ANNULAR DIFFUSER USING A GENETIC ALGORITHM
   WITH THREE OBJECTIVES}},
Booktitle = {{PROCEEDINGS OF THE ASME TURBO EXPO 2012, VOL 8, PTS A-C}},
Year = {{2012}},
Pages = {{1033-1042}},
Note = {{ASME Turbo Expo 2012, Copenhagen, DENMARK, JUN 11-15, 2012}},
Organization = {{Int Gas Turbine Inst}},
Abstract = {{A genetic algorithm was implemented to determine preferential solutions
   of a short annular diffuser exhaust system of length 1.5D(0) (outer
   annulus diameters). Five free variables defined the centre body shape
   and two variables determined the outer wall profile. Diffuser
   performance was evaluated using three objectives-(i) diffuser pressure
   recovery, (ii) outlet velocity uniformity, and (iii) total pressure
   loss-that were calculated from steady state solutions obtained using the
   computational fluid dynamics software FLUENT 13.0 with the realizable
   k-epsilon turbulence model and enhanced wall treatment. Inlet conditions
   were Re-Dh = 8.5 x 10(4) and M = 0.23. After thirty-five generations, a
   paraboloid-shaped centre body with length 0.74D(0) and initial annular
   expansion of approximately 14 degrees produced preferential solutions. A
   configuration with a converging outer wall above the centre body
   developed greater outlet flow uniformity and lower total pressure loss
   whereas a straight outer wall followed by the solid diffuser generated
   more static pressure recovery.}},
