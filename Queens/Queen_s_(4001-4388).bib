@article{ ISI:000300496400002,
Author = {Lafia-Araga, Ruth A. and Hassan, Aziz and Yahya, R. and Rahman,
   Normasmira A. and Hornsby, Peter R. and Heidarian, J.},
Title = {{Thermal and mechanical properties of treated and untreated Red Balau
   (Shorea dipterocarpaceae)/LDPE composites}},
Journal = {{JOURNAL OF REINFORCED PLASTICS AND COMPOSITES}},
Year = {{2012}},
Volume = {{31}},
Number = {{4}},
Pages = {{215-224}},
Month = {{FEB}},
Abstract = {{Red Balau saw dust was heat-treated at 180 degrees C and 200 degrees C
   for 1 h. Treated and untreated wood flour were compounded with LDPE at
   9\%, 20\%, and 37\% by weight and molded in an injection molding
   machine. Thermal and mechanical properties of the resultant composites
   were investigated as a function of filler loadings and treatment
   temperature. Thermogravimetric analysis revealed an increase in
   degradation peak temperature (T-p) of the heat-treated wood and
   composites. DSC revealed a decreasing trend in the degree of
   crystallinity (X-c) of the matrix when heat-treated wood was used as
   filler. On the other hand, untreated wood showed an increase in X-c with
   increasing wood content. Tensile modulus increased with heat treatment
   and filler loading. Furthermore, flexural strength and modulus were
   found to increase with filler loading.}},
