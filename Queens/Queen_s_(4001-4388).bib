@article{ ISI:000298845000011,
Author = {Lee, Sung Hyo and Bailly, Mathieu and Kontopoulou, Marianna},
Title = {{Morphology and Properties of Poly(propylene)/Ethylene-Octene Copolymer
   Blends Containing Nanosilica}},
Journal = {{MACROMOLECULAR MATERIALS AND ENGINEERING}},
Year = {{2012}},
Volume = {{297}},
Number = {{1}},
Pages = {{95-103}},
Month = {{JAN}},
Abstract = {{The effect of nanosilica addition on the morphology and mechanical
   properties of blends of isotactic PP and an ethylene/octene copolymer
   (EOC) is studied. TEM reveals that the well-dispersed nanoparticles are
   localized exclusively in the PP phase. In the presence of a maleated PP
   compatibilizer addition of nanosilica leads to more finely dispersed EOC
   domains and a finer co-continuous morphology. The nanoparticles reduce
   the rate of coalescence of the dispersed phase domains. The mechanical
   properties depend on the EOC and PP-g-MA content. Tensile and flexural
   properties are significantly enhanced in the presence of the silica
   nanoparticles, whereas impact properties are not affected. These
   enhancements are attributed to the favorable microstructure of the
   blends.}},
