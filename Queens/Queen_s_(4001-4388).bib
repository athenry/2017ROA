@article{ ISI:000302647400056,
Author = {Pourmottaghi, A. and Taban, M. R. and Gazor, S.},
Title = {{A CFAR Detector in a Nonhomogenous Weibull Clutter}},
Journal = {{IEEE TRANSACTIONS ON AEROSPACE AND ELECTRONIC SYSTEMS}},
Year = {{2012}},
Volume = {{48}},
Number = {{2}},
Pages = {{1747-1758}},
Month = {{APR}},
Abstract = {{We propose a method that improves the constant false alarm rate (CFAR)
   detection of targets in a nonhomogenous clutter environment. We assume
   that the clutter data from neighboring cells may have an edge with
   unknown location dividing data samples into two parts with different
   independent and identically distributed (IID) Weibull distributions. We
   propose automatic clutter edge localization which allows us to eliminate
   the outlier/misleading data, and thereby improves the CFAR detection
   performance. We also suggest a CFAR detector (CFARD) which uses this
   estimator.}},
