@inproceedings{ ISI:000304871800053,
Author = {Chung, Jonathan and Daoud, Mohammad I. and Imani, Farhad and Mousavi,
   Parvin and Abolmaesumi, Purang},
Editor = {{Bosch, JG and Doyley, MM}},
Title = {{GPU Accelerated Implementation of Ultrasound Radio-Frequency Time Series
   Analysis}},
Booktitle = {{MEDICAL IMAGING 2012: ULTRASONIC IMAGING, TOMOGRAPHY, AND THERAPY}},
Series = {{Proceedings of SPIE}},
