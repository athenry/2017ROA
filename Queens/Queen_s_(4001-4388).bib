@inproceedings{ ISI:000347871500068,
Author = {Wiebe, J. D. and da Silva, A. M. Ferreira},
Editor = {{Munoz, RM}},
Title = {{Adjustment of fully-developed dunes to sudden changes in flow depth}},
Booktitle = {{RIVER FLOW 2012, VOLS 1 AND 2}},
Year = {{2012}},
Pages = {{515-520}},
Note = {{6th International Conference on Fluvial Hydraulics (River Flow), San
   Jose, COSTA RICA, SEP 05-07, 2012}},
Organization = {{Colegio Ingenieros Civiles Costa Rica; Int Assoc Hydro Environm Engn \&
   Res, Comm Fluvial Hydraul}},
Abstract = {{In non-steady flows, dune dimensions are known to lag changes in
   discharge. This research examines the adjustment of equilibrium dunes,
   developed under steady and uniform flow conditions, to a sudden increase
   or decrease in discharge. For the present purposes, nineteen laboratory
   runs were carried out in a 21m long, 0.76m wide sediment transport flume
   at the Queen's University Coastal Engineering Research Laboratory. In
   these runs, the flow was kept uniform so that the sudden change in
   discharge is represented as a sudden change in flow depth. Dunes were
   observed to initially adapt to a change in flow by adjusting their
   heights. Only after some time had past, allowing for the coalescence and
   splitting of individual dunes, did the average dune length adjust to
   equilibrium dimensions. It was also found that dunes forming from an
   initially undulated bed produced by a different flow tended to acquire
   the same dimensions as those developed from an initially flat bed. This
   suggests that the developed dune dimensions are related only to the flow
   conditions, and are independent of the initial shape of the bed.}},
