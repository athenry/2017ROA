@article{ ISI:000305855000004,
Author = {He, Yi and Bagley, David M. and Leung, Kam Tin and Liss, Steven N. and
   Liao, Bao-Qiang},
Title = {{Recent advances in membrane technologies for biorefining and bioenergy
   production}},
