@article{ ISI:000310780400006,
Author = {Freeman, C. Daniel and Grubb, Michael P. and Lane, Ian C. and North,
   Simon W.},
Title = {{Vibrational state-selected photodissociation of ClO+}},
Journal = {{CHEMICAL PHYSICS}},
Year = {{2012}},
Volume = {{408}},
Pages = {{43-49}},
Month = {{OCT 26}},
Abstract = {{We report on the UV photodissociation of specific vibrational states (v
   = 2-45) of ClO+ using velocity map ion imaging. The high vibrational
   states of ClO+ are prepared via a double resonant scheme through the ClO
   (A (2)Pi) state and ion-pair states followed by photoionization with a
   third photon. The absorption of a fourth photon results in
   photodissociation of the ClO+ into two dominant asymptotic channels. The
   Cl+ and O+ fragment ion images reveal information on both the energetics
   of high-lying cation vibrational states and the low-lying dissociative
   electronic states that correlate to Cl+(P-3) + O(P-3) and Cl(P-2) +
   O+(S-4) asymptotic channels. We also report ab initio potentials for the
   bound ClO+ and ion-pair states as well as calculations of the ClO+
   excited states relevant to the photodissociation process. (C) 2012
   Elsevier B.V. All rights reserved.}},
