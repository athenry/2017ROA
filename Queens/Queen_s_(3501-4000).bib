@inproceedings{ ISI:000391821200090,
Author = {Zhang, Jun-tian and Li, Qingguo},
Book-Group-Author = {{IEEE}},
Title = {{Investigation of a passive inter-limb device on step-to-step transition
   of human walking}},
Booktitle = {{2013 IEEE 13TH INTERNATIONAL CONFERENCE ON REHABILITATION ROBOTICS
   (ICORR)}},
Series = {{International Conference on Rehabilitation Robotics ICORR}},
Year = {{2013}},
