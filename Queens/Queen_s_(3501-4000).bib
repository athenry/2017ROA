@article{ ISI:000312510000015,
Author = {Ishii, Hideaki and Tempo, Roberto and Bai, Er-Wei},
Title = {{PageRank computation via a distributed randomized approach with lossy
   communication}},
Journal = {{SYSTEMS \& CONTROL LETTERS}},
Year = {{2012}},
Volume = {{61}},
Number = {{12}},
Pages = {{1221-1228}},
Month = {{DEC}},
Abstract = {{For ranking web pages in search results, Google employs the so-called
   PageRank algorithm, which provides a measure of importance to each page
   based on the web structure. Recently, we have developed a distributed
   randomized approach for this algorithm, where pages compute their own
   PageRank by communicating over selected links. In this paper, the focus
   is on the effects of unreliability in communication channels.
   Specifically, we consider random data losses modeled as a Markov chain
   and introduce a generalized version of the distributed scheme. Its
   convergence properties and the error in the approximated PageRank are
   analyzed. (C) 2012 Elsevier B.V. All rights reserved.}},
