@article{ ISI:000304793000015,
Author = {Tulk, E. and Kerr, M. and Daymond, M. R.},
Title = {{Study on the effects of matrix yield strength on hydride phase stability
   in Zircaloy-2 and Zr 2.5 wt\% Nb}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2012}},
Volume = {{425}},
Number = {{1-3}},
Pages = {{93-104}},
Month = {{JUN}},
Abstract = {{A study on the effect of matrix yield strength on precipitation of
   hydride phases in zirconium alloys is presented. X-ray diffraction
   measurements from the National Synchrotron Light Source (NSLS) at
   Brookhaven National Laboratory were used to characterise the quantity of
   delta (fcc) and gamma (fct) hydride phases in zirconium alloys as a
   function of yield stress and cooling rate. While experimental results
   indicate that yield stress has a pronounced effect on hydride phase
   stability in the zirconium-hydrogen system, other factors including
   defect structure and beta-phase distribution also influence the hydride
   phase stability, for material of a given yield stress. These results
   offer a possible explanation for the wide range of hydride phase
   stability reported in the literature for the zirconium-hydrogen system.
