@article{ ISI:000309239900021,
Author = {Wang, Wei and Yan, Wei and Sha, Wei and Shan, Yiyin and Yang, Ke},
Title = {{Microstructural Evolution and Mechanical Properties of Short-Term
   Thermally Exposed 9/12Cr Heat-Resistant Steels}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS A-PHYSICAL METALLURGY AND
   MATERIALS SCIENCE}},
Year = {{2012}},
Volume = {{43A}},
Number = {{11}},
Pages = {{4113-4122}},
Month = {{NOV}},
Note = {{Symposium on Approaches for Investigating Phase Transformations at the
   Atomic Scale, San Diego, CA, FEB 27-MAR 03, 2011}},
Abstract = {{The microstructural evolution during short-term (up to 3000 hours)
   thermal exposure of three 9/12Cr heat-resistant steels was studied, as
   well as the mechanical properties after exposure. The tempered
   martensitic lath structure, as well as the precipitation of carbide and
   MX type carbonitrides in the steel matrix, was stable after 3000 hours
   of exposure at 873 K (600 A degrees C). A microstructure observation
   showed that during the short-term thermal exposure process, the change
   of mechanical properties was caused mainly by the formation and growth
   of Laves-phase precipitates in the steels. On thermal exposure, with an
   increase of cobalt and tungsten contents, cobalt could promote the
   segregation of tungsten along the martensite lath to form Laves phase,
   and a large size and high density of Laves-phase precipitates along the
   grain boundaries could lead to the brittle intergranular fracture of the
   steels.}},
