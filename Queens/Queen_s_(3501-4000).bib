@article{ ISI:000311066600012,
Author = {Anderson, Jennifer L. and Peterson, Ronald C. and Swainson, Ian},
Title = {{The atomic structure of deuterated boyleite ZnSO4 center dot 4D(2)O,
   ilesite MnSO4 center dot 4D(2)O, and bianchite ZnSO4 center dot 6D(2)O}},
Journal = {{AMERICAN MINERALOGIST}},
Year = {{2012}},
Volume = {{97}},
Number = {{11-12}},
Pages = {{1905-1914}},
Month = {{NOV-DEC}},
Abstract = {{Deuterated boyleite ZnSO4 center dot 4D(2)O, was synthesized and the
   atomic structure, including D positions, was successfully refined in a
   combined histogram neutron diffraction refinement. The cell dimensions
   for boyleite are a = 5.9144(2), b = 13.5665(4), c = 7.8924(2) angstrom,
   and beta = 90.668(2)degrees with space group P2(1)/n and Z = 4. The
   atomic structure including D positions of the isostructural mineral
   ilesite, MnSO4 center dot 4D(2)O, was refined and the cell dimensions
   are a = 5.9753(1), b = 13.8186(3), c = 8.0461(1) angstrom, and beta =
   90.826(2)degrees. Deuterated bianchite ZnSO4 center dot 6D(2)O was
   synthesized and the atomic structure, including D positions, was
   successfully refined with a unit cell of a = 9.969(1), b = 7. 2441(7), c
   = 24.249(3) angstrom, and beta = 98.488(5)degrees in space group C2/c
   and Z = 8. A comparison of the hydrogen bonding in M2+SO4 center dot
   4D(2)O with that in M2+SO4 center dot 6D(2)O shows that bifurcated
   hydrogen bonds are common in the tetrahydrate sulfates but nonexistent
   in the hexahydrate structures. This is a result of the packing
   constraints of the rings of the sulfate and metal-containing octahedra
   in the tetrahydrates. In the hexahydrate sulfates there is no direct
   linkage between the sulfate and metal-containing octahedra and hydrogen
   bonds are optimized without packing constraints, and no bifurcated
   hydrogen bonds are observed.}},
