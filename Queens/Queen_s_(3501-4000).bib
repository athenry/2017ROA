@inproceedings{ ISI:000357299604061,
Author = {Mehrpouyan, Hani and Blostein, Steven D. and Ottersten, Bjoern},
Book-Group-Author = {{IEEE}},
Title = {{Simultaneous Estimation of Multi-Relay MIMO Channels}},
Booktitle = {{2013 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
Series = {{IEEE Global Communications Conference}},
Year = {{2013}},
Pages = {{4264-4269}},
Note = {{IEEE Global Communications Conference (GLOBECOM), Atlanta, GA, DEC
   09-13, 2013}},
Organization = {{IEEE}},
Abstract = {{This paper addresses training-based channel estimation in distributed
   amplift-and-forward (AF) multi-input multi-output (MAIO) multi-relay
   networks. To reduce channel estimation overhead and delay, a training
   algorithm that allows for simultaneous estimation of the entire MIN()
   cooperative network's channel parameters at the destination node is
   proposed. The exact CramerRao lower bound (CRLB) for the problem is
   presented in closedform. Channel estimators that are capable of
   estimating the overall source-relay-destination channel parameters at
   the destination are also derived. Numerical results show that while
   reducing delay, the proposed channel estimators are close to the derived
   CRLB over a wide range of signal-to-noise ratio values and outperform
   existing channel estimation methods. Finally, extensive simulations
   demonstrate that the proposed training method and channel estimators can
   be effectively deployed in combination with cooperative optimization
   algorithms to significantly enhance the performance of AF relaying MIMO
   systems in terms of average-bit-error-rate.}},
