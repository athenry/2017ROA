@article{ ISI:000312196600002,
Author = {Blesic, Marijana and Gunaratne, H. Q. Nimal and Nockemann, Peter and
   McCarron, Philip and Seddon, Kenneth R.},
Title = {{Controlled fragrance delivery in functionalised ionic liquid-enzyme
   systems}},
Journal = {{RSC ADVANCES}},
Year = {{2013}},
Volume = {{3}},
Number = {{2}},
Pages = {{329-333}},
Abstract = {{It is often believed that both ionic liquids and surfactants generally
   behave as non-specific denaturants of proteins. In this paper, it is
   shown that amphiphilic ionic liquids bearing a long alkyl chain and a
   target molecule, where the target molecule is appended via a carboxylic
   ester functionality, can represent super-substrates that enable the
   catalytic activity of an enzyme, even at high concentrations in
   solution. Menthol has been chosen as the target molecule for slow and
   controlled fragrance delivery, and it was found that the rate of the
   menthol release can be controlled by the chemical structure of the ionic
   liquid. At a more fundamental level, this study offers an insight into
   the complex hydrophobic, electrostatic, and hydrogen bond interactions
   between the enzyme and substrate.}},
