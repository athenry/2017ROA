@article{ ISI:000307552600007,
Author = {Liu, Xueqin and Li, Kang and McAfee, Marion and Deng, Jing},
Title = {{Application of nonlinear PCA for fault detection in polymer extrusion
   processes}},
Journal = {{NEURAL COMPUTING \& APPLICATIONS}},
Year = {{2012}},
Volume = {{21}},
Number = {{6, SI}},
Pages = {{1141-1148}},
Month = {{SEP}},
Abstract = {{This paper describes the application of an improved nonlinear principal
   component analysis (PCA) to the detection of faults in polymer extrusion
   processes. Since the processes are complex in nature and nonlinear
   relationships exist between the recorded variables, an improved
   nonlinear PCA, which incorporates the radial basis function (RBF)
   networks and principal curves, is proposed. This algorithm comprises two
   stages. The first stage involves the use of the serial principal curve
   to obtain the nonlinear scores and approximated data. The second stage
   is to construct two RBF networks using a fast recursive algorithm to
   solve the topology problem in traditional nonlinear PCA. The benefits of
   this improvement are demonstrated in the practical application to a
   polymer extrusion process.}},
