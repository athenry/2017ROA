@article{ ISI:000304733000005,
Author = {Gow, Elizabeth A. and Stutchbury, Bridget J. M. and Done, T. and Kyser,
   T. K.},
Title = {{An examination of stable hydrogen isotope (delta D) variation in adult
   and juvenile feathers from a migratory songbird}},
Journal = {{CANADIAN JOURNAL OF ZOOLOGY-REVUE CANADIENNE DE ZOOLOGIE}},
Year = {{2012}},
Volume = {{90}},
Number = {{5}},
Pages = {{585-594}},
Month = {{MAY}},
Abstract = {{Stable hydrogen isotopes (delta D) can help predict geographic origin of
   tissue growth but within-site variation of feather delta D (delta D-f)
   exists. Multiple hypotheses explain delta D-f variation: moult timing,
   physiology, and spatio-temporal differences in source deuterium, but
   there have been few direct tests of these factors. We assessed delta D-f
   variation within a breeding population of Wood Thrush (Hylocichla
   mustelina (Gmelin, 1789)), by sampling feathers from hatch-year birds,
   returning adults (after migration), and to eliminate confounding
   variables from newly grown adult primary feathers during moult. We
   evaluated potential mechanisms explaining delta D-f variation by
   examining intraindividual variation (different feather types),
   precipitation delta D, prey delta D, corticosterone (CORT), and breeding
   productivity. In addition, we directly compared delta D-f of newly grown
   primary feathers to delta D blood hematocrit (delta D-b). We found
   significant differences in delta D-f between years and age classes and
   observed no significant intraindividual variation. delta D-f and delta
   D-b were not significantly correlated. CORT weakly contributed to delta
   D-f, but there are other factors influencing delta D-f. Ten percent of
   all feather samples had outlying delta D-f values, and were likely grown
   away from the breeding grounds. Our results document intrapopulation and
   intraindividual variation of delta D-f at a single location and
   indicated that age class and year-to-year differences contributed to
   high delta D-f variance.}},
