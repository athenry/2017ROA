@article{ ISI:000310403900083,
Author = {Soutsos, M. N. and Le, T. T. and Lampropoulos, A. P.},
Title = {{Flexural performance of fibre reinforced concrete made with steel and
   synthetic fibres}},
Journal = {{CONSTRUCTION AND BUILDING MATERIALS}},
Year = {{2012}},
Volume = {{36}},
Pages = {{704-710}},
Month = {{NOV}},
Abstract = {{The ductility of concrete made with commercially available steel and
   synthetic fibres has been investigated. Flexural stress-deflection
   relationships have been used to determine: flexural strength, flexural
   toughness, equivalent flexural strength, and equivalent flexural
   strength ratio. The flexural toughness of concrete was found to increase
   considerably when steel and synthetic fibres were used. However, equal
   dosages of different fibres did not result in specimens with the same
   flexural toughness. Flexural toughness differences of almost 35 J
   existed even at the same fibre dosage. This also resulted in
   considerable differences in the minimum required ground supported slab
   thickness. (C) 2012 Elsevier Ltd. All rights reserved.}},
