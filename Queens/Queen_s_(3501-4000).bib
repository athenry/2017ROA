@article{ ISI:000304091300019,
Author = {Liu, Xueqin and Li, Kang and McAfee, Marion and Bao Kha Nguyen and
   McNally, Gerard M.},
Title = {{Dynamic gray-box modeling for on-line monitoring of polymer extrusion
   viscosity}},
Journal = {{POLYMER ENGINEERING AND SCIENCE}},
Year = {{2012}},
Volume = {{52}},
Number = {{6}},
Pages = {{1332-1341}},
Month = {{JUN}},
Abstract = {{Melt viscosity is a key indicator of product quality in polymer
   extrusion processes. However, real time monitoring and control of
   viscosity is difficult to achieve. In this article, a novel soft sensor
   approach based on dynamic gray-box modeling is proposed. The soft sensor
   involves a nonlinear finite impulse response model with adaptable linear
   parameters for real-time prediction of the melt viscosity based on the
   process inputs; the model output is then used as an input of a model
   with a simple-fixed structure to predict the barrel pressure which can
   be measured online. Finally, the predicted pressure is compared to the
   measured value and the corresponding error is used as a feedback signal
   to correct the viscosity estimate. This novel feedback structure enables
   the online adaptability of the viscosity model in response to modeling
   errors and disturbances, hence producing a reliable viscosity estimate.
   The experimental results on different material/die/extruder confirm the
