@article{ ISI:000310571100005,
Author = {Ondracek, Martin and Mutombo, Pingo and Chvoj, Zdenek and Mark, Andrew
   G. and Chromcova, Zdenka and McLean, Alastair B. and Jelinek, Pavel},
Title = {{Stability, interaction and influence of domain boundaries in
   Ge/Si(111)-5 x 5}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{2012}},
Volume = {{24}},
Number = {{44}},
Month = {{NOV 7}},
Abstract = {{We present a theoretical investigation of the influence of domain
   boundaries on the Ge/Si(111)-5 x 5 phase using both large-scale DFT
   simulations and an analytical model. It is shown that different boundary
   types modify the atomic and electronic structure of the adjoining 5 x 5
   domains in very different ways. A simple theoretical model, that
   describes the energy interaction J between the boundaries and the 5 x 5
   phase, is presented and the interaction energy decay J(x) approximate to
   x(-n) for different domain boundaries is estimated. Additionally, the
   influence of the boundaries on the atomic and electronic structure of
   adatoms in the parental 5 x 5 phase is analyzed and it is argued that
   the presence of domain boundaries may strongly affect not only the
   physical but also the chemical properties of the Ge/Si(111)-5 x 5 phase.}},
