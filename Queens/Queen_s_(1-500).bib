@article{ ISI:000388954000004,
Author = {Chen, Jian and Dean, Thomas R. and Alalfi, Manar H.},
Title = {{Clone detection in MATLAB Stateflow models}},
Journal = {{SOFTWARE QUALITY JOURNAL}},
