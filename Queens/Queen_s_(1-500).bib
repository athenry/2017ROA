@article{ ISI:000382135600030,
Author = {Yazdian, Ehsan and Gazor, Saeed and Bastani, Mohammad Hasan and
   Sharifitabar, Mohsen},
Title = {{Eigenvalue Estimation of the Exponentially Windowed Sample Covariance
   Matrices}},
Journal = {{IEEE TRANSACTIONS ON INFORMATION THEORY}},
Year = {{2016}},
Volume = {{62}},
Number = {{7}},
Pages = {{4300-4311}},
Month = {{JUL}},
Abstract = {{In this paper, we consider an exponentially windowed sample covariance
   matrix (EWSCM) and propose an improved estimator for its eigenvalues. We
   use new advances in random matrix theory, which describe the limiting
   spectral distribution of the large dimensional doubly correlated Wishart
   matrices to find the support and distribution of the eigenvalues of the
   EWSCM. We then employ the complex integration and residue theorem to
   design an estimator for the eigenvalues, which satisfies the cluster
   separability condition, assuming that the eigenvalue multiplicities are
   known. We show that the proposed estimator is consistent in the
   asymptotic regime and has good performance in finite sample size
   situations. Simulation results show that the proposed estimator
   outperforms the traditional estimator, significantly.}},
