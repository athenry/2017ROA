@article{ ISI:000386758200030,
Author = {Bodaghi, Masoud and Catalanotti, Giuseppe and Correia, Nuno},
Title = {{On the statistics of transverse permeability of randomly distributed
   fibers}},
Journal = {{COMPOSITE STRUCTURES}},
Year = {{2016}},
Volume = {{158}},
Pages = {{323-332}},
Month = {{DEC 15}},
Abstract = {{An RVE-based stochastic numerical model is used to calculate the
   permeability of randomly generated porous media at different values of
   the fiber volume fraction for the case of transverse flow in a
   unidirectional ply. Analysis of the numerical results shows that the
   permeability is not normally distributed. With the aim of proposing a
   new understanding on this particular topic, permeability data are fitted
   using both a mixture model and a unimodal distribution. Our findings
   suggest that permeability can be fitted well using a mixture model based
   on the lognormal and power law distributions. In case of a unimodal
   distribution, it is found, using the maximum-likelihood estimation
   method (MLE), that the generalized extreme value (GEV) distribution
   represents the best fit. Finally, an expression of the permeability as a
   function of the fiber volume fraction based on the GEV distribution is
   discussed in light of the previous results. (C) 2016 Elsevier Ltd. All
   rights reserved.}},
