@article{ ISI:000390745300016,
Author = {Pinto, R. F. and Catalanotti, G. and Camanho, P. P.},
Title = {{Measuring the intralaminar crack resistance curve of fibre reinforced
   composites at extreme temperatures}},
Journal = {{COMPOSITES PART A-APPLIED SCIENCE AND MANUFACTURING}},
Year = {{2016}},
Volume = {{91}},
Number = {{1}},
Pages = {{145-155}},
Month = {{DEC}},
Abstract = {{This work focuses on the development of methods to measure the
   intralaminar fracture toughness of 1M7/8552 CFRP at extreme
   temperatures. The size effect law of scaled double-edge-notched (DEN)
   specimens is used to obtain the crack resistance curves associated with
   longitudinal failure of polymer composites. The compressive and tensile
   crack resistance curves are determined and compared to those measured at
   room temperature. Scanning electron microscopy is used to analyse the
   fracture surface of tensile DEN specimens as a means to understand the
   damage mechanisms involved and their dependence on temperature
   variations. It is shown that the methods proposed enable the measurement
   of the crack resistance curve under different environment conditions.
   The results obtained can be used in analytical models to evaluate the
   integrity of composite structures at extreme temperatures. (C) 2016
   Elsevier Ltd. All rights reserved.}},
