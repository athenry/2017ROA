@article{ ISI:000386822900018,
Author = {Dong, Qingshan and Yu, Hongbing and Yao, Zhongwen and Long, Fei and
   Balogh, Levente and Daymond, Mark R.},
Title = {{Study of microstructure and precipitates of a Zr-2.5Nb-0.5Cu CANDU
   spacer material}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2016}},
Volume = {{481}},
Pages = {{153-163}},
Month = {{DEC}},
Abstract = {{Synchrotron X-ray diffraction, scanning electron microscopy, and
   transmission electron microscope bright field and high angle annular
   dark field were employed to investigate the microstructure and
   precipitates of a Zr-2.5Nb-0.5Cu alloy wire, with potential application
   as a spacer in the CANDU (R) reactor design. Results show that three
   types of microstructure co-exist in the alloy: Widmanstatten structure,
   alpha-Zr grains without precipitates and alpha-Zr grains with
   precipitates. In the meantime, three types of second phase particles are
   detected: beta-Nb, Zr2Cu and Zr2Fe, which have different distributions
   within the microstructure. The beta-Nb precipitates are observed to be
   extensively distributed among the alpha plate boundaries and twin
   interfaces in the Widmanstatten structure, whereas the Zr2Cu and Zr2Fe
   precipitates are only found in the alpha plate boundaries. The
   orientation relations of the Zr2Cu precipitates with respect to the
   alpha-Zr and beta-Nb are found to be (013)(Zr2Cu)//(0001)(alpha)
   (013)(Zr2Cu)//(0001)(alpha), {[}0 (3) over bar1](Zr2Cu)//{[}(1) over bar
   100](alpha) and (013)(Zr2Cu)//(011)(beta-Nb), {[}0 (3) over
   bar1](Zr2Cu)//{[}(21) over bar1](beta-Nb) respectively. (C) 2016
   Elsevier B.V. All rights reserved.}},
