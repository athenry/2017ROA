@article{ ISI:000343618600002,
Author = {Krstulja, Aleksandra and De Schutter, Coralie and Favetta, Patrick and
   Manesiotis, Panagiotis and Agrofoglio, Luigi A.},
Title = {{Artificial receptors for the extraction of nucleoside metabolite
   7-methylguanosine from, aqueous media made by molecular imprinting}},
Journal = {{JOURNAL OF CHROMATOGRAPHY A}},
Year = {{2014}},
Volume = {{1365}},
Pages = {{12-18}},
Month = {{OCT 24}},
Abstract = {{A series of imprinted polymers targeting nucleoside metabolites,
   prepared using a template analogue approach, are presented. These were
   prepared following selection of the optimum functional monomer by
   solution association studies using 1H NMR titrations whereby methacrylic
   acid was shown to be the strongest receptor with and affinity constant
   of 621 51 L mo1-1 vs. 110 16 L mol-1 for acrylamide. The best performing
   polymers were prepared using methanol as porogenic co-solvent and
   although average binding site affinities were marginally reduced, 2.3 x
   104 L mol-1 vs. 2.7 x 104 Lmo1-1 measured for a polymer prepared in
   acetonitrile, these polymers contained the highest number of binding
   sites, 5.27 pAnol g-1 vs. 1.64 innol g-1, while they also exhibited
   enhanced selectivity for methylated guanosine derivatives. When applied
   as sorbents in the extraction of nucleoside derivative cancer biomarkers
   from synthetic urine samples, significant sample clean-up and recoveries
   of up to 90\% for 7-methylguanosine were achieved. (C) 2014 Elsevier
   B.V. All rights reserved.}},
