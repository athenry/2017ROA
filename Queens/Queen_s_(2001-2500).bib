@article{ ISI:000330457100014,
Author = {MacHattie, Jeremy A. and Irwin, Judith A. and Madden, Suzanne C. and
   Cormier, Diane and Remy-Ruyer, Aurelie},
Title = {{Detection of HI absorption in the dwarf galaxy Haro 11}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2014}},
Volume = {{438}},
Number = {{1}},
Pages = {{L66-L70}},
Month = {{FEB}},
Abstract = {{We present the results of an analysis of archival 21 cm (HI) data of the
   blue compact dwarf galaxy Haro 11 (ESO 350-IG038). Observations were
   obtained at the Very Large Array, and the presence of a compact
   absorption feature near the optical centre of the galaxy has been
   detected. The central location of the absorption feature coincides with
   the centre of the continuum background of the galaxy, as well as with
   the location of knot B. The absorption feature yields an HI mass in the
   range of 3-10 x 10(8) M-circle dot, corresponding to spin temperatures
   from 91 K to 200 K, respectively. The absence of HI seen in emission
   places an upper limit of 1.7 x 10(9) M-circle dot on the mass. To our
   knowledge this is the first example of a dwarf galaxy that shows HI
   absorption from its own background continuum. The continuum emission
   from the galaxy is also used to determine star formation rates, namely
   6.85 +/- 0.05 M-circle dot yr(-1) (for a stellar mass range of 5
