@article{ ISI:000330306000005,
Author = {Kilic, Yakup and Wymeersch, Henk and Meijerink, Arjan and Bentum, Mark
   J. and Scanlon, William G.},
Title = {{Device-Free Person Detection and Ranging in UWB Networks}},
Journal = {{IEEE JOURNAL OF SELECTED TOPICS IN SIGNAL PROCESSING}},
Year = {{2014}},
Volume = {{8}},
Number = {{1}},
Pages = {{43-54}},
Month = {{FEB}},
Abstract = {{We present a novel device-free stationary person detection and ranging
   method, that is applicable to ultra-wide bandwidth (UWB) networks. The
   method utilizes a fixed UWB infrastructure and does not require a
   training database of template waveforms. Instead, the method capitalizes
   on the fact that a human presence induces small low-frequency variations
   that stand out against the background signal, which is mainly affected
   by wideband noise. We analyze the detection probability, and validate
   our findings with numerical simulations and experiments with
   off-the-shelf UWB transceivers in an indoor environment.}},
