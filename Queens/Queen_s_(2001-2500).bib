@article{ ISI:000344145000026,
Author = {Toal, David J. J. and Keane, Andy J. and Benito, Diego and Dixon,
   Jeffery A. and Yang, Jingbin and Price, Matthew and Robinson, Trevor and
   Remouchamps, Alain and Kill, Norbert},
Title = {{Multifidelity Multidisciplinary Whole-Engine Thermomechanical Design
   Optimization}},
Journal = {{JOURNAL OF PROPULSION AND POWER}},
Year = {{2014}},
Volume = {{30}},
Number = {{6}},
Pages = {{1654-1666}},
Month = {{NOV-DEC}},
Abstract = {{Traditionally, the optimization of a turbomachinery engine casing for
   tip clearance has involved either two-dimensional transient
   thermomechanical simulations or three-dimensional mechanical
   simulations. This paper illustrates that three-dimensional transient
   whole-engine thermomechanical simulations can be used within tip
   clearance optimizations and that the efficiency of such optimizations
   can be improved when a multifidelity surrogate modeling approach is
   employed. These simulations are employed in conjunction with a rotor
   suboptimization using surrogate models of rotor-dynamics performance,
   stress, mass and transient displacements, and an engine
   parameterization.}},
