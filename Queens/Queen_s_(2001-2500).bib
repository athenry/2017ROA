@article{ ISI:000338618600012,
Author = {Zhang, He Ken and Yao, Zhongwen and Morin, Gregory and Griffiths,
   Malcolm},
Title = {{TEM characterization of in-reactor neutron irradiated CANDU spacer
   material Inconel X-750}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2014}},
Volume = {{451}},
Number = {{1-3}},
Pages = {{88-96}},
Month = {{AUG}},
Abstract = {{The irradiation induced defects in CANDU Inconel X-750 spacers, which
   were removed from reactors after about 14 effective full power years,
   were examined by transmission electron microscopy (TEM). The spacers in
   the form of garter springs were reported to operate at various
   temperatures depending on locations. Two samples from different
   locations with different estimated irradiation temperatures were tested:
   (1) similar to 480 degrees C at 6 o'clock position and (2) >= 300
   degrees C at 12 o'clock position. Obvious temperature effects were
   observed. In the similar to 180 degrees C irradiated sample, a high
   density of small lattice defects (1-3 nm) developed during irradiation,
   including stacking fault tetrahedra and both 1/3 < 111 > and 1/2 < 110 >
   type dislocation loops. A uniform distribution of small cavities
   (similar to 1-3 nm) was observed. In >300 degrees C irradiated sample,
   apart from small point defect clusters, large Frank type interstitial
   loops presented. The sizes of the cavities were also greater than those
   in the similar to 480 degrees C irradiated sample. The distribution of
   cavities was more heterogeneous and an obvious agglomeration of cavities
   to grain boundaries and phase boundaries were observed. In both samples,
   dissolution of the primary strengthening phase gamma' was noted. (C)
   2014 Elsevier B.V. All rights reserved.}},
