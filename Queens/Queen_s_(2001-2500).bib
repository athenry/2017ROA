@article{ ISI:000335613900005,
Author = {Steele, J. A. M. and Halle, J. -P. and Poncelet, D. and Neufeld, R. J.},
Title = {{Therapeutic cell encapsulation techniques and applications in diabetes}},
Journal = {{ADVANCED DRUG DELIVERY REVIEWS}},
Year = {{2014}},
Volume = {{67-68}},
Pages = {{74-83}},
Month = {{APR 10}},
Abstract = {{The encapsulation of therapeutic cells permits the implantation of
   allogeneic and xenogeneic cells for the regulation of certain
   physiological processes damaged by the death or senescence of host
   tissues. The encapsulation of pancreatic cells for the treatment of
