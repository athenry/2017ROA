@article{ ISI:000337877900001,
Author = {Lyberis, Spyros and Kalokerinos, George and Lygerakis, Michalis and
   Papaefstathiou, Vassilis and Mavroidis, Iakovos and Katevenis, Manolis
   and Pnevmatikatos, Dionisios and Nikolopoulos, Dimitrios S.},
Title = {{FPGA prototyping of emerging manycore architectures for parallel
   programming research using Formic boards}},
Journal = {{JOURNAL OF SYSTEMS ARCHITECTURE}},
Year = {{2014}},
Volume = {{60}},
Number = {{6}},
Pages = {{481-493}},
Month = {{JUN}},
Abstract = {{Performance evaluation of parallel software and architectural
   exploration of innovative hardware support face a common challenge with
   emerging manycore platforms: they are limited by the slow running time
   and the low accuracy of software simulators. Manycore FPGA prototypes
   are difficult to build, but they offer great rewards. Software running
   on such prototypes runs orders of magnitude faster than current
   simulators. Moreover, researchers gain significant architectural insight
   during the modeling process. We use the Formic FPGA prototyping board
   {[}1], which specifically targets scalable and cost-efficient
   multi-board prototyping, to build and test a 64-board model of a
   512-core, MicroBlaze-based, non-coherent hardware prototype with a full
   network-on-chip in a 3D-mesh topology. We expand the hardware
   architecture to include the ARM Versatile Express platforms and build a
   520-core heterogeneous prototype of 8 Cortex-A9 cores and 512 MicroBlaze
   cores. We then develop an MPI library for the prototype and evaluate it
   extensively using several bare-metal and MPI benchmarks. We find that
   our processor prototype is highly scalable, models faithfully
   single-chip multicore architectures, and is a very efficient platform
   for parallel programming research, being 50,000 times faster than
   software simulation. (C) 2014 Elsevier B.V. All rights reserved.}},
