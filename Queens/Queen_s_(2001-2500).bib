@article{ ISI:000346152500004,
Author = {Blecher, J. J. and Galbraith, C. M. and Van Vlack, C. and Palmer, T. A.
   and Fraser, J. M. and Webster, P. J. L. and DebRoy, T.},
Title = {{Real time monitoring of laser beam welding keyhole depth by laser
   interferometry}},
Journal = {{SCIENCE AND TECHNOLOGY OF WELDING AND JOINING}},
Year = {{2014}},
Volume = {{19}},
Number = {{7}},
Pages = {{560-564}},
Month = {{OCT}},
Abstract = {{The utility of a new laser interferometric technique, inline coherent
   imaging, for real time keyhole depth measurement during laser welding is
   demonstrated on five important engineering alloys. The keyhole depth was
   measured at 200 kHz with a spatial resolution of 22 mm using a probe
   beam, which enters the keyhole coaxially with the process beam. Keyhole
   fluctuations limited average weld depth determination to a resolution on
   the order of 100 mm. Real time keyhole depth data are compared with the
   weld depths measured from the corresponding metallographic
   crosssections. With the exception of an aluminium alloy, the technique
   accurately measured the average weld depth with differences of less than
   5\%. The keyhole depth growth rates at the start of welding are measured
   and compare well with order of magnitude calculations. The method
   described here is recommended for the real time measurement and control
   of keyhole depth in at least five different alloys.}},
