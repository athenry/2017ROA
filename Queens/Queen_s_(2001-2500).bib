@article{ ISI:000333083100018,
Author = {Zhang, He and Yao, Zhongwen and Daymond, Mark R. and Kirk, Marquis A.},
Title = {{Cavity morphology in a Ni based superalloy under heavy ion irradiation
   with hot pre-injected helium. II}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2014}},
Volume = {{115}},
Number = {{10}},
Month = {{MAR 14}},
Abstract = {{In the current investigation, TEM in-situ heavy ion (1 MeV Kr2+)
   irradiation with helium pre-injected at elevated temperature (400
   degrees C) was conducted to simulate in-reactor neutron irradiation
   induced damage in CANDU spacer material Inconel X-750, in an effort to
   understand the effects of helium on irradiation induced cavity
   microstructures. Three different quantities of helium, 400 appm, 1000
   appm, and 5000 appm, were pre-injected directly into TEM foils at 400
   degrees C. The samples containing helium were then irradiated in-situ
   with 1 MeV Kr2+ at 400 degrees C to a final dose of 5.4 dpa
   (displacement per atom). Cavities were formed from the helium injection
   solely and the cavity density and size increased with increasing helium
   dosage. In contrast to previous heavy ion irradiations with cold
   pre-injected helium, heterogeneous nucleation of cavities was observed.
   During the ensuing heavy ion irradiation, dynamical observation showed
   noticeable size increase in cavities which nucleated close to the grain
   boundaries. A ``bubble-void{''} transformation was observed after Kr2+
   irradiation to high dose (5.4 dpa) in samples containing 1000 appm and
   5000 appm helium. Cavity distribution was found to be consistent with
   in-reactor neutron irradiation induced cavity microstructures. This
   implies that the distribution of helium is greatly dependent on the
   injection temperature, and helium pre-injection at high temperature is
   preferred for simulating the migration of the transmutation produced
   helium. (C) 2014 AIP Publishing LLC.}},
