@article{ ISI:000336044500001,
Author = {Deng, Weihua and Li, Kang and Fei, Minrui},
Title = {{Identification and output tracking control of Hammerstein systems
   through wireless networks}},
Journal = {{TRANSACTIONS OF THE INSTITUTE OF MEASUREMENT AND CONTROL}},
Year = {{2014}},
Volume = {{36}},
Number = {{1}},
Pages = {{3-13}},
Month = {{FEB}},
Abstract = {{This paper investigates the identification and output tracking control
   of a class of Hammerstein systems through a wireless network within an
   integrated framework and the statistic characteristics of the wireless
   network are modelled using the inverse Gaussian cumulative distribution
   function. In the proposed framework, a new networked identification
   algorithm is proposed to compensate for the influence of the wireless
   network delays so as to acquire the more precise Hammerstein system
   model. Then, the identified model together with the model-based approach
   is used to design an output tracking controller. Mean square stability
   conditions are given using linear matrix inequalities (LMIs) and the
   optimal controller gains can be obtained by solving the corresponding
   optimization problem expressed using LMIs. Illustrative numerical
   simulation examples are given to demonstrate the effectiveness of our
   proposed method.}},
