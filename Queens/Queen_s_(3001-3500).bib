@inproceedings{ ISI:000328113100103,
Author = {Day, J. J. and Diederichs, M. S. and Hutchinson, D. J.},
Editor = {{Anagnostou, G and Ehrbar, H}},
Title = {{Incorporation of geostructural data into discrete analysis for tunnel
   design}},
Booktitle = {{UNDERGROUND - THE WAY TO THE FUTURE}},
Year = {{2013}},
Pages = {{794-801}},
Note = {{World Tunnel Congress (WTC) / 39th General Assembly of the
   International-Tunnelling-and-Underground-Space-Association (ITA), Swiss
   Tunnelling Soc, Geneva, SWITZERLAND, MAY 31-JUN 07, 2013}},
Organization = {{Pizzarotti; BASF; Herrenknecht; Holcim; Implenia; Marti; Normet Int;
   Atlas Copco Schweiz; CIFA; Caterpillar Tunneling Canada; Elkuch Bator;
   Maccaferri; Mapei; ROWA Belloli; SIKA; Sandvik; Swiss IT Factory; B S}},
Abstract = {{The conventional approach for preliminary geotechnical analysis is based
   on an equivalent continuum representation of a rockmass containing
   joints, bedding, and/or other systematic discontinuities. The authors
   have previously proposed that meso-scale structures (e.g. veinlets,
   stockwork, and sedimentary structures) that occur within blocks bounded
   by macro-scale structure (e.g. joints) have a significant impact on
   rockmass strength. These structures may not be accounted for in
   conventional core logging and yet should be included in rockmass
   characterization and numerical modelling for geotechnical design. Three
   case examples of arched underground excavations are used to highlight
   the implications of inclusion and exclusion of discrete structure in
   Finite Element Method numerical models. The cases discussed in this
   paper show that it is very important to consider meso- scale and
   intrablock structure in numerical models. In cases that are too
   computationally demanding to allow for discrete modelling of such
   structure, it is important to understand their influence on rockmass
   strength and behaviour, and to use an equivalent continuum GSI approach
   in their place.}},
