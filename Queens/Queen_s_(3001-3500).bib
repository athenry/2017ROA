Title = {{An evaluation of 1D loss model collections for the off-design
   performance prediction of automotive turbocharger compressors}},
Booktitle = {{6TH INTERNATIONAL CONFERENCE ON PUMPS AND FANS WITH COMPRESSORS AND WIND
   TURBINES (ICPF2013)}},
Series = {{IOP Conference Series-Materials Science and Engineering}},
Year = {{2013}},
Volume = {{52}},
