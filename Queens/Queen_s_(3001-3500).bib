@article{ ISI:000317795600017,
Author = {Mangwandi, Chirangano and Albadarin, Ahmad B. and Al-Muhtaseb, Ala'a H.
   and Allen, Stephen J. and Walker, Gavin M.},
Title = {{Optimisation of high shear granulation of multicomponent fertiliser
   using response surface methodology}},
Journal = {{POWDER TECHNOLOGY}},
Year = {{2013}},
Volume = {{238}},
Number = {{SI}},
Pages = {{142-150}},
Month = {{APR}},
Abstract = {{This work describes a novel method of producing multicomponent
   fertiliser granules using high shear granulation. The granulation
   process was optimised using the response surface methodology technique.
   The variables used in the optimisation process include granulation time,
   batch size, impeller speed and binder concentration. Granulation time,
   binder concentration and interaction between the batch size and
   granulation time were found to be the main factors affecting the granule
   median size. The product yield is mainly influenced by granulation time
   and binder concentration. The interaction between the impeller speed and
   batch size also have a significant influence on the product yield.
   Product yield (2-4 mm) of approximately 60\% could be obtained with high
   sphericity and granule strength (>05 MPa). A low product recycle ratio
   of about 2:3 can be obtained at the optimised process conditions,
   compared to typical recycle rations of 6:1 which are obtained in typical
   fertiliser plants. (C) 2012 Elsevier B.V. All rights reserved.}},
