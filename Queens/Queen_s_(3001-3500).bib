@inproceedings{ ISI:000328113100257,
Author = {Diederichs, M. S. and Eberhardt, E. and Fisher, B.},
Editor = {{Anagnostou, G and Ehrbar, H}},
Title = {{Consideration of stress and structural influence on high stress response
   in deep tunnelling - the Olmos Tunnel, Peru}},
Booktitle = {{UNDERGROUND - THE WAY TO THE FUTURE}},
Year = {{2013}},
Pages = {{1998-2005}},
Note = {{World Tunnel Congress (WTC) / 39th General Assembly of the
   International-Tunnelling-and-Underground-Space-Association (ITA), Swiss
   Tunnelling Soc, Geneva, SWITZERLAND, MAY 31-JUN 07, 2013}},
Organization = {{Pizzarotti; BASF; Herrenknecht; Holcim; Implenia; Marti; Normet Int;
   Atlas Copco Schweiz; CIFA; Caterpillar Tunneling Canada; Elkuch Bator;
   Maccaferri; Mapei; ROWA Belloli; SIKA; Sandvik; Swiss IT Factory; B S}},
Abstract = {{The Olmos tunnel in Peru is a key component of a large water
   infrastructure initiative in that country to transfer water from the
   Huancabamba River on the Eastern side of the Andes to drought-ridden
   areas on the Pacific Ocean Watershed. The tunnel is more than 20 km long
   and reaches depths of 2500 m under the Andes. Problems in the 12.5 km
   TBM section have included stress driven spalling from the roof which was
   managed though innovations in roof support to large scale rockburst
   events which have damage the TBM and held up operations. The tunnelling
   has been successfully completed with finishing works ongoing. This paper
   examines the causes of significant behavioural change from continuous
   but manageable roof spalling to large strain and shear bursting events
   around the perimeter and face of the tunnel. The in situ stresses in
   this area are locally influenced by changes in the volcanic geology
   within the tunnel project. In addition there is significant macro-scale
   variability in rock structure. Finally, the rock units themselves
   transition from heterogenous volcanic breccias to homogenous dacites.
   These factors together control both the in situ stress acting locally at
   the tunnel horizon as well as the damage strength and post damage
   response of the rockmass as described in this case study. Site data and
   numerical analyses are used to highlight these relationships.}},
