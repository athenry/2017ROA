@inproceedings{ ISI:000328054700178,
Author = {Donohue, S. and Gavin, K. and Tolooiyan, A.},
Editor = {{Coutinho, RQ and Mayne, PW}},
Title = {{Railway earthwork stability assessment using geophysics}},
Booktitle = {{GEOTECHNICAL AND GEOPHYSICAL SITE CHARACTERIZATION 4, VOLS I AND II}},
Year = {{2013}},
Pages = {{1519-1525}},
