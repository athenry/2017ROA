Title = {{A GRAPHICAL SOFTWARE TOOL WITH INTEGER LINEAR PROGRAMMING FOR
   MICROCONTROLLER INPUT/OUTPUT INTERFACE SELECTION FEASIBILITY}},
Booktitle = {{2013 26TH ANNUAL IEEE CANADIAN CONFERENCE ON ELECTRICAL AND COMPUTER
   ENGINEERING (CCECE)}},
Series = {{Canadian Conference on Electrical and Computer Engineering}},
Year = {{2013}},
