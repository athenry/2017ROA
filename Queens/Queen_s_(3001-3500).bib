@inproceedings{ ISI:000328113100094,
Author = {Paraskevopoulou, C. and Diederichs, M.},
Editor = {{Anagnostou, G and Ehrbar, H}},
Title = {{A comparison of viscous models under constant strain and constant
   stress: implications for tunnel analysis}},
Booktitle = {{UNDERGROUND - THE WAY TO THE FUTURE}},
Year = {{2013}},
Pages = {{723-730}},
Note = {{World Tunnel Congress (WTC) / 39th General Assembly of the
   International-Tunnelling-and-Underground-Space-Association (ITA), Swiss
   Tunnelling Soc, Geneva, SWITZERLAND, MAY 31-JUN 07, 2013}},
Organization = {{Pizzarotti; BASF; Herrenknecht; Holcim; Implenia; Marti; Normet Int;
   Atlas Copco Schweiz; CIFA; Caterpillar Tunneling Canada; Elkuch Bator;
   Maccaferri; Mapei; ROWA Belloli; SIKA; Sandvik; Swiss IT Factory; B S}},
Abstract = {{Time-dependent deformation processes can occur under sustained loading
   or as a relaxation process under conditions of fixed strain. Both
   boundary conditions may within the influence zone of an excavation. The
   mechanics of response can be visco-elastic deformation or relaxation,
   time-dependent plastic creep, or time-dependent strength degradation. It
   is important to consider the mechanics of the problem when selecting the
   appropriate viscous model for analysis of tunnels and shafts. This paper
   illustrates these different mechanisms through simple analogue models,
   before using combinations of the models to approximate time-dependent
   behaviour of rock masses using industry-conventional 2D methods. Drawing
   on the results of a sensitivity analysis utilizing accepted methods and
   standards used in research, this paper highlights the understanding of
   the visco-elastic models in the long-term period using as an example a
   rock specimen under both stress-rate and strain-rate conditions.
   Practical guidelines are given for the use of these models.}},
