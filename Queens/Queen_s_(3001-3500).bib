@article{ ISI:000316972800042,
Author = {Guo, Xin and Xie, Kongying and Campbell, Robert J. and Lai, Yongjun},
Title = {{3D stepped electrodes on a flexible substrate with permanently bonded
   poly(dimethylsiloxane) channels for moving microfluid}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY B}},
Year = {{2013}},
Volume = {{31}},
Number = {{2}},
Month = {{MAR}},
Abstract = {{Interdigitated microelectrode arrays have been exploited to move
   electrolyte via ac electroosmosis. The performance of three dimensional
   (3D) stepped electrodes has been shown to exceed that of planar
   electrodes. However, to date all prototypes described have been based on
   solid silicon or glass substrates, limiting the usage of such devices.
   This report, to our best knowledge, is the first to describe a 3D
   stepped microelectrode array on a flexible Kapton (R) substrate. The
   Kapton surface was modified to make it capable of bonding with
   poly(dimethylsiloxane) microchannels. The metal electrode was patterned
   by wet etching, a simpler process than previous approaches. Results of
   bonding, bending, and fluidic testing are reported. The frequency
   response obtained using deionized water showed maximum velocities of
   approximately 370 and 180 mu m/s at 5V(pp) for testing with a flat and
   bent substrate, respectively. (C) 2013 American Vacuum Society.
   {[}http://dx.doi.org/10.1116/1.4790651]}},
