Author = {Das, Pritam and Pahlevaninezhad, Majid and Moschopoulos, Gerry and Jain,
   Praveen},
Book-Group-Author = {{IEEE}},
Title = {{A Novel Three-Level DC-DC Converter with Load Adaptive ZVS Auxiliary
   Circuit}},
Booktitle = {{2013 TWENTY-EIGHTH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC 2013)}},
Series = {{Annual IEEE Applied Power Electronics Conference and Exposition (APEC)}},
