Author = {Pahlevaninezhad, Majid and Daneshpajooh, Hamid and Bakhshai, Alireza and
   Jain, Praveen},
Book-Group-Author = {{IEEE}},
Title = {{A Multi-Variable Control Technique for ZVS Phase-Shift Full-Bridge DC/DC
   Converter}},
Booktitle = {{2013 TWENTY-EIGHTH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC 2013)}},
