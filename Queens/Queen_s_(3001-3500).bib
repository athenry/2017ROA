@article{ ISI:000320416800002,
Author = {Tomei, M. Concetta and Daugulis, Andrew J.},
Title = {{Feasibility of operating a solid-liquid bioreactor with used automobile
   tires as the sequestering phase for the biodegradation of inhibitory
   compounds}},
Journal = {{JOURNAL OF ENVIRONMENTAL MANAGEMENT}},
Year = {{2013}},
Volume = {{125}},
Pages = {{7-11}},
Month = {{AUG 15}},
Abstract = {{Finding new uses for waste or discarded material is an important
   environmental goal; being able to use a waste material to treat another
   waste is an even more attractive objective, and this was the purpose of
   the present work. We previously showed that used automobile tires have
   an affinity for a toxic contaminant, dichlorophenol (DCP), absorbing and
   releasing it based on concentration driving forces. Here we have
   exploited this phenomenon by using used tires as the sequestering phase
   in a Two-Phase Partitioning Bioreactor (TPPB) to treat otherwise-toxic
   levels of DCP, far out-performing single phase operation in a sequencing
   batch bioreactor. A comprehensive examination of substrate loading,
   reactor exchange ratio, and tire fraction used, demonstrated that the
   tire-TPPB system could handle a 40\% higher influent substrate loading
   and an increase of the exchange ratio value from 0.5 (prohibitive for
   single phase operation) to 0.7. Such improvement was obtained with a
   tire fraction <= 9\%, comparable to that for commercial polymers
   previously employed in TPPBs. This study has opened the door to the
   identification of other waste plastics suitable for use in TPPBs for the
   treatment of recalcitrant organic contaminants. (C) 2013 Elsevier Ltd.
   All rights reserved.}},
