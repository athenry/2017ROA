@article{ ISI:000319642500007,
Author = {Kennedy, Ian J. and Spence, Stephen W. T. and Spratt, Gordon R. and
   Early, Juliana M.},
Title = {{Investigation of heat exchanger inclination in forced-draught air-cooled
   heat exchangers}},
Journal = {{APPLIED THERMAL ENGINEERING}},
Year = {{2013}},
Volume = {{54}},
Number = {{2}},
Pages = {{413-421}},
Month = {{MAY 30}},
Abstract = {{The purpose of this study is to determine the influence of inclining the
   heat exchanger relative to the fan in a forced draught air-cooled heat
   exchanger. Since inclination increases plenum depth, the effect of
   inclination is also compared with increasing plenum depth without
   inclination. The experimental study shows that inclination improves
   thermal performance by only 0.5\%, when compared with a baseline
   non-inclined case with a shallow plenum. Similarly, increasing plenum
   depth without inclination has a thermal performance benefit of
   approximately 1\%. The numerical study shows that, as the heat exchanger
   is inclined, the low velocity core at the centre of the heat exchanger
   moves to one side. (C) 2013 Elsevier Ltd. All rights reserved.}},
