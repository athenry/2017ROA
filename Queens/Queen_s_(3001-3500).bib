@inproceedings{ ISI:000335775300048,
Author = {Ihemadu, O. C. and Naeem, W. and Ferguson, R. S. and Deng, Jing},
Title = {{Invariant Information Local Sub-map Filter (IILSF) for Efficient
   Simultaneous Localisation and Mapping of Large Environments}},
Booktitle = {{2013 9TH INTERNATIONAL WORKSHOP ON ROBOT MOTION AND CONTROL (ROMOCO)}},
Year = {{2013}},
Pages = {{282-287}},
Note = {{9th International Workshop on Robot Motion and Control (RoMoCo), Wasowo,
   POLAND, JUL 03-05, 2013}},
Organization = {{Poznan Univ Technol; IEEE; RA; CSS; IFAC; Polspar}},
Abstract = {{This paper presents an Invariant Information Local Sub-map Filter
   (IILSF) as a technique for consistent Simultaneous Localisation and
   Mapping (SLAM) in a large environment. It harnesses the benefits of
   sub-map technique to improve the consistency and efficiency of Extended
   Kalman Filter (EKF) based SLAM. The IILSF makes use of invariant
   information obtained from estimated locations of features in independent
   sub-maps, instead of incorporating every observation directly into the
   global map. Then the global map is updated at regular intervals.
   Applying this technique to the EKF based SLAM algorithm: (a) reduces the
   computational complexity of maintaining the global map estimates and (b)
   simplifies transformation complexities and data association ambiguities
   usually experienced in fusing sub-maps together. Simulation results show
   that the method was able to accurately fuse local map observations to
   generate an efficient and consistent global map, in addition to
   significantly reducing computational cost and data association
   ambiguities.}},
