@article{ ISI:000359835000025,
Author = {Yang, Sheng and Zhao, Yaoyao Fiona},
Title = {{Additive manufacturing-enabled design theory and methodology: a critical
   review}},
Journal = {{INTERNATIONAL JOURNAL OF ADVANCED MANUFACTURING TECHNOLOGY}},
