@article{ ISI:000363068100027,
Author = {Adewale, Peter and Dumont, Marie-Josee and Ngadi, Michael},
Title = {{Enzyme-Catalyzed Synthesis and Kinetics of Ultrasonic-Assisted
   Methanolysis of Waste Choice White Grease for Fatty Acid Methyl Ester
   Production}},
