@article{ ISI:000368141900012,
Author = {Beygelzimer, Yan and Toth, Laszlo S. and Jonas, John J.},
Title = {{Some Physical Characteristics of Strain Hardening in Severe Plastic
   Deformation}},
Journal = {{ADVANCED ENGINEERING MATERIALS}},
Year = {{2015}},
Volume = {{17}},
Number = {{12, SI}},
Pages = {{1783-1791}},
Month = {{DEC}},
Note = {{12th International Conference on Nanostructured Materials (NANO),
   Lomonosov Moscow State Univ, Moscow, RUSSIA, JUL 13-18, 2014}},
Abstract = {{It is shown here using group theory principles that the Hencky strain is
   not an appropriate measure for tests involving simple shear, for which
   the von Mises formulation should be used exclusively. It is also shown
   analytically that the application of self-similarity principles to
   Stages III-IV indicates that these obey power laws, while the same
   principles do not apply to the other stages. As a result, they do not
   develop self-similar microstructures. Finally, the effect of strain path
   on the work hardening rates themselves is also examined. It is shown
   that the lowest rates are again associated with simple shear because
