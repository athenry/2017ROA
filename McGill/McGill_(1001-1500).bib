@inproceedings{ ISI:000379883000029,
Author = {Chen, Han and Zhao, Yaoyao F.},
Book-Group-Author = {{ASME}},
Title = {{LEARNING ALGORITHM BASED MODELING AND PROCESS PARAMETERS RECOMMENDATION
   SYSTEM FOR BINDER JETTING ADDITIVE MANUFACTURING PROCESS}},
Booktitle = {{INTERNATIONAL DESIGN ENGINEERING TECHNICAL CONFERENCES AND COMPUTERS AND
   INFORMATION IN ENGINEERING CONFERENCE, 2015, VOL 1A}},
