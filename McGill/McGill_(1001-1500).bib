@inproceedings{ ISI:000379883900016,
Author = {Ghotbi, Bahareh and Gonzalez, Francisco and Kovecses, Jozsef and
   Angeles, Jorge},
Book-Group-Author = {{ASME}},
Title = {{EFFECT OF REDUNDANT ACTUATION ON THE MOBILITY OF WHEELED ROBOTS ON
   UNSTRUCTURED TERRAIN}},
