@article{ ISI:000360776500053,
Author = {Garcia-Salaberri, Pablo A. and Gostick, Jeff T. and Hwang, Gisuk and
   Weber, Adam Z. and Vera, Marcos},
Title = {{Effective diffusivity in partially-saturated carbon-fiber gas diffusion
   layers: Effect of local saturation and application to macroscopic
   continuum models}},
