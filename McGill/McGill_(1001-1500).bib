@article{ ISI:000360420300034,
Author = {Helmus, R. and Hinterhoelzl, R. and Hubert, P.},
Title = {{A stochastic approach to model material variation determining tow
   impregnation in out-of-autoclave prepreg consolidation}},
Journal = {{COMPOSITES PART A-APPLIED SCIENCE AND MANUFACTURING}},
Year = {{2015}},
Volume = {{77}},
Pages = {{293-300}},
Month = {{OCT}},
Abstract = {{Material variations are always present even though out-of-autoclave
   prepregs are machine-made. They strongly determine the consolidation and
   may eventually lead to voids within the final part, depending on applied
   process conditions. To capture any contingencies, stochastic
   differential equations are derived to describe various interacting
   phenomena in OoA consolidation. In a second step the probabilistic space
   is discretized using the Karhunen-Loeve truncation and the Probabilistic
   Collocation method is applied in order to use deterministic solvers for
   flow and compaction problems. The initial degree of impregnation is
   represented by an Ornstein Uhlenbeck process and calibrated with
   CT-images. (C) 2015 Elsevier Ltd. All rights reserved.}},
