@article{ ISI:000384467100019,
Author = {Olanrewaju, A. O. and Robillard, A. and Dagher, M. and Juncker, D.},
Title = {{Autonomous microfluidic capillaric circuits replicated from 3D-printed
   molds}},
Journal = {{LAB ON A CHIP}},
Year = {{2016}},
Volume = {{16}},
