@article{ ISI:000376083700001,
Author = {Shih, Steve C. C. and Moraes, Christopher},
Title = {{Next generation tools to accelerate the synthetic biology process}},
Journal = {{INTEGRATIVE BIOLOGY}},
Year = {{2016}},
Volume = {{8}},
Number = {{5}},
Pages = {{585-588}},
Abstract = {{Synthetic biology follows the traditional engineering paradigm of
   designing, building, testing and learning to create new biological
   systems. While such approaches have enormous potential, major challenges
   still exist in this field including increasing the speed at which this
   workflow can be performed. Here, we present recently developed
   microfluidic tools that can be used to automate the synthetic biology
   workflow with the goal of advancing the likelihood of producing desired
   functionalities. With the potential for programmability, automation, and
   robustness, the integration of microfluidics and synthetic biology has
   the potential to accelerate advances in areas such as bioenergy, health,
   and biomaterials.}},
