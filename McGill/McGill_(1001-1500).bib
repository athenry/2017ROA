@article{ ISI:000362603600045,
Author = {Hoang, Thang M. and Osman, Mohamed M. and Chagnon, Mathieu and Qiu, Meng
   and Patel, David and Sowailem, Mohammed and Xu, Xian and Plant, David},
Title = {{Phase-diversity method using phase-shifting interference algorithms for
   digital coherent receivers}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2015}},
Volume = {{356}},
Pages = {{269-277}},
Month = {{DEC 1}},
Abstract = {{We describe phase-diversity optical coherent receivers (CRx) using the
   phase-shifting-interference (PSI) framework. The goals of the analysis
   are several-fold. First, we show that coherent detection can be realized
   with optical hybrids that have an arbitrary number of branches and phase
   shifts using a closed-form solution of the inphase-quadrature mapping.
   Second, we show that CRx with 2 x 4 90 degrees hybrids using balanced
   detection (BD) and CRx with 2 x 3 120 degrees hybrid using single-ended
   detection (SED) perform optimally compared to alternative
   configurations. A proof-of-concept WDM colorless 10 x 132-Gb/s PDM-QPSK
   transmission experiment is conducted. We demonstrate that an example of
   arbitrary phase diversity CRx with a 2 x 3 90 degrees hybrid SED
   operates with a 03 dB signal-to-noise-ratio (SNR) penalty relative to
   conventional CRx at 6400 km and 4480 km for bit-error-rates below the
   threshold of 2 x 10(-2) and the threshold of 3.8 x 10(-3) respectively.
   The sensitivity degradation of the 2 x 390 degrees hybrid SED with
   respect to the 2 x 4 90 degrees hybrid BD in the shot noise limited
   regime at a distance of 4480 km is 3 dB, which matches well with the
   predicted penalty from the PSI analytical model. To the best of our
   knowledge, this paper is the first attempt to model a CRx using the PSI
   model. (C) 2015 Elsevier B.V. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Hoang, TM (Reprint Author), McGill Univ, Dept Elect \& Comp Engn, Montreal, PQ, Canada.
   Hoang, Thang M.; Osman, Mohamed M.; Chagnon, Mathieu; Qiu, Meng; Patel, David; Sowailem, Mohammed; Xu, Xian; Plant, David, McGill Univ, Dept Elect \& Comp Engn, Montreal, PQ, Canada.}},
DOI = {{10.1016/j.optcom.2015.07.086}},
ISSN = {{0030-4018}},
EISSN = {{1873-0310}},
