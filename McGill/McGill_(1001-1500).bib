@article{ ISI:000359171900031,
Author = {Hosinieh, Milad Mohammadi and Aoude, Hassan and Cook, William D. and
   Mitchell, Denis},
Title = {{Behavior of ultra-high performance fiber reinforced concrete columns
   under pure axial loading}},
Journal = {{ENGINEERING STRUCTURES}},
Year = {{2015}},
Volume = {{99}},
Pages = {{388-401}},
Month = {{SEP 15}},
Abstract = {{This paper presents the results of a study examining the axial load
   performance of ultra-high performance fiber reinforced concrete (UHPFRC)
   columns. As part of the experimental program six large-scale columns
   were tested under pure axial loading to examine the effect of UHPFRC and
   transverse reinforcement detailing on column performance. The results
   demonstrate that the provision of closely-spaced and well-detailed
   transverse reinforcement allows for the development of excellent
   ductility in UHPFRC columns. The results also indicate that spacing and
   configuration of transverse reinforcement are important factors
   affecting the axial strength and toughness of UHPFRC columns. The
   analytical investigation examines the suitability of using existing
   high-strength concrete and fiber reinforced concrete confinement models
   to predict the axial response of the columns tested in this research
   program. The results indicate the need for the development of
   UHPFRC-specific confinement models. (C) 2015 Elsevier Ltd. All rights
   reserved.}},
