@inproceedings{ ISI:000300400100059,
Author = {DaBreo, J. and Shamim, I. and Rogers, C. A.},
Editor = {{Mazzolani, F and Herrera, R}},
Title = {{Impact of gravity loads on the lateral performance of cold-formed steel
   frame/steel sheathed shear walls}},
Booktitle = {{STESSA 2012: PROCEEDINGS OF THE 7TH INTERNATIONAL CONFERENCE ON
   BEHAVIOUR OF STEEL STRUCTURES IN SEISMIC AREAS}},
