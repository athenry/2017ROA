@article{ ISI:000301161800011,
Author = {Rinaldi, Stephanie and Paidoussis, Michael P.},
Title = {{Theory and experiments on the dynamics of a free-clamped cylinder in
   confined axial air-flow}},
Journal = {{JOURNAL OF FLUIDS AND STRUCTURES}},
Year = {{2012}},
Volume = {{28}},
Pages = {{167-179}},
Month = {{JAN}},
Abstract = {{Dynamics of a flexible cantilevered cylinder in confined axial flow is
   investigated theoretically and experimentally, in the case where the
   flow is directed from the free end towards the clamped one. First, the
   dynamical behaviour is described, as observed in ad hoc experiments with
   air flow: the system develops small-amplitude first-mode oscillations at
   low flow velocities, which could be flutter, and at higher flows the
   oscillatory behaviour is succeeded by a static divergence. Then, a
   relatively simple, linear theoretical model for the system is developed,
   and the theoretically predicted behaviour is compared to that observed.
   The model captures the essentials of the observed behaviour. (C) 2011
   Elsevier Ltd. All rights reserved.}},
