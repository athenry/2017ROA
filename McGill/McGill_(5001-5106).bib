@article{ ISI:000300866200008,
Author = {Payant, R. and Rosenblum, F. and Nesset, J. E. and Finch, J. A.},
Title = {{The self-heating of sulfides: Galvanic effects}},
Journal = {{MINERALS ENGINEERING}},
Year = {{2012}},
Volume = {{26}},
Pages = {{57-63}},
Month = {{JAN}},
Abstract = {{Under certain conditions of moisture and oxygen, sulfides can
   spontaneously heat or self-heat. In this paper, we test a hypothesis
   that galvanic interaction between some sulfides can promote
   self-heating. Using a self-heating test procedure, four cases were
   examined. Case 1 (specimen sulfides) and case 2 (concentrates) comprised
   four sulfides: pyrite, chalcopyrite, sphalerite and galena. They were
   tested individually and as 50:50 weight mixtures. The individual samples
   and mixtures of low rest-potential difference did not self-heat but
   mixtures of high rest-potential difference did. In case 3, pyrite was
   mixed 50:50 with two materials that self-heated alone, pyrrhotite and
   nickel concentrate, and self-heating further increased. For case 4,
   mixtures of pyrite-sphalerite were tested as a function of particle
   size. Self-heating increased as particle size decreased and it was the
   fineness of the pyrite (high rest-potential or cathodic mineral) that
   governed the response. The increase in self-heating with high
   rest-potential difference and increasing particle fineness of cathodic
   mineral supports that galvanic interaction can contribute to sulfide
   self-heating. A possible mechanism based on the hydrogen sulfide
   hypothesis is proposed where the reduction reaction on the cathodic
   mineral surface is ferric to ferrous. The understanding gained will be
   of interest to those involved in mining, storage, shipping and disposal
   of sulfide minerals. (C) 2011 Elsevier Ltd. All rights reserved.}},
