Title = {{Inelastic seismic performance of brace connections in steel tension-only
   concentrically braced frames}},
Booktitle = {{STESSA 2012: PROCEEDINGS OF THE 7TH INTERNATIONAL CONFERENCE ON
   BEHAVIOUR OF STEEL STRUCTURES IN SEISMIC AREAS}},
Year = {{2012}},
Pages = {{289-295}},
