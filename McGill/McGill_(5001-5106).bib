@inproceedings{ ISI:000302774300384,
Author = {Gregoire, Y. and Frost, D. L. and Petel, O.},
Editor = {{Elert, ML and Buttler, WT and Borg, JP and Jordan, JL and Vogler, TJ}},
Title = {{DEVELOPMENT OF INSTABILITIES IN EXPLOSIVELY DISPERSED PARTICLES}},
Booktitle = {{SHOCK COMPRESSION OF CONDENSED MATTER - 2011, PTS 1 AND 2}},
Series = {{AIP Conference Proceedings}},
Year = {{2012}},
Volume = {{1426}},
Note = {{7th Biennial Conference of the American-Physical-Society-Topical-Group
   on Shock Compression of Condensed Matter, Chicago, IL, JUN 26-JUL 01,
   2011}},
Organization = {{Amer Phys Soc (APS), Top Grp; Los Alamos Natl Lab; Lawrence Livermore
   Natl Lab; Sandia Natl Labs}},
Abstract = {{Models for explosives containing metal particles must consider the
   complex gas-particle flow that is generated following detonation of the
   explosive. Previous experimental studies have shown that when a layer of
   solid particles is explosively dispersed, the particles often develop a
   non-uniform spatial distribution. The instabilities within the particles
   and at the particle layer interface likely form on the timescale of the
   detonation propagation through the particles. The mesoscale
   perturbations are manifested at later times in experiments by the
   formation of clusters of particles or coherent jet-like particle
   structures which persist for some distance during the dispersal process.
   These particle jets influence the particle-gas mixing and hence particle
   reaction rates at the macro scale. The particle instabilities that occur
   in explosively dispersed particles are investigated with a mesh-free
   computational method (smoothed-particle hydrodynamics). The simulations
   are compared with experimental results for the dispersal of a
   cylindrical packed bed of particles surrounding a central explosive
   charge. Of particular interest is the effect of the particle density and
   charge/particle mass ratio on the susceptibility of the particles to
   form jets.}},
