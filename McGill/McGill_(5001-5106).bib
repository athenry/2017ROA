@inproceedings{ ISI:000320654000178,
Author = {Karangelos, Efthymios and Bouffard, Francois},
Book-Group-Author = {{IEEE}},
Title = {{Integrating Demand Response into Agent-Based Models of Electricity
   Markets}},
Booktitle = {{2012 50TH ANNUAL ALLERTON CONFERENCE ON COMMUNICATION, CONTROL, AND
   COMPUTING (ALLERTON)}},
