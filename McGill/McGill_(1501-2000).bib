Title = {{Architectures for deep neural network based acoustic models defined over
   windowed speech waveforms}},
Booktitle = {{16TH ANNUAL CONFERENCE OF THE INTERNATIONAL SPEECH COMMUNICATION
   ASSOCIATION (INTERSPEECH 2015), VOLS 1-5}},
Year = {{2015}},
Pages = {{6-10}},
