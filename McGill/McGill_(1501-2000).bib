@inproceedings{ ISI:000380545700015,
Author = {Wong, Christopher Yee and Turker, Korhan and Sharf, Inna and Beckman,
   Blake},
Editor = {{Mejias, L and Corke, P and Roberts, J}},
Title = {{Posture Reconfiguration and Navigation Maneuvers on a Wheel-Legged
   Hydraulic Robot}},
Booktitle = {{Field and Service Robotics}},
