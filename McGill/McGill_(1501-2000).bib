@article{ ISI:000376080700005,
Author = {Ma, Ming and Rochette, Martin and Chen, Lawrence R.},
Title = {{Generating Chirped Microwave Pulses Using an Integrated Distributed
   Fabry-Perot Cavity in Silicon-on-Insulator}},
Journal = {{IEEE PHOTONICS JOURNAL}},
Year = {{2015}},
Volume = {{7}},
Number = {{2}},
Month = {{APR}},
Abstract = {{We demonstrate photonic generation of chirped microwave pulses (CMPs)
   using an integrated distributed Fabry-Perot cavity (DFPC) in
   silicon-on-insulator as a spectral shaper. The DPFC is based on a pair
   of spatially offset chirped Bragg gratings and has a response with
   varying free spectral range. The spectral shaper is used to filter
   broadband pulses from a mode-locked fiber laser, and CMPs are obtained
   at the output of a photodetector following a wavelength-to-time mapping
   process in a dispersive medium. We generate CMPs with a central
   frequency of up to 20 GHz, a radio-frequency (RF) chirp of 0.012 GHz/ps,
   and a time-bandwidth product of 14.5.}},
