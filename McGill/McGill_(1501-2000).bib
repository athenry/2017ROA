@article{ ISI:000347902900001,
Author = {Hieu Pham Trung Nguyen and Djavid, Mehrdad and Woo, Steffi Y. and Liu,
   Xianhe and Connie, Ashfiqua T. and Sadaf, Sharif Md. and Wang, Qi and
   Botton, Gianluigi A. and Shih, Ishiang and Mi, Zetian},
Title = {{Engineering the Carrier Dynamics of InGaN Nanowire White Light-Emitting
   Diodes by Distributed p-AlGaN Electron Blocking Layers}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2015}},
Volume = {{5}},
Month = {{JAN 16}},
Abstract = {{We report on the demonstration of a new type of axial nanowire LED
   heterostructures, with the use of self-organized InGaN/AlGaN
   dot-in-a-wire core-shell nanowire arrays. The large bandgap AlGaN shell
   is spontaneously formed on the sidewall of the nanowire during the
   growth of AlGaN barrier of the quantum dot active region. As such,
   nonradiative surface recombination, that dominates the carrier dynamics
   of conventional axial nanowire LED structures, can be largely
   eliminated, leading to significantly increased carrier lifetime from
   similar to 0.3 ns to 4.5 ns. The luminescence emission is also enhanced
   by orders of magnitude. Moreover, the p-doped AlGaN barrier layers can
   function as distributed electron blocking layers (EBLs), which is found
   to be more effective in reducing electron overflow, compared to the
   conventional AlGaN EBL. The device displays strong white-light emission,
   with a color rendering index of similar to 95. An output power of >5 mW
   is measured for a 1 mm 3 1 mm device, which is more than 500 times
   stronger than the conventional InGaN axial nanowire LEDs without AlGaN
   distributed EBLs.}},
