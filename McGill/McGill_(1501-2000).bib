@article{ ISI:000377306800003,
Author = {Jumba, Vikas and Parsaeefard, Saeedeh and Derakhshani, Mahsa and Tho
   Le-Ngoc},
Title = {{Resource Provisioning in Wireless Virtualized Networks via Massive-MIMO}},
Journal = {{IEEE WIRELESS COMMUNICATIONS LETTERS}},
Year = {{2015}},
Volume = {{4}},
Number = {{3}},
Pages = {{237-240}},
Month = {{JUN}},
Abstract = {{This letter proposes a dynamic resource provisioning scheme for an OFDMA
   wireless virtualized network (WVN), where one base-station equipped with
   a large number of antennas serves users belonging to a number of service
   providers via different slices. In particular, joint power, sub-carrier,
   and antenna allocation problems are presented for both perfect and
   imperfect channel knowledge cases, aiming to maximize a sum-utility
   while maintaining a minimum rate per slice. Subsequently, relaxation and
   variable transformation are applied to develop the efficient algorithm
   to solve the formulated non-convex, combinational optimization problem.
   Simulation results reveal the benefits of applying a large number of
   antennas in this setup and evaluate the network performance for
   different system conditions.}},
