Author = {Yuhas, Michael and Feng, Yi and Bajcsy, Jan},
Book-Group-Author = {{IEEE}},
Title = {{On the Capacity of Faster-than-Nyquist MIMO Transmission with CSI at the
   Receiver}},
Booktitle = {{2015 IEEE GLOBECOM WORKSHOPS (GC WKSHPS)}},
