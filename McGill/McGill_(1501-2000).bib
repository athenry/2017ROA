Title = {{Dual Decomposition Method for Energy-Efficient Resource Allocation in
   D2D Communications Underlying Cellular Networks}},
Booktitle = {{2015 IEEE GLOBAL COMMUNICATIONS CONFERENCE (GLOBECOM)}},
Series = {{IEEE Global Communications Conference}},
Year = {{2015}},
