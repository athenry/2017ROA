@article{ ISI:000350182700020,
Author = {Rudinsky, S. and Brochu, M.},
Title = {{Interdiffusion between copper and nickel powders and sintering map
   development during spark plasma sintering}},
Journal = {{SCRIPTA MATERIALIA}},
Year = {{2015}},
Volume = {{100}},
Pages = {{74-77}},
Month = {{APR 15}},
Abstract = {{An investigation into spark plasma sintering was performed using Cu-Ni
   powder blends. Pucks were sintered between 700 and 900 degrees C.
   Isolated copper particles were analyzed by energy-dispersive
   spectroscopy. Compositional maps and diffusion coefficients showed that
   diffusion was uniform in all directions, while activation energies
   demonstrated an increase in diffusion efficiency. A solution to the
   diffusion equation was used to model sintering times to obtain
   homogeneous components, given various powder blend compositions, and the
   results were experimentally validated. (C) 2014 Acta Materialia Inc.
   Published by Elsevier Ltd. All rights reserved.}},
