Author = {Mani, Vidhyasagar and Champagne, Benoit and Zhu, Wei-Ping},
Book-Group-Author = {{IEEE}},
Title = {{Speech Enhancement in Modulation Domain Using Codebook-based Speech and
   Noise Estimation}},
