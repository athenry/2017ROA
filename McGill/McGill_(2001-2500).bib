Title = {{Heat transfer analysis of large scale seasonal thermal energy storage
   for underground mine ventilation}},
Booktitle = {{CLEAN, EFFICIENT AND AFFORDABLE ENERGY FOR A SUSTAINABLE FUTURE}},
Series = {{Energy Procedia}},
