@article{ ISI:000345524600002,
Author = {Sakib, Meer Nazmus and Hai, Mohammed Shafiqul and Liboiron-Ladouceur,
   Odile},
Title = {{A Silicon Photonic Integrated Packaged Coherent Receiver Front-End For
   Soft-Decision Decoding}},
Journal = {{JOURNAL OF LIGHTWAVE TECHNOLOGY}},
Year = {{2014}},
Volume = {{32}},
Number = {{24}},
Pages = {{4151-4156}},
Month = {{DEC 15}},
Abstract = {{In this paper, a packaged integrated coherent receiver and optical
   front-end for soft decision based on 25 Gbaud/s quadrature phase-shift
   keying (QPSK) is investigated for on-chip applications. The front-end
   consists of a 90 degrees hybrid, balanced photodetectors, and unbalanced
   couplers monolithically integrated in a CMOS compatible
   silicon-on-insulator technology. The silicon photonic device is packaged
   onto a ceramic substrate with RF and dc connectors. The proposed
   front-end is an example of integration of system on chip. The integrated
   solution is able to provide 6.2 dB performance improvements over uncoded
   system without error correction. The front-end outperforms optical
   system with hard-decision forward error correction by 2.2 dB at the BER
   of 10(-7).}},
