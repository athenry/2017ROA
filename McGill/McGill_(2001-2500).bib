@article{ ISI:000344004900106,
Author = {Wang, Wei and Zhuge, Qunbi and Morsy-Osman, Mohamed and Gao, Yuliang and
   Xu, Xian and Chagnon, Mathieu and Qiu, Meng and Minh Thang Hoang and
   Zhang, Fangyuan and Li, Rui and Plant, David V.},
Title = {{Decision-aided sampling frequency offset compensation for
   reduced-guard-interval coherent optical OFDM systems}},
Journal = {{OPTICS EXPRESS}},
Year = {{2014}},
Volume = {{22}},
Number = {{22}},
Pages = {{27553-27564}},
Month = {{NOV 3}},
Abstract = {{We propose a decision-aided algorithm to compensate the sampling
   frequency offset (SFO) between the transmitter and receiver for
   reduced-guard-interval (RGI) coherent optical (CO) OFDM systems. In this
   paper, we first derive the cyclic prefix (CP) requirement for preventing
   OFDM symbols from SFO induced inter-symbol interference (ISI). Then we
   propose a new decision-aided SFO compensation (DA-SFOC) algorithm, which
   shows a high SFO tolerance and reduces the CP requirement. The
   performance of DA-SFOC is numerically investigated for various
   situations. Finally, the proposed algorithm is verified in a single
   channel 28 Gbaud polarization division multiplexing (PDM) RGI CO-OFDM
   experiment with QPSK, 8 QAM and 16 QAM modulation formats, respectively.
   Both numerical and experimental results show that the proposed DA-SFOC
   method is highly robust against the standard SFO in optical fiber
   transmission. (C) 2014 Optical Society of America}},
