@inproceedings{ ISI:000369336200223,
Author = {Ghodoosipour, Golnaz and Plourde, Eric and Champagne, Benoit},
Book-Group-Author = {{IEEE}},
Title = {{On the Use of a Codebook-based Modeling Approach for Bayesian STSA
   Speech Enhancement}},
