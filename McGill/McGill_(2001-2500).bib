@inproceedings{ ISI:000370076902153,
Author = {Zhang, Fei and Joos, Geza},
Book-Group-Author = {{IEEE}},
Title = {{A Predictive Nearest Level Control of Modular Multilevel Converter}},
Booktitle = {{2015 THIRTIETH ANNUAL IEEE APPLIED POWER ELECTRONICS CONFERENCE AND
   EXPOSITION (APEC 2015)}},
Series = {{Annual IEEE Applied Power Electronics Conference and Exposition (APEC)}},
Year = {{2015}},
Pages = {{2846-2851}},
Note = {{30th Annual IEEE Applied Power Electronics Conference and Exposition
   (APEC), Charlotte, NC, MAR 15-19, 2015}},
Organization = {{IEEE; Power Sources Manufacturers Assoc; IEEE Power Elect Soc; IEEE Ind
   Applicat Soc}},
Abstract = {{Model predictive control (MPC) is an emerging control technique which
   can include the multiple control objectives, nonlinearities and system
   constraints easily. The main drawback of MPC for modular multilevel
   converter (MMC) application is its large computation, since the high
   level MMC has huge combination of switching states. Choosing the
   weighting factors of the cost function for MMC is also very complex. In
   this paper, a new predictive nearest level control (PNLC) is proposed,
   which uses the predicted values of control objectives to directly
