@article{ ISI:000365988200008,
Author = {Chu, Vincent H. and Altai, Wihel},
Title = {{LAGRANGIAN BLOCK SIMULATION OF BUOYANCY AT TURBULENCE INTERFACES}},
Journal = {{COMPUTATIONAL THERMAL SCIENCES}},
Year = {{2015}},
Volume = {{7}},
Number = {{2}},
Pages = {{181-189}},
Abstract = {{Simulations of turbulent interfaces produced by positive and negative
   buoyancy are conducted by moving blocks of vorticity and buoyancy in the
   direction of the flow. The second moment of the blocks increases at a
   rate proportional to the diffusivity. The block simulation has been
   shown to be relatively free of numerical oscillations. Unlike most
   classical methods, the error associated with Lagrangian block simulation
   (LBS) is not cumulative. Artificial diffusion error is negligible. This
   relatively non-diffusive LBS method is employed to simulate buoyancy at
   turbulence interfaces that are under the stabilizing and destabilizing
   influences by the force of buoyancy. In one series of simulation, the
   turbulence is initiated by shear and subjected to the stabilizing
   influence of the buoyancy force. In the other series, turbulence is
   produced by the destabilizing influence of the buoyancy force. The two
   series of simulations are evaluated to compare performance between (i)
   the large eddy simulation (LES) method and (ii) the K-epsilon modeling
   of turbulent flow.}},
