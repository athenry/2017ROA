@article{ ISI:000346788600051,
Author = {Borkar, Hemant and Pekguleryuz, Mihriban},
Title = {{Effect of Extrusion Parameters on Texture and Microstructure Evolution
   of Extruded Mg-1 pctMn and Mg-1 pctMn-Sr Alloys}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS A-PHYSICAL METALLURGY AND
   MATERIALS SCIENCE}},
Year = {{2015}},
Volume = {{46A}},
Number = {{1}},
Pages = {{488-495}},
Month = {{JAN}},
Abstract = {{Three Mg alloys Mg-1 pctMn (M1), Mg-1 pctMn-1.3 pctSr, and Mg-1
   pctMn-2.1 pctSr were subjected to two different extrusion temperatures
   and two different extrusion speeds in lab-scale extrusion. The extrusion
   temperatures of 573 K and 673 K (300 A degrees C and 400 A degrees C)
   and two ram speeds of 4 and 8 mm/s were used at constant extrusion ratio
   of 7. M1 exhibited strong basal texture after extrusion at 673 K (400 A
   degrees C) at higher speed. At 573 K (300 A degrees C),
   recrystallization in all alloys takes place completely or partially by
   continuous dynamic recrystallization mechanism, while particle
   stimulated nucleation (PSN) occurs in all M1-Sr alloys at both extrusion
   temperatures and speeds. At 673 K (400 A degrees C), grain boundary
   bulging is the only recrystallization mechanism in alloy M1, while it
   occurs in combination with PSN in M1-Sr alloys. The effect of texture
   weakening by PSN is more significant in M1-Sr alloys extruded at 573 K
   (400 A degrees C). The plant extrusion trials were carried out on Mg-1
