@article{ ISI:000345849100038,
Author = {Akbarzadeh-Sharbaf, Ali and Giannacopoulos, Dennis D.},
Title = {{On the Development of Nonoverlapping and Stable Hybrid FETD-FDTD
   Formulations}},
Journal = {{IEEE TRANSACTIONS ON ANTENNAS AND PROPAGATION}},
Year = {{2014}},
Volume = {{62}},
Number = {{12}},
Pages = {{6299-6306}},
Month = {{DEC}},
Abstract = {{A general framework to combine the finite-difference time-domain (FDTD)
   and the finite-element time-domain (FETD) formulations, both based on
   the vector wave equation, is proposed. In contrast to the existing
   stable hybrid FETD-FDTD, there is no transition layer between two
   subdomains. In addition, the stability of the proposed approach is
   analytically proved. This framework allows combining different FDTD and
   FETD formulations together. Particularly, a fully unconditionally stable
   hybrid method is proposed, which is proved to be energy conservative
   too. The key ingredient is a finite-element tearing and interconnecting
   method for electromagnetic problems with a new interface condition that
   preserves the stability of the numerical method in each region. Several
   numerical examples are considered in order to validate the proposed
   methods. The numerical results match with the reference solutions very
   well in all cases.}},
