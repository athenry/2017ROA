@article{ ISI:000343352400033,
Author = {Kabir, Abu Syed Hurnaun and Sanjari, Mehdi and Su, Jing and Jung, In-Ho
   and Yue, Stephen},
Title = {{Effect of strain-induced precipitation on dynamic recrystallization in
   Mg-Al-Sn alloys}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2014}},
Volume = {{616}},
Pages = {{252-259}},
Month = {{OCT 20}},
Abstract = {{Two different amounts of tin (Sn) were added to a Mg-3 wt\% Al binary
   alloy to form different amounts of precipitates during hot deformation.
   The thermodynamic modeling software, FactSage (TM), was used to
   calculate the amounts of Sn to generate the desired relative levels of
   precipitation. The alloys were deformed at four different temperatures
   and three different strain rates to generate different amounts of
   precipitates. The objective was to study the effect of these
   precipitates on dynamic recrystallization. The results indicated that
   the formation of strain-induced precipitates is a function of
   deformation temperature, strain, and strain rate. The findings also
   revealed that higher amounts of precipitates reduced the volume fraction
   of dynamic recrystallization and refined the dynamically recrystallized
   grain size. (C) 2014 Elsevier B.V. All rights reserved.}},
