@article{ ISI:000380081600017,
Author = {Cuinat-Guerraz, Nils and Dumont, Marie-Josee and Hubert, Pascal},
Title = {{Environmental resistance of flax/bio-based epoxy and flax/polyurethane
   composites manufactured by resin transfer moulding}},
Journal = {{COMPOSITES PART A-APPLIED SCIENCE AND MANUFACTURING}},
Year = {{2016}},
Volume = {{88}},
Pages = {{140-147}},
Month = {{SEP}},
Abstract = {{As biocomposites are highly sensitive to water absorption, the aim of
   this study was to compare the physical properties two biocomposites: (1)
   a flax/bio-based epoxy (Entropy SUPER SAP CLR/INS) and (2) a
   flax/polyurethane (HENKEL LOCTITE MAX 3). Both materials were reinforced
   with 14 layers of flax (TEXONIC twill 2 x 2) and manufactured using a
   resin transfer moulding process. Post-cured composite samples were aged
   at 90\% RH and 30 degrees C for various periods of time up to 720 h. The
   results showed that both composites followed a Fickian diffusion
   behaviour. Water had a plasticizing effect on the composites and it
   changed their failure mode. This effect took longer to appear for the
   polyurethane composites. The chemical bonds between the hydroxyl groups
   of the fibres and the isocyanate lead to a stronger interface which
   improved the mechanical properties (short beam and compressive
   strengths) as compared to the flax/bio-epoxy composites. (C) 2016
   Elsevier Ltd. All rights reserved.}},
