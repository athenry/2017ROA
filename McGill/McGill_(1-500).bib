@article{ ISI:000384126300055,
Author = {Abdukerim, Nurmemet and Li, Lizhu and Rochette, Martin},
Title = {{Chalcogenide-based optical parametric oscillator at 2 mu m}},
Journal = {{OPTICS LETTERS}},
Year = {{2016}},
Volume = {{41}},
Number = {{18}},
Pages = {{4364-4367}},
Month = {{SEP 15}},
Abstract = {{We report the first chalcogenide-based optical parametric oscillator
   (OPO) relying on pure parametric gain. The all-fiber OPO operates in the
   wavelength range of 2 mu m and is tunable over 290 nm from the combined
   Stokes and anti-Stokes contributions. The gain medium is a 10 cm long
   chalcogenide microwire made from a high modal confinement As2Se3 core
   with cyclo olefin polymer cladding, leading to optimized chromatic
   dispersion, high nonlinearity, and broadband transparency. With a power
   threshold of only a fraction of a milliwatt, this design is promising
   for the fabrication of tunable, compact, and low-power consumption
   mid-infrared sources. (C) 2016 Optical Society of America}},
