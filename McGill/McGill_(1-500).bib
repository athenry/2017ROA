@article{ ISI:000383828500037,
Author = {Gholami, Farnood and Nasri, Mostafa and Kovecses, Jozsef and Teichmann,
   Marek},
Title = {{A linear complementarity formulation for contact problems with
   regularized friction}},
Journal = {{MECHANISM AND MACHINE THEORY}},
Year = {{2016}},
Volume = {{105}},
Pages = {{568-582}},
Month = {{NOV}},
Abstract = {{A mathematical formulation in terms of a linear complementarity problem
   with regularized friction is introduced for multibody contact problems.
   In this approach, contacts are characterized based on kinematic
   constraints while the friction forces are simultaneously regularized and
   incorporated into the formulation. The variables of the resulting linear
   complementarity problem are only the normal forces. The main advantage
   of this formulation is that the dimension of the resulting linear
   complementarity problem is significantly less than its counterpart
   formulations in the literature, and hente, faster simulations can be
   achieved. The proposed formulation is examined for a set of benchmark
   examples yielding promising results. (C) 2016 Elsevier Ltd. All rights
   reserved.}},
