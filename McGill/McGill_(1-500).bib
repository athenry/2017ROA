@article{ ISI:000386070400022,
Author = {Delgado, Oscar and Labeau, Fabrice},
Title = {{Uplink Energy-Efficient Load Balancing Over Multipath Wireless Networks}},
Journal = {{IEEE WIRELESS COMMUNICATIONS LETTERS}},
Year = {{2016}},
Volume = {{5}},
Number = {{4}},
Pages = {{424-427}},
Month = {{AUG}},
Abstract = {{This letter studies the case of multi-interface mobile devices
   transmitting video traffic over multiple interfaces simultaneously
   (uplink). We examine three important issues. First, how to dynamically
   distribute traffic among each network interface such that the load is
   balanced. Second, how to minimize the mobile device's power consumption.
   Third, how to reduce packet reordering without increasing the end-to-end
   delay. We propose an analytical model and develop a green
   energy-efficient load balancing algorithm that finds a sub-optimal
   solution. Extensive simulations under a practical scenario demonstrate
   that our proposed algorithm manages to reduce power consumption without
   sacrificing the quality of service.}},
