@article{ ISI:000390648100004,
Author = {Kabir, Muhammad and Khazaka, Roni},
Title = {{Loewner Matrix Macromodeling for Y-Parameter Data With a Priori D Matrix
   Extraction}},
Journal = {{IEEE TRANSACTIONS ON MICROWAVE THEORY AND TECHNIQUES}},
Year = {{2016}},
Volume = {{64}},
Number = {{12, 1}},
Pages = {{4098-4107}},
Month = {{DEC}},
Abstract = {{Macromodeling techniques using Loewner matrix (LM) interpolation were
   proposed recently as a way to generate time-domain macromodels based on
   simulated Y-parameters. These approaches scale very well with respect to
   the number of ports as well as the number of poles in the system.
   However, these methods become less efficient in terms of accuracy and
   passivity for Y-parameters obtained using electromagnetic simulators. In
   this paper, we propose an LM-based interpolation technique that is
   applicable for large-scale distributed systems described by full-wave
   Y-parameters. An algorithm to approximate and extract the port impedance
   matrix D directly from the data is proposed. Additionally, an order
   selection scheme is proposed that results in an accurate macromodel
   while maintaining passivity. The efficiency and accuracy of the proposed
   approach is illustrated using comparisons with a standard technique.}},
