Language = {{English}},
Affiliation = {{Meunier, JL (Reprint Author), McGill Univ, Dept Chem Engn, Plasma Proc Lab, Montreal, PQ, Canada.
   Meunier, J. -L.; Mendoza-Gonzalez, N. -Y.; Pristavita, R.; Binny, D.; Berk, D., McGill Univ, Dept Chem Engn, Plasma Proc Lab, Montreal, PQ, Canada.}},
DOI = {{10.1007/s11090-014-9524-6}},
ISSN = {{0272-4324}},
