@article{ ISI:000335095600039,
Author = {Kaplin, Cory and Brochu, Mathieu},
Title = {{The effect of grain size on the oxidation of NiCoCrAlY}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{2014}},
Volume = {{301}},
Number = {{SI}},
Pages = {{258-263}},
Month = {{MAY 15}},
Abstract = {{Ultrafine NiCoCrAlY samples have been obtained through cryomilling of
   NiCoCrAlY feedstock powder and sintering via SPS, these samples have
   been subjected to isothermal oxidation tests at 950, 1000 and 1050
   degrees C. Oxidation rates on the order of 10(-13) g(2)/cm(4)/s for
   samples oxidized at 950 degrees C, 10(-11) g(2)/cm(4)/s at 1000 degrees
   C and 10(-12)g(2)/cm(4)/s at 1050 degrees C have been found. In all
   cases cryomilled samples demonstrate lower overall oxidation rates than
   their conventional counterparts. Lower overall oxidation rates found in
   the cryomilled samples are ascribed to an earlier transition trough the
   metastable alumina phases, resulting in a stable slow growing
   alpha-Al2O3. Characterization by XRD and SEM corresponds well with this
   assessment. Lower oxidation rates seen at 1050 than at 1000 degrees C in
   both conventional and cryomilled samples is the result of a change from
   oxidation dominated by theta-alumina to oxidation dominated by
   alpha-alumina, as the theta-alpha transition speeds up with increasing
   temperature. This change in oxidation regime reduces, but does not
   eliminate, the beneficial impact of smaller grain size on oxidation
   rate. (C) 2014 Elsevier B.V. All rights reserved.}},
