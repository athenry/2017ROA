@article{ ISI:000331949000012,
Author = {Didar, Tohid Fatanat and Bowey, Kristen and Almazan, Guillermina and
   Tabrizian, Maryam},
Title = {{A Miniaturized Multipurpose Platform for Rapid, Label- Free, and
   Simultaneous Separation, Patterning, and In Vitro Culture of Primary and
   Rare Cells}},
Journal = {{ADVANCED HEALTHCARE MATERIALS}},
Year = {{2014}},
Volume = {{3}},
Number = {{2}},
Pages = {{253-260}},
Month = {{FEB}},
Abstract = {{Given that current cell isolation techniques are expensive, time
   consuming, yield low isolation purities, and/or alter target cell
   properties, a versatile, cost effective, and easy-to-operate microchip
   with the capability to simultaneously separate, capture, pattern, and
   culture rare and primary cells in vitro is developed. The platform is
   based on target cell adhesion onto the micro-fabricated interfaces
   produced by microcontact printing of cell-specific antibodies. Results
   show over 95\% separation efficiency in less than 10 min for the
   separation of oligodendrocyte progenitor cells (OPCs) and cardiomyocytes
   from rat brain and heart mixtures, respectively. Target cell attachment
   and single cell spreading can be precisely controlled on the basis of
