@article{ ISI:000332471700003,
Author = {Aghabarati, Ali and Webb, Jon P.},
Title = {{Multilevel Preconditioning for Time-Harmonic Eddy-Current Problems
   Solved With Hierarchical Finite Elements}},
Journal = {{IEEE TRANSACTIONS ON MAGNETICS}},
Year = {{2014}},
Volume = {{50}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{A multilevel preconditioner is described for high-order finite element
   analysis of eddy currents using the T - Omega formulation. The
   preconditioner combines p-type multiplicative Schwartz for the higher
   order degrees of freedom and auxiliary space preconditioning for the
   lowest order. Algebraic multigrid is applied to the first-order scalar
   spaces. The new preconditioner performs four times faster than a
   standard preconditioner on a test problem with 13.3 million unknowns.}},
