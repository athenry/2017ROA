@article{ ISI:000334159500012,
Author = {Webb, J. P.},
Title = {{hp-Adaption for Computing Scattering Parameters on Tetrahedral Meshes}},
Journal = {{ELECTROMAGNETICS}},
Year = {{2014}},
Volume = {{34}},
Number = {{3-4, SI}},
Pages = {{298-306}},
Month = {{APR 3}},
Abstract = {{An adaptive strategy is described in this article for finding the
   scattering parameters of microwave devices by the finite-element method.
   The strategy involves increasing both the number of elements
   (h-refinement) and their polynomial order (p-refinement). Any set of
   vector and hierarchical elements can be used, but it is important to use
   an h-refinement scheme that can preserve the polynomial order of the
   elements. Here the elements are tetrahedral and up to the third order.
   The hp-strategy p-refines an element until the limit is reached, then
   h-refines it. Results are given for adaptive calculation of the
   reflection coefficient of obstacles in a rectangular waveguide.}},
