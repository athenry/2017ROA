@article{ ISI:000338979700066,
Author = {Salehzadeh, O. and Tran, N. H. and Liu, X. and Shih, I. and Mi, Z.},
Title = {{Exciton Kinetics, Quantum Efficiency, and Efficiency Droop of Monolayer
   MoS2 Light-Emitting Devices}},
Journal = {{NANO LETTERS}},
Year = {{2014}},
Volume = {{14}},
Number = {{7}},
Pages = {{4125-4130}},
Month = {{JUL}},
Abstract = {{We have investigated the quantum efficiency of monolayer MoS2
   light-emitting devices through detailed temperature and power-dependent
   photoluminescence studies and rate equation analysis. The internal
   quantum efficiency can reach 45 and 8.3\% at 83 and 300 K, respectively.
   However, efficiency droop is clearly measured with increasing carrier
   injection due to the unusually large Auger recombination coefficient,
   which is found to be similar to 10(-24) cm(6)/s at room temperature,
   nearly 6 orders of magnitude higher than that of conventional bulk
   semiconductors. The significantly elevated Auger recombination in the
   emerging two-dimensional (2D) semiconductors is primarily an indirect
   process and is attributed to the abrupt bounding surfaces and the
   enhanced correlation, mediated by magnified Coulomb interactions,
   between electrons and holes confined in a 2D structure.}},
Publisher = {{AMER CHEMICAL SOC}},
Address = {{1155 16TH ST, NW, WASHINGTON, DC 20036 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Mi, Z (Reprint Author), McGill Univ, Dept Elect \& Comp Engn, 3480 Univ St, Montreal, PQ H3A 0E9, Canada.
