@article{ ISI:000333784200009,
Author = {Ngabe, Barnabe and Finch, James A.},
Title = {{Determination of specific heat capacity of sulphide materials at
   temperatures below 100 degrees C in presence of moisture}},
Journal = {{MINERALS ENGINEERING}},
Year = {{2014}},
Volume = {{58}},
Pages = {{73-79}},
Month = {{APR}},
Abstract = {{The specific heat capacity (C-p) of one copper and three nickel
   concentrates was determined using a self-heating apparatus and by drop
   calorimetry over the temperature range 50 to 80 degrees C in the
   presence of 6\% moisture. The C-p values from both techniques were
   comparable and shown to be measuring the same property. The C-p values
   were similar for all four concentrates increasing from ca 0.4 to 1.4J
   g(-1) K-1 as temperature increased from 50 to 80 degrees C. Uses of C-p
   to identify self-heating risk and to modify the Rosenblum standard test
   are discussed. (C) 2014 Elsevier Ltd. All rights reserved.}},
