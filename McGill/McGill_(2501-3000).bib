@article{ ISI:000340685600115,
Author = {Qiu, Meng and Zhuge, Qunbi and Chagnon, Mathieu and Gao, Yuliang and Xu,
   Xian and Morsy-Osman, Mohamed and Plant, David V.},
Title = {{Digital subcarrier multiplexing for fiber nonlinearity mitigation in
   coherent optical communication systems}},
Journal = {{OPTICS EXPRESS}},
Year = {{2014}},
Volume = {{22}},
Number = {{15}},
Pages = {{18770-18777}},
Month = {{JUL 28}},
Abstract = {{In this work we experimentally investigate the improved intra-channel
   fiber nonlinearity tolerance of digital subcarrier multiplexed (SCM)
   signals in a single-channel coherent optical transmission system. The
   digital signal processing (DSP) for the generation and reception of the
   SCM signals is described. We show experimentally that the SCM signal
   with a nearly-optimum number of subcarriers can extend the maximum reach
   by 23\% in a 24 GBaud DP-QPSK transmission with a BER threshold of 3.8 x
   10(-3) and by 8\% in a 24 GBaud DP-16-QAM transmission with a BER
   threshold of 2 x 10(-2). Moreover, we show by simulations that the
   improved performance of SCM signals is observed over a wide range of
   baud rates, further indicating the merits of SCM signals in baud-rate
   flexible agile transmissions and future high-speed optical transport
   systems. (C)2014 Optical Society of America}},
