@article{ ISI:000339280200029,
Author = {Gleeson, Tom and Moosdorf, Nils and Hartmann, Jens and van Beek, L. P.
   H.},
Title = {{A glimpse beneath earth's surface: GLobal HYdrogeology MaPS (GLHYMPS) of
   permeability and porosity}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2014}},
Volume = {{41}},
Number = {{11}},
Pages = {{3891-3898}},
Month = {{JUN 16}},
Abstract = {{The lack of robust, spatially distributed subsurface data is the key
   obstacle limiting the implementation of complex and realistic
   groundwater dynamics into global land surface, hydrologic, and climate
   models. We map and analyze permeability and porosity globally and at
   high resolution for the first time. The new permeability and porosity
   maps are based on a recently completed high-resolution global lithology
   map that differentiates fine and coarse-grained sediments and
   sedimentary rocks, which is important since these have different
   permeabilities. The average polygon size in the new map is similar to
   100 km(2), which is a more than hundredfold increase in resolution
   compared to the previous map which has an average polygon size of
   similar to 14,000 km(2). We also significantly improve the
   representation in regions of weathered tropical soils and permafrost.
   The spatially distributed mean global permeability similar to 10(-15)
   m(2) with permafrost or similar to 10(-14) m(2) without permafrost. The
   spatially distributed mean porosity of the globe is 14\%. The maps will
   enable further integration of groundwater dynamics into land surface,
   hydrologic, and climate models.}},
