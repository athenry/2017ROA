@article{ ISI:000340787400025,
Author = {Liao, Peicheng and Zhang, Chunshu and Lu, Xi and Shafiei, Mehrdad Mir
   and Cerutti, Isabella and Andriolli, Nicola and Liboiron-Ladouceur,
   Odile},
Title = {{Gain Effect on Scalable Energy-Proportional SOA-Based Optical Space
   Switches}},
Journal = {{IEEE PHOTONICS TECHNOLOGY LETTERS}},
Year = {{2014}},
Volume = {{26}},
Number = {{16}},
Pages = {{1683-1686}},
Month = {{AUG 15}},
Abstract = {{Energy-proportional optical interconnections are enabled by
   semiconductor optical amplifiers (SOAs) remaining off (or idle) when not
   routing data. In this letter, the physical layer scalability of
   SOA-based optical space switches configured with different gain and loss
   is assessed using an experimental recirculating loop. Results show that
   for a gain/loss configuration of 13.0 dB, optical multichannel packets
   successfully propagate through a series of 14 SOAs with a bit-error-rate
   <10(-9), which indicates that a single-stage space switch can scale up
   to 10(9) ports. Scalability improves for lower gain/loss configuration
   at the cost of more SOAs per routing path. As expected, scalability is
   limited by cross-gain modulation and optical signal-to-noise ratio
   degradation, but their detrimental impact depends on the architecture
   configuration.}},
