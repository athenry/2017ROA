Title = {{Macromodeling of Distributed Networks From Frequency-Domain Data Using
   the Loewner Matrix Approach}},
Journal = {{IEEE TRANSACTIONS ON MICROWAVE THEORY AND TECHNIQUES}},
Year = {{2012}},
Volume = {{60}},
Number = {{12, 2, SI}},
