@inproceedings{ ISI:000346481201063,
Author = {Tahat, Ashraf and Champagne, Benoit and D'Amours, Claude},
Book-Group-Author = {{IEEE}},
Title = {{Subspace Decomposition Approach to Multi-User MIMO Channel Estimation in
   SC-FDE Systems}},
Booktitle = {{2013 IEEE 24TH INTERNATIONAL SYMPOSIUM ON PERSONAL, INDOOR, AND MOBILE
   RADIO COMMUNICATIONS (PIMRC)}},
