@article{ ISI:000311129400015,
Author = {Nobari, Nona A. and Misra, Arun K.},
Title = {{Attitude Dynamics and Control of Satellites with Fluid Ring Actuators}},
Journal = {{JOURNAL OF GUIDANCE CONTROL AND DYNAMICS}},
Year = {{2012}},
Volume = {{35}},
Number = {{6}},
Pages = {{1855-1864}},
Month = {{NOV-DEC}},
Note = {{AIAA/AAS Astrodynamics Specialist Conference, Toronto, CANADA, AUG
   02-05, 2010}},
Organization = {{Amer Inst Aeronaut \& Astronaut; Amer Astronaut Soc}},
Abstract = {{Attitude control of satellites using fluid rings is studied in this
   paper. A complete dynamics model of a rigid satellite containing fluid
   rings is developed. It is noticed that when the fluid rings are used as
   passive controllers they can damp out all attitude disturbances if the
   satellite is in a circular orbit and only roll-yaw disturbances if the
   satellite is in an elliptic orbit. However, this passive damping effect
   is fairly slow. Hence, an active controller is developed to reduce the
   settling time. To add redundancy to the attitude control system (ACS) of
   the satellite, four fluid rings in a pyramidal configuration are used.
   Also, the effect of the failure of one fluid ring on the performance of
   the ACS of the satellite is tested. Finally, a hybrid controller that is
   composed of a sliding mode controller and a proportional integral
   derivative is designed in order to cope with the model uncertainties and
   eliminate chattering in the response of the system.}},
