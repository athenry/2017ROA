@article{ ISI:000309072800010,
Author = {Ahmad, Raja and Rochette, Martin},
Title = {{Chalcogenide microwire based Raman laser}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2012}},
Volume = {{101}},
Number = {{10}},
Month = {{SEP 3}},
Abstract = {{We present a Raman fiber laser based on a chalcogenide glass microwire.
   The microwire is pumped in the C-band, and the resulting laser
   oscillates in the L-band. The power conversion efficiency of the device
   is similar to 17\%, and the laser threshold is 23.2 dBm (pulse energy =
   4.65 pJ) in peak pump power. By extension, the device can be used for
   all-optical wavelength conversion of existing laser sources in the
   useful transmission window of chalcogenide, from the C-band up to a
   wavelength of similar to 10 mu m in the mid-infrared. (C) 2012 American
   Institute of Physics. {[}http://dx.doi.org/10.1063/1.4751850]}},
