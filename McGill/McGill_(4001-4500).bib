Author = {Monk, Scott and Leib, Harry},
Editor = {{Yao, JT and Dinh, A and Mehrandezh, M and Paranjape, R and Gelowitz, C}},
Title = {{A SELF EXCITING POINT PROCESS MODEL FOR NEURAL SPIKE SEQUENCES, AND ITS
   RATE ESTIMATION}},
Booktitle = {{2013 26TH ANNUAL IEEE CANADIAN CONFERENCE ON ELECTRICAL AND COMPUTER
   ENGINEERING (CCECE)}},
