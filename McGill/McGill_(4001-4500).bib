@article{ ISI:000310304900060,
Author = {AbdelGhany, M. and Ledwosinska, E. and Szkopek, T.},
Title = {{Theory of the suspended graphene varactor}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2012}},
Volume = {{101}},
Number = {{15}},
Month = {{OCT 8}},
Abstract = {{We report the theory and design equations of the suspended graphene
   varactor, wherein device capacitance is tuned by the voltage controlled
   deflection of a suspended graphene membrane. A simple model based on
   large membrane deflection predicts a capacitor tuning range of 76\%,
   with a membrane pull-in voltage tuneable from 1V to 30V. An intrinsic
   electric quality factor approaching 200 is predicted based on
