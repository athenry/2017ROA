@inproceedings{ ISI:000340874300099,
Author = {Al Haija, Ahmad Abu and Vu, Mai},
Book-Group-Author = {{IEEE}},
Title = {{Outage Analysis for Uplink Mobile-to-Mobile Cooperation}},
