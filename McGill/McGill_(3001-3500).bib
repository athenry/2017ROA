@article{ ISI:000329144300020,
Author = {Ngabe, Barnabe and Finch, James A.},
Title = {{Self-heating activation energy and specific heat capacity of sulphide
   mixtures at low temperature}},
Journal = {{MINERALS ENGINEERING}},
Year = {{2014}},
Volume = {{55}},
Pages = {{154-161}},
Month = {{JAN}},
Abstract = {{Energy of activation (E-a) and specific heat capacity (C-p) for mixtures
   of sulphide minerals that on their own do not self-heat (SH),
   sphalerite/pyrite, pyrite/galena, chalcopyrite/galena and
   sphalerite/galena, were determined using a self-heating apparatus at
   temperatures below 100 degrees C in the presence of moisture. The
   mixtures all gave E-a ranging from 22.0 to 27.8 kJ mol(-1), similar to
   the range reported for Ni- and Cu-concentrates. The E-a is close to that
   for partial oxidation of H2S which adds to the contention that the
   partial oxidation of H2S contributes to SH of sulphides at low
   temperature. The C-p values ranged from 0.152 to 1.071 JK(-1) g(-1) as
   temperature rose from 50 degrees C to 80 degrees C, similar to the
   reported findings on Ni- and Cu-concentrates. The role of galvanic
   interaction in promoting SH is tested by examining correlations with the
   rest potential difference of the sulphides in the mixture. (C) 2013
   Elsevier Ltd. All rights reserved.}},
