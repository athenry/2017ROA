@inproceedings{ ISI:000350044700066,
Author = {Robles-Rubio, Carlos A. and Brown, Karen A. and Bertolizio, Gianluca and
   Kearney, Robert E.},
Book-Group-Author = {{IEEE}},
Title = {{Automated Analysis of Respiratory Behavior for the Prediction of Apnea
   in Infants following General Anesthesia}},
Booktitle = {{2014 36TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
   MEDICINE AND BIOLOGY SOCIETY (EMBC)}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
Year = {{2014}},
