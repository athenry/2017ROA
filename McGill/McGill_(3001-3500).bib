Author = {Zhao, Ming-Min and Cai, Yunlong and Champagne, Benoit and Zhao, Minjian},
Book-Group-Author = {{IEEE}},
Title = {{Min-Max MSE Transceiver with Switched Preprocessing for MIMO
   Interference Channels}},
Booktitle = {{2014 IEEE 25TH ANNUAL INTERNATIONAL SYMPOSIUM ON PERSONAL, INDOOR, AND
