@inproceedings{ ISI:000343655306072,
Author = {Ghalehjegh, Sina Hamidi and Rose, Richard C.},
Book-Group-Author = {{IEEE}},
Title = {{TWO-STAGE SPEAKER ADAPTATION IN SUBSPACE GAUSSIAN MIXTURE MODELS}},
Booktitle = {{2014 IEEE INTERNATIONAL CONFERENCE ON ACOUSTICS, SPEECH AND SIGNAL
   PROCESSING (ICASSP)}},
