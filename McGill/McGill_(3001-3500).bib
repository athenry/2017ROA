@inproceedings{ ISI:000356049700005,
Author = {Mianroodi, Jamshid Rezaei and Tho Le-Ngoc},
Book-Group-Author = {{IEEE}},
Title = {{Distributed Fast Decodable Space-Frequency Coding for CoMP OFDM Cellular
   Networks}},
