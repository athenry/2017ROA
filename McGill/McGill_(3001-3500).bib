@article{ ISI:000326923300005,
Author = {He, Zhen and Shao, Yixin and Yang, Huamei and Liu, Meiyan},
Title = {{Early carbonation behaviour of no-clinker steel slag binder}},
Journal = {{ADVANCES IN CEMENT RESEARCH}},
Year = {{2013}},
Volume = {{25}},
Number = {{6}},
Pages = {{342-351}},
Month = {{DEC}},
Abstract = {{The use of steel slag as a no-clinker binder was studied. Steel slag was
   activated by CO2 to develop strength as a cementing material for
   building product applications. The performance of slag paste compacts
   formed by low water to slag ratio was characterised by X-ray diffraction
   analysis, thermal analysis and field emission scanning electron
   microscopy analysis affiliated with energy dispersive spectrometer
   analysis. Results showed that the mineral components of steel slag, such
   as calcium hydroxide and calcium silicates (C3S and C2S), were CO2
   reactive. The reaction products were strength contributing. Immediate
   carbonation of 2 h was capable of developing rapid early strength while
   promoting subsequent hydration. The reaction products of steel slag
   after carbonation and hydration were the combination of poorly
   crystalline CaCO3 intermingled with C-S-H gel, which was the
   characteristic of the early hardening structure of no-clinker steel
   slag.}},
