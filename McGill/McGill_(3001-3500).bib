@article{ ISI:000328669300010,
Author = {Kafiabad, Hossein Amini and Chan, Pak Wai and Haller, George},
Title = {{Lagrangian Detection of Wind Shear for Landing Aircraft}},
Journal = {{JOURNAL OF ATMOSPHERIC AND OCEANIC TECHNOLOGY}},
Year = {{2013}},
Volume = {{30}},
Number = {{12}},
Pages = {{2808-2819}},
Month = {{DEC}},
Abstract = {{Recent studies have shown that aerial disturbances affecting landing
   aircraft have a coherent signature in the Lagrangian aerial particle
   dynamics inferred from ground-based lidar scans. Specifically,
   attracting Lagrangian coherent structures (LCSs) mark the intersection
   of localized material upwelling within the cone of the lidar scan. This
   study tests the detection power of LCSs on historical landing data and
   corresponding pilot reports of disturbances from Hong Kong International
   Airport. The results show that a specific LCS indicator, the gradient of
   the finite-time Lyapunov exponent (FTLE) field along the landing path,
   is a highly efficient marker of turbulent upwellings. In particular, in
   the spring season, projected FTLE gradients closely approach the
   efficiency of the wind shear alert system currently in operation at the
   airport, even though the latter system relies on multiple sources of
   data beyond those used in this study. This shows significant potential
   for the operational use of FTLE gradients in the real-time detection of
   aerial disturbances over airports.}},
