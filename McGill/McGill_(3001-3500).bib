Author = {Vadnerkar, Abhishek and Figueiredo, Sabrina and Mayo, Nancy E. and
   Kearney, Robert E.},
Book-Group-Author = {{IEEE}},
Title = {{Classification of Gait Quality for Biofeedback to Improve Heel-to-toe
   Gait}},
Booktitle = {{2014 36TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
   MEDICINE AND BIOLOGY SOCIETY (EMBC)}},
