Title = {{A Statistical Solution to Efficiently Optimize the Design of an
   Inverter-fed Permanent Magnet Motor}},
Booktitle = {{2016 XXII INTERNATIONAL CONFERENCE ON ELECTRICAL MACHINES (ICEM)}},
Year = {{2016}},
