@article{ ISI:000375026600022,
Author = {Hamidizadeh, Sara and Alatawneh, Natheer and Chromik, Richard R. and
   Lowther, David A.},
Title = {{Comparison of Different Demagnetization Models of Permanent Magnet in
   Machines for Electric Vehicle Application}},
Journal = {{IEEE TRANSACTIONS ON MAGNETICS}},
Year = {{2016}},
Volume = {{52}},
Number = {{5, 1}},
Month = {{MAY}},
Note = {{22nd Conference on Soft Magnetic Materials (SMM), Sao Paulo, BRAZIL, SEP
   12-16, 2015}},
Organization = {{Brazilian Met, Mat \& Min Assoc}},
Abstract = {{The knee point in the B-H curve of a permanent magnet (PM) is considered
   crucial in electric motor applications. In the case of severe fault
   conditions, such as overheating or a short circuit in electric motors,
   the working point might fall below the knee point causing irreversible
   demagnetization of the PM. Hence, the remanence flux decreases and the
   motor operation would be reduced or stalled. In this paper, two
   demagnetization models of PMs are investigated, i.e., a linear model and
   an exponential model. Demagnetization curves for NdFeB were measured at
   temperatures varying from room temperature to 180 degrees C. It was
   found that all the parameters in the exponential model, including Br,
   H-jc, mu(r), and the fitting parameter K-1, are temperature dependent.
   Therefore, an exponential model can be developed as a function of
   temperature which allows a more efficient implementation within electric
   motor applications using PMs. However, comparison of the exponential and
   linear models indicates that the latter has a better accuracy near the
   knee point for the demagnetization curves of NdFeB magnets.}},
