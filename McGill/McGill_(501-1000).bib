@inproceedings{ ISI:000390094701221,
Author = {Hashemi, Seyyed Ali and Condo, Carlo and Gross, Warren J.},
Book-Group-Author = {{IEEE}},
Title = {{Matrix Reordering for Efficient List Sphere Decoding of Polar Codes}},
Booktitle = {{2016 IEEE INTERNATIONAL SYMPOSIUM ON CIRCUITS AND SYSTEMS (ISCAS)}},
Series = {{IEEE International Symposium on Circuits and Systems}},
Year = {{2016}},
