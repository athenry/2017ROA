@article{ ISI:000365612200001,
Author = {Sainoki, A. and Mitri, H. S.},
Title = {{Influence of undulating fault surface properties on its seismic waves
   during fault-slip}},
Journal = {{INTERNATIONAL JOURNAL OF MINING RECLAMATION AND ENVIRONMENT}},
Year = {{2016}},
Volume = {{30}},
Number = {{1}},
Pages = {{1-12}},
Month = {{JAN 2}},
Abstract = {{Seismic waves arising from fault-slips induced by mining activities in
   underground mines can inflict severe damage to mine openings. Laboratory
   experiments on the cataclastic rock-flour found on surfaces of
   mining-induced faults have revealed that intense shock pulses can arise
   due to the unloading of fault surface asperities during fault-slip. This
   paper focuses on investigating the effect of fault surface asperities on
   the intensity of the seismic waves. Dynamic modelling of fault-slip with
   a mine-wide model has revealed that particle velocity of the rockmass
   can increase substantially when unloading takes place. It is concluded
   that considering the occurrence of locally intense seismic waves is
   indispensable for optimising secondary support systems.}},
