@article{ ISI:000370454200021,
Author = {Bhattachatjee, Sourjya and Basnet, Mohan and Tufenkji, Nathalie and
   Ghoshal, Subhasis},
Title = {{Effects of Rhamnolipid and Carboxymethylcellulose Coatings on Reactivity
   of Palladium-Doped Nanoscale Zerovalent Iron Particles}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2016}},
Volume = {{50}},
Number = {{4}},
Pages = {{1812-1820}},
Month = {{FEB 16}},
Abstract = {{Nanoscale zerovalent iron (NZVI) particles are often coated with
   polymeric surface modifiers for improved colloidal stability and
   transport during remediation of contaminated aquifers. Doping the NZVI
   surface with palladium (Pd-NZVI) increases its reactivity to pollutants
   such as trichloroethylene (TCE). In this study, we investigate the
   effects of coating Pd-NZVI with two surface modifiers of very different
   molecular size: rhamnolipid (RL, anionic biosurfactant, M.W. 600 g
   mol(-1)) and carboxymethylcellulose (CMC, anionic polyelectrolyte, M.W.
   700 000 g mol(-1)) on TCE degradation. RL loadings of 13-133 mg TOC/g
   NZVI inhibited deposition of Pd in a concentration-dependent manner,
