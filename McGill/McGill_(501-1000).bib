@inproceedings{ ISI:000405493000027,
Author = {He, Chuan and Mussbacher, Gunter},
Book-Group-Author = {{IEEE}},
Title = {{Model-Driven Engineering and Elicitation Techniques: A Systematic
   Literature Review}},
Booktitle = {{2016 IEEE 24TH INTERNATIONAL REQUIREMENTS ENGINEERING CONFERENCE
   WORKSHOPS (REW)}},
Year = {{2016}},
Pages = {{180-189}},
