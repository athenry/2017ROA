Author = {Gao, Yuan and Cai, Yunlong and Shi, Qingjiang and Champagne, Benoit and
   Zhao, Minjian},
Book-Group-Author = {{IEEE}},
Title = {{JOINT TRANSCEIVER DESIGNS FOR SECURE COMMUNICATIONS OVER MIMO RELAY}},
