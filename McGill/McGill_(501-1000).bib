@inproceedings{ ISI:000390883100162,
Author = {Khan, Waqas and Nahon, Meyer},
Book-Group-Author = {{IEEE}},
Title = {{Modeling Dynamics of Agile Fixed-Wing UAVs for Real-Time Applications}},
Booktitle = {{2016 INTERNATIONAL CONFERENCE ON UNMANNED AIRCRAFT SYSTEMS (ICUAS)}},
