@inproceedings{ ISI:000410525800022,
Author = {Meguid, M. A. and Gao, Ge and Abouelkair, M. M. and Abdelrahman, M. Z.},
Editor = {{Mitri, H and Shnorhokian, S and Kumral, M and Sasmito, A and Sainoki, A}},
Title = {{Modelling the impact of particle flow on rigid structures: experimental
   and numerical investigations}},
Booktitle = {{OPERATIONAL AND ENVIRONMENTAL MINE HEALTH AND SAFETY PRACTICE AND
   INNOVATION}},
Year = {{2016}},
Pages = {{140-144}},
