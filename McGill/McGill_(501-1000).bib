@article{ ISI:000373055700002,
Author = {Djavid, Mehrdad and Mi, Zetian},
Title = {{Enhancing the light extraction efficiency of AlGaN deep ultraviolet
   light emitting diodes by using nanowire structures}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2016}},
Volume = {{108}},
Number = {{5}},
Month = {{FEB 1}},
Abstract = {{The performance of conventional AlGaN deep ultraviolet light emitting
   diodes has been limited by the extremely low light extraction efficiency
   (<10\%), due to the unique transverse magnetic (TM) polarized light
   emission. Here, we show that, by exploiting the lateral side emission,
   the extraction efficiency of TM polarized light can be significantly
   enhanced in AlGaN nanowire structures. Using the three-dimensional
   finite-difference time domain simulation, we demonstrate that the
   nanowire structures can be designed to inhibit the emission of guided
   modes and redirect trapped light into radiated modes. A light extraction
   efficiency of more than 70\% can, in principle, be achieved by carefully
   optimizing the nanowire size, nanowire spacing, and p-GaN thickness. (C)
   2016 AIP Publishing LLC.}},
