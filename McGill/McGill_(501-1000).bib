@inproceedings{ ISI:000389495600010,
Author = {Shahshahani, Amirhossein and Tolstikhin, Andrey and Zilic, Zeljko},
Book-Group-Author = {{IEEE}},
Title = {{Enabling Debug in IoT Wireless Development and Deployment With Security
   Considerations}},
Booktitle = {{2016 IEEE 25th North Atlantic Test Workshop (NATW)}},
Year = {{2016}},
Pages = {{53-58}},
Note = {{25th IEEE North Atlantic Test Workshop (NATW), Providence, RI, MAY
   09-11, 2016}},
Organization = {{IEEE; Cadence; ADVANTEST; ON Semiconductor; Mentor Graph; AdamsIP com;
   IEEE Providence Sect; IEEE Green Mountain Sect; IEEE Comp Soc}},
Abstract = {{This paper considers the debug needs of IoT devices and systems
   throughout their lifecycle, that also aims to preserve security required
   for IoT. A generic wireless debugging infrastructure over Bluetooth Low
   Energy (BLE) is proposed. We show how it is possible to balance the
   conflicting needs of security and visibility in a coherent framework
   based on leveraging an Android-powered device with protocol stack
   access. The concept of ``virtual fuses{''} is employed that allows
   selective access to the debug interface based on trust at stages
   starting from individual node development to the deployment and update
   of the system. The debug and security needs are solved with suitable
   trust management which allows selective activation or deactivation of
   debug resources throughout the system life. Lastly, the proposed system
   is implemented in a practical experiment.}},
