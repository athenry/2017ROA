@inproceedings{ ISI:000410525800013,
Author = {Abdellah, Wael and Mitri, Hani},
Editor = {{Mitri, H and Shnorhokian, S and Kumral, M and Sasmito, A and Sainoki, A}},
Title = {{Estimating the probability of unsatisfactory performance associated with
   the instability of mine developments}},
Booktitle = {{OPERATIONAL AND ENVIRONMENTAL MINE HEALTH AND SAFETY PRACTICE AND
   INNOVATION}},
Year = {{2016}},
Pages = {{84-89}},
