@inproceedings{ ISI:000400048100049,
Author = {Zlotnik, David E. and Forbes, James R.},
Book-Group-Author = {{IEEE}},
Title = {{Exteroceptive Measurement Filtering Embedded within an SO(3)-based
   Attitude Estimator}},
Booktitle = {{2016 IEEE 55TH CONFERENCE ON DECISION AND CONTROL (CDC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2016}},
