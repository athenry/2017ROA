@article{ ISI:000310593900003,
Author = {Pekguleryuz, M. O. and Masoumi, M. and Becerra, A.},
Title = {{Texture evolution in Mg-Mn and Mg-Mn-Ce alloys and the effect of as-cast
   grain size}},
Journal = {{PHILOSOPHICAL MAGAZINE}},
Year = {{2012}},
Volume = {{92}},
Number = {{31}},
Pages = {{3766-3779}},
Abstract = {{Texture evolution in rolled (400 degrees C) and annealed (450 degrees C)
   Mg-1\% Mn-based alloys containing different levels of Ce was examined.
   It was found that Ce refines the as-cast and rolled/annealed grain
   structure. The overall intensity of the basal texture decreases with Ce
   additions in both the rolled and subsequently annealed condition. A
   strong relationship was found between maximum intensity (M) of rolling
   and annealing textures and the as-cast effective grain size, d', i.e. M
   proportional to e(0.4d') (R-2 = 0.8), which was attributed to the role
   of plastic compatibility stresses which lead to grain boundary
   deformation altering the balance of deformation modes. Contributions
   from Ce solute effects and lattice parameter changes are also discussed.}},
