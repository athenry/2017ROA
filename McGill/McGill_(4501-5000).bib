@article{ ISI:000305479800002,
Author = {Vien, Nam H. and Nguyen, Ha H. and Tho Le-Ngoc},
Title = {{Performance Analysis of Smart Relaying with Equal Gain Combining}},
Journal = {{WIRELESS PERSONAL COMMUNICATIONS}},
Year = {{2012}},
Volume = {{65}},
Number = {{2}},
Pages = {{273-292}},
Month = {{JUL}},
Abstract = {{Relaying communications has been proposed as a way to provide spatial
   diversity. In general, one is interested in a relaying system that can
   achieve the maximal diversity order with a low system complexity. One
   enabling technique is equal gain combining (EGC) and its application in
   relaying systems is the main focus of this paper. In particular, the
   techniques of EGC and smart relaying are combined in the
   decode-and-forward (DF) processing method. It is shown that for a system
   with one relay and M-ary phase-shift-keying (M-PSK) modulation, maximal
   diversity orders of 2m and 2 are achieved over Nakagami-m and Hoyt
   fading environments, respectively. With K relays, simulation results
   suggest that the corresponding diversity orders are m(K + 1) and (K +
   1).}},
