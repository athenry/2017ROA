@inproceedings{ ISI:000325479800223,
Author = {Heard, David W. and Boselli, Julien and Gauvin, Raynald and Brochu,
   Mathieu},
Editor = {{Weiland, H and Rollett, AD and Cassada, WA}},
Title = {{RAPID SOLIDIFICATION OF A NEW GENERATION ALUMINUM-LITHIUM ALLOY VIA
   ELECTROSPARK DEPOSITION}},
Booktitle = {{PROCEEDINGS OF THE 13TH INTERNATIONAL CONFERENCE ON ALUMINUM ALLOYS
   (ICAA13)}},
Year = {{2012}},
Pages = {{1469-1474}},
Note = {{13th International Conference on Aluminum Alloys (ICAA), Carnegie Mellon
   Univ, Pittsburgh, PA, JUN 03-07, 2012}},
Organization = {{TMS}},
Abstract = {{Electrospark deposition (ESD) is a rapid solidification processing
   technique capable of depositing a metal onto a conductive substrate. The
   short pulse duration and high pulse frequency, combined with the small
   amount of material transferred during each pulse, results in high
   cooling rates being realized, on the order of 10(5)-10(6) C/sec. This
   study investigates the ability to induce solute trapping behavior, for a
   new generation aluminum-lithium alloy, AA2199, using ESD.
   Time-of-Flight Secondary-Ion-Mass-Spectroscopy (TOF-SIMS) and X-Ray
   Photoelectron Spectroscopy (XPS) were employed to determine solute
   distribution. Scanning Electron Microscopy and Scanning Transmission
   Electron Microscopy analysis were performed to investigate the
   microstructure of the rapidly solidified Al-Li deposits.
