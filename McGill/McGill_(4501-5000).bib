@inproceedings{ ISI:000310623800237,
Author = {Akbari, Mohsen and Labeau, Fabrice},
Title = {{OUTPUT RECONSTRUCTION IN AN OFB WITH INSTANTANEOUS ERASURE USING
   PARITY-CHECK MATRIX}},
