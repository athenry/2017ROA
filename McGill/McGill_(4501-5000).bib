@inproceedings{ ISI:000312901703053,
Author = {Ammar, Moataz and Joos, Geza},
Book-Group-Author = {{IEEE}},
Title = {{Coordinated Active/Reactive Power Control for Flicker Mitigation in
   Distributed Wind Power}},
Booktitle = {{2012 IEEE ENERGY CONVERSION CONGRESS AND EXPOSITION (ECCE)}},
Series = {{IEEE Energy Conversion Congress and Exposition}},
