@article{ ISI:000307524300013,
Author = {Reid, Thomas and Baruzzi, Guido S. and Habashi, Wagdi G.},
Title = {{FENSAP-ICE: Unsteady Conjugate Heat Transfer Simulation of
   Electrothermal De-Icing}},
Journal = {{JOURNAL OF AIRCRAFT}},
Year = {{2012}},
Volume = {{49}},
Number = {{4}},
Pages = {{1101-1109}},
Month = {{JUL-AUG}},
Note = {{2nd AIAA Atmospheric and Space Environments Conference, Toronto, CANADA,
   AUG 09-12, 2010}},
Organization = {{AIAA}},
Abstract = {{This paper presents a truly unsteady approach for the numerical
   simulation of in-flight electrothermal anti-icing or de-icing, using a
   conjugate heat transfer technique. This numerical approach has been
   implemented in FENSAP-ICE to compute the complex heat transfer phenomena
   occurring during in-flight de-icing with multiple heating elements
   following independent cycling. At each time step, the energy fluxes
   through the aircraft's solid skin, the melting ice layer, the liquid
   water film, and the external fluid are computed. The ice shape is then
   modified by taking into account the opposing mass balance effects of ice
   accreting due to the impact of supercooled droplets and/or water
   runback, and the partial or total melting of the existing ice layer due
   to heating. The results of the verification of this phase-change
   conduction code are presented, followed by a study of intercycle
   de-icing on a wing, showing intercycle ice growth.}},
