@article{ ISI:000315516200021,
Author = {Jalaleddini, Kian and Moezzi, Kaveh and Aghdam, Amir G. and Alasti,
   Mehdi and Tarokh, Vahid},
Title = {{Rate Assignment in Wireless Networks: Stability Analysis and Controller
   Design}},
Journal = {{IEEE TRANSACTIONS ON CONTROL SYSTEMS TECHNOLOGY}},
Year = {{2013}},
Volume = {{21}},
Number = {{2}},
Pages = {{521-529}},
Month = {{MAR}},
Abstract = {{In this brief, data-rate assignment in IS-856 uplink (reverse link) is
   studied. The problem is first formulated using an interference model,
   and then a dynamic control strategy is developed for efficient rate
   assignment. In the first step, the controller is designed for the
   special case when the number of users in the network is fixed. Then, the
   designed controller is further developed for a dynamic network (where
   the number of users is subject to change) to achieve the desired
   performance. To this end, the network is formulated in the framework of
   switched systems, where any new activation or deactivation of users is
   considered as switching from one system to another. The controllers
   obtained are then modified properly to retain network stability and
   performance in the presence of time-delay. Simulation results are
   presented to elucidate the effectiveness of the proposed approach.}},
