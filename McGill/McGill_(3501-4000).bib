@article{ ISI:000317167300017,
Author = {Erythropel, Hanno C. and Dodd, Patrick and Leask, Richard L. and Maric,
   Milan and Cooper, David G.},
Title = {{Designing green plasticizers: Influence of alkyl chain length on
   biodegradation and plasticization properties of succinate based
   plasticizers}},
Journal = {{CHEMOSPHERE}},
Year = {{2013}},
Volume = {{91}},
Number = {{3}},
Pages = {{358-365}},
Month = {{APR}},
Abstract = {{Phthalate diesters such as di (2-ethylhexyl) phthalate (DEHP) are
   considered ubiquitous contaminants and are poorly biodegraded in the
   environment. Moreover, both the parent compound and stable metabolites
   such as mono (2-ethylhexyl) phthalate (MEHP) are linked to several
   negative impacts on the environment and human health. Earlier work
   established that saturated diester compounds, such as succinates, showed
   better biodegradation characteristics and comparable plasticizer
   properties compared to DEHP. In this work we examine the effect of alkyl
   chain length of succinate molecules on plasticizer and biodegradation
   properties. This included both the side chains (n-ethyl to n-octyl) as
   well as substituents on the middle part of the succinate molecule. We
   showed that the common soil bacterium Rhodococcus rhodocrous could
   rapidly break down all unsubstituted succinates, without the appearance
   of stable metabolites. Furthermore, the organisms used the plasticizer
   metabolites as carbon source. The introduction of a large cyclohexyl
   substituent on the succinate resulted in a poorer degradation rate.
   Glass Transition Temperature (Tg) measurements were performed to
   evaluate plasticizer properties and showed that longer side chains
   reduced the Tg more efficiently, while large cyclohexyl substituents on
   the succinate decreased this effect. However, all compounds performed
   better or equal to DEHP at reducing the Tg. (C) 2012 Elsevier Ltd. All
   rights reserved.}},
