@article{ ISI:000315162800001,
Author = {Li, Jia and Huang, Tianye and Chen, Lawrence R.},
Title = {{A Comprehensive Study of Actively Mode-Locked Fiber Optical Parametric
   Oscillators for High-Speed Pulse Generation}},
