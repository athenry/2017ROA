@article{ ISI:000317000400002,
Author = {Al-Kadry, Alaa and Rochette, Martin},
Title = {{Maximized Soliton Self-Frequency Shift in Non-Uniform Microwires by the
   Control of Third-Order Dispersion Perturbation}},
Journal = {{JOURNAL OF LIGHTWAVE TECHNOLOGY}},
Year = {{2013}},
Volume = {{31}},
Number = {{9}},
Pages = {{1462-1467}},
Month = {{MAY 1}},
Abstract = {{We present a simple method based on the soliton perturbative theory to
   design microwires of non-uniform diameter profiles. In contrast to
   previous methods, the one presented here relies on minimizing the
   soliton perturbation by third order dispersion (TOD) while taking into
   account the change of the soliton local duration along the microwire.
   The method leads to a design that maximizes the soliton self-frequency
   shift in non-uniform microwires. The microwire design comprises a unique
   dispersion profile such that a wavelength-shifting soliton experiences
   only weak perturbations from the TOD and avoids shedding its energy into
   the dispersive waves. The TOD perturbation is quantified with an
   analytic expression that is kept below a threshold value, thus keeping a
   soliton weakly perturbed by TOD in every position within the microwire.
   Numerical simulations are conducted to check the validity of the method.
   We consider a fundamental soliton centered at a wavelength of 2000 nm
   propagating in As Se microwires of length as short as 10 cm. The results
   show that optimized non-uniform diameter profile allows the tuning of
   the self-frequency shifted soliton over a spectral range of 860 nm.}},
