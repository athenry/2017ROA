@article{ ISI:000323821100008,
Author = {Malas, K. W. and Jordens, A. and Mirnezami, M. and Chu, P. and Gauvin,
   R. and Waters, K. E.},
Title = {{Surface characterisation of fergusonite}},
Journal = {{CANADIAN METALLURGICAL QUARTERLY}},
Year = {{2013}},
Volume = {{52}},
Number = {{3, SI}},
Pages = {{278-284}},
Month = {{JUL}},
Abstract = {{There is currently very little information on the physico-chemical
   properties of rare earth element bearing minerals, or on their
   processing. With the increasing demand for rare earths, this is
   something that needs to be addressed. This paper introduces some surface
   chemistry analysis of fergusonite (YNbO4), and the bubble particle
   attachment. Zeta potential measurements indicate adsorption of sodium
   oleate and, to a lesser degree, potassium amyl xanthate onto the
   surface, and an increase in attachment to an air bubble is observed at
   pH 4.5 and 6.5. At a pH of 10.5, no attachment is observed, whether in
   the presence of the reagents or not. This analysis, although initial,
   will be used to initiate flotation tests, in order to facilitate the
   selective recovery of fergusonite. Two extensively characterised
   minerals, one hydrophilic (quartz) and one hydrophobic (galena) were
   also investigated as a comparison.}},
