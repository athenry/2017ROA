@article{ ISI:000314691000001,
Author = {Hill, Reghan J. and Li, Fei},
Title = {{Hydrodynamic drag coefficient for soft core-shell nanoparticles in
   hydrogels}},
Journal = {{CHEMICAL ENGINEERING SCIENCE}},
Year = {{2013}},
Volume = {{89}},
Pages = {{1-9}},
Month = {{FEB 15}},
Abstract = {{Nanoparticle transport in porous materials, including nanoparticle gel
   electrophoresis, hinges on knowledge of the hydrodynamic drag
   coefficient. Here, we compute the drag force on a soft core-shell sphere
   translating in a porous medium from a continuum hydrodynamic model in
   which slow viscous flow in the soft shell and embedding porous medium
   are modelled using Brinkman's equations. This model unifies the Brinkman
   force for bare spheres in porous media and the drag force of Masliyah et
   al. (1987) for soft core-shell spheres in pure viscous fluids. We
   compare the drag force with the Brinkman force evaluated using the
   composite sphere hydrodynamic radius, showing that this convenient
   approximation is reasonable when the Brinkman screening length of the
   embedding porous medium is large compared to the core radius; otherwise,
   the Brinkman force can significantly underestimate the force. (C) 2012
   Elsevier Ltd. All rights reserved.}},
