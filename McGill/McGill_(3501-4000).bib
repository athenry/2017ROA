@article{ ISI:000317390100010,
Author = {AlOtaibi, B. and Harati, M. and Fan, S. and Zhao, S. and Nguyen, H. P.
   T. and Kibria, M. G. and Mi, Z.},
Title = {{High efficiency photoelectrochemical water splitting and hydrogen
   generation using GaN nanowire photoelectrode}},
Journal = {{NANOTECHNOLOGY}},
Year = {{2013}},
Volume = {{24}},
Number = {{17}},
Month = {{MAY 3}},
Abstract = {{We have studied the photoelectrochemical properties of both undoped and
   Si-doped GaN nanowire arrays in 1 mol l(-1) solutions of hydrogen
   bromide and potassium bromide, which were used separately as
   electrolytes. It is observed that variations of the photocurrent with
   bias voltage depend strongly on the n-type doping in GaN nanowires in
   both electrolytes, which are analyzed in the context of GaN surface band
   bending and its variation with the incorporation of Si-doping. Maximum
   incident-photon-to-current-conversion efficiencies of similar to 15\%
   and 18\% are measured for undoped and Si-doped GaN nanowires under
   similar to 350 nm light illumination, respectively. Stable hydrogen
   generation is also observed at a zero bias potential versus the
   counter-electrode.}},
