@article{ ISI:000316827300003,
Author = {Fossati, M. and Guardone, A. and Vigevano, L. and Xu, K.},
Title = {{Kinetic Node-Pair Formulation for Two-Dimensional Flows from Continuum
   to Transitional Regime}},
Journal = {{AIAA JOURNAL}},
Year = {{2013}},
