@article{ ISI:000317294000013,
Author = {Girard-Lauriault, Pierre-Luc and Dietrich, Paul M. and Gross, Thomas and
   Wirth, Thomas and Unger, Wolfgang E. S.},
Title = {{Chemical Characterization of the Long-Term Ageing of Nitrogen-Rich
   Plasma Polymer Films under Various Ambient Conditions}},
Journal = {{PLASMA PROCESSES AND POLYMERS}},
Year = {{2013}},
Volume = {{10}},
Number = {{4}},
Pages = {{388-395}},
Month = {{APR}},
Abstract = {{Nitrogen rich plasma polymer films (L-PPE:N) were prepared by
   low-pressure RF plasma using a 1:1 mixture of ethylene and ammonia and
   aged for 345d in four different ambient conditions: (i) at room
   temperature (RT) in air; (ii) at RT in nitrogen; (iii) at 20 degrees C
   in air and; (iv) at 20 degrees C in nitrogen. The films were analyzed by
